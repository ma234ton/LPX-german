    # mx "Vendré a verte de todas maneras... {w}Cuando ya te sientas más tranquilo."
    mx "Ich werde dich auf jeden Fall besuchen kommen... {w}Wenn du dich wieder ruhiger fühlst."

# game/hmaxy.rpy:2472
translate german PA_HM_EV1_e81aab3f:

    # mx "¡Adiós, Lucky Boy!"
    mx "Auf Wiedersehen, Lucky Boy!"

# game/hmaxy.rpy:2501
translate german PA_HM_EV2_21d18d78:

    # mx "¡Hola, Lucky Boy!"
    mx "Hallo, Lucky Boy!"

# game/hmaxy.rpy:2503
translate german PA_HM_EV2_9205f24a:

    # mx "Hola de nuevo lindurita... {w}¿Cuál es tu nombre?"
    mx "Hallo nochmal, meine Schöne... {w}Wie heisst du?"

# game/hmaxy.rpy:2505
translate german PA_HM_EV2_353e4c5e:

    # p "Primrose... {w}B-barrie..."
    p "Primrose... {w}B-barrie..."

# game/hmaxy.rpy:2507
translate german PA_HM_EV2_9c84d36f:

    # mx "¡Eres una dulzura!"
    mx "Du bist so süss!"

# game/hmaxy.rpy:2508
translate german PA_HM_EV2_02d3469a:

    # mx "¡Casi como una pequeña ternurita!"
    mx "Eine richtig süsse!"

# game/hmaxy.rpy:2509
translate german PA_HM_EV2_57de866f:

    # mx "Ternurita es un bonito nombre, creo que te llamaré de esa forma desde ahora."
    mx "Süsse ist ein schöner Name, ich glaube, ich werde dich ab jetzt so nennen."

# game/hmaxy.rpy:2511
translate german PA_HM_EV2_e7e50f69:

    # p "Está bien..."
    p "Okay..."

# game/hmaxy.rpy:2513
translate german PA_HM_EV2_89dc2c5a:

    # mx "¡No deberías estar tan cabizbaja!"
    mx "Du solltest nicht so niedergeschlagen sein!"

# game/hmaxy.rpy:2514
translate german PA_HM_EV2_8d6d561d:

    # mx "¡Levanta esos ánimos!"
    mx "Kopf hoch!"

# game/hmaxy.rpy:2516
translate german PA_HM_EV2_c8ea8a4a:

    # p "..."
    p "..."

# game/hmaxy.rpy:2518
translate german PA_HM_EV2_c516cade:

    # mx "¿Sabes que le sucede a la ternurita, Lucky boy?"
    mx "Weisst du, was mit der kleinen Süssen passiert ist, Lucky Boy?"

# game/hmaxy.rpy:2520
translate german PA_HM_EV2_6de424cb:

    # mc "Podría apostar a que te tiene miedo."
    mc "Ich könnte wetten, dass sie Angst vor dir hat."

# game/hmaxy.rpy:2522
translate german PA_HM_EV2_f12c6cf5:

    # mx "¿De verdad?"
    mx "Wirklich?"

# game/hmaxy.rpy:2524
translate german PA_HM_EV2_4dda1a8f:

    # mx "¿Por qué sientes miedo, ternurita?"
    mx "Warum hast du Angst, mein Liebling?"

# game/hmaxy.rpy:2526
translate german PA_HM_EV2_c8ea8a4a_1:

    # p "..."
    p "..."

# game/hmaxy.rpy:2527
translate german PA_HM_EV2_e5b90fa7:

    # mc "Porque manoseaste sus pechos sin previo aviso la última vez que viniste aquí."
    mc "Weil du ihr beim letzten Mal, als du hier warst, ohne Vorwarnung an die Brüste gefasst hast."

# game/hmaxy.rpy:2529
translate german PA_HM_EV2_17cfcdb3:

    # p "¡Lucky!"
    p "Lucky!"

# game/hmaxy.rpy:2531
translate german PA_HM_EV2_31e3aeb2:

    # mx "¿Es eso cierto ternurita?"
    mx "Ist das wahr, meine Süsse?"

# game/hmaxy.rpy:2533
translate german PA_HM_EV2_5f8e22ef:

    # p "Quizás..."
    p "Vielleicht..."

# game/hmaxy.rpy:2535
translate german PA_HM_EV2_8e601ca1:

    # mx "No era mi intención hacerte sentir incómoda."
    mx "Ich wollte dich nicht in unangenehme Gefühle bringen."

# game/hmaxy.rpy:2536
translate german PA_HM_EV2_307856b1:

    # mx "¿Me perdonas?"
    mx "Verzeihst du mir?"

# game/hmaxy.rpy:2537
translate german PA_HM_EV2_05a1e1c8:

    # mx "Prometo que no volveré a hacer algo como eso."
    mx "Ich verspreche, dass ich so etwas nie wieder tun werde."

# game/hmaxy.rpy:2538
translate german PA_HM_EV2_236c6003:

    # mx "Es solo que a veces me dejo llevar un poco más de la cuenta."
    mx "Es ist nur so, dass ich mich manchmal etwas zu sehr mitreißen lasse."

# game/hmaxy.rpy:2540
translate german PA_HM_EV2_010d75a4:

    # p "Está bien, te perdono."
    p "Okay, ich vergebe dir."

# game/hmaxy.rpy:2542
translate german PA_HM_EV2_0143d182:

    # mc "Me gusta ver como hacen las paces."
    mc "Ich finde es cool zu sehen, wie ihr euch versöhnt."

# game/hmaxy.rpy:2544
translate german PA_HM_EV2_a20ac038:

    # p "¿Por qué no nos sentamos y empezamos de nuevo?"
    p "Warum setzen wir uns nicht hin und fangen noch einmal von vorne an?"

# game/hmaxy.rpy:2546
translate german PA_HM_EV2_0eb27106:

    # mc "¿Qué quieres decir con eso?"
    mc "Was meinst du damit?"

# game/hmaxy.rpy:2548
translate german PA_HM_EV2_9f19e6dd:

    # p "Creo que he sido un poco prejuiciosa con ella."
    p "Ich glaube, ich war ihr gegenüber etwas voreingenommen."

# game/hmaxy.rpy:2550
translate german PA_HM_EV2_e4e9d49c:

    # mc "Tampoco voy a negar que Maxy es un poco excéntrica."
    mc "Ich werde auch nicht leugnen, dass Maxy ein wenig exzentrisch ist."

# game/hmaxy.rpy:2552
translate german PA_HM_EV2_a1504adf:

    # mx "Y también puedo ser muy cariñosa y apasionada."
    mx "Und ich kann auch sehr liebevoll und leidenschaftlich sein."

# game/hmaxy.rpy:2553
translate german PA_HM_EV2_f35718aa:

    # mx "¿Por qué no nos sentamos a conversar?"
    mx "Warum setzen wir uns nicht zusammen und reden darüber?"

# game/hmaxy.rpy:2555
translate german PA_HM_EV2_aefcb28e:

    # p "Está bien, nos ayudaría a conocernos mejor."
    p "Okay, das würde helfen, uns besser kennenzulernen."

# game/hmaxy.rpy:2557
translate german PA_HM_EV2_6e555d49:

    # mc "¿Quieren algo para beber?"
    mc "Möchtest du etwas trinken?

# game/hmaxy.rpy:2559
translate german PA_HM_EV2_0f11dcb3:

    # mx "Para mí un café dulce."
    mx "Für mich einen süssen Kaffee."

# game/hmaxy.rpy:2561
translate german PA_HM_EV2_1a2027cb:

    # mc "¿Y tú, Prim?"
    mc "Und du, Prim?"

# game/hmaxy.rpy:2563
translate german PA_HM_EV2_a61857f8:

    # p "Un té de frambuesa, como los que te gustan a ti."
    p "Ein Himbeertee, so wie du ihn magst."

# game/hmaxy.rpy:2565
translate german PA_HM_EV2_a67e172d:

    # mc "Marchando un té de frambuesa y un café dulce."
    mc "Einen Himbeertee und einen süssen Kaffee, kommt sofort."

# game/hmaxy.rpy:2567
translate german PA_HM_EV2_2fdcece2:

    # p "Iré a quitarme el uniforme de trabajo para sentirme más cómoda."
    p "Ich werde meine Arbeitskleidung ausziehen, um mich wohler zu fühlen."

# game/hmaxy.rpy:2570
translate german PA_HM_EV2_d6c2108f:

    # "<Te diriges a la cocina y preparas un té y un café.>"
    "<Du gehst in die Küche und machst Tee und Kaffee.>"

# game/hmaxy.rpy:2573
translate german PA_HM_EV2_c37a6c0f:

    # p "¡Fue muy gracioso!"
    p "Das war sehr lustig!"

# game/hmaxy.rpy:2574
translate german PA_HM_EV2_3e6315fc:

    # p "¡A mi hermano le corrían las lágrimas de la risa!"
    p "Mein Bruder hatte Tränen in den Augen vor Lachen!"

# game/hmaxy.rpy:2576
translate german PA_HM_EV2_c4742caa:

    # mx "¡Me hubiese gustado estar allí!"
    mx "Ich wäre gerne dabei gewesen!"

# game/hmaxy.rpy:2578
translate german PA_HM_EV2_69eb3bae:

    # mc "Creo que he llegado en mitad de una buena conversación."
    mc "Ich glaube, ich bin mitten in einem guten Gespräch dazugekommen."

# game/hmaxy.rpy:2580
translate german PA_HM_EV2_7255ee75:

    # mx "Prim me estaba hablando sobre ti."
    mx "Prim hat mir von dir erzählt."

# game/hmaxy.rpy:2582
translate german PA_HM_EV2_fd8bd229:

    # p "Contaba sobre esa vez que dejaste la ropa interior de la profesora Archer en el armario del instructor Guilliman."
    p "Ich erzählte von dem Vorfall, als du die Unterwäsche von Lehrerin Archer im Schrank von Ausbilder Guilliman zurückgelassen hast."

# game/hmaxy.rpy:2583
translate german PA_HM_EV2_7f25e4aa:

    # p "Y enviaste una carta a su esposo diciendo que le estaba poniendo los cuernos."
    p "Und du hast ihrem Mann einen Brief geschickt, in dem du geschrieben hast, dass sie ihn betrügt."

# game/hmaxy.rpy:2585
translate german PA_HM_EV2_e1b6a14b:

    # mc "¿Estaban hablando sobre eso?"
    mc "Habt ihr darüber gesprochen?"

# game/hmaxy.rpy:2586
translate german PA_HM_EV2_e37f1e46:

    # mc "Cuando vi su reacción casi muero de la risa."
    mc "Wenn ihr blos wüssted, als ich seine Reaktion sah, bin ich fast vor Lachen gestorben."

# game/hmaxy.rpy:2587
translate german PA_HM_EV2_8bebe440:

    # mc "Y luego fue el castigo... {w}Tuve que limpiar los baños durante dos meses con mi cepillo de dientes."
    mc "Und dann kam die Strafe... {w}Ich musste zwei Monate lang die Toiletten mit meiner Zahnbürste putzen."

# game/hmaxy.rpy:2588
translate german PA_HM_EV2_16078822:

    # mc "Anton me ayudaba durante las noches."
    mc "Anton half mir nachts."

# game/hmaxy.rpy:2589
translate german PA_HM_EV2_f9050c4e:

    # mc "Fue asqueroso, pero valió totalmente la pena."
    mc "Es war eklig, aber es hat sich absolut gelohnt."

# game/hmaxy.rpy:2591
translate german PA_HM_EV2_31c93e66:

    # mx "Que travieso eres, Lucky boy... {w}Me gustan los chicos traviesos."
    mx "Du bist aber böse, Lucky Boy... {w}Ich mag böse Jungs."

# game/hmaxy.rpy:2593
translate german PA_HM_EV2_b678b022:

    # mc "Aunque... {w}Aún no me dices que es lo que querías de mí la última vez que viniste, Maxy."
    mc "Obwohl... {w}Du hast mir immer noch nicht gesagt, was du das letzte Mal, als du hier warst, von mir wolltest, Maxy."

# game/hmaxy.rpy:2595
translate german PA_HM_EV2_68c2c791:

    # mx "¡Ya lo había olvidado!"
    mx "Das hatte ich schon ganz vergessen!"

# game/hmaxy.rpy:2597
translate german PA_HM_EV2_3505f5a6:

    # mx "Mi padre dijo que podía buscarte si me encontraba en problemas."
    mx "Mein Vater sagte, ich könne dich aufsuchen, wenn ich in Schwierigkeiten gerate."

# game/hmaxy.rpy:2599
translate german PA_HM_EV2_1a9f62a9:

    # mc "¿Tu padre? ¿Quién es tu padre?"
    mc "Dein Vater? Wer ist dein Vater?"

# game/hmaxy.rpy:2601
translate german PA_HM_EV2_55cdd6d4:

    # mx "Mi padre se llama Johan Conwey y es el alcalde de Argleton."
    mx "Mein Vater heisst Johan Conwey und ist Bürgermeister von Argleton."

# game/hmaxy.rpy:2603
translate german PA_HM_EV2_13df33e7:

    # mc "Es primera vez que vengo a este pueblo y nunca había escuchado hablar de él antes de llegar aquí."
    mc "Ich bin zum ersten Mal in diesem Dorf und hatte vor meiner Ankunft noch nie von ihn gehört."

# game/hmaxy.rpy:2605
translate german PA_HM_EV2_fb0603ed:

    # mx "Aún no... {w}Pero si conocía a un amigo tuyo llamado Anton."
    mx "Noch nicht... {w}Aber er kennt einen Freund von dir namens Anton."

# game/hmaxy.rpy:2607
translate german PA_HM_EV2_9225df3f:

    # p "Anton es mi hermano, pero él no está aquí en estos momentos."
    p "Anton ist mein Bruder, aber er ist gerade nicht hier."

# game/hmaxy.rpy:2609
translate german PA_HM_EV2_35162abb:

    # mx "Es una pena..."
    mx "Das ist schade..."

# game/hmaxy.rpy:2611
translate german PA_HM_EV2_2e453524:

    # mc "¿Y qué tipo de ayuda necesitas?"
    mc "Und welche Art von Hilfe brauchst du?"

# game/hmaxy.rpy:2613
translate german PA_HM_EV2_694ed1fe:

    # mx "Estoy buscando un lugar en donde quedarme por un tiempo."
    mx "Ich suche einen Ort, an dem ich eine Weile bleiben kann."

# game/hmaxy.rpy:2614
translate german PA_HM_EV2_c6e9a857:

    # mx "Parecerá raro, pero no conozco a nadie más en Argleton a quien recurrir."
    mx "Es mag seltsam erscheinen, aber ich kenne niemanden sonst in Argleton, an den ich mich wenden könnte."

# game/hmaxy.rpy:2615
translate german PA_HM_EV2_052cd98a:

    # mx "Tampoco hay hoteles..."
    mx "Es gibt auch keine Hotels..."

# game/hmaxy.rpy:2616
translate german PA_HM_EV2_985fbbbe:

    # mx "Esta es la única posibilidad que tengo."
    mx "Das ist meine einzige Chance."

# game/hmaxy.rpy:2618
translate german PA_HM_EV2_a3db9a7f:

    # mc "¿Qué?"
    mc "Was?"

# game/hmaxy.rpy:2620
translate german PA_HM_EV2_4e409d02:

    # mx "Lamento decir esto de manera tan repentina."
    mx "Es tut mir leid, dass ich das so plötzlich sage."

# game/hmaxy.rpy:2622
translate german PA_HM_EV2_6e662629:

    # p "Por mí no hay problema, si necesitas un lugar puedes quedarte aquí."
    p "Von mir aus kein Problem, wenn du eine Unterkunft brauchst, kannst du hier bleiben."

# game/hmaxy.rpy:2624
translate german PA_HM_EV2_5367af7b:

    # mc "Espera..."
    mc "Warte..."

# game/hmaxy.rpy:2625
translate german PA_HM_EV2_44b1a38a:

    # mc "¿Estás segura de eso Prim?"
    mc "Bist du dir da sicher, Prim?"

# game/hmaxy.rpy:2627
translate german PA_HM_EV2_d2b4b69a:

    # p "Siempre que pueda ayudar a alguien en problemas, lo voy a hacer sin pensarlo dos veces."
    p "Wann immer ich jemandem in Schwierigkeiten helfen kann, werde ich es ohne zu zögern tun."

# game/hmaxy.rpy:2628
translate german PA_HM_EV2_1270c549:

    # p "Aunque primero habría que hacer algunos arreglos a la habitación del segundo piso..."
    p "Allerdings müssten zunächst einige Reparaturen im Zimmer im zweiten Stock vorgenommen werden..."

# game/hmaxy.rpy:2630
translate german PA_HM_EV2_09ef1385:

    # mx "¡No te preocupes por eso!"
    mx "Mach dir darüber keine Sorgen!"

# game/hmaxy.rpy:2631
translate german PA_HM_EV2_07e35389:

    # mx "Estoy dispuesta a pagar por el arreglo de la habitación y una renta por quedarme aquí."
    mx "Ich bin bereit, für die Reparatur des Zimmers und die Miete zu zahlen, damit ich hier bleiben kann."

# game/hmaxy.rpy:2633
translate german PA_HM_EV2_13c90c51:

    # p "No es necesario que pagues nada."
    p "Du musst nichts bezahlen."

# game/hmaxy.rpy:2635
translate german PA_HM_EV2_ab2d884e:

    # mc "A veces pienso que Prim es demasiado generosa..."
    mc "Manchmal denke ich, dass Prim zu grosszügig ist..."

# game/hmaxy.rpy:2637
translate german PA_HM_EV2_70321a67:

    # p "¿Tú no estas de acuerdo con que Maxy se quede?"
    p "Bist du nicht damit einverstanden, dass Maxy bleibt?"

# game/hmaxy.rpy:2639
translate german PA_HM_EV2_5f17a2a3:

    # mc "Ella perfectamente podría estar mintiendo..."
    mc "Sie könnte durchaus lügen..."

# game/hmaxy.rpy:2640
translate german PA_HM_EV2_d143f5b9:

    # mc "¿Cómo sabes si realmente es la hija del alcalde?"
    mc "Woher weisst du, dass sie wirklich die Tochter des Bürgermeisters ist?"

# game/hmaxy.rpy:2642
translate german PA_HM_EV2_9777af03:

    # p "Voy a ayudar a alguien que lo necesite... {w}Aunque pueda equivocarme."
    p "Ich werde jemandem helfen, der Hilfe braucht... {w}Auch wenn ich mich dabei irren könnte."

# game/hmaxy.rpy:2644
translate german PA_HM_EV2_a92692a1:

    # mc "Es justo supongo..."
    mc "Das ist wohl fair..."

# game/hmaxy.rpy:2646
translate german PA_HM_EV2_f1f218df:

    # mx "Prometo que no haré nada malo, Lucky boy."
    mx "Ich verspreche, dass ich nichts Schlimmes tun werde, Lucky Boy."

# game/hmaxy.rpy:2648
translate german PA_HM_EV2_e7ade661:

    # mx "Y tú, ternurita, eres muy amable."
    mx "Und du, meine Liebe, bist sehr nett."

# game/hmaxy.rpy:2649
translate german PA_HM_EV2_c9074c06:

    # mx "No sé cómo agradecer toda tu hospitalidad."
    mx "Ich weiss gar nicht, wie ich dir für deine Gastfreundschaft danken soll."

# game/hmaxy.rpy:2651
translate german PA_HM_EV2_7a1a5846:

    # p "¡No tienes que agradecerme nada!"
    p "Du musst mir nicht danken!"

# game/hmaxy.rpy:2653
translate german PA_HM_EV2_ed4079d4:

    # mx "Creo que por ahora me piro, vampiro."
    mx "Ich glaube, ich haue jetzt ab."

# game/hmaxy.rpy:2654
translate german PA_HM_EV2_d9700304:

    # mx "Tengo que pensar en lo que debo traer durante la mudanza..."
    mx "Ich muss mir überlegen, was ich beim Umzug mitnehmen soll..."

# game/hmaxy.rpy:2655
translate german PA_HM_EV2_0581dcbb:

    # mx "Espero me disculpes pequeñita."
    mx "Ich hoffe, du verzeihst mir, Kleine."

# game/hmaxy.rpy:2657
translate german PA_HM_EV2_709951da:

    # p "¡Vuelve cuando quieras!"
    p "Komm wieder, wann immer du möchtest!"

# game/hmaxy.rpy:2661
translate german PA_HM_EV2_59431fa1:

    # mx "Ahora estamos solos, Lucky boy."
    mx "Jetzt sind wir allein, Lucky Boy."

# game/hmaxy.rpy:2662
translate german PA_HM_EV2_e9b20973:

    # mx "¿Sabes que significa eso?"
    mx "Weisst du, was das bedeutet?"

# game/hmaxy.rpy:2664
translate german PA_HM_EV2_c971c80e:

    # mc "No quiero pensar mal..."
    mc "Ich möchte nichts Schlechtes denken..."

# game/hmaxy.rpy:2666
translate german PA_HM_EV2_240d7fae:

    # mx "Yo si quiero pensar mal."
    mx "Ich will schlecht denken."

# game/hmaxy.rpy:2667
translate german PA_HM_EV2_f3ab5064:

    # mx "Y dime..."
    mx "Und sag mir..."

# game/hmaxy.rpy:2669
translate german PA_HM_EV2_2909d9b5:

    # mx "¿Te gusta la música, Lucky boy?"
    mx "Magst du Musik, Lucky Boy?"

# game/hmaxy.rpy:2670
translate german PA_HM_EV2_d27fc014:

    # mx "Podría darte un concierto personal."
    mx "Ich könnte dir ein Privatkonzert geben."

# game/hmaxy.rpy:2671
translate german PA_HM_EV2_d2192025:

    # mx "Y después de eso, podrías darme un beso aquí."
    mx "Und danach könntest du mich hier küssen."

# game/hmaxy.rpy:2673
translate german PA_HM_EV2_53ed2a78:

    # mc "¿Sabes cantar?"
    mc "Kannst du singen?"

# game/hmaxy.rpy:2675
translate german PA_HM_EV2_535b2dbd:

    # mx "Sé tocar la guitarra y algún que otro instrumento."
    mx "Ich kann Gitarre und einige andere Instrumente spielen."

# game/hmaxy.rpy:2676
translate german PA_HM_EV2_dacf3c53:

    # mx "Nunca he intentado cantar, pero si tú me lo pides, puedo hacerlo."
    mx "Ich habe noch nie versucht zu singen, aber wenn du mich darum bittest, kann ich es tun."

# game/hmaxy.rpy:2678
translate german PA_HM_EV2_2b4195ad:

    # mc "Quizás un día de estos."
    mc "Vielleicht eines Tages."

# game/hmaxy.rpy:2680
translate german PA_HM_EV2_e2a3c004:

    # mx "¿Crees en el amor a primera vista?"
    mx "Glaubst du an Liebe auf den ersten Blick?"

# game/hmaxy.rpy:2693
translate german PA_HM_EV2_8a1a5585:

    # mc "Puede que sí."
    mc "Vielleicht ja."

# game/hmaxy.rpy:2695
translate german PA_HM_EV2_2353baf3:

    # mx "Que triste..."
    mx "Wie traurig..."

# game/hmaxy.rpy:2696
translate german PA_HM_EV2_0a536b96:

    # mx "Eso significa que posiblemente no te enamoraste de mí a primera vista."
    mx "Das bedeutet, dass du dich möglicherweise nicht auf den ersten Blick in mich verliebt hast."

# game/hmaxy.rpy:2706
translate german PA_HM_EV2_a63a57cf:

    # mc "Creo que no."
    mc "Ich glaube nicht."

# game/hmaxy.rpy:2708
translate german PA_HM_EV2_a26d9f7c:

    # mx "Eso me da esperanzas."
    mx "Das gibt mir Hoffnung."

# game/hmaxy.rpy:2709
translate german PA_HM_EV2_656da0c4:

    # mx "Significa que aún te puedo gustar."
    mx "Das bedeutet, dass ich dir immer noch gefallen kann."

# game/hmaxy.rpy:2712
translate german PA_HM_EV2_ee21b633:

    # mx "¿Crees que podría gustarte algún día?"
    mx "Glaubst du, dass du mich eines Tages mögen könntest?"

# game/hmaxy.rpy:2714
translate german PA_HM_EV2_ec345c8b:

    # mc "No me gusta descartar posibilidades."
    mc "Ich mag es nicht, Möglichkeiten auszuschließen."

# game/hmaxy.rpy:2716
translate german PA_HM_EV2_22a31d8b:

    # mx "¿Eso es un quizás?"
    mx "Ist das ein Vielleicht?"

# game/hmaxy.rpy:2718
translate german PA_HM_EV2_a9401191:

    # mc "Es un posiblemente..."
    mc "Es besteht eine Schance..."

# game/hmaxy.rpy:2720
translate german PA_HM_EV2_ad574c8c:

    # mx "¡Qué felicidad!"
    mx "Was für ein Glück!"

# game/hmaxy.rpy:2721
translate german PA_HM_EV2_49625fb7:

    # mx "¿Y Qué puedo hacer para que me quieras, Lucky Boy?"
    mx "Was kann ich tun, damit du mich liebst, Lucky Boy?"

# game/hmaxy.rpy:2723
translate german PA_HM_EV2_9587cf5f:

    # mc "Primero que todo... {w}No me llames Lucky Boy."
    mc "Zunächst einmal... {w}Nenn mich nicht Lucky Boy."

# game/hmaxy.rpy:2724
translate german PA_HM_EV2_5b2e1ba2:

    # mc "Preferiría que usaras mi nombre o Lucky a secas."
    mc "Ich würde es vorziehen, wenn du meinen Namen oder einfach nur Lucky verwendest."

# game/hmaxy.rpy:2726
translate german PA_HM_EV2_8a0ba00d:

    # mx "¿Qué más debería hacer para mejorar?"
    mx "Was sollte ich noch tun, um mich zu verbessern?"

# game/hmaxy.rpy:2728
translate german PA_HM_EV2_5ac98d21:

    # mc "¿Por qué no me hablas un poco sobre ti?"
    mc "Warum erzählst du mir nicht ein bisschen was über dich?"

# game/hmaxy.rpy:2730
translate german PA_HM_EV2_577c988e:

    # mx "La verdad no hay gran cosa que te pueda contar."
    mx "Ehrlich gesagt gibt es nicht viel, was ich dir erzählen könnte."

# game/hmaxy.rpy:2731
translate german PA_HM_EV2_e0babd1c:

    # mx "Soy una chica solitaria y sumergida en las tinieblas y la oscuridad."
    mx "Ich bin ein einsames Mädchen, versunken in Finsternis und Dunkelheit."

# game/hmaxy.rpy:2732
translate german PA_HM_EV2_87f99a5a:

    # mx "Mi forma de ser me trae problemas... {w}A veces soy un poco cursi."
    mx "Meine Art bereitet mir Probleme... {w}Manchmal bin ich ein bisschen kitschig."

# game/hmaxy.rpy:2733
translate german PA_HM_EV2_a609c81f:

    # mx "También me han dicho que soy rara o loca... {w}O todos esos adjetivos juntos y mezclados."
    mx "Man hat mir auch gesagt, ich sei seltsam oder verrückt... {w}Oder all diese Adjektive zusammen und gemischt."

# game/hmaxy.rpy:2735
translate german PA_HM_EV2_669fbba2:

    # mc "Puede ser por la forma que tienes de hablar con la gente."
    mc "Vielleicht liegt es daran, wie du mit Menschen sprichst."

# game/hmaxy.rpy:2737
translate german PA_HM_EV2_88aa78da:

    # mx "Dices eso solo porque te dije que eras lindo la primera vez que te vi."
    mx "Das sagst du nur, weil ich dir gesagt habe, dass du süss bist, als ich dich zum ersten Mal gesehen habe."

# game/hmaxy.rpy:2738
translate german PA_HM_EV2_7a8a5b3c:

    # mx "Me gusta ser muy sincera con mis sentimientos."
    mx "Ich bin gerne sehr ehrlich mit meinen Gefühlen."

# game/hmaxy.rpy:2739
translate german PA_HM_EV2_731e941b:

    # mx "Deberías ver eso como una cualidad positiva."
    mx "Du solltest das als positive Eigenschaft betrachten."

# game/hmaxy.rpy:2741
translate german PA_HM_EV2_6204d764:

    # mx "¡Me siento tan feliz de haber encontrado este lugar y poder conocerte a ti y a la ternurita!"
    mx "Ich bin so glücklich, diesen Ort gefunden zu haben und dich und die Kleine kennenzulernen!"

# game/hmaxy.rpy:2742
translate german PA_HM_EV2_5dd257b9:

    # mx "Pensaba que nunca podría encontrar un lugar para quedarme."
    mx "Ich dachte, ich würde nie eine Bleibe finden."

# game/hmaxy.rpy:2743
translate german PA_HM_EV2_b5f9c18d:

    # mx "Por culpa de la fama de mi padre, tampoco nadie iba a querer recibirme en su casa."
    mx "Wegen des Rufs meines Vaters wollte mich auch niemand in seinem Haus aufnehmen."

# game/hmaxy.rpy:2745
translate german PA_HM_EV2_e800e9b2:

    # mc "¿Cuál es el problema con tu padre?"
    mc "Was ist das Problem mit deinem Vater?"

# game/hmaxy.rpy:2746
translate german PA_HM_EV2_3c6c7e87:

    # mc "Estoy comenzando a pensar que es alguien muy peligroso..."
    mc "Ich beginne zu glauben, dass es sich um jemanden handelt, der sehr gefährlich ist..."

# game/hmaxy.rpy:2748
translate german PA_HM_EV2_073f37bc:

    # mx "Él no haría daño ni siquiera a una mosca."
    mx "Er würde keiner Fliege etwas zuleide tun."

# game/hmaxy.rpy:2750
translate german PA_HM_EV2_e07851f1:

    # mc "¿Entonces?"
    mc "Also?"

# game/hmaxy.rpy:2752
translate german PA_HM_EV2_84d0bb94:

    # mx "La gente siempre a sentido miedo por los alcaldes de Argleton."
    mx "Die Menschen hatten schon immer Angst vor den Bürgermeistern von Argleton."

# game/hmaxy.rpy:2753
translate german PA_HM_EV2_d9ba2403:

    # mx "Hay mucho misticismo y se cuentan historias raras sobre ellos."
    mx "Es gibt viel Mystik und seltsame Geschichten über sie."

# game/hmaxy.rpy:2755
translate german PA_HM_EV2_810e9850:

    # mx "..."
    mx "..."

# game/hmaxy.rpy:2756
translate german PA_HM_EV2_79241e3a:

    # mc "No te me quedes mirando así."
    mc "Schau mich nicht so an."

# game/hmaxy.rpy:2758
translate german PA_HM_EV2_530fb028:

    # mx "No puedo controlarlo... {w}Siento que te quiero mucho."
    mx "Ich kann es nicht kontrollieren... {w}Ich liebe dich sehr."

# game/hmaxy.rpy:2759
translate german PA_HM_EV2_6e63989a:

    # mx "¿Quieres saber más cosas sobre mí?"
    mx "Möchtest du mehr über mich erfahren?"

# game/hmaxy.rpy:2761
translate german PA_HM_EV2_13d94083:

    # mc "Creo que por hoy ha sido suficiente información."
    mc "Ich glaube, das reicht für heute an Informationen."

# game/hmaxy.rpy:2763
translate german PA_HM_EV2_23638171:

    # mx "Si tú lo dices, entonces estoy de acuerdo."
    mx "Wenn du das sagst, dann stimme ich dir zu."

# game/hmaxy.rpy:2764
translate german PA_HM_EV2_e1f4ebaa:

    # mx "Pero no me quiero ir sin que me des mi premio."
    mx "Aber ich möchte nicht gehen, ohne dass du mir meinen Preis gibst."

# game/hmaxy.rpy:2766
translate german PA_HM_EV2_a3db9a7f_1:

    # mc "¿Qué?"
    mc "Was?"

# game/hmaxy.rpy:2768
translate german PA_HM_EV2_cc28684b:

    # mx "Un beso aquí."
    mx "Ein Kuss hier."

# game/hmaxy.rpy:2769
translate german PA_HM_EV2_b7a6a7ee:

    # mx "Solo uno."
    mx "Nur einer."

# game/hmaxy.rpy:2786
translate german PA_HM_EV2_ddf49cfd:

    # mx "¡Qué suave se ha sentido!"
    mx "Wie weich es sich angefühlt hat!"

# game/hmaxy.rpy:2787
translate german PA_HM_EV2_1886636f:

    # mx "Ahora puedo irme feliz."
    mx "Jetzt kann ich glücklich gehen."

# game/hmaxy.rpy:2789
translate german PA_HM_EV2_7a2291a7:

    # mc "Será para otro día..."
    mc "Das heben wir uns für einen anderen Tag auf..."

# game/hmaxy.rpy:2791
translate german PA_HM_EV2_47109811:

    # mx "Está bien."
    mx "In Ordnung."

# game/hmaxy.rpy:2793
translate german PA_HM_EV2_23d278aa:

    # mx "¡Nos vemos otro día!"
    mx "Bis zum nächsten Mal!"

# game/hmaxy.rpy:2794
translate german PA_HM_EV2_87101885:

    # mx "¡Adiós, Lucky boy!"
    mx "Auf Wiedersehen, Lucky Boy!"

# game/hmaxy.rpy:2796
translate german PA_HM_EV2_65452732:

    # mc "Habías prometido que no volverías a llamarme Lucky boy..."
    mc "Du hattest versprochen, mich nicht mehr Lucky Boy zu nennen..."

# game/hmaxy.rpy:2810
translate german PA_HM_EV3_5a864988:

    # mx "¡Hola, Lucky boy!"
    mx "Hallo, Lucky boy!"

# game/hmaxy.rpy:2811
translate german PA_HM_EV3_386d471b:

    # mx "¡Ya tengo todo listo para la mudanza!"
    mx "Ich habe schon alles für den Umzug vorbereitet!"

# game/hmaxy.rpy:2813
translate german PA_HM_EV3_22b92d11:

    # p "¡Llegas justo a tiempo!"
    p "Du kommst gerade rechtzeitig!"

# game/hmaxy.rpy:2815
translate german PA_HM_EV3_440bd277:

    # mc "¿Estás completamente segura de esto, Prim?"
    mc "Bist du dir da ganz sicher, Prim?"

# game/hmaxy.rpy:2817
translate german PA_HM_EV3_80702449:

    # mx "No quiero que se sientan obligados."
    mx "Ich möchte nicht, dass du dich verpflichtet fühlst."

# game/hmaxy.rpy:2818
translate german PA_HM_EV3_163c046f:

    # mx "Si se arrepintieron a último momento, pueden ser sinceros conmigo."
    mx "Wenn du es dir in letzter Minute anders überlegt hast, kannst du mir gegenüber ehrlich sein."

# game/hmaxy.rpy:2820
translate german PA_HM_EV3_58b3f833:

    # p "No deberías ser tan desconfiado, Lucky."
    p "Du solltest nicht so misstrauisch sein, Lucky."

# game/hmaxy.rpy:2822
translate german PA_HM_EV3_13e3c6ff:

    # mc "Lo siento, no puedo evitarlo."
    mc "Es tut mir leid, ich kann nichts dagegen tun."

# game/hmaxy.rpy:2824
translate german PA_HM_EV3_ea31b240:

    # p "Todavía hay que hacer algunos arreglos en el segundo piso."
    p "Im zweiten Stock müssen noch einige Reparaturen durchgeführt werden."

# game/hmaxy.rpy:2826
translate german PA_HM_EV3_74aff787:

    # mx "¡He comprado pintura violeta para llenar de vida el lugar!"
    mx "Ich habe violette Farbe gekauft, um den Raum mit Leben zu füllen!"

# game/hmaxy.rpy:2827
translate german PA_HM_EV3_02e4ce7f:

    # mx "¿Me ayudarías a pintar, Lucky boy?"
    mx "Kannst du mir beim Streichen helfen, Lucky Boy?"

# game/hmaxy.rpy:2829
translate german PA_HM_EV3_f7c38274:

    # mc "No hay problema."
    mc "Kein Problem."

# game/hmaxy.rpy:2831
translate german PA_HM_EV3_e6a4fe0d:

    # mx "Tenemos tiempo suficiente mientras el servicio de mudanza de Kamen Rainor trae mis cosas."
    mx "Wir haben genug Zeit, während der Umzugsdienst von Kamen Rainor meine Sachen herbringt."

# game/hmaxy.rpy:2833
translate german PA_HM_EV3_fcd92984:

    # p "¿Necesitas ayuda con eso?"
    p "Brauchst du dabei Hilfe?"

# game/hmaxy.rpy:2835
translate german PA_HM_EV3_8d365bba:

    # mc "Yo puedo encargarme de todo."
    mc "Ich kann mich um alles kümmern."

# game/hmaxy.rpy:2836
translate german PA_HM_EV3_c97abe06:

    # mc "Tú mereces un descanso ahora mismo Prim, has sido un día agotador en el [restaurant]."
    mc "Du hast dir jetzt eine Pause verdient, Prim, es war ein anstrengender Tag im [restaurant]."

# game/hmaxy.rpy:2838
translate german PA_HM_EV3_742ba8e3:

    # mx "¡Entonces pongámonos manos a la obra!"
    mx "Dann legen wir los!"

# game/hmaxy.rpy:2840
translate german PA_HM_EV3_fc652cba:

    # "<Ayudas a pintar la habitación y acomodar los muebles de Maxy.>"
    "<Du hilst dabei, das Zimmer zu streichen und Maxys Möbel aufzustellen.>"

# game/hmaxy.rpy:2841
translate german PA_HM_EV3_b5000950:

    # "<Después de varias horas de trabajo, te sientes satisfecho con el trabajo, pero también te sientes agotado.>"
    "<Nach mehreren Stunden Arbeit bist du zufrieden mit dem Ergebnis, aber auch erschöpft.>"

# game/hmaxy.rpy:2843
translate german PA_HM_EV3_36ac837f:

    # mx "¡Ha quedado precioso!"
    mx "Es ist wunderschön geworden!"

# game/hmaxy.rpy:2850
translate german PA_HM_EV3_89e71fad:

    # mx "¡Creo que ha superado totalmente mis expectativas!"
    mx "Ich glaube, es hat meine Erwartungen völlig übertroffen!"

# game/hmaxy.rpy:2851
translate german PA_HM_EV3_3c8e00d3:

    # mx "Siento mucho que hayas gastado todo el día para dejar mi habitación lista."
    mx "Es tut mir sehr leid, dass du den ganzen Tag damit verbracht hast, mein Zimmer fertig zu machen."

# game/hmaxy.rpy:2853
translate german PA_HM_EV3_4994232b:

    # mc "Al menos ha sido un buen trabajo."
    mc "Zumindest war es eine gute Arbeit."

# game/hmaxy.rpy:2855
translate german PA_HM_EV3_bb6f77f9:

    # mx "Te noto muy pálido... {w}Debes estar muy cansado después de todo esto."
    mx "Du siehst sehr blass aus... {w}Du musst nach all dem sehr müde sein."

# game/hmaxy.rpy:2856
translate german PA_HM_EV3_85129218:

    # mx "Sin mencionar que fue justo después de tu jornada de trabajo."
    mx "Ganz zu schweigen davon, dass es direkt nach deinem Arbeitstag war."

# game/hmaxy.rpy:2858
translate german PA_HM_EV3_739ac246:

    # mc "Espero que este lugar sea de tu agrado."
    mc "Ich hoffe, dass dir dieser Ort gefällt."

# game/hmaxy.rpy:2860
translate german PA_HM_EV3_a0e6f2c5:

    # mx "¡Qué lindo eres!"
    mx "Wie süss du bist!"

# game/hmaxy.rpy:2861
translate german PA_HM_EV3_059b335e:

    # mx "¡Claro que será de mi agrado!"
    mx "Natürlich wird es mir gefallen!"

# game/hmaxy.rpy:2862
translate german PA_HM_EV3_71ba75ef:

    # mx "¡Qué emoción!"
    mx "Wie aufregend!"

# game/hmaxy.rpy:2863
translate german PA_HM_EV3_1eb29f99:

    # mx "Desde ahora tú serás mi guardián protector."
    mx "Von nun an wirst du mein Beschützer sein."

# game/hmaxy.rpy:2864
translate german PA_HM_EV3_167c1371:

    # mx "Y la ternurita será como una hermanita menor."
    mx "Und die Kleine wird wie eine kleine Schwester sein."

# game/hmaxy.rpy:2866
translate german PA_HM_EV3_f7192a57:

    # mc "Creo que necesito recostarme..."
    mc "Ich glaube, ich muss mich hinlegen..."

# game/hmaxy.rpy:2868
translate german PA_HM_EV3_c40dcd8f:

    # mx "Quédate a dormir aquí conmigo durante esta noche."
    mx "Bleib heute Nacht hier bei mir."

# game/hmaxy.rpy:2869
translate german PA_HM_EV3_9e81f814:

    # mx "Podría hacerte unos merecidos masajes para que te sientas más cómodo después de todo ese esfuerzo."
    mx "Ich könnte dir eine wohlverdiente Massage geben, damit du dich nach all der Anstrengung wohler fühlst."

# game/hmaxy.rpy:2871
translate german PA_HM_EV3_1bd0ee9a:

    # mc "¿No estarás acelerando mucho las cosas?"
    mc "Überstürzst du die Dinge nicht ein bisschen?"

# game/hmaxy.rpy:2873
translate german PA_HM_EV3_c9cc3abc:

    # mx "¡Qué aguafiestas!"
    mx "Was für ein Spielverderber!"

# game/hmaxy.rpy:2874
translate german PA_HM_EV3_4ac35921:

    # mx "¡No sabes lo divertido que sería!"
    mx "Du weisst gar nicht, wie viel Spass das machen würde!"

# game/hmaxy.rpy:2875
translate german PA_HM_EV3_26685e5e:

    # mx "Entonces creo que es momento de un buenas noches, cariño."
    mx "Dann denke ich, es ist Zeit für eine gute Nacht, Liebling."

# game/hmaxy.rpy:2877
translate german PA_HM_EV3_7f3823df:

    # mc "Buenas noches, Maxy."
    mc "Guten Abend, Maxy."

# game/hmaxy.rpy:2879
translate german PA_HM_EV3_f6068531:

    # mx "Adiós bebé, nos vemos mañana."
    mx "Tschüss, Baby, wir sehen uns morgen."

# game/hmaxy.rpy:2880
translate german PA_HM_EV3_1b50cddc:

    # mx "Espero que sueñes con los diablitos."
    mx "Ich hoffe, du träumst von deinem kleinen Teufel."

# game/hmaxy.rpy:2883
translate german PA_HM_EV3_f9d2732c:

    # "<Estás tan agotado que después de despedirte de Maxy te duermes de inmediato.>"
    "<Du bist so erschöpft, dass du, nachdem du dich von Maxy verabschiedet hast, sofort einschläfst.>"

# game/hmaxy.rpy:2901
translate german PA_HM_EV4_36f7f8da:

    # mc "Quería saber como estabas."
    mc "Ich wollte wissen, wie es dir geht."

# game/hmaxy.rpy:2903
translate german PA_HM_EV4_3990c0f2:

    # mx "Hola Lucky."
    mx "Hallo Lucky."

# game/hmaxy.rpy:2905
translate german PA_HM_EV4_ba90657b:

    # mc "Que sorpresa."
    mc "Was für eine Überraschung."

# game/hmaxy.rpy:2907
translate german PA_HM_EV4_498291f6:

    # mx "¿Qué sucede cariño?"
    mx "Was ist los, Liebling?"

# game/hmaxy.rpy:2909
translate german PA_HM_EV4_f8f7bf4a:

    # mc "¿He llegado en un mal momento?"
    mc "Ist es gerade ein ungünstiger Zeitpunkt?"

# game/hmaxy.rpy:2911
translate german PA_HM_EV4_d545a8ab:

    # mx "Si lo dices porque estoy en ropa interior..."
    mx "Wenn du das sagst, weil ich Unterwäsche trage..."

# game/hmaxy.rpy:2912
translate german PA_HM_EV4_6c77261c:

    # mx "Es porque no me diste tiempo de ponerme algo más abrigador."
    mx "Das liegt daran, dass du mir keine Zeit gegeben hast, mir etwas Wärmeres anzuziehen."

# game/hmaxy.rpy:2914
translate german PA_HM_EV4_bf1340bb:

    # mc "Creo que debí tocar la puerta antes de entrar."
    mc "Ich glaube, ich hätte vor dem Eintreten anklopfen sollen."

# game/hmaxy.rpy:2916
translate german PA_HM_EV4_4287a7c3:

    # mx "Puedes dejar de preocuparte por eso, cariño."
    mx "Du kannst aufhören, dir darüber Gedanken zu machen, Liebling."

# game/hmaxy.rpy:2917
translate german PA_HM_EV4_62e09619:

    # mx "No me molesta que me veas en ropa interior."
    mx "Es stört mich nicht, dass du mich in Unterwäsche siehst."

# game/hmaxy.rpy:2919
translate german PA_HM_EV4_039180e6:

    # mc "Creo que empiezo a apreciar de mejor manera tu compañía."
    mc "Ich glaube, ich beginne deine Gesellschaft mehr zu schätzen."

# game/hmaxy.rpy:2920
translate german PA_HM_EV4_248811cf:

    # mc "¿Y qué te ha parecido el lugar?"
    mc "Und wie hat dir der Ort gefallen?"

# game/hmaxy.rpy:2922
translate german PA_HM_EV4_551c61d6:

    # mx "¡Me encanta mucho! {w}¡Me encanta más que muy mucho! {w}¡Me encantan montones de muchos montones!"
    mx "Ich liebe es sehr! {w}Ich liebe es mehr als sehr! {w}Ich liebe es unendlich viel!"

# game/hmaxy.rpy:2923
translate german PA_HM_EV4_95939c80:

    # mx "¡No podría estar más feliz!"
    mx "Ich könnte nicht glücklicher sein!"

# game/hmaxy.rpy:2927
translate german PA_HM_EV4_68617368:

    # mc "Me alegra que te guste."
    mc "Ich freue mich, dass es dir gefällt."

# game/hmaxy.rpy:2928
translate german PA_HM_EV4_4b0c5bc4:

    # mc "Si necesitas algo, solo tienes que decírmelo."
    mc "Wenn du etwas brauchst, sag mir einfach Bescheid."

# game/hmaxy.rpy:2938
translate german PA_HM_EV4_9bc42691:

    # mx "Gracias por preocuparte por mí, cariño."
    mx "Danke, dass du dich um mich sorgst, Liebling."

# game/hmaxy.rpy:2940
translate german PA_HM_EV4_f08edd46:

    # mc "Espero que puedas solucionar pronto tu problema..."
    mc "Ich hoffe, du kannst dein Problem bald lösen..."

# game/hmaxy.rpy:2941
translate german PA_HM_EV4_b278861f:

    # mc "Así no tienes que quedarte por mucho tiempo."
    mc "So musst du nicht lange bleiben."

# game/hmaxy.rpy:2943
translate german PA_HM_EV4_dcb10a4c:

    # mx "¡Qué cruel eres!"
    mx "Wie grausam du bist!"

# game/hmaxy.rpy:2944
translate german PA_HM_EV4_5992522f:

    # mx "Supongo que estaré aquí hasta que arregle las cosas con mi madre."
    mx "Ich schätze, ich werde hier bleiben, bis ich die Dinge mit meiner Mutter geklärt habe."

# game/hmaxy.rpy:2945
translate german PA_HM_EV4_2e223aa6:

    # mx "No quiero molestar."
    mx "Ich möchte nicht stören."

# game/hmaxy.rpy:2948
translate german PA_HM_EV4_f4e4d1c9:

    # mx "Se que quizás no te importe..."
    mx "Ich weiss, dass es dir vielleicht egal ist..."

# game/hmaxy.rpy:2949
translate german PA_HM_EV4_65e7d9ed:

    # mx "Pero traje conmigo unos cuantos parlantes."
    mx "Aber ich habe ein paar Lautsprecher mitgebracht."

# game/hmaxy.rpy:2950
translate german PA_HM_EV4_eb7e3652:

    # mx "Ya que me gusta tocar la guitarra cuando me siento sola."
    mx "Da ich gerne Gitarre spiele, wenn ich mich einsam fühle."

# game/hmaxy.rpy:2951
translate german PA_HM_EV4_513c0e11:

    # mx "Espero no molestar a la ternurita."
    mx "Ich hoffe, ich störe die Kleine nicht."

