    # ca "Oui..."
    ca "Oui..."

# game/hchar.rpy:13051
translate german PA_HC_EV36_60c367a4:

    # ca "Si esa es la última voluntad de mémé..."
    ca "Wenn das Mémés letzter Wille ist..."

# game/hchar.rpy:13053
