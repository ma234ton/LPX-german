    mc "Sag mir nicht, dass es um deine Mutter geht."

# game/hchar.rpy:9565
translate german PA_HC_EV32_0957e9d3:

    # so "AHhhh... {w}Oui... {w}Oui oui oui..."
    so "AHhhh... {w}Oui... {w}Oui oui oui..."

# game/hchar.rpy:9567
translate german PA_HC_EV32_6438fcf2:

    # so "¡Ella es la Innombrable!"
    so "Sie ist die, die man nich ausprechen soll!"

# game/hchar.rpy:9569
translate german PA_HC_EV32_c1e285dd:

