    unknown "Satané signal téléphonique..."

# game/hchar.rpy:9637
translate german PA_HC_EV32_b7fb5168:

    # unknown "Bonjour."
    unknown "Bonjour."

# game/hchar.rpy:9639
translate german PA_HC_EV32_194d89bd:

    # mc "¿Disculpa?"
    mc "Entschuldigung?"

