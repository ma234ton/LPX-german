    # akiko "Ahora si me lo permite, debo retirarme."
    akiko "Wenn Sie mir nun gestatten, ich muss andere Gäste bedienen."

# game/hchar.rpy:13677
translate german PA_HC_EV37_62bf9f13:

    # akiko "Que disfruten de la comida."
    akiko "Guten Appetit!"

# game/hchar.rpy:13685
translate german PA_HC_EV37_6a121ecd_2:

    # jul "..."
    jul "..."

# game/hchar.rpy:13686
translate german PA_HC_EV37_6448d8e9:

    # so "Me pregunto a qué se debe ese silencio, Madre."
    so "Ich frage mich, woran diese Stille liegt, Mutter."

# game/hchar.rpy:13688
translate german PA_HC_EV37_ab6c77af:

    # jul "Espero no estar volviéndome loca..."
    jul "Ich hoffe, ich werde nicht verrückt..."

# game/hchar.rpy:13692
translate german PA_HC_EV37_5c2cdd2d:

    # jul "Quizás sea una cámara indiscreta para algún programa de televisión."
    jul "Vielleicht ist es eine versteckte Kamera für eine Fernsehsendung."

# game/hchar.rpy:13694
translate german PA_HC_EV37_b5766092:

    # jul "¡Eso debe ser!"
    jul "Das muss es sein!"

# game/hchar.rpy:13696
translate german PA_HC_EV37_191446d4:

    # so "Estas pensando demasiado sobre..."
    so "Du denkst wieder zu weit..."

# game/hchar.rpy:13698
translate german PA_HC_EV37_0a9c0261:

    # jul "Quieren hacer un reality show acerca de la familia Charpentier."
    jul "Die wollen eine Reality-Show über die Familie Charpentier drehen."

# game/hchar.rpy:13700
translate german PA_HC_EV37_63a62478:

    # so "Madre..."
    so "Mutter..."

# game/hchar.rpy:13702
translate german PA_HC_EV37_ba73dbba:

    # jul "¡Silencio!"
    jul "Ruhe!"

# game/hchar.rpy:13703
translate german PA_HC_EV37_d72ed840:

    # jul "¡Estoy convencida de que ustedes estan involucradas!"
    jul "Ich bin überzeugt, dass ihr daran beteiligt sind!"

# game/hchar.rpy:13705
translate german PA_HC_EV37_601a306c:

    # jul "Desde que llegue a este pueblo, todo ha sido extraño..."
    jul "Seit ich in dieses Dorf gekommen bin, ist alles seltsam..."

# game/hchar.rpy:13706
translate german PA_HC_EV37_a1677760:

    # jul "¡No hay señal telefonica! ¡No hay television! ¡No hay contacto con el exterior!"
    jul "Kein Handysignal! Kein Fernsehen! Kein Kontakt zur Aussenwelt!"

# game/hchar.rpy:13708
translate german PA_HC_EV37_f0f0850b:

    # jul "¡Tal como si este fuese un mega set de television!"
    jul "Als wäre dies ein riesiges Fernsehstudio!"

# game/hchar.rpy:13709
translate german PA_HC_EV37_6051f540:

    # jul "¿Como mas podrias explicar todo esto?"
    jul "Wie könntest du das alles noch erklären?"

# game/hchar.rpy:13711
translate german PA_HC_EV37_460d036c:

    # jul "Ademas, grabar en una localidad remota desconectada de todo resultaria mas economico."
    jul "Ausserdem wäre das Filmen an einem abgelegenen Ort, der von allem abgeschnitten ist, kostengünstiger."

# game/hchar.rpy:13713
translate german PA_HC_EV37_8c78c63a:

    # jul "Ahora todo tiene sentido..."
    jul "Jetzt macht alles Sinn..."

# game/hchar.rpy:13715
