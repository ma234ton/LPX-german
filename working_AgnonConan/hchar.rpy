    mc "(Was soll diese direkte Bemerkung?)"

# game/hchar.rpy:9655
translate german PA_HC_EV32_71119795:

    # unknown "Aunque primero debo presentarme, mi nombre es Juliette..."
    unknown "Zunächst muss ich mich vorstellen: Mein Name ist Juliette..."

# game/hchar.rpy:9656
translate german PA_HC_EV32_19d3256a:

    # jul "Juliette Charpentier."
    jul "Juliette Charpentier."

# game/hchar.rpy:9658
translate german PA_HC_EV32_9c744dc6:

    # mc "Vaya, es un nombre muy bonito."
    mc "Wow, das ist ein sehr schöner Name."

# game/hchar.rpy:9660
translate german PA_HC_EV32_c4649448:

    # jul "¿Podría conocer el tuyo?"
    jul "Darf ich deins kennenlernen?"

# game/hchar.rpy:9662
translate german PA_HC_EV32_9c583341:

    # mc "Mi nombre es [nombre], mucho gusto."
    mc "Mein Name ist [nombre], sehr erfreut."

# game/hchar.rpy:9664
translate german PA_HC_EV32_936bebbe:

    # jul "Supongo que no piensas decir tu apellido a una extraña."
    jul "Ich nehme an, du wirst einer Fremden nicht deinen Nachnamen verraten."

# game/hchar.rpy:9666
translate german PA_HC_EV32_3f12ca6f:

    # mc "No tengo problema con eso."
    mc "Ich habe kein Problem damit."

# game/hchar.rpy:9667
translate german PA_HC_EV32_ef0ce6d4:

    # mc "Pero se podría decir que no tengo un apellido definido."
    mc "Aber man könnte sagen, dass ich keinen festen Nachnamen habe."

# game/hchar.rpy:9669
translate german PA_HC_EV32_541c5f33:

    # jul "¿Ni siquiera un escudo familiar?"
    jul "Nicht einmal ein Familienwappen?"

# game/hchar.rpy:9671
translate german PA_HC_EV32_a6bfcb17:

    # mc "¿Un escudo familiar?"
    mc "Ein Familienwappen?"

# game/hchar.rpy:9673
translate german PA_HC_EV32_3bef628b:

    # jul "No importa, disculpa mi imprudencia, no pretendo meterme en tu vida privada."
    jul "Macht nichts, entschuldige meine Unbesonnenheit, ich möchte mich nicht in dein Privatleben einmischen."

# game/hchar.rpy:9675
translate german PA_HC_EV32_c554115f:

    # mc "Y sobre esa ayuda de la que hablaba al comienzo de nuestra conversación..."
    mc "Und was die Hilfe angeht, von der sie zu Beginn unseres Gesprächs gesprochen haben..."

# game/hchar.rpy:9677
translate german PA_HC_EV32_440002ed:

    # jul "Oui, lo siento, a veces me dejo llevar por los temas que van apareciendo."
    jul "Oui, tut mir leid, manchmal lasse ich mich von den Themen, die auftauchen, mitreissen."

# game/hchar.rpy:9679
translate german PA_HC_EV32_651044f1:

    # mc "(Es mi oportunidad para sacar un poco de información.)"
    mc "(Das ist meine Gelegenheit, ein paar Informationen zu bekommen.)"

# game/hchar.rpy:9680
translate german PA_HC_EV32_86930fc5:

    # mc "Parece ser que usted es nueva en Argleton."
    mc "Sie scheinen neu in Argleton zu sein."

# game/hchar.rpy:9682
translate german PA_HC_EV32_93d13ca9:

    # jul "Veo que eres muy observador."
    jul "Ich sehe, dass du sehr aufmerksam bist."

# game/hchar.rpy:9684
translate german PA_HC_EV32_7884bca4:

    # mc "Podría apostar a que viene a disfrutar de unas relajantes vacaciones."
    mc "Ich würde wetten, dass sie hergekommen sind, um einen entspannten Urlaub zu geniessen."

# game/hchar.rpy:9685
translate german PA_HC_EV32_7a5b6522:

    # mc "Mucha gente viene a Argleton para disfrutar de la tranquilidad."
    mc "Viele Menschen kommen nach Argleton, um die Ruhe zu geniessen."

# game/hchar.rpy:9687
translate german PA_HC_EV32_fed21e64:

    # jul "La verdad es que vengo a buscar a una de mis hijas."
    jul "Die Wahrheit ist, dass ich gekommen bin, um eine meiner Töchter abzuholen."

# game/hchar.rpy:9688
translate german PA_HC_EV32_01c71a0d:

    # jul "Lleva bastante tiempo desaparecida."
    jul "Sie ist schon seit geraumer Zeit verschwunden."

# game/hchar.rpy:9689
translate german PA_HC_EV32_4dc399cb:

    # jul "Y tengo entendido ha estado aquí en Argleton todo este tiempo."
    jul "Und soweit ich weiss, warst du die ganze Zeit hier in Argleton."

# game/hchar.rpy:9690
translate german PA_HC_EV32_721fb77e:

    # jul "¿No has visto a una chica extranjera por casualidad?"
    jul "Hast du zufällig ein ausländisches Mädchen gesehen?"

# game/hchar.rpy:9692
translate german PA_HC_EV32_73c0fbbf:

    # mc "He visto bastantes chicas extranjeras."
    mc "Ich habe schon viele ausländische Mädchen gesehen."

# game/hchar.rpy:9693
translate german PA_HC_EV32_0d8dfee0:

    # mc "Suelen venir muchos turistas de visita a Argleton."
    mc "Es kommen oft viele Touristen nach Argleton."

# game/hchar.rpy:9695
translate german PA_HC_EV32_b779a4a0:

    # jul "Debería ser muy parecida a mí, con el cabello tranzado hacia la izquierda."
    jul "Sie sollte mir sehr ähnlich sehen, mit nach links geflochtenen Haaren."

# game/hchar.rpy:9696
translate german PA_HC_EV32_73e848db:

    # jul "Suele llevar los ojos delineados y los labios pintados de color rosa brillante."
    jul "Sie trägt meist Eyeliner und leuchtend rosa Lippenstift."

# game/hchar.rpy:9698
translate german PA_HC_EV32_972f189b:

    # mc "No me suena de nada."
    mc "Das sagt mir überhaupt nichts."

# game/hchar.rpy:9699
translate german PA_HC_EV32_50936674:

    # mc "(Esa descripción no calza del todo con la Charlotte que conozco.)"
    mc "(Diese Beschreibung passt nicht ganz zu der Charlotte, die ich kenne.)"

# game/hchar.rpy:9700
translate german PA_HC_EV32_e47411fb:

    # mc "(Ella suele llevar el cabello tranzado hacia la derecha y usar lentes.)"
    mc "(Normalerweise trägt sie ihr Haar nach rechts gekämmt und eine Brille.)"

# game/hchar.rpy:9702
translate german PA_HC_EV32_43e076ee:

    # jul "¿Acaso has recordado a alguien que encaja con mi descripción?"
    jul "Fällt dir jemand ein, auf den meine Beschreibung passt?"

# game/hchar.rpy:9704
translate german PA_HC_EV32_c08b0d92:

    # mc "Estaba intentando pensar, pero no se me viene nadie a la mente."
    mc "Ich habe versucht, mir jemanden vorzustellen, aber mir fällt niemand ein."

# game/hchar.rpy:9705
translate german PA_HC_EV32_ba295729:

    # mc "La verdad es que yo también estoy de visita por el pueblo."
    mc "Die Wahrheit ist, dass ich auch nur zu Besuch im Dorf bin."

# game/hchar.rpy:9707
translate german PA_HC_EV32_4ee437cd:

    # jul "Eso explica por qué tu acento parecía diferente a la gente con la que he hablado."
    jul "Das erklärt, warum dein Akzent anders klang als der der Leute, mit denen ich gesprochen habe."

# game/hchar.rpy:9709
translate german PA_HC_EV32_0b3c1461:

    # jul "Entonces supongo que es todo por el día de hoy..."
    jul "Dann ist das wohl alles für heute..."

# game/hchar.rpy:9710
translate german PA_HC_EV32_14a72501:

    # jul "Creo que continuaré con mi búsqueda mañana."
    jul "Ich glaube, ich werde meine Suche morgen fortsetzen."

# game/hchar.rpy:9711
translate german PA_HC_EV32_93c74922:

    # jul "Estoy muy cansada y mis pies me están matando."
    jul "Ich bin sehr müde und meine Füsse tun mir weh."

# game/hchar.rpy:9713
translate german PA_HC_EV32_22c5cca7:

    # mc "Espero que tenga un lugar donde quedarse."
    mc "Ich hoffe, Sie haben eine Unterkunft."

# game/hchar.rpy:9715
translate german PA_HC_EV32_f48d3b90:

    # jul "Tengo entendido que mi madre tenía una antigua casa en este pueblo."
    jul "Ich habe gehört, dass meine Mutter ein altes Haus in diesem Dorf hatte."

# game/hchar.rpy:9716
translate german PA_HC_EV32_33447a06:

    # jul "Mi intención era preguntarte por una dirección."
    jul "Ich wollte dich nach einer Adresse fragen."

# game/hchar.rpy:9717
translate german PA_HC_EV32_03d4edd0:

    # jul "Pero me he dejado llevar por la conversación, como ya te había dicho."
    jul "Aber ich habe mich, wie ich dir schon gesagt habe, von dem Gespräch mitreissen lassen."

# game/hchar.rpy:9718
translate german PA_HC_EV32_b2082790:

    # jul "Debería ser una pequeña casa celeste, junto a la orilla de la playa."
    jul "Es sollte ein kleines himmelblaues Haus sein, direkt am Strand."

# game/hchar.rpy:9719
translate german PA_HC_EV32_54a094e7:

    # jul "Pero camino hacia aquí he visto varias viviendas que cumplen con esas características."
    jul "Aber auf dem Weg hierher habe ich mehrere Häuser gesehen, die diese Merkmale aufweisen."

# game/hchar.rpy:9721
translate german PA_HC_EV32_ca6fc6b6:

    # mc "(Debería buscar una forma de desviar su atención.)"
    mc "(Ich sollte einen Weg finden, ihre Aufmerksamkeit abzulenken.)"

# game/hchar.rpy:9722
translate german PA_HC_EV32_7921de5f:

    # mc "(Seguramente Sophia no ha tenido tiempo para sacar sus cosas de la casa.)"
    mc "(Sophia hatte sicherlich keine Zeit, ihre Sachen aus dem Haus zu holen.)"

# game/hchar.rpy:9723
translate german PA_HC_EV32_4a1ecf40:

    # mc "¿No tiene alguna otra pista?"
    mc "Haben Sie keine weiteren Hinweise?"

# game/hchar.rpy:9725
translate german PA_HC_EV32_8f81ea2f:

    # jul "Me dijeron que debería estar cerca de la playa Gold."
    jul "Man sagte mir, ich solle in der Nähe von Gold Beach sein."

# game/hchar.rpy:9727
translate german PA_HC_EV32_ca19a23e:

    # mc "Déjame pensar un momento."
    mc "Lass mich kurz überlegen."

# game/hchar.rpy:9728
translate german PA_HC_EV32_b8dc15ad:

    # mc "Así que junto a la playa Gold..."
    mc "Also neben dem Gold Beach..."

# game/hchar.rpy:9729
translate german PA_HC_EV32_d23042b6:

    # mc "Lamento decir que se ha desviado más de la cuenta."
    mc "Leider muss ich sagen, dass Sie zu weit spaziert sind."

# game/hchar.rpy:9730
translate german PA_HC_EV32_14b435c4:

    # mc "La playa Gold está justo al otro extremo del pueblo, caminando por la costa."
    mc "Der Strand Gold liegt direkt am anderen Ende des Dorfes, wenn man an der Küste entlangläuft."

# game/hchar.rpy:9732
translate german PA_HC_EV32_1d6c0899:

    # jul "¿Estás seguro?"
    jul "Bist du sicher?"

# game/hchar.rpy:9733
translate german PA_HC_EV32_f4645a1e:

    # jul "Vengo desde allí y la mujer del muelle dijo todo lo contrario."
    jul "Ich komme von dort und die Frau am Kai hat genau das Gegenteil gesagt."

# game/hchar.rpy:9735
translate german PA_HC_EV32_a17f5c98:

    # mc "La mujer del muelle suele equivocarse."
    mc "Die Frau am Kai irrt sich oft."

# game/hchar.rpy:9737
translate german PA_HC_EV32_c961061f:

    # jul "Quizás debería ir a preguntar en el restaurant de los Barrie."
    jul "Vielleicht sollte ich im Restaurant der Barries nachfragen."

# game/hchar.rpy:9739
translate german PA_HC_EV32_e3783ff7:

    # mc "(Mierda, si va al [restaurant] se va a encontrar cara a cara con Charlotte.)"
    mc "(Mist, wenn sie ins [restaurant] geht, wird sie Charlotte begegnen.)"

# game/hchar.rpy:9740
translate german PA_HC_EV32_fcc44a4e:

    # mc "¿Por qué a ese lugar en específico?"
    mc "Warum gerade dieser Ort?"

# game/hchar.rpy:9742
translate german PA_HC_EV32_cab5708a:

    # jul "Mi anciana madre dijo que esa familia siempre está dispuesta a ayudar a los extraños."
    jul "Meine alte Mutter sagte, dass diese Familie immer bereit ist, Fremden zu helfen."

# game/hchar.rpy:9744
translate german PA_HC_EV32_e4977016:

    # mc "(Bien, cambios de planes.)"
    mc "(Nun gut, Planänderung.)"

# game/hchar.rpy:9745
translate german PA_HC_EV32_f174790e:

    # mc "Ahora que lo menciona..."
    mc "Jetzt, wo Sie es erwähnen..."

# game/hchar.rpy:9746
translate german PA_HC_EV32_79dc2e4d:

    # mc "Creo que no será necesario."
    mc "Ich glaube, das wird nicht nötig sein."

# game/hchar.rpy:9747
translate german PA_HC_EV32_b66ddcf0:

    # mc "He tenido un lapsus mental y ya sé cuál podría ser la casa que está buscando."
    mc "Ich hatte einen Gedächtnisausfall und weiss jetzt, welches Haus Sie suchen könnten."

# game/hchar.rpy:9749
translate german PA_HC_EV32_cb174c85:

    # jul "¿De verdad?"
    jul "Wirklich?"

# game/hchar.rpy:9751
translate german PA_HC_EV32_90ce18ae:

    # mc "Desde este punto debe caminar en dirección a la iglesia."
    mc "Von diesem Punkt aus müssen Sie in Richtung Kirche gehen."

# game/hchar.rpy:9752
translate german PA_HC_EV32_0f085140:

    # mc "El jardín debería estar repleto de flores blancas y azules."
    mc "Der Garten sollte voller weißer und blauer Blumen sein."

# game/hchar.rpy:9753
translate german PA_HC_EV32_e19e1b69:

    # mc "Y la casa es celeste, tal como dijo en su descripción."
    mc "Und das Haus ist hellblau, genau wie in Ihrer Beschreibung angegeben."

# game/hchar.rpy:9755
translate german PA_HC_EV32_8f23d617:

    # jul "¿Entonces esta era la playa Gold después de todo?"
    jul "War das also doch der Gold Beach"

# game/hchar.rpy:9757
translate german PA_HC_EV32_b3fa0e92:

    # mc "Si, por alguna razón me he visto desorientado por un segundo."
    mc "Ja, aus irgendeinem Grund war ich für einen Moment verwirrt."

# game/hchar.rpy:9759
translate german PA_HC_EV32_0bb7c31b:

    # jul "¡Hermoso!"
    jul "Wunderschön!"

# game/hchar.rpy:9760
translate german PA_HC_EV32_3ae5fb73:

    # jul "¡Por fin podré descansar!"
    jul "Endlich kann ich mich ausruhen!"

# game/hchar.rpy:9761
translate german PA_HC_EV32_c3b1a500:

    # jul "¡Merci por la ayuda joven [nombre]!"
    jul "Merci für die Hilfe, junger Mann, wie hiessen gleich nochmal? Ach ja... [Name]!"

# game/hchar.rpy:9763
translate german PA_HC_EV32_5e097648:

    # mc "De nada, espero que disfrute su estancia en Argleton."
    mc "Gern geschehen, ich hoffe, Sie geniessen Ihren Aufenthalt in Argleton."

# game/hchar.rpy:9768
translate german PA_HC_EV32_fdf464d4:

    # "<La mujer te dedica una sonrisa y se dirige a paso lento hacia la casa celeste.>"
    "<Die Frau schenkt dir ein Lächeln und geht langsam auf das hellblaue Haus zu.>"

# game/hchar.rpy:9769
translate german PA_HC_EV32_66eabce9:

    # "<Cuando se ha alejado lo suficiente, vas a buscar a Sophia, que aún estaba escondida.>"
    "<Als sie weit genug entfernt ist, suchst du Sophia, die sich noch versteckt hält.>"

# game/hchar.rpy:9774
translate german PA_HC_EV32_303aa781:

    # so "¿Qué hiciste qué?"
    so "Was hast du getan?"

# game/hchar.rpy:9776
translate german PA_HC_EV32_624fddac:

    # so "¡Te has vuelto completamente loco!"
    so "Du bist völlig verrückt geworden!"

# game/hchar.rpy:9778
translate german PA_HC_EV32_40dd8ced:

    # mc "No me ha dejado otra opción."
    mc "Sie hat mir keine andere Wahl gelassen."

# game/hchar.rpy:9780
translate german PA_HC_EV32_c5d24c0e:

    # so "¡Cuando entre a la casa va a ver mis cosas!"
    so "Wenn sie das Haus betrittst, wird sie meine Sachen sehen!"

# game/hchar.rpy:9781
translate german PA_HC_EV32_1694af40:

    # so "¡Mi ropa debe estar toda desordenada y no he tenido tiempo de limpiar mi habitación!"
    so "Meine Kleidung muss total durcheinander sein und ich hatte keine Zeit, mein Zimmer aufzuräumen!"

# game/hchar.rpy:9783
translate german PA_HC_EV32_6bd1d851:

    # mc "¿De verdad eso es lo que mas te preocupa?"
    mc "Ist das wirklich das, was dich am meisten beschäftigt?"

# game/hchar.rpy:9785
translate german PA_HC_EV32_aa5060d7:

    # so "No solo eso..."
    so "Nicht nur das..."

# game/hchar.rpy:9786
translate german PA_HC_EV32_70190f52:

    # so "Cuando vea todo ese desorden..."
    so "Wenn sie all dieses Durcheinander sieht..."

# game/hchar.rpy:9787
translate german PA_HC_EV32_c3b3ee3d:

    # so "Llegará a la conclusión de que solo he estado perdiendo el tiempo aquí."
    so "Wird sie zu dem Schluss kommen, dass ich hier nur meine Zeit verschwendet habe."

# game/hchar.rpy:9789
translate german PA_HC_EV32_267fa221:

    # mc "No entiendo por qué tienes tanto miedo."
    mc "Ich verstehe nicht, warum du so viel Angst hast."

# game/hchar.rpy:9790
translate german PA_HC_EV32_caffe1ab:

    # mc "Tu madre parecía ser una persona muy amable."
    mc "Deine Mutter schien eine sehr freundliche Person zu sein."

# game/hchar.rpy:9792
translate german PA_HC_EV32_ae0a0453:

    # so "Eso es porque aún no la has visto enojada."
    so "Das liegt daran, dass du sie noch nie wütend gesehen hast."

# game/hchar.rpy:9794
translate german PA_HC_EV32_864bb27f:

    # so "Oh no... non non non..."
    so "Oh non... non non non..."

# game/hchar.rpy:9795
translate german PA_HC_EV32_38e374cb:

    # so "Esto es terrible."
    so "Das ist schrecklich."

# game/hchar.rpy:9796
translate german PA_HC_EV32_08340d8e:

    # so "Me gustaría ser tragada por la tierra ahora mismo."
    so "Ich würde mich am liebsten im Erdboden versinken lassen."

# game/hchar.rpy:9798
translate german PA_HC_EV32_40329150:

    # mc "Intenta tranquilizarte, Sophia."
    mc "Versuche dich zu beruhigen, Sophia."

# game/hchar.rpy:9800
translate german PA_HC_EV32_d5721432:

    # so "No me pidas eso cuando todo mi cuerpo está temblando."
    so "Bitte mich nicht darum, wenn mein ganzer Körper zittert."

# game/hchar.rpy:9801
translate german PA_HC_EV32_83163abc:

    # so "Como he terminado metida en esta situación..."
    so "Wie bin ich nur in diese Situation geraten..."

# game/hchar.rpy:9802
translate german PA_HC_EV32_ac5a132e:

    # so "Ahora ni siquiera puedo ir a casa para cambiarme de ropa."
    so "Jetzt kann ich nicht einmal mehr nach Hause gehen, um mich umzuziehen."

# game/hchar.rpy:9803
translate german PA_HC_EV32_1f04dbce:

    # so "Porque me voy a encontrar con ella."
    so "Weil ich sie treffen werde."

# game/hchar.rpy:9805
translate german PA_HC_EV32_7ff8b424:

    # so "Oh no... non non non non non..."
    so "Oh non... non non non non non..."

# game/hchar.rpy:9807
translate german PA_HC_EV32_b286c944:

    # mc "Tu madre dijo que iba a continuar con la búsqueda mañana."
    mc "Deine Mutter sagte, sie würde morgen mit der Suche fortfahren."

# game/hchar.rpy:9808
translate german PA_HC_EV32_25045e77:

    # mc "Eso nos da un poco de tiempo para pensar en alguna alternativa."
    mc "Das gibt uns etwas Zeit, um über eine Alternative nachzudenken."

# game/hchar.rpy:9809
translate german PA_HC_EV32_846acb3f:

    # mc "Pero primero deberíamos volver al [restaurant] y contarle lo sucedido a Charlotte."
    mc "Aber zuerst sollten wir zum [restaurant] zurückkehren und Charlotte erzählen, was passiert ist."

# game/hchar.rpy:9811
translate german PA_HC_EV32_7a2e421f:

    # so "¡Ay, por favor, espero que no hables en serio!"
    so "Oh, bitte, ich hoffe, das meinst du nicht ernst!"

# game/hchar.rpy:9812
translate german PA_HC_EV32_7c7269f7:

    # so "¿De verdad piensas que voy a caminar por medio pueblo con estas vestimentas?"
    so "Glaubst du wirklich, ich würde in diesen Kleidern durch die halbe Stadt laufen?"

# game/hchar.rpy:9813
translate german PA_HC_EV32_b9a24ec7:

    # so "O sea, eso sería superridiculo y humillante."
    so "Das wäre total lächerlich und erniedrigend."

# game/hchar.rpy:9815
translate german PA_HC_EV32_2fd35bb1:

    # mc "Vamos, es solo un traje de baño, no sucederá nada."
    mc "Komm schon, es ist nur ein Badeanzug, es wird schon nichts passieren."

# game/hchar.rpy:9817
translate german PA_HC_EV32_21f40239:

    # so "Eso lo dices porque no tienes el mínimo sentido de la moda y la vergüenza."
    so "Das sagst du nur, weil du keinen Sinn für Mode hast und keine Scham empfindest."

# game/hchar.rpy:9819
translate german PA_HC_EV32_aa898ad8:

    # mc "Bien, haz lo que quieras."
    mc "Gut, mach, was du willst."

# game/hchar.rpy:9820
translate german PA_HC_EV32_f571f74e:

    # mc "Yo debo ir a advertir a Charlotte sobre la llegada de su madre."
    mc "Ich muss Charlotte über die Ankunft ihrer Mutter informieren."

# game/hchar.rpy:9822
translate german PA_HC_EV32_c25d6773:

    # so "¡Está bien!"
    so "Okay!"

# game/hchar.rpy:9823
translate german PA_HC_EV32_ea4112d5:

    # so "¡Me has convencido!"
    so "Du hast mich überzeugt!"

# game/hchar.rpy:9824
translate german PA_HC_EV32_cb89ab04:

    # so "¡Iré contigo, pero procura caminar lo más rápido que puedas!"
    so "Ich komme mit, aber versuch, so schnell wie möglich zu gehen!"

# game/hchar.rpy:9829
translate german PA_HC_EV32_b098f9ba:

    # "<Vas rápidamente junto a Sophia en dirección al [restaurant], mientras se oculta tras de ti durante todo el trayecto.>"
    "<Du gehst schnell neben Sophia in Richtung [restaurant], während sie sich die ganze Zeit hinter dir versteckt.>"

# game/hchar.rpy:9834
translate german PA_HC_EV32_a1aef22f:

    # c "¿Se puede saber qué haces aquí con el bikini puesto Sophia?"
    c "Darf ich fragen, was du hier in deinem Bikini machst, Sophia?"

# game/hchar.rpy:9835
translate german PA_HC_EV32_e95faf4d:

    # c "Espera, déjame adivinar..."
    c "Warte, lass mich raten..."

# game/hchar.rpy:9836
translate german PA_HC_EV32_5c83fba7:

    # c "Si vienes a buscarme para ir a la playa, puedes irte olvidando."
    c "Wenn du mich abholen willst, um an den Strand zu gehen, kannst du das vergessen."

# game/hchar.rpy:9838
translate german PA_HC_EV32_3985d938:

    # so "Lo último que haría en ese momento sería ir a la playa."
    so "Das Letzte, was ich in diesem Moment tun würde, wäre, an den Strand zu gehen."

# game/hchar.rpy:9840
translate german PA_HC_EV32_8aedbd64:

    # c "Como sea..."
    c "Wie auch immer..."

# game/hchar.rpy:9841
translate german PA_HC_EV32_798296da:

    # c "Ahora estoy ocupada Sophia, hablaremos más tarde."
    c "Ich bin gerade beschäftigt, Sophia. Wir sprechen später."

# game/hchar.rpy:9843
translate german PA_HC_EV32_f11b8435:

    # so "¡Deja el trabajo que esto es mucho más importante!"
    so "Lass die Arbeit liegen, das hier ist viel wichtiger!"

# game/hchar.rpy:9845
translate german PA_HC_EV32_826abafc:

    # c "Estoy comenzando a enojarme por tus constantes interrupciones."
    c "Ich werde langsam wütend wegen deiner ständigen Unterbrechungen."

# game/hchar.rpy:9847
translate german PA_HC_EV32_01d3c617:

    # so "Madre ha llegado a Argleton."
    so "Mutter ist in Argleton angekommen."

# game/hchar.rpy:9849
translate german PA_HC_EV32_6406c2be:

    # c "Si, lo que sea, pero ahora necesito trabajar."
    c "Ja, was auch immer, aber jetzt muss ich arbeiten."

# game/hchar.rpy:9851
translate german PA_HC_EV32_41da6424:

    # c "Espera..."
    c "Warte..."

# game/hchar.rpy:9852
translate german PA_HC_EV32_fc0af323:

    # c "¿Qué es lo que acabas de decir?"
    c "Was hast du gerade gesagt?"

# game/hchar.rpy:9854
translate german PA_HC_EV32_095d0c64:

    # so "Debería estar en la casa de Lissette ahora mismo."
    so "Ich sollte jetzt bei Lissette zu Hause sein."

# game/hchar.rpy:9856
translate german PA_HC_EV32_b23bf872:

    # c "No puede ser..."
    c "Das kann nicht sein..."

# game/hchar.rpy:9857
translate german PA_HC_EV32_367ca96e:

    # c "¿Y qué fue lo que te dijo?"
    c "Und was hat sie dir gesagt?"

# game/hchar.rpy:9859
translate german PA_HC_EV32_626ffdbb:

    # so "¡No me dijo nada!"
    so "Sie hat mir nichts gesagt!"

# game/hchar.rpy:9860
translate german PA_HC_EV32_e0ec658d:

    # so "¡Cuando la vi a lo lejos entre en pánico!"
    so "Als ich sie in der Ferne sah, geriet ich in Panik!"

# game/hchar.rpy:9861
translate german PA_HC_EV32_1390bf0a:

    # so "¡Y mi única reacción fue correr y esconderme!"
    so "Und meine einzige Reaktion war, wegzulaufen und mich zu verstecken!"

# game/hchar.rpy:9863
translate german PA_HC_EV32_baa7a881:

    # c "Merde..."
    c "Merde..."

# game/hchar.rpy:9864
translate german PA_HC_EV32_b68d4a6a:

    # c "Pero era de esperarse..."
    c "Aber das war zu erwarten..."

# game/hchar.rpy:9865
translate german PA_HC_EV32_a8a45089:

    # c "Esto tenía que suceder en algún momento."
    c "Das musste irgendwann passieren."

# game/hchar.rpy:9866
translate german PA_HC_EV32_4ae3e0d4:

    # c "Aunque no pensaba que sería tan pronto."
    c "Ich hätte allerdings nicht gedacht, dass es so schnell gehen würde."

# game/hchar.rpy:9868
translate german PA_HC_EV32_e08c092a:

    # so "¿Y ahora que vamos a hacer?"
    so "Was machen wir jetzt?"

# game/hchar.rpy:9870
translate german PA_HC_EV32_d19f855d:

    # c "..."
    c "..."

# game/hchar.rpy:9872
translate german PA_HC_EV32_0c3c75d1:

    # c "No se me viene nada a la mente, Sophia."
    c "Mir fällt nichts ein, Sophia."

# game/hchar.rpy:9874
translate german PA_HC_EV32_61818996:

    # so "¡Pensaba que tendrías un plan pensado!"
    so "Ich dachte, du hättest einen Plan!"

# game/hchar.rpy:9876
translate german PA_HC_EV32_3a31e746:

    # so "¡Si acabas de decir que esperabas la llegada de Madre!"
    so "Hast du gerade gesagt, dass du auf die Ankunft von Mutter gewartet hast?"

# game/hchar.rpy:9878
translate german PA_HC_EV32_48122fc3:

    # so "No puedo creerlo..."
    so "Ich kann es nicht glauben..."

# game/hchar.rpy:9879
translate german PA_HC_EV32_a2091737:

    # so "¡Esto es superestresante!"
    so "Das ist super stressig!"

# game/hchar.rpy:9880
translate german PA_HC_EV32_9ae0a7a1:

    # so "Oh non..."
    so "Oh non..."

# game/hchar.rpy:9881
translate german PA_HC_EV32_9ff30fa2:

    # so "Siento que me falta el aire y me voy a morir en cualquier momento."
    so "Ich habe das Gefühl, keine Luft zu bekommen und jeden Moment zu sterben."

# game/hchar.rpy:9882
translate german PA_HC_EV32_a000fa52:

    # so "¡Quizás morir sea el único escape a esta situación!"
    so "Vielleicht ist der Tod der einzige Ausweg aus dieser Situation!"

# game/hchar.rpy:9884
translate german PA_HC_EV32_9090f5ee:

    # c "¡Cállate de una vez Sophia!"
    c "Halt endlich die Klappe, Sophia!"

# game/hchar.rpy:9885
translate german PA_HC_EV32_38943144:

    # c "Estoy intentando pensar y tú no haces más que gritar."
    c "Ich versuche nachzudenken, und du schreist nur herum."

# game/hchar.rpy:9887
translate german PA_HC_EV32_31c97a2a:

    # so "¡Eso es porque he terminado metida en este problema por tu culpa!"
    so "Das liegt daran, dass ich wegen dir in diese Schwierigkeiten geraten bin!"

# game/hchar.rpy:9888
translate german PA_HC_EV32_5d0b26c9:

    # so "¡La Charlotte que recordaba tendría bajo control una situación como esta!"
    so "Die Charlotte, an die ich mich erinnerte, hätte eine solche Situation unter Kontrolle gehabt!"

# game/hchar.rpy:9889
translate german PA_HC_EV32_cc8506bf:

    # so "¡No sé en que momento te volviste una irresponsable!"
    so "Ich weiss nicht, seit wann du so unverantwortlich geworden bist!"

# game/hchar.rpy:9891
translate german PA_HC_EV32_90b9f803:

    # c "¡Si no te callas, usaré el cuchillo de cocina y te serviré en el menú de mañana!"
    c "Wenn du nicht den Mund hältst, benutze ich das Küchenmesser und serviere dich morgen auf der Speisekarte!"

# game/hchar.rpy:9893
translate german PA_HC_EV32_3f5c4177:

    # mc "Alto ahí Hannibal Lecter."
    mc "Halt, Hannibal Lecter."

# game/hchar.rpy:9895
translate german PA_HC_EV32_afbd772a:

    # c "No estoy de ánimo para tus chistes, [nombre]."
    c "Ich bin nicht in der Stimmung für deine Witze, [nombre]."

# game/hchar.rpy:9897
translate german PA_HC_EV32_87358912:

    # mc "Si perdemos la calma, no podremos pensar en una solución."
    mc "Wenn wir die Ruhe verlieren, können wir nicht über eine Lösung nachdenken."

# game/hchar.rpy:9899
translate german PA_HC_EV32_bd6fb8c0:

    # c "¡Pero!"
    c "Aber!"

# game/hchar.rpy:9901
translate german PA_HC_EV32_50365074:

    # c "Oui..."
    c "Oui..."

# game/hchar.rpy:9902
translate german PA_HC_EV32_eff4dc9f:

    # c "Supongo que tienes razón."
    c "Ich denke, du hast Recht."

# game/hchar.rpy:9904
translate german PA_HC_EV32_72a36f61:

    # so "Yo tengo una idea..."
    so "Ich habe eine Idee..."

# game/hchar.rpy:9905
translate german PA_HC_EV32_12af2b93:

    # so "Primero nos fugaremos a un país en medio del Caribe."
    so "Zuerst fliehen wir in ein Land mitten in der Karibik."

# game/hchar.rpy:9906
translate german PA_HC_EV32_75b89162:

    # so "Tendremos que hacer muchos transbordos para que sea imposible rastrear la ruta exacta."
    so "Wir müssen viele Umstiege machen, damit es unmöglich ist, die genaue Route nachzuvollziehen."

# game/hchar.rpy:9908
translate german PA_HC_EV32_49417bc4:

    # so "Luego nos haremos una cirugía de cara y cambiaremos nuestros nombres."
    so "Dann lassen wir uns einer Gesichtsoperation unterziehen und ändern unsere Namen."

# game/hchar.rpy:9909
translate german PA_HC_EV32_8aff3c72:

    # so "Yo me llamaré Margaret y tu Carolina."
    so "Ich werde Margaret heissen und du Carolina."

# game/hchar.rpy:9910
translate german PA_HC_EV32_5bdd0a43:

    # so "Y tendremos un negocio de arriendo de botes para pesca."
    so "Und wir werden ein Geschäft für die Vermietung von Fischerbooten betreiben."

# game/hchar.rpy:9912
translate german PA_HC_EV32_6a0e47d3:

    # c "Por favor Sophia..."
    c "Bitte, Sophia..."

# game/hchar.rpy:9913
translate german PA_HC_EV32_289db1c1:

    # c "¡Deja de seguir pensando como una niña pequeña!"
    c "Hör auf, wie ein kleines Mädchen zu denken!"

# game/hchar.rpy:9915
translate german PA_HC_EV32_0ad62f8a:

    # so "¡Al menos estoy dando ideas!"
    so "Zumindest gebe ich Ideen!"

# game/hchar.rpy:9917
translate german PA_HC_EV32_0951cd01:

    # c "Bien..."
    c "Gut..."

# game/hchar.rpy:9918
translate german PA_HC_EV32_afe159b4:

    # c "Como sea."
    c "Wie auch immer."

# game/hchar.rpy:9920
translate german PA_HC_EV32_4e77b58a:

    # so "¿De verdad piensas rendirte tan fácil?"
    so "Willst du wirklich so leicht aufgeben?"

# game/hchar.rpy:9922
translate german PA_HC_EV32_f3797bbd:

    # mc "No puedo creer el miedo que sienten por su propía madre."
    mc "Ich kann nicht glauben, wie viel Angst ihr vor eurer eigenen Mutter habt."

# game/hchar.rpy:9924
translate german PA_HC_EV32_f0eee11c:

    # so "Eso es porque no la conoces [nombre]."
    so "Das liegt daran, dass du sie nicht kennst [nombre]."

# game/hchar.rpy:9925
translate german PA_HC_EV32_8ea5df74:

    # so "Todos los que conocen el lado oscuro de Madre, tienen miedo de su temperamento."
    so "Alle, die die dunkle Seite von Mutter kennen, fürchten sich vor ihrem Temperament."

# game/hchar.rpy:9926
translate german PA_HC_EV32_ca530683:

    # so "Ninguna persona que conozca intenta discutir con ella, ni siquiera Padre o Lissette."
    so "Niemand, der sie kennt, versucht, mit ihr zu diskutieren, nicht einmal Vater oder Lissette."

# game/hchar.rpy:9928
translate german PA_HC_EV32_07bc8c54:

    # mc "No me ha dado esa impresión cuando hablamos."
    mc "Das habe ich nicht so empfunden, als wir miteinander gesprochen haben."

# game/hchar.rpy:9930
translate german PA_HC_EV32_9fd8db24:

    # c "¡Espera un momento [nombre]!"
    c "Warte mal kurz, [nombre]!"

# game/hchar.rpy:9931
translate german PA_HC_EV32_ef3d5eed:

    # c "¿Has hablado con Juliette?"
    c "Hast du mit Juliette gesprochen?"

# game/hchar.rpy:9932
translate german PA_HC_EV32_367ca96e_1:

    # c "¿Y qué fue lo que te dijo?"
    c "Und was hat sie dir gesagt?"

# game/hchar.rpy:9934
translate german PA_HC_EV32_f46c7713:

    # mc "Estaba buscando a una de sus hijas, de ojos delineados, labios color rosas brillante y con el cabello tranzado hacia la izquierda."
    mc "Sie suchte eine ihrer Töchter mit geschminkten Augen, leuchtend rosa Lippen und nach links geflochtenen Haaren."

# game/hchar.rpy:9935
translate german PA_HC_EV32_42576ae6:

    # mc "Yo diría que esa descripción no encaja del todo."
    mc "Ich würde sagen, dass diese Beschreibung nicht ganz zutrifft."

# game/hchar.rpy:9937
translate german PA_HC_EV32_9016326b:

    # c "No hay dudas..."
    c "Es gibt keinen Zweifel..."

# game/hchar.rpy:9938
translate german PA_HC_EV32_606a5c6c:

    # c "Ella viene por mí."
    c "Sie kommt, um mich zu holen."

# game/hchar.rpy:9939
translate german PA_HC_EV32_5a78258d:

    # c "Ese era el estilo usual que solía llevar la antigua Charlotte."
    c "Das war der übliche Stil, den die alte Charlotte pflegte."

# game/hchar.rpy:9941
translate german PA_HC_EV32_f21e475d:

    # mc "¿La antigua Charlotte?"
    mc "Die alte Charlotte?"

# game/hchar.rpy:9943
translate german PA_HC_EV32_24ecb3ec:

    # c "Juliette me obligaba a llevar un estilo específico para dar una buena imagen."
    c "Juliette zwang mich, einen bestimmten Stil zu tragen, um einen guten Eindruck zu machen."

# game/hchar.rpy:9944
translate german PA_HC_EV32_0c2c0336:

    # c "Pasaba horas en el salón de belleza hasta quedar perfecta, según su propio criterio."
    c "Sie verbrachte Stunden im Schönheitssalon, bis sie nach ihrem eigenen Ermessen perfekt aussah."

# game/hchar.rpy:9946
translate german PA_HC_EV32_d4a80188:

    # c "Aunque suene estúpido..."
    c "Auch wenn es dumm klingt..."

# game/hchar.rpy:9947
translate german PA_HC_EV32_770f2a80:

    # c "Se podría decir que era menos Charlotte, pero más Charpentier."
    c "Man könnte sagen, dass sie weniger Charlotte, sondern eher Charpentier war."

# game/hchar.rpy:9949
translate german PA_HC_EV32_f32bd7c6:

    # so "Yo también suelo llevar este peinado por Madre."
    so "Ich trage diese Frisur auch oft für meine Mutter."

# game/hchar.rpy:9950
translate german PA_HC_EV32_207fd17a:

    # so "Decía que de esta forma nuestros estilos serían más asimétricos y atractivos."
    so "Sie sagte, dass unsere Stile auf diese Weise asymmetrischer und attraktiver würden."

# game/hchar.rpy:9952
translate german PA_HC_EV32_31bd3680:

    # mc "Parece ser una mujer muy obsesiva."
    mc "Sie scheint eine sehr obsessive Frau zu sein."

# game/hchar.rpy:9954
translate german PA_HC_EV32_d4d92999:

    # c "Todos tienen que hacer lo que ella dice."
    c "Alle müssen tun, was sie sagt."

# game/hchar.rpy:9955
translate german PA_HC_EV32_004d9852:

    # c "Llevar una sonrisa permanente y evitar otro tipo de expresiones faciales."
    c "Ein ständiges Lächeln aufsetzen und andere Gesichtsausdrücke vermeiden."

# game/hchar.rpy:9956
translate german PA_HC_EV32_42b7349b:

    # c "Ya sabes... {w}Para evitar las arrugas y todo eso..."
    c "Du weißt schon... {w}Um Falten und so was zu vermeiden..."

# game/hchar.rpy:9958
translate german PA_HC_EV32_cb779899:

    # so "Yo sé qué Madre hace todo eso por nuestro bien, Charlotte."
    so "Ich weiss, dass Mutter das alles zu unserem Besten tut, Charlotte."

# game/hchar.rpy:9960
translate german PA_HC_EV32_ff9460c0:

    # c "A mí no me gusta llevar esa vida tan plástica, Sophia."
    c "Ich mag dieses künstliche Leben nicht, Sophia."

# game/hchar.rpy:9961
translate german PA_HC_EV32_d24d9afd:

    # c "No es mi estilo..."
    c "Das ist nicht mein Stil..."

# game/hchar.rpy:9962
translate german PA_HC_EV32_5d7718d5:

    # c "Aunque tengo que aceptar que en esos estratos sociales, la gente suele medirse por ese tipo de cosas."
    c "Allerdings muss ich zugeben, dass Menschen in diesen sozialen Schichten sich oft an solchen Dingen messen."

# game/hchar.rpy:9964
translate german PA_HC_EV32_b154e2f2:

    # so "Lo dices como si no disfrutaras de ese estilo de vida."
    so "Du sagst das, als ob du diesen Lebensstil nicht geniessen würdest."

# game/hchar.rpy:9965
translate german PA_HC_EV32_1d279e22:

    # so "¿Acaso no te gusta la ropa cara, las joyas y la mansión?"
    so "Magst du denn keine teuren Kleider, keinen Schmuck und keine Villen?"

# game/hchar.rpy:9967
translate german PA_HC_EV32_d3af6265:

    # c "Eso no es lo que me molesta, Sophia."
    c "Das ist es nicht, was mich stört, Sophia."

# game/hchar.rpy:9968
translate german PA_HC_EV32_bfaf79d2:

    # c "Disfrutaría de esos lujos si me los ganara con mi propio esfuerzo."
    c "Ich würde diesen Luxus geniessen, wenn ich ihn mir durch meine eigene Anstrengung verdient hätte."

# game/hchar.rpy:9969
translate german PA_HC_EV32_1f78abc9:

    # c "Pero este no es el caso..."
    c "Aber das ist nicht der Fall..."

# game/hchar.rpy:9970
translate german PA_HC_EV32_3034f305:

    # c "Disfrutamos de esos lujos gracias al esfuerzo de Mémé."
    c "Dank Mémés Bemühungen können wir diesen Luxus geniessen."

# game/hchar.rpy:9971
translate german PA_HC_EV32_83bbb5d6:

    # c "Es por eso que ella siempre fue distinta a ustedes."
    c "Deshalb war sie immer anders als ihr."

# game/hchar.rpy:9973
translate german PA_HC_EV32_397777b3:

    # c "Ella no nació en cuna de oro..."
    c "Sie wurde nicht in eine wohlhabende Familie hineingeboren..."

# game/hchar.rpy:9974
translate german PA_HC_EV32_36e05590:

    # c "Y por eso es la única que me entiende."
    c "Und deshalb ist sie die Einzige, die mich versteht."

# game/hchar.rpy:9975
translate german PA_HC_EV32_362df900:

    # c "Además vine a Argleton en primer lugar."
    c "Ausserdem bin ich zuerst nach Argleton gekommen."

# game/hchar.rpy:9976
translate german PA_HC_EV32_6baf1d4f:

    # c "Para seguir sus pasos y hacer realidad mis sueños."
    c "Um in ihre Fussstapfen zu treten und meine Träume zu verwirklichen."

# game/hchar.rpy:9978
translate german PA_HC_EV32_fc5a7bca:

    # so "No sé en que momento te has vuelto tan idealista."
    so "Ich weiss nicht, seit wann du so idealistisch geworden bist."

# game/hchar.rpy:9980
translate german PA_HC_EV32_8b612fd3:

    # so "Espera..."
    so "Warte..."

# game/hchar.rpy:9982
translate german PA_HC_EV32_5e7fd9ea:

    # so "¡No me digas que piensas enfrentar a Madre!"
    so "Sag mir nicht, dass du dich mit Mutter anlegen willst!"

# game/hchar.rpy:9984
translate german PA_HC_EV32_09fa95c8:

    # c "Quizás sea la única solución."
    c "Vielleicht ist das die einzige Lösung."

# game/hchar.rpy:9986
translate german PA_HC_EV32_80ea5ddf:

    # so "¿Acaso te has vuelto loca?"
    so "Bist du verrückt geworden?"

# game/hchar.rpy:9987
translate german PA_HC_EV32_612c9452:

    # so "¿Sabes que va a suceder si le llevas la contraria?"
    so "Weisst du, was passiert, wenn du ihr widersprichst?"

# game/hchar.rpy:9988
translate german PA_HC_EV32_27bfa367:

    # so "¡Ella podría ser capaz de dejarte sin herencia!"
    so "Sie könnte dir dein Erbe vorenthalten!"

# game/hchar.rpy:9990
translate german PA_HC_EV32_177d90d3:

    # c "¿De verdad eso es lo único que te preocupa?"
    c "Ist das wirklich das Einzige, was dich beschäftigt?"

# game/hchar.rpy:9992
translate german PA_HC_EV32_b595c2d5:

    # so "¿Y qué podría ser más importante?"
    so "Und was könnte wichtiger sein?"

# game/hchar.rpy:9994
translate german PA_HC_EV32_5333d311:

    # c "Pensaba que habías cambiado un poco, durante el tiempo que pasamos juntas aquí."
    c "Ich dachte, du hättest dich ein wenig verändert, während wir hier zusammen waren."

# game/hchar.rpy:9995
translate german PA_HC_EV32_648bb2d1:

    # c "Pero sigues siendo una materialista."
    c "Aber du bist immer noch materialistisch."

# game/hchar.rpy:9997
translate german PA_HC_EV32_57321bf3:

    # so "Y tu una superficial."
    so "Und du bist oberflächlich."

# game/hchar.rpy:9999
translate german PA_HC_EV32_b038a6e6:

    # so "Mira Charlotte... {w}He disfrutado mucho de nuestro tiempo de caridad."
    so "Schau mal, Charlotte... {w}Ich habe unsere Zeit der Nächstenliebe sehr genossen."

# game/hchar.rpy:10000
translate german PA_HC_EV32_c18f810d:

    # so "Me has enseñado cosas nuevas que no sabía que podía hacer."
    so "Du hast mir neue Dinge beigebracht, von denen ich nicht wusste, dass ich sie kann."

# game/hchar.rpy:10001
translate german PA_HC_EV32_0574d117:

    # so "Desde que éramos unas niñas que no disfrutábamos así."
    so "Seit wir kleine Mädchen waren, hatten wir nicht mehr so viel Spass gehabt."

# game/hchar.rpy:10002
translate german PA_HC_EV32_fdb763b0:

    # so "Cuando no teníamos responsabilidades."
    so "Als wir noch keine Verpflichtungen hatten."

# game/hchar.rpy:10004
translate german PA_HC_EV32_fbf68c7d:

    # c "Y cuando volvamos a casa, seguramente todo volverá a ser como antes."
    c "Und wenn wir nach Hause zurückkehren, wird sicherlich alles wieder so sein wie zuvor."

# game/hchar.rpy:10005
translate german PA_HC_EV32_6cbb072a:

    # c "¿De verdad eso es lo que quieres?"
    c "Ist es wirklich das, was du willst?"

# game/hchar.rpy:10007
translate german PA_HC_EV32_35e7b507:

    # so "Por supuesto que no."
    so "Natürlich nicht."

# game/hchar.rpy:10008
translate german PA_HC_EV32_a46cf963_1:

    # so "Pero..."
    so "Aber..."

# game/hchar.rpy:10009
translate german PA_HC_EV32_ab3f7fec:

    # so "Creo que eso es lo que significa madurar."
    so "Ich glaube, das ist es, was es bedeutet, erwachsen zu werden."

# game/hchar.rpy:10010
translate german PA_HC_EV32_e537db36:

    # so "Hacer los sacrificios necesarios para continuar con el legado de la familia."
    so "Die notwendigen Opfer bringen, um das Vermächtnis der Familie fortzuführen."

# game/hchar.rpy:10011
translate german PA_HC_EV32_8ff3f3e7:

    # so "Ser responsables de nuestros deberes."
    so "Verantwortung für unsere Pflichten übernehmen."

# game/hchar.rpy:10013
translate german PA_HC_EV32_bce34dad:

    # c "Escucharte decir eso me ha hecho sentir como la hermana menor."
    c "Als ich das von dir hörte, fühlte ich mich wie die kleine Schwester."

# game/hchar.rpy:10015
translate german PA_HC_EV32_8b18ade7:

    # so "No hay otra alternativa."
    so "Es gibt keine andere Alternative."

# game/hchar.rpy:10016
translate german PA_HC_EV32_26c2fed7:

    # so "Ya has creado lindos recuerdos en este lugar."
    so "Du hast bereits schöne Erinnerungen an diesen Ort geschaffen."

# game/hchar.rpy:10017
translate german PA_HC_EV32_db253107:

    # so "Pero ahora es necesario volver y retomar tus responsabilidades."
    so "Aber jetzt ist es notwendig, zurückzukehren und deine Aufgaben wieder aufzunehmen."

# game/hchar.rpy:10019
translate german PA_HC_EV32_5e02c4ad:

    # c "Mis responsabilidades..."
    c "Meine Aufgaben..."

# game/hchar.rpy:10021
translate german PA_HC_EV32_fdc69186:

    # c "¿Que dices [nombre]?"
    c "Was sagst du, [nombre]?"

# game/hchar.rpy:10023
translate german PA_HC_EV32_73ce2c27:

    # mc "Debes hacer lo que creas correcto, Charlotte."
    mc "Du musst tun, was du für richtig hältst, Charlotte."

# game/hchar.rpy:10024
translate german PA_HC_EV32_4483dd22:

    # mc "Te voy a apoyar cualquier sea la decisión que escojas."
    mc "Ich werde dich unterstützen, egal wie du dich entscheidest."

# game/hchar.rpy:10026
translate german PA_HC_EV32_bd3ab792:

    # c "Ahora tú suenas como Mémé."
    c "Jetzt klingst du wie Mémé."

# game/hchar.rpy:10028
translate german PA_HC_EV32_8c033d3e:

    # so "¿Necesitas más tiempo para tomar una decisión?"
    so "Brauchst du mehr Zeit, um eine Entscheidung zu treffen?"

# game/hchar.rpy:10029
translate german PA_HC_EV32_f4c9e424:

    # so "¡Porque lo que menos tenemos ahora es tiempo!"
    so "Denn Zeit ist das, was wir derzeit am wenigsten haben!"

# game/hchar.rpy:10031
translate german PA_HC_EV32_ba6e7d1b:

    # c "Ya he pensado en una solución, Sophia."
    c "Ich habe mir schon eine Lösung überlegt, Sophia."

# game/hchar.rpy:10033
translate german PA_HC_EV32_e1bb43f0:

    # so "¿De verdad?"
    so "Wirklich?"

# game/hchar.rpy:10035
translate german PA_HC_EV32_0f227797:

    # c "Pasarás conmigo esta noche."
    c "Du wirst heute Nacht bei mir bleiben."

# game/hchar.rpy:10036
translate german PA_HC_EV32_d97d4edc:

    # c "Mañana te daré un poco de mi ropa y volverás a casa."
    c "Morgen gebe ich dir ein paar meiner Kleider, dann kannst du nach Hause gehen."

# game/hchar.rpy:10037
translate german PA_HC_EV32_825422ea:

    # c "Cuando llegues, dirás a Padre que me has estado buscando, pero no has podido encontrarme."
    c "Wenn du ankommst, erzählst du Vater, dass du mich gesucht hast, mich aber nicht finden konntest."

# game/hchar.rpy:10039
translate german PA_HC_EV32_2d7ab2dc:

    # so "¿Y tú que piensas hacer?"
    so "Und was hast du vor?"

# game/hchar.rpy:10041
translate german PA_HC_EV32_bb321b0c:

    # c "Aún no he pensado en eso."
    c "Darüber habe ich noch nicht nachgedacht."

# game/hchar.rpy:10042
translate german PA_HC_EV32_a47f4388:

    # c "Pero toda esta situación es mi responsabilidad."
    c "Aber diese ganze Situation ist meine Verantwortung."

# game/hchar.rpy:10043
translate german PA_HC_EV32_a1cc1a05:

    # c "No es justo que te veas involucrada."
    c "Es ist nicht fair, dass du da mit hineingezogen wirst."

# game/hchar.rpy:10044
translate german PA_HC_EV32_3988fd62:

    # c "¿De eso se trata madurar, verdad?"
    c "Darum geht es doch beim Erwachsenwerden, oder?"

# game/hchar.rpy:10046
translate german PA_HC_EV32_e89ea4d8:

    # so "Pero Charlotte..."
    so "Aber Charlotte..."

# game/hchar.rpy:10048
translate german PA_HC_EV32_0f047fd6:

    # c "No pienso continuar hablando sobre el tema, Sophia."
    c "Ich werde nicht weiter über dieses Thema sprechen, Sophia."

# game/hchar.rpy:10050
translate german PA_HC_EV32_a043d003:

    # so "Madre debe saber que aún me encuentro en Argleton."
    so "Mutter muss wissen, dass ich mich noch immer in Argleton befinde."

# game/hchar.rpy:10051
translate german PA_HC_EV32_aa5afcfb:

    # so "Seguramente ya visto mi equipaje al entrar a la casa de Lissette."
    so "Sicherlich hat Sie mein Gepäck gesehen, als sie Lissettes Haus betrat."

# game/hchar.rpy:10053
translate german PA_HC_EV32_fedbd9f5:

    # c "No te preocupes, le diré que yo te he robado esas cosas cuando escapé de casa."
    c "Keine Sorge, ich werde ihr sagen, dass ich diese Sachen gestohlen habe, als ich von zu Hause weggelaufen bin."

# game/hchar.rpy:10054
translate german PA_HC_EV32_6e8f4c26:

    # c "Seguramente se lo va a creer al ver que vuelves con mi ropa puesta."
    c "Sie wird es sicher glauben, wenn sie sieht, dass du mit meinen Kleidern zurückkommst."

# game/hchar.rpy:10056
translate german PA_HC_EV32_a3a5f849:

    # so "Si tú lo dices..."
    so "Wenn du das sagst..."

# game/hchar.rpy:10057
translate german PA_HC_EV32_7e404db9:

    # so "Aun así no puedo evitar sentirme muy mal con este plan."
    so "Trotzdem kann ich nicht anders, als mich bei diesem Plan sehr unwohl zu fühlen."

# game/hchar.rpy:10058
translate german PA_HC_EV32_61f4db39:

    # so "Siento que quieres cargar con toda la culpa."
    so "Ich habe das Gefühl, dass du die ganze Schuld auf dich nehmen willst."

# game/hchar.rpy:10059
translate german PA_HC_EV32_d33367a6:

    # so "Cuando yo también decidí quedarme más tiempo del necesario."
    so "Als auch ich mich entschied, länger als nötig zu bleiben."

# game/hchar.rpy:10061
translate german PA_HC_EV32_af397cf5:

    # c "Es preferible que una caiga en el pantano a que seamos las dos."
    c "Es ist besser, wenn eine von uns in den Sumpf fällt, als dass wir beide hineinfallen."

# game/hchar.rpy:10063
translate german PA_HC_EV32_a46cf963_2:

    # so "Pero..."
    so "Aber..."

# game/hchar.rpy:10065
translate german PA_HC_EV32_573fcc41:

    # c "Tranquila, Sophia."
    c "Keine Sorge, Sophia."

# game/hchar.rpy:10066
translate german PA_HC_EV32_72b4c5dd:

    # c "No te quiero ver con esa cara triste."
    c "Ich will dich nicht mit diesem traurigen Gesicht sehen."

# game/hchar.rpy:10068
translate german PA_HC_EV32_5b5d848c:

    # so "Seguramente Madre va a descansar por el día de hoy."
    so "Mutter wird sich heute sicher ausruhen."

# game/hchar.rpy:10069
translate german PA_HC_EV32_a63f2e5e:

    # so "Pero es cosa de tiempo que llegue aquí y te encuentre."
    so "Aber es ist nur eine Frage der Zeit, bis sie hier ankommt und dich findet."

# game/hchar.rpy:10071
translate german PA_HC_EV32_8ac8cd9b:

    # so "¿Y sabes que va a suceder cuando eso ocurra?"
    so "Und weisst du, was passieren wird, wenn das geschieht?"

# game/hchar.rpy:10073
translate german PA_HC_EV32_e34125c8:

    # c "No tiene importancia..."
    c "Das ist nicht wichtig..."

# game/hchar.rpy:10074
translate german PA_HC_EV32_52a6c937:

    # c "Tenía asimilado que este día llegaría de todas formas."
    c "Ich hatte akzeptiert, dass dieser Tag sowieso kommen würde."

# game/hchar.rpy:10076
translate german PA_HC_EV32_4e5964d0:

    # so "Y solo por unos ideales inmaduros..."
    so "Und das nur wegen unreifer Ideale..."

# game/hchar.rpy:10078
translate german PA_HC_EV32_db8c7f4d:

    # c "Cuando llegue el momento, sabré si mis ideales eran tan fuertes como pensaba."
    c "Wenn es soweit ist, werde ich wissen, ob meine Ideale so stark waren, wie ich dachte."

# game/hchar.rpy:10080
translate german PA_HC_EV32_7cf75e14:

    # so "Pero la herencia..."
    so "Aber das Erbe..."

# game/hchar.rpy:10082
translate german PA_HC_EV32_93c80215:

    # c "Si Madre piensa quitarme la herencia o no."
    c "Ob Mutter mir mein Erbe vorenthalten will oder nicht."

# game/hchar.rpy:10083
translate german PA_HC_EV32_6dd6b53d:

    # c "No tiene importancia para mi."
    c "Das ist für mich nicht wichtig."

# game/hchar.rpy:10085
translate german PA_HC_EV32_e2694e6c:

    # c "Y si lo piensas detenidamente."
    c "Und wenn man genau darüber nachdenkt."

# game/hchar.rpy:10086
translate german PA_HC_EV32_5b0d3e92:

    # c "Esa sería una situación ideal para ti, Sophia."
    c "Das wäre eine ideale Situation für dich, Sophia."

# game/hchar.rpy:10087
translate german PA_HC_EV32_0dcc69c0:

    # c "De esa forma no tendrías que compartir la herencia conmigo."
    c "Auf diese Weise müsstest du das Erbe nicht mit mir teilen."

# game/hchar.rpy:10089
translate german PA_HC_EV32_0ef388d4:

    # so "No soy de esa clase de personas, Charlotte."
    so "Ich bin nicht so jemand, Charlotte."

# game/hchar.rpy:10091
translate german PA_HC_EV32_ed64ac79:

    # c "Con esa cantidad de dinero, podrás tener una vida tranquila y llena de lujos."
    c "Mit dieser Summe Geld kannst du ein ruhiges Leben voller Luxus führen."

# game/hchar.rpy:10092
translate german PA_HC_EV32_c6c31740:

    # c "Eso es lo que tú buscas."
    c "Das ist es, was du suchst."

# game/hchar.rpy:10093
translate german PA_HC_EV32_bd8d8dbd:

    # c "En cambio, el camino que yo quiero recorrer es otro."
    c "Ich hingegen möchte einen anderen Weg einschlagen."

# game/hchar.rpy:10095
translate german PA_HC_EV32_62ecf5f1_1:

    # so "..."
    so "..."

# game/hchar.rpy:10097
translate german PA_HC_EV32_4c2ac2f2:

    # so "Bien."
    so "Gut."

# game/hchar.rpy:10099
translate german PA_HC_EV32_a8f98d13:

    # c "Ahora continuaré con mi trabajo."
    c "Jetzt werde ich meine Arbeit fortsetzen."

# game/hchar.rpy:10101
translate german PA_HC_EV32_3db79bce:

    # so "¿Piensas seguir trabajando a pesar de todo?"
    so "Willst du trotz allem weiterarbeiten?"

# game/hchar.rpy:10103
translate german PA_HC_EV32_87bbdf2c:

    # c "Es lo que debo hacer si quiero ser una profesional."
    c "Das muss ich tun, wenn ich professionell sein will."

# game/hchar.rpy:10104
translate german PA_HC_EV32_a28adf72:

    # c "No puedo dejar de hacer mi trabajo por miedo."
    c "Ich kann meine Arbeit nicht aus Angst unterlassen."

# game/hchar.rpy:10106
translate german PA_HC_EV32_f3c65047:

    # c "¿Me darías una mano con algunas cosas pesadas [nombre]?"
    c "Könntest du mir bei ein paar schweren Sachen helfen, [nombre]?"

# game/hchar.rpy:10108
translate german PA_HC_EV32_e1371faf:

    # mc "Claro que si."
    mc "Natürlich!"

# game/hchar.rpy:10113
translate german PA_HC_EV32_4e94a8f9:

    # "<Ayudas a Charlotte con el trabajo pendiente de la cocina del [restaurant], moviendo algunos sacos pesados.>"
    "<Du hilfst Charlotte bei der Arbeit in der Küche von [restaurant] und trägst einige schwere Säcke.>"

# game/hchar.rpy:10114
translate german PA_HC_EV32_582ed2c8:

    # "<Al finalizar se dirigen a su habitación donde aún espera impaciente Sophia.>"
    "<Am Ende begibt ihr euch in ihr Zimmer, wo Sophia noch immer ungeduldig wartet.>"

# game/hchar.rpy:10119
translate german PA_HC_EV32_5c2f7f6a:

    # so "Al fin terminaron con eso..."
    so "Endlich habt ihr damit aufgehört..."

# game/hchar.rpy:10120
translate german PA_HC_EV32_ee328905:

    # so "Estaba superaburrida y ansiosa esperando por ustedes."
    so "Ich war total gelangweilt und ungeduldig, als ich auf euch gewartet habe."

# game/hchar.rpy:10121
translate german PA_HC_EV32_68ad3838:

    # so "No podía para de pensar que Madre había llegado y por eso tardaban."
    so "Ich konnte nicht aufhören, daran zu denken, dass Mutter angekommen war und ihr deshalb so lange brauchten."

# game/hchar.rpy:10123
translate german PA_HC_EV32_01c1be6f:

    # mc "Ella dijo que reanudaría su búsqueda el día de mañana."
    mc "Sie sagte, sie würde ihre Suche morgen fortsetzen."

# game/hchar.rpy:10125
translate german PA_HC_EV32_d7a660b8:

    # c "Yo solo dire que los temores de Sophia son justificados."
    c "Ich sage nur, dass Sophias Befürchtungen berechtigt sind."

# game/hchar.rpy:10126
translate german PA_HC_EV32_0a1369a8:

    # c "Juliette suele tener comportamientos impredecibles."
    c "Juliette verhält sich oft unberechenbar."

# game/hchar.rpy:10128
translate german PA_HC_EV32_e6c6d36e:

    # so "Sabes Charlotte..."
    so "Weisst du, Charlotte..."

# game/hchar.rpy:10129
translate german PA_HC_EV32_547191ad:

    # so "No sabes cuanto me molesta que llames a Madre por su nombre real."
    so "Du weißt gar nicht, wie sehr es mich stört, dass du Mutter bei ihrem richtigen Namen nennst."

# game/hchar.rpy:10131
translate german PA_HC_EV32_1ce835d9:

    # c "Si Mémé escogió ese nombre para su hija, no veo la razón para no usarlo."
    c "Wenn Mémé diesen Namen für ihre Tochter gewählt hat, sehe ich keinen Grund, ihn nicht zu verwenden."

# game/hchar.rpy:10133
translate german PA_HC_EV32_6b594b90:

    # so "O sea si, te entiendo, pero me hace sentir mucha incomodidad."
    so "Also ja, ich verstehe dich, aber es ist mir sehr unangenehm."

# game/hchar.rpy:10134
translate german PA_HC_EV32_835d101f:

    # so "Como si estuvieses faltándole el respeto."
    so "Als ob du ihr gegenüber respektlos wärst."

# game/hchar.rpy:10136
translate german PA_HC_EV32_a26590d0:

    # c "Tú haces lo mismo con Mémé."
    c "Du machst dasselbe mit Mémé."

# game/hchar.rpy:10137
translate german PA_HC_EV32_c7245648:

    # c "Siempre te has dirigido a ella por su nombre de pila."
    c "Du hast sie immer mit ihrem Vornamen angesprochen."

# game/hchar.rpy:10139
translate german PA_HC_EV32_277c2183:

    # so "Tú lo has dicho..."
    so "Du sagst es..."

# game/hchar.rpy:10140
translate german PA_HC_EV32_6e20e6fa:

    # so "Siempre la he llamado Lissette."
    so "Ich habe sie immer Lissette genannt."

# game/hchar.rpy:10141
translate german PA_HC_EV32_38d40caa:

    # so "Pero eso no era algo común viniendo de ti."
    so "Aber das war nicht typisch für dich."

# game/hchar.rpy:10142
translate german PA_HC_EV32_9e845343:

    # so "Cuando estabas en casa siempre te referiste a ella como Madre."
    so "Als du zu Hause warst, hast du sie immer als Mutter bezeichnet."

# game/hchar.rpy:10144
translate german PA_HC_EV32_ca44a654:

    # c "Eso es porque durante mi estancia en Argleton me he dado cuenta de muchas cosas."
    c "Das liegt daran, dass mir während meines Aufenthalts in Argleton viele Dinge klar geworden sind."

# game/hchar.rpy:10145
translate german PA_HC_EV32_4b253081:

    # c "Madre no era la persona que yo creía que era."
    c "Meine Mutter war nicht die Person, für die ich sie gehalten hatte."

# game/hchar.rpy:10146
translate german PA_HC_EV32_0500f786:

    # c "Antes estaba completamente ciega..."
    c "Früher war ich völlig blind..."

# game/hchar.rpy:10148
translate german PA_HC_EV32_fde99827:

    # so "¿Acaso estás diciendo que ya no la respetas como antes?"
    so "Willst du damit sagen, dass du sie nicht mehr so respektierst wie früher?"

# game/hchar.rpy:10150
translate german PA_HC_EV32_7caa6832:

    # c "Oui, probablemente."
    c "Oui, wahrscheinlich."

# game/hchar.rpy:10152
translate german PA_HC_EV32_17d44995:

    # so "Eres como un tren a punto de descarrilarse, Charlotte."
    so "Du bist wie ein Zug, der kurz vor dem Entgleisen steht, Charlotte."

# game/hchar.rpy:10153
translate german PA_HC_EV32_abbafb61:

    # so "Supongo que sabes lo que va a suceder si llegas a tutear a Madre."
    so "Ich nehme an, du weisst, was passiert, wenn du Mutter duzt."

# game/hchar.rpy:10154
translate german PA_HC_EV32_e20b07a8:

    # so "Si la tuteas, va a explotar en furia."
    so "Wenn du sie duzt, wird sie vor Wut explodieren."

# game/hchar.rpy:10156
translate german PA_HC_EV32_760ba8ee:

    # c "Entonces tendré que evitarlo cuando tenga que dirigirme a ella personalmente."
    c "Dann muss ich das vermeiden, wenn ich mich persönlich an sie wenden muss."

# game/hchar.rpy:10157
translate german PA_HC_EV32_49be573a:

    # c "Pero al hablar con otras personas seguiré llamándola Juliette."
    c "Aber wenn ich mit anderen Leuten spreche, werde ich sie weiterhin Juliette nennen."

# game/hchar.rpy:10159
translate german PA_HC_EV32_09ccebd7:

    # so "¿Por qué?"
    so "Warum?"

# game/hchar.rpy:10161
translate german PA_HC_EV32_b1ef2179:

    # c "Porque su imagen... {w}Esa sombra siempre me ha forzado a vivir una vida que no quiero."
    c "Weil ihr Image... {w}Dieser Schatten hat mich immer dazu gezwungen, ein Leben zu führen, das ich nicht will."

# game/hchar.rpy:10162
translate german PA_HC_EV32_d6692ad1:

    # c "Dirigirme a ella como Juliette en lugar de Madre."
    c "Sie als Juliette statt als Mutter anzusprechen."

# game/hchar.rpy:10163
translate german PA_HC_EV32_bc2a4f95:

    # c "Me ayuda a percibirla como un ser humano común y corriente."
    c "Das hilft mir, sie als ganz normalen Menschen wahrzunehmen."

# game/hchar.rpy:10164
translate german PA_HC_EV32_37d40184:

    # c "Y no como una figura de autoridad todopoderosa, como antes."
    c "Und nicht mehr wie früher als allmächtige Autoritätsperson."

# game/hchar.rpy:10166
translate german PA_HC_EV32_7e33010c:

    # so "Ay ósea, pero que dices..."
    so "Was sagst du da?"

# game/hchar.rpy:10168
translate german PA_HC_EV32_df30c383:

    # c "Te estoy diciendo que seguiré dirigiéndome a ella como Juliette de ahora en adelante."
    c "Ich sage dir, dass ich sie von nun an weiterhin Juliette nennen werde."

# game/hchar.rpy:10169
translate german PA_HC_EV32_638c7956:

    # c "¡Aunque te produzca vergüenza ajena!"
    c "Auch wenn es dir peinlich ist!"

# game/hchar.rpy:10171
translate german PA_HC_EV32_beb6a38a:

    # so "¡Eres una incorregible, Charlotte!"
    so "Du bist nicht zu retten, Charlotte!"

# game/hchar.rpy:10172
translate german PA_HC_EV32_15beb9b6:

    # so "Si Lissette te escuchara decir todas estas cosas, se decepcionaría mucho de ti."
    so "Wenn Lissette all diese Dinge von dir hören würde, wäre sie sehr enttäuscht von dir."

# game/hchar.rpy:10174
translate german PA_HC_EV32_7dabe3f7:

    # c "Mémé ya sabe como soy realmente."
    c "Mémé weiss schon, wie ich wirklich bin."

# game/hchar.rpy:10176
translate german PA_HC_EV32_3f319164:

    # so "Quizás tanto escuchar a Lissette, te está llevando a tomar las decisiones equivocadas."
    so "Vielleicht verleitet dich das ständige Zuhören bei Lissette dazu, falsche Entscheidungen zu treffen."

# game/hchar.rpy:10178
translate german PA_HC_EV32_3b1d2e2a:

    # c "O quizás sea la única realmente cuerda en esta familia."
    c "Oder vielleicht bin ich die Einzige in dieser Familie, die wirklich bei Verstand ist."

# game/hchar.rpy:10183
translate german PA_HC_EV32_0ca7c735:

    # c "Al menos parece alguien genuina y no busca aparentar lo que no es a todas horas."
    c "Zumindest scheint sie authentisch zu sein und versucht nicht ständig, etwas vorzutäuschen, was sie nicht ist."

# game/hchar.rpy:10189
translate german PA_HC_EV32_4853b976:

    # c "Ya casi es el momento, [nombre]."
    c "Es ist fast soweit, [nombre]."

# game/hchar.rpy:10193
translate german PA_HC_EV32_ec121189:

    # c "Deberías prepararte para lo que viene."
    c "Du solltest dich auf das vorbereiten, was kommt."

# game/hchar.rpy:10195
translate german PA_HC_EV32_5d2914aa:

    # mc "¿Prepararme para qué?"
    mc "Wofür vorbereiten?"

# game/hchar.rpy:10196
translate german PA_HC_EV32_d19f855d_1:

    # c "..."
    c "..."

# game/hchar.rpy:10198
translate german PA_HC_EV32_16bc1364:

    # c "Sacreblue..."
    c "Sacreblue..."

# game/hchar.rpy:10199
translate german PA_HC_EV32_ef3cc052:

    # c "No me digas que..."
    c "Sag mir nicht, dass..."

# game/hchar.rpy:10201
translate german PA_HC_EV32_e131a0ce:

    # mc "¿Dónde estamos?"
    mc "Wo waren wir stehen geblieben?"

# game/hchar.rpy:10202
translate german PA_HC_EV32_1fb59fb6:

    # mc "¿En qué momento te has cambiado de vestimentas?"
    mc "Wann hast du dich umgezogen?"

# game/hchar.rpy:10204
translate german PA_HC_EV32_0756b588:

    # c "[nombre]..."
    c "[nombre]..."

# game/hchar.rpy:10206
translate german PA_HC_EV32_f9ff9ce9:

    # c "Porque tiene que sucederte esto en un momento como este."
    c "Warum muss dir so etwas gerade jetzt passieren?"

# game/hchar.rpy:10207
translate german PA_HC_EV32_5b0f36db:

    # c "Todo se hace aún más doloroso..."
    c "Alles wird noch schmerzhafter..."

# game/hchar.rpy:10208
translate german PA_HC_EV32_7ad9f75c:

    # c "Pero te prometí que no iba a llorar."
    c "Aber ich habe dir versprochen, dass ich nicht weinen werde."

# game/hchar.rpy:10210
translate german PA_HC_EV32_ea3fb982:

    # mc "No estoy entendiendo nada."
    mc "Ich verstehe gar nichts."

# game/hchar.rpy:10212
translate german PA_HC_EV32_21aadd17:

    # c "Primero que nada, intenta guardar la calma [nombre]."
    c "Versuche zunächst einmal, ruhig zu bleiben, [nombre]."

# game/hchar.rpy:10213
translate german PA_HC_EV32_8513c043:

    # c "Intenta disimular, por favor..."
    c "Versuch bitte, es zu verbergen..."

# game/hchar.rpy:10214
translate german PA_HC_EV32_40daf392:

    # c "Si Sophia ve tu cara de confusión montará un escándalo."
    c "Wenn Sophia deine verwirrte Visage sieht, wird sie einen Skandal machen."

# game/hchar.rpy:10215
translate german PA_HC_EV32_bc1dd454:

    # c "Y lo último que quiero es un drama innecesario."
    c "Und das Letzte, was ich will, ist unnötiges Drama."

# game/hchar.rpy:10216
translate german PA_HC_EV32_c9db84c8:

    # c "Al menos por este día..."
    c "Zumindest für diesen Tag..."

# game/hchar.rpy:10218
translate german PA_HC_EV32_c58de4f8:

    # mc "Dijiste que hoy es el funeral, pero no recuerdo ninguna muerte reciente."
    mc "Du hast gesagt, dass heute die Beerdigung ist, aber ich kann mich an keinen kürzlichen Todesfall erinnern."

# game/hchar.rpy:10220
translate german PA_HC_EV32_0beaeffd:

    # c "¿Dónde dejaste el medicamento que te ha recetado el doctor?"
    c "Wo hast du die Medikamente hingelegt, die dir der Arzt verschrieben hat?"

# game/hchar.rpy:10222
translate german PA_HC_EV32_e67c76e1:

    # mc "No sé dé qué doctor me estás hablando."
    mc "Ich weiss nicht, von welchem Arzt du sprichst."

# game/hchar.rpy:10223
translate german PA_HC_EV32_5a7112d0:

    # mc "Estábamos en Argleton hasta hace un momento y de pronto..."
    mc "Wir waren gerade noch in Argleton und plötzlich..."

# game/hchar.rpy:10225
translate german PA_HC_EV32_56426788:

    # c "Ya lo sé..."
    c "Ich weiss schon..."

# game/hchar.rpy:10226
translate german PA_HC_EV32_1c242dc3:

    # c "Siempre me cuentas la misma historia, [nombre]."
    c "Du erzählst mir immer dieselbe Geschichte, [nombre]."

# game/hchar.rpy:10227
translate german PA_HC_EV32_f5728166:

    # c "Los recuerdos de Argleton una y otra vez..."
    c "Die Erinnerungen an Argleton immer und immer wieder..."

# game/hchar.rpy:10228
translate german PA_HC_EV32_e60d40d9:

    # c "Seguramente has olvidado tomar la medicina para la memoria."
    c "Du hast bestimmt vergessen, deine Medikamente für das Gedächtnis einzunehmen."

# game/hchar.rpy:10230
translate german PA_HC_EV32_25825409:

    # mc "¿Qué medicina?"
    mc "Welches Medikament?"

# game/hchar.rpy:10232
translate german PA_HC_EV32_273db715:

    # c "Y tampoco puedes recordar el accidente que sufriste durante esos últimos días en Argleton."
    c "Und du kannst dich auch nicht an den Unfall erinnern, den du in den letzten Tagen in Argleton hattest."

# game/hchar.rpy:10234
translate german PA_HC_EV32_cd1970c6:

    # mc "¿De que accidente me estás hablando?"
    mc "Von welchem Unfall sprichst du?"

# game/hchar.rpy:10236
translate german PA_HC_EV32_7e2b1065:

    # c "Fue debido a un fenómeno climático."
    c "Es war auf ein klimatisches Phänomen zurückzuführen."

# game/hchar.rpy:10237
translate german PA_HC_EV32_fc5f8418:

    # c "Es la razón por la que has perdido tu memoria a corto plazo."
    c "Das ist der Grund, warum du dein Kurzzeitgedächtnis verloren hast."

# game/hchar.rpy:10239
translate german PA_HC_EV32_876a3e60:

    # mc "Tengo tantas preguntas ahora mismo..."
    mc "Ich habe gerade so viele Fragen..."

# game/hchar.rpy:10241
translate german PA_HC_EV32_c5027ac9:

    # c "No importa [nombre]."
    c "Das macht nichts, [nombre]."

# game/hchar.rpy:10242
translate german PA_HC_EV32_33ff08b6:

    # c "Solo te pido que me esperes aquí durante unos minutos."
    c "Ich bitte dich nur, ein paar Minuten hier auf mich zu warten."

# game/hchar.rpy:10243
translate german PA_HC_EV32_f615c2a5:

    # c "El funeral va a comenzar y quiero darle mi último adiós a Mémé."
    c "Die Beerdigung beginnt gleich und ich möchte mich ein letztes Mal von Mémé verabschieden."

# game/hchar.rpy:10245
translate german PA_HC_EV32_2b35b6fe:

    # mc "..."
    mc "..."

# game/hchar.rpy:10247
translate german PA_HC_EV32_ac906cf8:

    # c "Cuando termine, te explicaré todo para que puedas recordar."
    c "Wenn ich fertig bin, werde ich dir alles erklären, damit du dich daran erinnern kannst."

# game/hchar.rpy:10248
translate german PA_HC_EV32_dbfd9d14:

    # c "Solo procura esperar aquí y no hagas nada estúpido."
    c "Warte einfach hier und mach keine Dummheiten."

# game/hchar.rpy:10250
translate german PA_HC_EV32_573f5534:

    # mc "Está bien, creo que no tengo otra alternativa."
    mc "Okay, ich glaube, ich habe keine andere Wahl."

# game/hchar.rpy:10252
translate german PA_HC_EV32_6f2a6c2f:

    # c "Sé que no es fácil adaptarte a tu nueva vida, [nombre]."
    c "Ich weiss, dass es nicht leicht ist, sich an dein neues Leben zu gewöhnen, [nombre]."

# game/hchar.rpy:10253
translate german PA_HC_EV32_b2d7dcc2:

    # c "Y tampoco lo es para mí..."
    c "Und für mich auch nicht..."

# game/hchar.rpy:10254
translate german PA_HC_EV32_f635962e:

    # c "Pero prometí que estaría junto a ti, hasta que te recuperes."
    c "Aber ich habe versprochen, dass ich bei dir bleiben werde, bis du dich erholt hast."

# game/hchar.rpy:10255
translate german PA_HC_EV32_24b3e82d:

    # c "Aunque ese día probablemente nunca llegará."
    c "Obwohl dieser Tag wahrscheinlich nie kommen wird."

# game/hchar.rpy:10257
translate german PA_HC_EV32_46dc4ba9:

    # mc "Lo siento, Charlotte."
    mc "Es tut mir leid, Charlotte."

# game/hchar.rpy:10259
translate german PA_HC_EV32_50365074_1:

    # c "Oui..."
    c "Oui..."

# game/hchar.rpy:10261
translate german PA_HC_EV32_f9070c90:

    # mc "Pepe pgomete no metegse en pgoblemás."
    mc "Pepe, misch dich nicht in die Probleme ein."

# game/hchar.rpy:10263
translate german PA_HC_EV32_16cce50c:

    # c "Al menos aún conservas tu sentido del humor."
    c "Wenigstens hast du deinen Sinn für Humor noch."

# game/hchar.rpy:10264
translate german PA_HC_EV32_5a30b7b4:

    # c "Merci por hacerme sonreír, [nombre]."
    c "Merci, dass du mich zum Lächeln gebracht hast, [nombre]."

# game/hchar.rpy:10266
translate german PA_HC_EV32_12bd4b9b:

    # c "No sabes cuanto lo necesitaba."
    c "Du weisst gar nicht, wie sehr ich das gebraucht habe."

# game/hchar.rpy:10272
translate german PA_HC_EV32_a6cf946d:

    # c "¿Cuánto tiempo vas a seguir ignorándome?"
    c "Wie lange willst du mich noch ignorieren?"

# game/hchar.rpy:10275
translate german PA_HC_EV32_1150006c:

    # mc "Yo..."
    mc "Hey..."

# game/hchar.rpy:10280
translate german PA_HC_EV32_0caf7e74:

    # so "¿Que sucede [nombre]?"
    so "Was ist los, [nombre]?"

# game/hchar.rpy:10281
translate german PA_HC_EV32_a0edf105:

    # so "Te has quedado con la mirada fija en Charlotte desde hace unos minutos."
    so "Du starrst Charlotte schon seit ein paar Minuten an."

# game/hchar.rpy:10283
translate german PA_HC_EV32_ac23232c:

    # c "¡Si estabas pensando en algo pervertido te obligaré a limpiar la cocina durante un mes!"
    c "Wenn du etwas Perverses gedacht hast, werde ich dich dazu zwingen, einen Monat lang die Küche zu putzen!"

# game/hchar.rpy:10285
translate german PA_HC_EV32_0e072762:

    # mc "Creo que he tenido una visión."
    mc "Ich glaube, ich hatte eine Vision."

# game/hchar.rpy:10286
translate german PA_HC_EV32_0b48daee:

    # mc "Pero por alguna razón..."
    mc "Aber aus irgendeinem Grund..."

# game/hchar.rpy:10287
translate german PA_HC_EV32_9dd9a324:

    # mc "Ahora no puedo recordar los detalles."
    mc "Ich kann mich jetzt nicht mehr an die Details erinnern."

# game/hchar.rpy:10289
translate german PA_HC_EV32_544809a1:

    # so "¿Una visión?"
    so "Eine Vision?"

# game/hchar.rpy:10291
translate german PA_HC_EV32_9861c03a:

    # mc "No es nada importante."
    mc "Es ist nichts Wichtiges."

# game/hchar.rpy:10292
translate german PA_HC_EV32_2f38dec9:

    # mc "Creo que me he quedado atrapado pensando más de la cuenta."
    mc "Ich glaube, ich habe mich in meinen Gedanken zu sehr verstrickt."

# game/hchar.rpy:10293
translate german PA_HC_EV32_1bd6a0d3:

    # mc "¿Qué es lo que me habías preguntado hace unos momentos Charlotte?"
    mc "Was hast du mich vorhin gefragt, Charlotte?"

# game/hchar.rpy:10295
translate german PA_HC_EV32_7ce088c4:

    # c "Te estaba diciendo que ya se ha hecho tarde."
    c "Ich habe dir gesagt, dass es schon spät geworden ist."

# game/hchar.rpy:10296
translate german PA_HC_EV32_0842252f:

    # c "Así que pensaba irme a descansar para prepararme para el dia de mañana."
    c "Also wollte ich mich ausruhen, um mich auf den nächsten Tag vorzubereiten."

# game/hchar.rpy:10298
translate german PA_HC_EV32_54f6564e:

    # so "¿Piensas que madre podrá encontrarte tan rápido?"
    so "Glaubst du, dass Mutter dich so schnell finden kann?"

# game/hchar.rpy:10300
translate german PA_HC_EV32_62e6d85d:

    # c "No es un pueblo muy grande, Sophia."
    c "Es ist kein sehr grosses Dorf, Sophia."

# game/hchar.rpy:10301
translate german PA_HC_EV32_92c198e8:

    # c "Si no es mañana, será pronto de todas formas."
    c "Wenn nicht morgen, dann jedenfalls bald."

# game/hchar.rpy:10303
translate german PA_HC_EV32_f8a972f1:

    # so "Antes de que te vayas a dormir."
    so "Bevor du schlafen gehst."

# game/hchar.rpy:10304
translate german PA_HC_EV32_5f69538e:

    # so "¿Podrías prestarme un poco de ropa?"
    so "Könntest du mir ein paar Kleidungsstücke leihen?"

# game/hchar.rpy:10305
translate german PA_HC_EV32_0d784f20:

    # so "Está haciendo mucho frío para ir solo con el bikini."
    so "Es ist zu kalt, um nur im Bikini unterwegs zu sein."

# game/hchar.rpy:10307
translate german PA_HC_EV32_86004f8d:

    # c "Tengo algunos uniformes de cocina o un delantal."
    c "Ich habe einige Kochuniformen oder eine Schürze."

# game/hchar.rpy:10309
translate german PA_HC_EV32_497a7d91:

    # so "¡Ay, pero qué horrendo!"
    so "Oh, wie schrecklich!"

# game/hchar.rpy:10310
translate german PA_HC_EV32_e527cf68:

    # so "¡No pienso ponerme esa ropa tan vulgar!"
    so "Ich werde diese vulgären Klamotten nicht anziehen!"

# game/hchar.rpy:10312
translate german PA_HC_EV32_74329d96:

    # c "¡No tengo nada más así que te tendrás que conformar!"
    c "Ich habe nichts anderes, also musst du dich damit zufrieden geben!"

# game/hchar.rpy:10313
translate german PA_HC_EV32_e58cead3:

    # c "¡Y vas a dormir en el sofá esta noche!"
    c "Und du schläfst heute Nacht auf dem Sofa!"

# game/hchar.rpy:10315
translate german PA_HC_EV32_6b526b5a:

    # so "Pero si en la cama hay espacio para las dos."
    so "Aber im Bett ist doch Platz für uns beide."

# game/hchar.rpy:10317
translate german PA_HC_EV32_7fce1d36:

    # c "No voy a debatir contigo."
    c "Ich werde mich nicht mit dir streiten."

# game/hchar.rpy:10318
translate german PA_HC_EV32_5ab72292:

    # c "Mi habitación, mis reglas."
    c "Mein Zimmer, meine Regeln."

# game/hchar.rpy:10320
translate german PA_HC_EV32_f6953847:

    # mc "Bien, yo igual iré a descansar."
    mc "Gut, ich werde mich auch ausruhen."

# game/hchar.rpy:10321
translate german PA_HC_EV32_59d374ec:

    # mc "También debo prepararme si debo ayudarte con lo de tu madre."
    mc "Ich muss mich auch darauf vorbereiten, dir bei der Sache mit deiner Mutter zu helfen."

# game/hchar.rpy:10323
translate german PA_HC_EV32_60a86674:

    # c "No es necesario que pases por eso, [nombre]."
    c "Du musst das nicht durchmachen, [nombre]."

# game/hchar.rpy:10325
translate german PA_HC_EV32_408f6da2:

    # mc "Yo pienso que necesitaras todo el apoyo posible."
    mc "Ich denke, du wirst jede Unterstützung brauchen, die du bekommen kannst."

# game/hchar.rpy:10327
translate german PA_HC_EV32_0287cb0b:

    # c "Supongo..."
    c "Ich nehme dein Angebot an..."

# game/hchar.rpy:10328
translate german PA_HC_EV32_b744274d:

    # c "Merci, [nombre]."
    c "Merci, [nombre]."

# game/hchar.rpy:10329
translate german PA_HC_EV32_bf884a96:

    # c "Y buenas noches adelantadas."
    c "Und schon mal gute Nacht."

# game/hchar.rpy:10331
translate german PA_HC_EV32_c7957028:

    # mc "Buenas noches, Charlotte."
    mc "Gute Nacht, Charlotte."

# game/hchar.rpy:10336
translate german PA_HC_EV32_6565bdd1:

    # so "¿No podrían dejar de ser tan cursis?"
    so "Könnt ihr bitte aufhören, so kitschig zu sein?"

# game/hchar.rpy:10337
translate german PA_HC_EV32_923b1948:

    # c "Guarda silencio, Sophia."
    c "Schau mal, Sophia."

# game/hchar.rpy:10338
translate german PA_HC_EV32_a46cf963_3:

    # so "Pero..."
    so "Aber..."

# game/hchar.rpy:10339
translate german PA_HC_EV32_d28c7ee5:

    # c "¡Si no sales de mi cama en cinco te voy a sacar a la fuerza!"
    c "Wenn du nicht in fünf Minuten aus meinem Bett verschwindest, werde ich dich mit Gewalt herausholen!"

# game/hchar.rpy:10352
translate german PA_HC_EV32_be05434a:

    # unknown "¡Por favor despierta [nombre]!"
    unknown "Bitte wach auf, [nombre]!"

# game/hchar.rpy:10353
translate german PA_HC_EV32_577f33b3:

    # mc "Mmm..."
    mc "Mmm..."

# game/hchar.rpy:10354
translate german PA_HC_EV32_f4fe5853:

    # unknown "No puedo creer que tengas el sueño tan pesado."
    unknown "Ich kann nicht glauben, dass du so einen tiefen Schlaf hast."

# game/hchar.rpy:10355
translate german PA_HC_EV32_f26c9987:

    # unknown "¿Podrías despertar de una vez?"
    unknown "Könntest du bitte endlich aufwachen?"

# game/hchar.rpy:10360
translate german PA_HC_EV32_2b35b6fe_1:

    # mc "..."
    mc "..."

# game/hchar.rpy:10361
translate german PA_HC_EV32_af41601e:

    # mc "¿Qué haces aquí Sophia?"
    mc "Was machst du hier, Sophia?"

# game/hchar.rpy:10363
translate german PA_HC_EV32_3cbb29ae:

    # so "Lamento haber entrado a tu habitación de imprevisto, [nombre]."
    so "Es tut mir leid, dass ich so unerwartet in dein Zimmer gekommen bin, [nombre]."

# game/hchar.rpy:10364
translate german PA_HC_EV32_1ebd1493:

    # so "Pero no sabía a quién recurrir."
    so "Aber ich wusste nicht, an wen ich mich wenden sollte."

# game/hchar.rpy:10366
translate german PA_HC_EV32_5d0354fb:

    # mc "¿Ha sucedido algún imprevisto?"
    mc "Ist etwas Unvorhergesehenes passiert?"

# game/hchar.rpy:10368
translate german PA_HC_EV32_7a735253:

    # so "¡Charlotte se ha vuelto completamente loca!"
    so "Charlotte ist völlig verrückt geworden!"

# game/hchar.rpy:10370
translate german PA_HC_EV32_864bb27f_1:

    # so "Oh no... non non non..."
    so "Oh non... non non non..."

# game/hchar.rpy:10372
translate german PA_HC_EV32_ba36a0a6:

    # so "Cuando pensaba que todo se estaba poniendo complicado."
    so "Und ich dachte es könnte weniger kompliziert werden."

# game/hchar.rpy:10373
translate german PA_HC_EV32_871e7228:

    # so "¡La tonta de Charlotte pierde la cabeza!"
    so "Die dumme Charlotte verliert den Verstand!"

# game/hchar.rpy:10375
translate german PA_HC_EV32_2a43dd8e:

    # mc "Si no me dices lo detalles, difícilmente podré ayudar."
    mc "Wenn du nicht in Detail sprichst, kann ich dir kaum helfen."

# game/hchar.rpy:10377
translate german PA_HC_EV32_46073913:

    # so "Hablamos mucho durante la noche."
    so "Wir haben die ganze Nacht lang viel geredet."

# game/hchar.rpy:10378
translate german PA_HC_EV32_256e8c8f:

    # so "Ya sabes... {w}Tuvimos ese tipo de conversación entre hermanas y todo eso."
    so "Du weisst schon... {w}Wir hatten diese Art von Gespräch zwischen Schwestern und so weiter."

# game/hchar.rpy:10379
translate german PA_HC_EV32_b47cbeb9:

    # so "Intentaba convencer a Charlotte de retractarse sobre sus malas decisiones."
    so "Ich habe versucht, Charlotte davon zu überzeugen, ihre schlechten Entscheidungen rückgängig zu machen."

# game/hchar.rpy:10380
translate german PA_HC_EV32_6f6dc6e8:

    # so "¡Y se ha enojado tanto que decidió ir a la casa de la playa personalmente!"
    so "Und sie war so wütend, dass sie beschloss, persönlich zum Strandhaus zu kommen!"

# game/hchar.rpy:10382
translate german PA_HC_EV32_a4478d20:

    # mc "¿Acaso Charlotte fue a enfrentar directamente a su madre?"
    mc "Ist Charlotte ihrer Mutter direkt gegenübergetreten?"

# game/hchar.rpy:10384
translate german PA_HC_EV32_1d892066:

    # so "¡Charlotte está fuera de sí!"
    so "Charlotte ist ausser sich!"

# game/hchar.rpy:10385
translate german PA_HC_EV32_4c9f673a:

    # so "¡Tienes que detenerla!"
    so "Du musst sie aufhalten!"

# game/hchar.rpy:10387
translate german PA_HC_EV32_75a0edfc:

    # mc "¿Por qué tomar esa decisión tan repentina?"
    mc "Warum diese so plötzliche Entscheidung?"

# game/hchar.rpy:10389
translate german PA_HC_EV32_2461c72e:

    # so "Dijo que no tenía sentido seguir ignorando la presencia de Madre en el pueblo."
    so "Sie sagte, es habe keinen Sinn, die Anwesenheit der Mutter im Dorf weiterhin zu ignorieren."

# game/hchar.rpy:10390
translate german PA_HC_EV32_591ff892:

    # so "Y no quería que ella la sorprendiera trabajando como cocinera."
    so "Und sie wollte nicht dabei erwischt werden, wie sie als Köchin arbeitete."

# game/hchar.rpy:10391
translate german PA_HC_EV32_7a0aa2f8:

    # so "Seguramente quería evitar que ella hiciese un escándalo aquí."
    so "Sicherlich wollte er verhindern, dass sie hier einen Skandal auslöst."

# game/hchar.rpy:10392
translate german PA_HC_EV32_3b2452cd:

    # so "Me cuesta creer que Charlotte sea tan estúpida."
    so "Ich kann kaum glauben, dass Charlotte so dumm ist."

# game/hchar.rpy:10394
translate german PA_HC_EV32_3ad1f7d9:

    # so "¡Le dije que sería más sencillo volver a casa a primera hora!"
    so "Ich habe gesagt, es wäre einfacher, gleich morgens nach Hause zu gehen!"

# game/hchar.rpy:10395
translate german PA_HC_EV32_9e79334c:

    # so "¡Pero siempre ignora mis consejos porque soy la menor!"
    so "Aber sie ignoriert meine Ratschläge immer, weil ich die Jüngste bin!"

# game/hchar.rpy:10397
translate german PA_HC_EV32_f7dc0495:

    # mc "No me digas que ya se ha marchado."
    mc "Sag mir nicht, dass sie schon gegangen ist."

# game/hchar.rpy:10399
translate german PA_HC_EV32_73de2932:

    # so "Hace tan solo unos segundos."
    so "Vor nur wenigen Sekunden."

# game/hchar.rpy:10400
translate german PA_HC_EV32_dc43a5fc:

    # so "Si corres lo suficientemente rápido podrías evitar una tragedia."
    so "Wenn du schnell genug rennst, könntest du eine Tragödie verhindern."

# game/hchar.rpy:10402
translate german PA_HC_EV32_b43be0ea:

    # mc "Mierda..."
    mc "Scheisse..."

# game/hchar.rpy:10403
translate german PA_HC_EV32_7832686b:

    # mc "Entonces iré enseguida."
    mc "Dann werde ich sofort gehen."

# game/hchar.rpy:10405
translate german PA_HC_EV32_2e9fda6e:

    # so "¿Te molesta si voy contigo [nombre]?"
    so "Stört es dich, wenn ich mitkomme, [nombre]?"

# game/hchar.rpy:10407
translate german PA_HC_EV32_73ceecc0:

    # mc "Como quieras."
    mc "Wie du willst."

# game/hchar.rpy:10412
translate german PA_HC_EV32_ea26c7ef:

    # "<Corres rápidamente en dirección a la playa Gold mientras Sophia intenta seguirte el paso.>"
    "<Du rennst schnell in Richtung Gold Beach, während Sophia versucht, mit dir Schritt zu halten.>"

# game/hchar.rpy:10413
translate german PA_HC_EV32_d5e78f98:

    # "<Pero al ver lo que está sucediendo a lo lejos te das cuenta de que has llegado demasiado tarde.>"
    "<Aber wenn man sieht, was in der Ferne geschieht, wird einem klar, dass man zu spät gekommen ist.>"

# game/hchar.rpy:10420
translate german PA_HC_EV32_2708236c:

    # jul "¡Estas siendo una impertinente niñita!"
    jul "Du benimmst dich wie ein unverschämtes kleines Mädchen!"

# game/hchar.rpy:10422
translate german PA_HC_EV32_60b64e7b:

    # c "¿De verdad me vas a llamar impertinente por querer tener mis propios objetivos?"
    c "Willst du mich wirklich als unverschämt bezeichnen, weil ich meine eigenen Ziele haben möchte?"

# game/hchar.rpy:10424
translate german PA_HC_EV32_864bb27f_2:

    # so "Oh no... non non non..."
    so "Oh non... non non non..."

# game/hchar.rpy:10425
translate german PA_HC_EV32_26738921:

    # so "No sé cuanto le ha dicho Charlotte a Madre."
    so "Ich weiss nicht, wie viel Charlotte Mutter erzählt hat."

# game/hchar.rpy:10426
translate german PA_HC_EV32_973a71f6:

    # so "Pero fue lo suficiente para provocar su enojo."
    so "Aber es reichte aus, um ihren Zorn zu erregen."

# game/hchar.rpy:10428
translate german PA_HC_EV32_19be26ab:

    # so "¿Y ahora que se supone que voy a hacer?"
    so "Und was soll ich jetzt tun?"

# game/hchar.rpy:10430
translate german PA_HC_EV32_6d98c2f9:

    # mc "Deberías ir a esconderte."
    mc "Du solltest dich verstecken."

# game/hchar.rpy:10432
translate german PA_HC_EV32_fe7e3b30:

    # so "Seguramente Charlotte ya le ha dicho a Madre que estoy aquí."
    so "Charlotte hat Mutter sicher schon gesagt, dass ich hier bin."

# game/hchar.rpy:10434
translate german PA_HC_EV32_f3fce2d9:

    # mc "Ella jamás haria eso."
    mc "Das würde sie niemals tun."

# game/hchar.rpy:10436
translate german PA_HC_EV32_de5e2380:

    # so "Ya no sé que esperar de mi hermana..."
    so "Ich weiss nicht mehr, was ich von meiner Schwester erwarten soll..."

# game/hchar.rpy:10437
translate german PA_HC_EV32_a4f0d1fe:

    # so "Su estancia en Argleton ha cambiado su personalidad por completo."
    so "Ihr Aufenthalt in Argleton hat ihre Persönlichkeit völlig verändert."

# game/hchar.rpy:10439
translate german PA_HC_EV32_61427590:

    # mc "Ella dijo que te cubriría, pero será en vano si se entera de que sigues aquí."
    mc "Sie sagte, sie würde dich decken, aber das wird umsonst sein, wenn sie erfährt, dass du noch hier bist."

# game/hchar.rpy:10441
translate german PA_HC_EV32_6c53ec6c:

    # so "Oui, tienes razón."
    so "Oui, du hast Recht."

# game/hchar.rpy:10443
translate german PA_HC_EV32_255d3ea9:

    # mc "Así que ahora ve a esconderte."
    mc "Also geh dich jetzt verstecken."

# game/hchar.rpy:10445
translate german PA_HC_EV32_3aaad84f:

    # so "¿Y tú que vas a hacer mientras tanto?"
    so "Und was wirst du in der Zwischenzeit machen?"

# game/hchar.rpy:10447
translate german PA_HC_EV32_09c72450:

    # mc "Intentaré ayudar a Charlotte."
    mc "Ich werde versuchen, Charlotte zu helfen."

# game/hchar.rpy:10448
translate german PA_HC_EV32_e5dea1fe:

    # mc "Probablemente necesite un poco de apoyo moral."
    mc "Wahrscheinlich braucht sie ein wenig moralische Unterstützung."

# game/hchar.rpy:10452
translate german PA_HC_EV32_d3a261fa:

    # c "¡Ya te lo dije!"
    c "Ich habe es dir doch gesagt!"

# game/hchar.rpy:10453
translate german PA_HC_EV32_d8b8d719:

    # c "¡No pienso ir contigo!"
    c "Ich werde nicht mit dir gehen!"

# game/hchar.rpy:10455
translate german PA_HC_EV32_15f44604:

    # jul "Por dios Charlotte, deja de gritar tan fuerte."
    jul "Um Gottes willen, Charlotte, hör auf, so laut zu schreien."

# game/hchar.rpy:10457
translate german PA_HC_EV32_b7a23ccb:

    # jul "Estamos en la vía pública..."
    jul "Wir befinden uns auf einer öffentlichen Strasse..."

# game/hchar.rpy:10458
translate german PA_HC_EV32_c6ffde5f:

    # jul "Me estás haciendo pasar un momento vergonzoso."
    jul "Du bringst mich in Verlegenheit."

# game/hchar.rpy:10460
translate german PA_HC_EV32_fab6ce27:

    # c "Siempre estás más preocupada de las apariencias que en tus hijas."
    c "Du bist immer mehr um den äusseren Schein besorgt als um deine Töchter."

# game/hchar.rpy:10462
translate german PA_HC_EV32_7a499edb:

    # jul "¿Cómo te atreves?"
    jul "Wie kannst du es wagen?"

# game/hchar.rpy:10464
translate german PA_HC_EV32_b32a5a0c:

    # c "¡Solo estoy diciendo la verdad!"
    c "Ich sage nur die Wahrheit!"

# game/hchar.rpy:10466
translate german PA_HC_EV32_4a5d0150:

    # jul "¡Vas a venir conmigo ahora mismo a la casa de Lissette!"
    jul "Du kommst jetzt sofort mit mir zu Lissette nach Hause!"

# game/hchar.rpy:10468
translate german PA_HC_EV32_e55b6f00:

    # c "¡Ya te he dicho que no!"
    c "Ich habe dir schon gesagt, dass ich nicht will!"

# game/hchar.rpy:10469
translate german PA_HC_EV32_38852ace:

    # c "¡Prefiero discutir contigo en un lugar más neutral!"
    c "Ich würde lieber an einem neutraleren Ort mit dir diskutieren!"

# game/hchar.rpy:10471
translate german PA_HC_EV32_e0b9bd96:

    # jul "Solo quieres dejarme en ridículo delante de estos pueblerinos mugrientos, Charlotte."
    jul "Du willst mich nur vor diesen schmutzigen Hinterwäldlern lächerlich machen, Charlotte."

# game/hchar.rpy:10472
translate german PA_HC_EV32_4c3b5c13:

    # jul "¿Qué ganas con todo esto?"
    jul "Was hast du davon?"

# game/hchar.rpy:10474
translate german PA_HC_EV32_5ba8bc44:

    # c "Lo único que quiero es que me escuches por primera vez en tu vida."
    c "Ich möchte nur, dass du mir zum ersten Mal in deinem Leben zuhörst."

# game/hchar.rpy:10475
translate german PA_HC_EV32_f18273fe:

    # c "¡Y no te atrevas a llamarlos pueblerinos mugrientos!"
    c "Und wage es nicht, sie als schmutzige Hinterwäldler zu bezeichnen!"

# game/hchar.rpy:10477
translate german PA_HC_EV32_461b03be:

    # jul "¿Que estás insinuando niña insolente?"
    jul "Was willst du damit andeuten, du freches Mädchen?"

# game/hchar.rpy:10478
translate german PA_HC_EV32_5a6e450d:

    # jul "¡Yo siempre he escuchado lo que tú y Sophia tenían que decir!"
    jul "Ich habe immer zugehört, was du und Sophia zu sagen hatten!"

# game/hchar.rpy:10479
translate german PA_HC_EV32_90332978:

    # jul "Pero ya se acabaron las discusiones."
    jul "Aber die Diskussionen sind nun vorbei."

# game/hchar.rpy:10481
translate german PA_HC_EV32_5a0a7366:

    # jul "¡Te vienes a casa conmigo mañana a primera hora!"
    jul "Du kommst morgen früh mit mir nach Hause!"

# game/hchar.rpy:10482
translate german PA_HC_EV32_92ad0f7a:

    # jul "¡Y no voy a continuar escuchando más réplicas!"
    jul "Und ich werde mir keine weiteren Erwiderungen mehr anhören!"

# game/hchar.rpy:10484
translate german PA_HC_EV32_129fcc4a:

    # c "¿Te das cuenta de que no me estás escuchando?"
    c "Ist dir klar, dass du mir nicht zuhörst?"

# game/hchar.rpy:10485
translate german PA_HC_EV32_0f129435:

    # c "¡Solo quieres imponer tu voluntad sobre la mía a toda costa!"
    c "Du willst mir nur um jeden Preis deinen Willen aufzwingen!"

# game/hchar.rpy:10487
translate german PA_HC_EV32_60596566:

    # jul "¡Ya te dije que te vienes conmigo!"
    jul "Ich habe dir schon gesagt, dass du mit mir kommst!"

# game/hchar.rpy:10488
translate german PA_HC_EV32_4863efd9:

    # jul "¡No me obligues a usar la fuerza!"
    jul "Zwing mich nicht, Gewalt anzuwenden!"

# game/hchar.rpy:10490
translate german PA_HC_EV32_3a61451e:

    # c "Incluso amenazas con golpearme si no hago lo que dices."
    c "Du drohst mir sogar, mich zu schlagen, wenn ich nicht tue, was du sagst."

# game/hchar.rpy:10492
translate german PA_HC_EV32_b8935506:

    # c "Eres una..."
    c "Du bist eine..."

# game/hchar.rpy:10497
translate german PA_HC_EV32_ccd2e7c8:

    # mc "(Creo que es momento de intervenir, antes de que esto siga escalando.)"
    mc "(Ich glaube, es ist Zeit, einzugreifen, bevor sich die Lage weiter zuspitzt.)"

# game/hchar.rpy:10498
translate german PA_HC_EV32_3cdd5f50:

    # mc "(Aunque se supone que no debería conocer a Charlotte, así que...)"
    mc "(Obwohl ich Charlotte eigentlich gar nicht kennen dürfte, also...)"

# game/hchar.rpy:10499
translate german PA_HC_EV32_79057949:

    # mc "Buenos días, Juliette."
    mc "Guten Morgen, Juliette."

# game/hchar.rpy:10504
translate german PA_HC_EV32_60d95ad4:

    # jul "¿Acaso he escuchado mi nombre?"
    jul "Habe ich gerade meinen Namen gehört?"

# game/hchar.rpy:10505
translate german PA_HC_EV32_4743c083:

    # jul "¡Pero mira qué sorpresa!"
    jul "Aber was für eine Überraschung!"

# game/hchar.rpy:10507
translate german PA_HC_EV32_d172520b:

    # jul "Eres el chico guapo que me ayudó el día de ayer."
    jul "Du bist der hübsche Junge, der mir gestern geholfen hat."

# game/hchar.rpy:10509
translate german PA_HC_EV32_05f4c564:

    # c "¿Qué quieres decir con que te ayudo?"
    c "Was meinst du damit, er hat dir geholfen?"

# game/hchar.rpy:10511
translate german PA_HC_EV32_2907e0cb:

    # jul "Al llegar a Argleton estaba muy perdida y buscaba la casa de Lissette."
    jul "Als ich in Argleton ankam, war ich völlig verloren und suchte nach Lissettes Haus."

# game/hchar.rpy:10512
translate german PA_HC_EV32_a9a1c702:

    # jul "Y este muchacho me ayudo a encontrar el lugar."
    jul "Und dieser Junge hat mir geholfen, den Ort zu finden."

# game/hchar.rpy:10513
translate german PA_HC_EV32_22887497:

    # jul "Creo recordar que su nombre es [nombre]."
    jul "Ich glaube mich zu erinnern, dass sein Name [nombre] ist."

# game/hchar.rpy:10515
translate german PA_HC_EV32_6ee7cf91:

    # c "¿Incluso recuerdas su nombre?"
    c "Dass du dich noch an seinen Namen erinnern kannst?"

# game/hchar.rpy:10517
translate german PA_HC_EV32_06501111:

    # jul "¿Acaso piensas que soy tan tonta que no podría recordar un simple nombre?"
    jul "Glaubst du etwa, ich sei so dumm, dass ich mir einen einfachen Namen nicht merken könnte?"

# game/hchar.rpy:10519
translate german PA_HC_EV32_3740816d:

    # c "Pensaba que un simple pueblerino entraría en tu categoría de insignificantes."
    c "Ich dachte, ein einfacher Dorfbewohner würde in deine Kategorie der Unbedeutenden fallen."

# game/hchar.rpy:10521
translate german PA_HC_EV32_f171b436:

    # jul "Estrictamente tienes razón, pero puedo recordar la cara de un chico tan guapo como él."
    jul "Streng genommen hast du Recht, aber ich kann mich an das Gesicht eines so gutaussehenden Jungen wie ihn erinnern."

# game/hchar.rpy:10526
translate german PA_HC_EV32_1e856685:

    # c "¡Deja de decir tonterías Madre!"
    c "Hör auf, Unsinn zu reden, Mutter!"

# game/hchar.rpy:10530
translate german PA_HC_EV32_cfb84e01:

    # c "¿Cómo te atreves a decir que es un chico guapo?"
    c "Wie kannst du es wagen, zu sagen, dass er ein gutaussehender Junge ist?"

# game/hchar.rpy:10532
translate german PA_HC_EV32_00e12f46:

    # jul "¿Por qué sobre reaccionas de esa forma Charlotte?"
    jul "Warum reagierst du so übertrieben, Charlotte?"

# game/hchar.rpy:10534
translate german PA_HC_EV32_0405b5a8:

    # c "¡No estoy sobre reaccionando!"
    c "Ich reagiere nicht übertrieben!"

# game/hchar.rpy:10536
translate german PA_HC_EV32_1981783a:

    # jul "¿Acaso es porque he piropeado a un muchacho desconocido para ti?"
    jul "Ist es vielleicht, weil ich einem Jungen, den du nicht kennst, ein Kompliment gemacht habe?"

# game/hchar.rpy:10538
translate german PA_HC_EV32_e6594fcc:

    # c "¡Deja ya de decir eso!"
    c "Hör auf, das zu sagen!"

# game/hchar.rpy:10540
translate german PA_HC_EV32_643c7e92:

    # jul "Parece que he tocado un punto sensible, Charlotte."
    jul "Es scheint, als hätte ich einen wunden Punkt getroffen, Charlotte."

# game/hchar.rpy:10542
translate german PA_HC_EV32_82865989:

    # jul "No me digas que te sientes atraída por este muchacho."
    jul "Sag mir nicht, dass du dich zu diesem Jungen hingezogen fühlst."

# game/hchar.rpy:10544
translate german PA_HC_EV32_9adf94d8:

    # c "¡Por supuesto que no!"
    c "Natürlich nicht!"

# game/hchar.rpy:10545
translate german PA_HC_EV32_ed178e5a:

    # c "¡Ni siquiera lo conozco!"
    c "Ich kenne ihn nicht einmal!"

# game/hchar.rpy:10546
translate german PA_HC_EV32_acc390b0:

    # c "¡Es solo que me molesta que piropees a otro hombre cuando Padre no está contigo!"
    c "Es ärgert mich nur, dass du einem anderen Mann Komplimente machst, wenn Vater nicht bei dir ist!"

# game/hchar.rpy:10548
translate german PA_HC_EV32_0f6301db:

    # jul "Oui, bastante curioso que sea tu primera vez reaccionando así bajo este contexto."
    jul "Oui, es ist schon merkwürdig, dass du zum ersten Mal in diesem Zusammenhang so reagierst."

# game/hchar.rpy:10550
translate german PA_HC_EV32_f3a326c9:

    # c "¡Estas tergiversando todo!"
    c "Du verdrehst alles!"

# game/hchar.rpy:10552
translate german PA_HC_EV32_78b8a3bd:

    # jul "Está bien estar enamorada, Charlotte."
    jul "Es ist in Ordnung, verliebt zu sein, Charlotte."

# game/hchar.rpy:10553
translate german PA_HC_EV32_cd40bcd8:

    # jul "Yo también viví esa experiencia cuando tenía tu edad."
    jul "Ich habe das auch erlebt, als ich in deinem Alter war."

# game/hchar.rpy:10554
translate german PA_HC_EV32_108f891d:

    # jul "Pero continuar con esa vida sería lo suficientemente provechoso."
    jul "Aber dieses Leben weiterzuführen, wäre lohnenswert genug."

# game/hchar.rpy:10555
translate german PA_HC_EV32_9a7a9b55:

    # jul "Es por eso que al final decidí quedarme con tu padre."
    jul "Deshalb habe ich mich letztendlich entschieden, bei deinem Vater zu bleiben."

# game/hchar.rpy:10557
translate german PA_HC_EV32_fa1bd637:

    # c "¡Deja de decir que estoy enamorada!"
    c "Hör auf zu sagen, dass ich verliebt bin!"

# game/hchar.rpy:10559
translate german PA_HC_EV32_bd30dece:

    # mc "(Debería desviar el tema de conversación antes de que se calienten los ánimos.)"
    mc "(Ich sollte das Thema wechseln, bevor die Gemüter sich erhitzen.)"

# game/hchar.rpy:10560
translate german PA_HC_EV32_46ed2527:

    # mc "Es primera vez que veo a dos personas discutir tanto en este pueblo."
    mc "Es ist das erste Mal, dass ich zwei Menschen in diesem Dorf so heftig streiten sehe."

# game/hchar.rpy:10562
translate german PA_HC_EV32_5c368008:

    # mc "Normalmente la gente visita Argleton para encontrar un poco de paz."
    mc "Normalerweise besuchen die Menschen Argleton, um ein wenig Ruhe zu finden."

# game/hchar.rpy:10564
translate german PA_HC_EV32_81c1dded:

    # jul "Disculpa mi mala conducta querido [nombre], pero hemos tenido algunos desacuerdos recientes, mi hija y yo."
    jul "Entschuldige mein schlechtes Benehmen mein Liebling, [nombre], aber meine Tochter und ich hatten in letzter Zeit einige Meinungsverschiedenheiten."

# game/hchar.rpy:10566
translate german PA_HC_EV32_dc34ec44:

    # c "¡No te atrevas a decirle querido de nuevo!"
    c "Wage es nicht, ihn noch einmal „Liebling“ zu nennen!"

# game/hchar.rpy:10568
translate german PA_HC_EV32_e978227e:

    # mc "(Mierda... {w}Si Charlotte sigue sobre reaccionando de esa forma hará más difícil todo.)"
    mc "(Scheisse... {w}Wenn Charlotte weiterhin so überreagiert, wird das alles nur noch schwieriger machen.)"

# game/hchar.rpy:10569
translate german PA_HC_EV32_2c06138a:

    # mc "(Y tengo el leve presentimiento de que Juliette lo está haciendo a propósito para provocar.)"
    mc "(Und ich habe das leise Gefühl, dass Juliette das absichtlich tut, um zu provozieren.)"

# game/hchar.rpy:10571
translate german PA_HC_EV32_4bfe65ec:

    # jul "Comienzo a sospechar de que ustedes se conocen bastante."
    jul "Ich beginne zu vermuten, dass Sie sich ziemlich gut kennen."

# game/hchar.rpy:10573
translate german PA_HC_EV32_04b7b681:

    # mc "¡Por supuesto que sí!"
    mc "Natürlich!"

# game/hchar.rpy:10578
translate german PA_HC_EV32_201b29af:

    # c "¿Qué?"
    c "Was?"

# game/hchar.rpy:10580
translate german PA_HC_EV32_fb5efcc8:

    # mc "Todo el mundo conoce a Charlotte por los alrededores."
    mc "Jeder in der Umgebung kennt Charlotte."

# game/hchar.rpy:10584
translate german PA_HC_EV32_02947302:

    # mc "Desde su llegada a Argleton a construido una enorme reputación a su alrededor, como la mejor chef de la región."
    mc "Seit ihrer Ankunft in Argleton hat sie sich einen hervorragenden Ruf als beste Köchin der Region erworben."

# game/hchar.rpy:10585
translate german PA_HC_EV32_7df1b3ad:

    # mc "Yo conozco muy bien a Charlotte, aunque quizás ella a mí no tanto, ya que tan solo soy un cliente más."
    mc "Ich kenne Charlotte sehr gut, auch wenn sie mich vielleicht nicht so gut kennt, da ich nur ein Gast unter vielen bin."

# game/hchar.rpy:10587
translate german PA_HC_EV32_1689139a:

    # jul "¿La mejor chef?"
    jul "Die beste Köchin?"

# game/hchar.rpy:10589
translate german PA_HC_EV32_80a44caf:

    # jul "¿Es eso verdad Charlotte?"
    jul "Ist das wahr, Charlotte?"

# game/hchar.rpy:10591
translate german PA_HC_EV32_b46f1ae9:

    # c "O-oui..."
    c "O-oui..."

# game/hchar.rpy:10593
translate german PA_HC_EV32_cfbe3052:

    # jul "Entonces es eso lo que has estado haciendo durante todo este tiempo."
    jul "Das hast du also die ganze Zeit gemacht."

# game/hchar.rpy:10594
translate german PA_HC_EV32_1abf192b:

    # jul "¿Puedo saber donde?"
    jul "Darf ich fragen, wo?"

# game/hchar.rpy:10596
translate german PA_HC_EV32_4a396b86:

    # c "En un lugar llamado [restaurant]."
    c "An einem Ort namens [restaurant]."

# game/hchar.rpy:10598
translate german PA_HC_EV32_a5c52e94:

    # jul "Me hace muy feliz escuchar eso Charlotte."
    jul "Das freut mich sehr zu hören, Charlotte."

# game/hchar.rpy:10599
translate german PA_HC_EV32_90f80440:

    # jul "Con unas buenas habilidades culinarias serás capaz de encontrar un mejor marido cuando vuelvas a casa."
    jul "Mit guten Kochkünsten wirst du einen besseren Ehemann finden, wenn du nach Hause zurückkehrst."

# game/hchar.rpy:10601
translate german PA_HC_EV32_f2c6d788:

    # c "¡No es mi intención cocinar con ese objetivo!"
    c "Es ist nicht meine Absicht, mit diesem Ziel zu kochen!"

# game/hchar.rpy:10603
translate german PA_HC_EV32_7e7b8002:

    # jul "Eso lo dices por ahora, pero ya cambiarás de opinión."
    jul "Das sagst du jetzt, aber du wirst deine Meinung noch ändern."

# game/hchar.rpy:10604
translate german PA_HC_EV32_3e43bdf2:

    # jul "Y ahora que lo recuerdo..."
    jul "Und jetzt, wo ich daran denke..."

# game/hchar.rpy:10605
translate german PA_HC_EV32_01d5b602:

    # jul "¿Puedo saber donde está tu hermana?"
    jul "Darf ich fragen, wo deine Schwester ist?"

# game/hchar.rpy:10607
translate german PA_HC_EV32_919d6f5e:

    # c "Volvió a casa hace tan solo unos días."
    c "Sie ist erst vor ein paar Tagen nach Hause zurückgekehrt."

# game/hchar.rpy:10609
translate german PA_HC_EV32_a3efc7d5:

    # jul "Eso es bastante extraño..."
    jul "Das ist ziemlich seltsam..."

# game/hchar.rpy:10610
translate german PA_HC_EV32_a8ef5468:

    # jul "Vi su equipaje en la casa de Lissette."
    jul "Ich habe ihr Gepäck bei Lissette gesehen."

# game/hchar.rpy:10612
translate german PA_HC_EV32_3316a1cb:

    # c "Eso es porque Sophia es tan descuidada que perdió las llaves de la casa."
    c "Das liegt daran, dass Sophia so unordentlich ist, dass sie ihre Hausschlüssel verloren hat."

# game/hchar.rpy:10613
translate german PA_HC_EV32_c70041c7:

    # c "Así que decidió comprar ropa nueva y dejo todo lo demás aquí."
    c "Also beschloss sie, neue Kleidung zu kaufen und alles andere hier zu lassen."

# game/hchar.rpy:10615
translate german PA_HC_EV32_1c21a91f:

    # mc "(Buena mentira, Charlotte.)"
    mc "(Gute Lüge, Charlotte.)"

# game/hchar.rpy:10617
translate german PA_HC_EV32_96ef72c0:

    # c "Además dijo que se tomaría unos días en algún lugar turístico antes de volver a casa."
    c "Sie sagte auch, dass sie sich ein paar Tage Urlaub an einem touristischen Ort gönnen würde, bevor sie nach Hause zurückkehren würde."

# game/hchar.rpy:10619
translate german PA_HC_EV32_7cffd269:

    # jul "Entonces nos encontraremos con ella cuando ambas volvamos a casa."
    jul "Dann treffen wir sie, wenn wir beide nach Hause kommen."

# game/hchar.rpy:10620
translate german PA_HC_EV32_d2d8f56a:

    # jul "Al menos eso me quita un peso de encima."
    jul "Zumindest fällt mir damit eine Last von den Schultern."

# game/hchar.rpy:10621
translate german PA_HC_EV32_394f39b8:

    # jul "Esperaba lo peor..."
    jul "Ich hatte das Schlimmste erwartet..."

# game/hchar.rpy:10623
translate german PA_HC_EV32_7421138c:

    # c "¿Qué quieres decir?"
    c "Was meinst du damit?"

# game/hchar.rpy:10625
translate german PA_HC_EV32_b734d43b:

    # jul "Sophia dijo que te habías fugado con un chico a un pueblo llamado Argleton."
    jul "Sophia sagte, du seist mit einem Jungen in Argleton geflohen."

# game/hchar.rpy:10626
translate german PA_HC_EV32_da52b6b7:

    # jul "Así que ella también decidió viajar para llevarte a casa."
    jul "Also beschloss auch sie zu reisen, um dich nach Hause zu bringen."

# game/hchar.rpy:10627
translate german PA_HC_EV32_483dad2b:

    # jul "Y como vi que no regresaban, tuve que venir personalmente a ver que estaba sucediendo."
    jul "Und als ich sah, dass sie nicht zurückgekommen ist, musste ich persönlich nachsehen, was los war."

# game/hchar.rpy:10628
translate german PA_HC_EV32_bdc402e5:

    # jul "Me volvía loca la idea de que arruinaras tu futuro, Charlotte."
    jul "Der Gedanke, dass du deine Zukunft ruinieren könntest, machte mich wahnsinnig, Charlotte."

# game/hchar.rpy:10630
translate german PA_HC_EV32_10d46a19:

    # c "Yo sé muy bien lo que estoy haciendo, Madre."
    c "Ich weiss sehr gut, was ich tue, Mutter."

# game/hchar.rpy:10632
translate german PA_HC_EV32_670ffe6a:

    # jul "Todas dicen lo mismo, hasta que ya es tarde y quedan embarazadas."
    jul "Alle sagen dasselbe, bis es zu spät ist und sie schwanger werden."

# game/hchar.rpy:10633
translate german PA_HC_EV32_31aafbdd:

    # jul "A los pocos días de llegar aquí también envió una carta a mi nombre."
    jul "Ein paar Tage nach meiner Ankunft hier schickte sie auch einen Brief an mich."

# game/hchar.rpy:10635
translate german PA_HC_EV32_bc86e149:

    # c "¿Qué decía esa carta?"
    c "Was stand in diesem Brief?"

# game/hchar.rpy:10637
translate german PA_HC_EV32_c91f4a5f:

    # jul "Que sospechaba que estabas saliendo con un muchacho que calza con las características de [nombre]."
    jul "Dass sie vermutete, dass du mit einem Jungen zusammen bist, der die Eigenschaften von [nombre] aufweist."

# game/hchar.rpy:10638
translate german PA_HC_EV32_a9a9934b:

    # jul "Al menos cuando lo vi, no tuve ninguna duda."
    jul "Als ich ihn sah, hatte ich zumindest keine Zweifel."

# game/hchar.rpy:10640
translate german PA_HC_EV32_cfe00bc6:

    # c "Sophia... {w}Traidora..."
    c "Sophia... {w}Verräterin..."

# game/hchar.rpy:10641
translate german PA_HC_EV32_dd2ec5b4:

    # c "Ni siquiera puedo confiar en mi hermana."
    c "Ich kann nicht einmal meiner Schwester vertrauen."

# game/hchar.rpy:10642
translate german PA_HC_EV32_af3587c0:

    # c "Incluso ella inventa mentiras en mi contra."
    c "Sie erfindet sogar Lügen gegen mich."

# game/hchar.rpy:10644
translate german PA_HC_EV32_426e129b:

    # mc "(Entonces esa era la razón detrás de los piropos.)"
    mc "(Das war also der Grund für die Komplimente.)"

# game/hchar.rpy:10645
translate german PA_HC_EV32_bc52beff:

    # mc "(Buscaba ver la reacción de Charlotte.)"
    mc "(Ich wollte Charlottes Reaktion sehen.)"

# game/hchar.rpy:10651
translate german PA_HC_EV32_a32edffe:

    # so "¡Yo no soy una traidora!"
    so "Ich bin keine Verräterin!"

# game/hchar.rpy:10653
translate german PA_HC_EV32_7d5cd60a:

    # jul "¿Sophia?"
    jul "Sophia?"

# game/hchar.rpy:10655
translate german PA_HC_EV32_b2694e9c:

    # so "H-hola M-m-madre..."
    so "H-hallo M-m-Mutter..."

# game/hchar.rpy:10660
translate german PA_HC_EV32_81b4c895:

    # c "¡Por qué tenías que salir y arruinar todo!"
    c "Warum musstest du rauskommen und alles ruinieren!"

# game/hchar.rpy:10662
translate german PA_HC_EV32_4a3e785c:

    # so "¡No podía soportar que me tacharas de traidora!"
    so "Ich konnte es nicht ertragen, dass du mich als Verräterin bezeichnet hast!"

# game/hchar.rpy:10663
translate german PA_HC_EV32_833e4cd0:

    # so "¡Lo de la carta fue durante los primeros días que estuve aquí!"
    so "Das mit dem Brief war in den ersten Tagen, als ich hier war!"

# game/hchar.rpy:10664
translate german PA_HC_EV32_0c216d2b:

    # so "¡Tú me trataste muy mal y quería vengarme de alguna forma!"
    so "Du hast mich sehr schlecht behandelt und ich wollte mich irgendwie rächen!"

# game/hchar.rpy:10666
translate german PA_HC_EV32_911036f5:

    # c "¡Acabas de arruinar todo hermana estúpida!"
    c "Du hast gerade alles ruiniert, du dumme Kuh!"

# game/hchar.rpy:10668
translate german PA_HC_EV32_5e11abdb:

    # jul "Así que Sophia no estaba viajando de vuelta."
    jul "Also reiste Sophia nicht zurück."

# game/hchar.rpy:10670
translate german PA_HC_EV32_e78a7de0:

    # so "¡Todo tiene una explicación Madre!"
    so "Alles hat eine Erklärung, Mutter!"

# game/hchar.rpy:10672
translate german PA_HC_EV32_c8971691:

    # jul "No sé que hice mal para tener unas hijas tan irresponsables."
    jul "Ich weiss nicht, was ich falsch gemacht habe, dass ich so unverantwortliche Töchter habe."

# game/hchar.rpy:10673
translate german PA_HC_EV32_2b50a719:

    # jul "Pero ya he sacado mis propias deducciones de toda esta conversación."
    jul "Aber ich habe bereits meine eigenen Schlussfolgerungen aus diesem ganzen Gespräch gezogen."

# game/hchar.rpy:10675
translate german PA_HC_EV32_27da81e7:

    # so "¡Te juro que lo de la carta era mentira!"
    so "Ich schwöre dir, das mit dem Brief war eine Lüge!"

# game/hchar.rpy:10677
translate german PA_HC_EV32_afa3f8ac:

    # jul "Falacias y más falacias..."
    jul "Täuschungen und noch mehr Täuschungen..."

# game/hchar.rpy:10678
translate german PA_HC_EV32_82fbf2c2:

    # jul "Ustedes vendrán a casa conmigo ahora mismo."
    jul "Ihr kommt jetzt sofort mit mir nach Hause."

# game/hchar.rpy:10679
translate german PA_HC_EV32_71fb6e26:

    # jul "Y tendrán el castigo más grandes de sus vidas."
    jul "Und ihr werdet die grösste Strafe eures Lebens erhalten."

# game/hchar.rpy:10681
translate german PA_HC_EV32_964be8dd:

    # c "¡No puedo creer lo imbécil que eres Sophia!"
    c "Ich kann nicht glauben, wie dumm du bist, Sophia!"

# game/hchar.rpy:10683
translate german PA_HC_EV32_864bb27f_3:

    # so "Oh no... non non non..."
    so "Oh non... non non non..."

# game/hchar.rpy:10685
translate german PA_HC_EV32_b3c9208c:

    # jul "¿Vendrán por las buenas o por las malas?"
    jul "Kommt ihr freiwillig, oder muss ich Gewalt anwenden?"

# game/hchar.rpy:10687
translate german PA_HC_EV32_1ed44ff2:

    # so "Por las buenas."
    so "Auf die nette Art."

# game/hchar.rpy:10689
translate german PA_HC_EV32_dcabbd34:

    # c "¡No me pienso ir de aquí hasta que yo quiera!"
    c "Ich werde hier nicht weggehen, bevor ich es will!"

# game/hchar.rpy:10691
translate german PA_HC_EV32_798f3764:

    # jul "Entonces será por las malas."
    jul "Dann wird es auf die harte Tour gehen."

# game/hchar.rpy:10693
translate german PA_HC_EV32_6a121ecd:

    # jul "..."
    jul "..."

# game/hchar.rpy:10695
translate german PA_HC_EV32_f8eb00f5:

    # mc "¿Por qué se ha acercado a aquella roca?"
    mc "Warum ist sie zu diesem Felsen gegangen?"

# game/hchar.rpy:10696
translate german PA_HC_EV32_baa7a881_1:

    # c "Merde..."
    c "Merde..."

# game/hchar.rpy:10698
translate german PA_HC_EV32_d2b682cc:

    # so "¡Corran y no miren para atrás!"
    so "Lauft und schaut nicht zurück!"

# game/hchar.rpy:10708
translate german PA_HC_EV32_c6a09a79:

    # jul "¡¡¡VENDRÁN A CASA AHORA MISMO!!!"
    jul "IHR KOMMT SOFORT NACHHAUSE!!!"

# game/hchar.rpy:10710
translate german PA_HC_EV32_ff61dc78:

    # jul "¡¡¡Y NO VOY A ACEPTAR UN NO COMO RESPUESTA!!!"
    jul "UND ICH WERDE EIN NEIN ALS ANTWORT NICHT AKZEPTIEREN!!!"

# game/hchar.rpy:10712
translate german PA_HC_EV32_2f30d224:

    # mc "Esto debe ser una broma..."
    mc "Das muss ein Scherz sein..."

# game/hchar.rpy:10714
translate german PA_HC_EV32_13d8dc97:

    # jul "¡¡¡VENGAN AQUÍ CHARLOTTE Y SOPHIA!!!"
    jul "KOMMT HER, CHARLOTTE UND SOPHIA!!!"

# game/hchar.rpy:10719
translate german PA_HC_EV32_7e9efec3:

    # c "¡¡¡SOPHIA ESTÚPIDA MIRA LO QUE HAS HECHO!!!"
    c "SOPHIA, DU DUMME, SCHAU DIR AN, WAS DU ANGERICHTET HAST!!!"

# game/hchar.rpy:10721
translate german PA_HC_EV32_5b23c617:

    # jul "¡¡¡NO PIENSO SOPORTAR MÁS INSOLENCIAS Y FALTAS DE RESPETO!!!"
    jul "ICH WERDE KEINE UNVERSCHÄMTHEITEN UND RESPEKTLOSIGKEITEN MEHR HINNEHMEN!!!"

# game/hchar.rpy:10726
translate german PA_HC_EV32_63708cc3:

    # so "¡¡¡AHHHHHHHHHHHHHHHHHHHHHH!!!"
    so "AHHHHHHHHHHHHHHHHHHHHHH!!!"

# game/hchar.rpy:10731
translate german PA_HC_EV32_5b71d26f:

    # "<Los tres corren con todas sus fuerzas sin mirar atrás hasta llegar al otro extremo de la playa.>"
    "<Die drei rennen mit aller Kraft, ohne sich umzusehen, bis sie das andere Ende des Strandes erreichen.>"

# game/hchar.rpy:10736
translate german PA_HC_EV32_abb519e3:

    # so "Creo que la hemos perdido..."
    so "Ich glaube, wir haben sie verloren..."

# game/hchar.rpy:10741
translate german PA_HC_EV32_c554f5fe:

    # c "Eso ha estado cerca."
    c "Das war knapp."

# game/hchar.rpy:10743
translate german PA_HC_EV32_8f4c5471:

    # mc "Qué mierda ha sido eso."
    mc "Was für ein Scheiss war das denn?"

# game/hchar.rpy:10744
translate german PA_HC_EV32_5b33fbd1:

    # mc "¿Cómo es posible que una mujer de su edad pueda levantar una roca como esa?"
    mc "Wie ist es möglich, dass eine Frau in ihrem Alter einen solchen Stein heben kann?"

# game/hchar.rpy:10746
translate german PA_HC_EV32_47e617e4:

    # c "Deben ser los genes de Mémé."
    c "Das müssen die Gene von Mémé sein."

# game/hchar.rpy:10747
translate german PA_HC_EV32_85fcff67:

    # c "Ella también era muy fuerte en su juventud."
    c "Auch sie war in ihrer Jugend sehr stark."

# game/hchar.rpy:10749
translate german PA_HC_EV32_41b9efc7:

    # so "Pensaba que íbamos a morir."
    so "Ich dachte, wir würden sterben."

# game/hchar.rpy:10751
translate german PA_HC_EV32_b2ac90c5:

    # mc "Cuando escuche que usaría la fuerza, pensaba que hablaba en sentido figurativo."
    mc "Als ich hörte, dass sie Gewalt anwenden würde, dachte ich, sie meine das im übertragenen Sinne."

# game/hchar.rpy:10752
translate german PA_HC_EV32_4f728673:

    # mc "¡No que se transformará en el puto King Kong!"
    mc "Aber nicht so, dass sie sich in den verdammten King Kong verwandelt!"

# game/hchar.rpy:10754
translate german PA_HC_EV32_0e5292a9:

    # c "¡Y todo es por culpa de Sophia!"
    c "Und das alles ist Sophias Schuld!"

# game/hchar.rpy:10756
translate german PA_HC_EV32_46945756:

    # so "Tienes razón, lo siento..."
    so "Du hast Recht, es tut mir leid..."

# game/hchar.rpy:10757
translate german PA_HC_EV32_21554cc4:

    # so "Pero no quería que pensaras que soy una traidora."
    so "Aber ich wollte nicht, dass du denkst, ich sei eine Verräterin."

# game/hchar.rpy:10759
translate german PA_HC_EV32_cb899629:

    # c "¡Aun así fuiste una traidora!"
    c "Trotzdem warst du eine Verräterin!"

# game/hchar.rpy:10761
translate german PA_HC_EV32_e6cdd32e:

    # so "Eso solo fue al comienzo..."
    so "Das war nur am Anfang..."

# game/hchar.rpy:10762
translate german PA_HC_EV32_03caa856:

    # so "Pero cuando entendí mejor tu forma de pensar, sentí que me había equivocado."
    so "Aber als ich deine Denkweise besser verstand, merkte ich, dass ich mich geirrt hatte."

# game/hchar.rpy:10763
translate german PA_HC_EV32_fb648ed7:

    # so "Estaba muy nerviosa por la llegada de Madre debido a que había sido mi culpa."
    so "Ich war wegen der Ankunft unserer Mutter sehr nervös, weil es meine Schuld gewesen war."

# game/hchar.rpy:10765
translate german PA_HC_EV32_5cd9af86:

    # c "No puedo creerlo..."
    c "Ich kann es nicht glauben..."

# game/hchar.rpy:10767
translate german PA_HC_EV32_ccd01633:

    # so "De verdad lo siento, intentaré una forma de compensarte por esto."
    so "Es tut mir wirklich leid, ich werde versuchen, es wieder gut zu machen."

# game/hchar.rpy:10769
translate german PA_HC_EV32_d079a8a1:

    # c "Ya no tiene importancia..."
    c "Das spielt keine Rolle mehr..."

# game/hchar.rpy:10770
translate german PA_HC_EV32_5035d7d1:

    # c "Madre se habría enterado de todas formas."
    c "Mutter hätte es sowieso erfahren."

# game/hchar.rpy:10771
translate german PA_HC_EV32_caed881d:

    # c "Tu Sophia, deberías dejar de tomar decisiones basadas en impulsos."
    c "Sophia, du solltest aufhören, Entscheidungen aus einer Laune heraus zu treffen."

# game/hchar.rpy:10773
translate german PA_HC_EV32_72c5358f:

    # c "Y tampoco entiendo lo despreocupado que eres [nombre]."
    c "Und ich verstehe auch nicht, wie sorglos du bist, [nombre]."

# game/hchar.rpy:10775
translate german PA_HC_EV32_3e5fae1f:

    # mc "¿Qué hice ahora?"
    mc "Was habe ich jetzt getan?"

# game/hchar.rpy:10777
translate german PA_HC_EV32_021941d7:

    # c "¿De verdad no te has dado cuenta?"
    c "Hast du es wirklich nicht bemerkt?"

# game/hchar.rpy:10779
translate german PA_HC_EV32_9ae0a7a1_1:

    # so "Oh non..."
    so "Oh non..."

# game/hchar.rpy:10781
translate german PA_HC_EV32_bd30a800:

    # mc "¿Qué sucede Sophia?"
    mc "Was ist los, Sophia?"

# game/hchar.rpy:10783
translate german PA_HC_EV32_5334e014:

    # c "¡Has estado todo este rato en pijama!"
    c "Du warst die ganze Zeit in Pyjamas!"

# game/hchar.rpy:10784
translate german PA_HC_EV32_0b456737:

    # c "¡Y Juliette te ha visto así!"
    c "Und Juliette hat dich so gesehen!"

# game/hchar.rpy:10789
translate german PA_HC_EV32_b43be0ea_1:

    # mc "Mierda..."
    mc "Scheisse..."

# game/hchar.rpy:10790
translate german PA_HC_EV32_cb1a0901:

    # mc "Todo sucedió tan rápido que no me detuve a pensar en los detalles."
    mc "Alles ging so schnell, dass ich nicht inne halten konnte, um über die Details nachzudenken."

# game/hchar.rpy:10792
translate german PA_HC_EV32_adfd306e:

    # mc "Supongo que me deje llevar por los nervios de Sophia antes de venir aquí."
    mc "Ich glaube, ich habe mich von Sophias Nervosität anstecken lassen, bevor ich hierher kam."

# game/hchar.rpy:10794
translate german PA_HC_EV32_ac76c022:

    # c "No sé que voy a hacer con ustedes dos."
    c "Ich weiss nicht, was ich mit euch beiden machen soll."

# game/hchar.rpy:10795
translate german PA_HC_EV32_67cc50d2:

    # c "Deberíamos volver al [restaurant]..."
    c "Wir sollten wieder ins [restaurant] gehen..."

# game/hchar.rpy:10796
translate german PA_HC_EV32_ae831b06:

    # c "Madre podría aparecer en cualquier momento y debo prepararme para lo peor."
    c "Meine Mutter könnte jeden Moment auftauchen, und ich muss mich auf das Schlimmste vorbereiten."

# game/hchar.rpy:10801
translate german PA_HC_EV32_40b8d4db:

    # "<Vuelves junto a Charlotte y Sophia al [restaurant] y al llegar a tu habitación te cambias de ropa.>"
    "<Du kehrst mit Charlotte und Sophia zum [Restaurant] zurück und ziehst dich in deinem Zimmer um.>"

# game/hchar.rpy:10821
translate german PA_HC_EV33_72e2b775:

    # mc "Ya hemos terminado por el día de hoy."
    mc "Wir sind für heute fertig."

# game/hchar.rpy:10822
translate german PA_HC_EV33_ca707718:

    # mc "No quería decir nada durante el trabajo, pero..."
    mc "Ich wollte während der Arbeit nichts sagen, aber..."

# game/hchar.rpy:10823
translate german PA_HC_EV33_c32ffe2f:

    # mc "Has estado muy distraída durante todo el día."
    mc "Du warst den ganzen Tag über sehr abgelenkt."

# game/hchar.rpy:10825
translate german PA_HC_EV33_50365074:

    # c "Oui..."
    c "Oui..."

# game/hchar.rpy:10826
translate german PA_HC_EV33_b8d52c84:

    # c "No puedo quitarme de la cabeza a Juliette."
    c "Ich kann Juliette nicht aus meinem Kopf bekommen."

# game/hchar.rpy:10828
translate german PA_HC_EV33_5a8fcc3c:

    # c "Pienso que va a aparecer por aquí en cualquier momento."
    c "Ich denke, sie wird jeden Moment hier auftauchen."

# game/hchar.rpy:10829
translate german PA_HC_EV33_8c21bf02:

    # c "Y no estoy segura de como manejar la situación."
    c "Und ich bin mir nicht sicher, wie ich mit der Situation umgehen soll."

# game/hchar.rpy:10830
translate german PA_HC_EV33_d1542b47:

    # c "Sin mencionar que Sophia ha empeorado toda esta situación."
    c "Ganz zu schweigen davon, dass Sophia diese ganze Situation noch verschlimmert hat."

# game/hchar.rpy:10831
translate german PA_HC_EV33_cff63ac5:

    # c "Pero lo único que me queda es luchar."
    c "Aber mir bleibt nichts anderes übrig, als zu kämpfen."

# game/hchar.rpy:10833
translate german PA_HC_EV33_f89ae1aa:

    # mc "Sabes que te estaré apoyando en todo momento."
    mc "Du weisst, dass ich dich jederzeit unterstützen werde."

# game/hchar.rpy:10835
translate german PA_HC_EV33_b744274d:

    # c "Merci, [nombre]."
    c "Merci, [nombre]."

# game/hchar.rpy:10836
translate german PA_HC_EV33_ec8231f0:

    # c "Desearía que Juliette no fuese tan cabeza dura."
    c "Ich wünschte, Juliette wäre nicht so stur."

# game/hchar.rpy:10837
translate german PA_HC_EV33_bb16f849:

    # c "Cuando fui a la casa de Mémé en la playa para intentar convencerla."
    c "Als ich zu Mémés Haus am Strand ging, um sie zu überzeugen."

# game/hchar.rpy:10838
translate german PA_HC_EV33_a53b21a0:

    # c "¿Sabes que fue lo primero que me pregunto?"
    c "Weisst du, was sie mich als Erstes gefragt hat?"

# game/hchar.rpy:10840
translate german PA_HC_EV33_ba0ba93c:

    # c "...Y por esa razón debo seguir esforzándome."
    c "... Und aus diesem Grund muss ich mich weiterhin anstrengen."

# game/hchar.rpy:10841
translate german PA_HC_EV33_d62345c9:

    # c "Debo considerar todo antes de volver a casa."
    c "Ich muss alles überdenken, bevor ich nach Hause zurückkehre."

# game/hchar.rpy:10843
translate german PA_HC_EV33_1b80b6a9:

    # jul "Como sea Charlotte..."
    jul "Wie auch immer, Charlotte..."

# game/hchar.rpy:10844
translate german PA_HC_EV33_16d85f96:

    # jul "¿Por qué mejor no me explicas qué ha sucedido con tu estilo?"
    jul "Warum erklärst du mir nicht lieber, was mit deinem Stil passiert ist?"

# game/hchar.rpy:10845
translate german PA_HC_EV33_538474fb:

    # jul "Ese peinado no te queda bien y no encaja con esa vestimenta de mal gusto."
    jul "Diese Frisur steht dir nicht gut und passt nicht zu diesem geschmacklosen Outfit."

# game/hchar.rpy:10846
translate german PA_HC_EV33_51d73f38:

    # jul "¿Y qué te he dicho de usar tus lentes en público?"
    jul "Und was habe ich dir über das Tragen deiner Brille in der Öffentlichkeit gesagt?"

# game/hchar.rpy:10848
translate german PA_HC_EV33_45def06b:

    # c "¿No has escuchado nada de lo que estaba hablando?"
    c "Hast du nichts von dem gehört, worüber ich gesprochen habe?"

# game/hchar.rpy:10850
translate german PA_HC_EV33_ca269a3f:

    # jul "Es imposible concentrarse si estás ensuciando mi campo visual de esa forma."
    jul "Es ist unmöglich, sich zu konzentrieren, wenn du mein Sichtfeld so verschmutzt."

# game/hchar.rpy:10851
translate german PA_HC_EV33_3194f948:

    # jul "Ahora déjate de tonterías y volvamos a casa."
    jul "Jetzt hör auf mit dem Unsinn und lass uns nach Hause gehen."

# game/hchar.rpy:10852
translate german PA_HC_EV33_6cd764f2:

    # jul "Es una lástima que mi teléfono no tenga señal desde que llegue a Argleton."
    jul "Es ist schade, dass mein Handy seit meiner Ankunft in Argleton keinen Empfang hat."

# game/hchar.rpy:10853
translate german PA_HC_EV33_52ccbfdb:

    # jul "Necesito hacer una llamada urgente al estilista."
    jul "Ich muss dringend den Friseur anrufen."

# game/hchar.rpy:10855
translate german PA_HC_EV33_c0f02561:

    # c "Puedo arreglar mi apariencia cuando quiera."
    c "Ich kann mein Aussehen jederzeit verbessern."

# game/hchar.rpy:10856
translate german PA_HC_EV33_2b413993:

    # c "Nunca he necesitado ayuda de un estilista."
    c "Ich habe noch nie die Hilfe eines Stylisten gebraucht."

# game/hchar.rpy:10858
translate german PA_HC_EV33_8bd972de:

    # jul "Claro que sí, es cosa de verte."
    jul "Natürlich, sieh dich ja nur an."

# game/hchar.rpy:10859
translate german PA_HC_EV33_c0dd4a61:

    # jul "Incluso has arruinado tu cabello."
    jul "Du hast sogar dein Haar ruiniert."

# game/hchar.rpy:10860
translate german PA_HC_EV33_8baa6e5b:

    # jul "En cuanto tenga un poco de señal me pondré en contacto con Giuseppe."
    jul "Sobald ich etwas Empfang habe, werde ich mich mit Giuseppe in Verbindung setzen."

# game/hchar.rpy:10861
translate german PA_HC_EV33_29a1f9b8:

    # jul "El sabrá como arreglarte de nuevo."
    jul "Er wird wissen, wie er dich wieder zusammenflicken kann."

# game/hchar.rpy:10863
translate german PA_HC_EV33_407be44c:

    # c "No sabes lo frustrante que puede llegar a ser Juliette."
    c "Du weisst gar nicht, wie frustrierend Juliette sein kann."

# game/hchar.rpy:10864
translate german PA_HC_EV33_58973f73:

    # c "Nunca se detiene a entender los sentimientos de los demás."
    c "Sie hält nie inne, um die Gefühle anderer zu verstehen."

# game/hchar.rpy:10865
translate german PA_HC_EV33_d27aa7cc:

    # c "Ni aunque se trate de su propia hija."
    c "Nicht einmal, wenn es sich um die eigene Tochter handelt."

# game/hchar.rpy:10867
translate german PA_HC_EV33_e7711433:

    # mc "Tienes que haber una forma."
    mc "Es muss einen Weg geben."

# game/hchar.rpy:10869
translate german PA_HC_EV33_31d0412f:

    # c "No sabes cuantas veces lo he intentado."
    c "Du weisst gar nicht, wie oft ich es versucht habe."

# game/hchar.rpy:10871
translate german PA_HC_EV33_38d8c9d7:

    # mc "Ahora comienzo a sentir un poco de curiosidad."
    mc "Jetzt werde ich langsam neugierig."

# game/hchar.rpy:10873
translate german PA_HC_EV33_8dd7c063:

    # c "¿Curiosidad por qué?"
    c "Neugierig, warum?"

# game/hchar.rpy:10875
translate german PA_HC_EV33_11f97eb0:

    # mc "¿Por qué cambiaste tu apariencia?"
    mc "Warum hast du dein Aussehen verändert?"

# game/hchar.rpy:10877
translate german PA_HC_EV33_04739670:

    # c "Cuando llegué a Argleton quería deshacerme de todo lo que me hiciera recordar a casa."
    c "Als ich in Argleton ankam, wollte ich alles loswerden, was mich an zu Hause erinnerte."

# game/hchar.rpy:10878
translate german PA_HC_EV33_d61303ff:

    # c "Mi intención era venir aquí para reencontrarme conmigo misma."
    c "Meine Absicht war es, hierher zu kommen, um wieder zu mir selbst zu finden."

# game/hchar.rpy:10879
translate german PA_HC_EV33_f71a2bfa:

    # c "Con el pasar de los días estaba tan enfocada en llegar a trabajar de chef en el Yorokobi."
    c "Mit jedem Tag, der verging, konzentrierte ich mich mehr darauf, zu einer Köchin im Yorokobi zu werden."

# game/hchar.rpy:10880
translate german PA_HC_EV33_42b98a4b:

    # c "Que apenas tenía tiempo para preocuparme por mi apariencia."
    c "Ich hatte kaum Zeit, mich um mein Aussehen zu kümmern."

# game/hchar.rpy:10881
translate german PA_HC_EV33_fc03ff7d:

    # c "Se podría decir que fueron una serie de coincidencias."
    c "Man könnte sagen, dass es eine Reihe von Zufällen waren."

# game/hchar.rpy:10883
translate german PA_HC_EV33_376e64e3:

    # mc "Eso me acaba de dar una idea."
    mc "Das bringt mich gerade auf eine Idee."

# game/hchar.rpy:10884
translate german PA_HC_EV33_e775745d:

    # mc "¿Tienes lo necesario para vestirte como lo hacías antes de conocernos?"
    mc "Hast du das Nötige, um dich so zu kleiden wie vor unserer Begegnung?"

# game/hchar.rpy:10886
translate german PA_HC_EV33_b6cda149:

    # c "Tengo el maquillaje y los vestidos guardados en mi habitación en el segundo piso."
    c "Ich bewahre mein Make-up und meine Kleider in meinem Zimmer im zweiten Stock auf."

# game/hchar.rpy:10887
translate german PA_HC_EV33_22055b43:

    # c "Si quisiera podría intentarlo."
    c "Wenn ich wollte, könnte ich es versuchen."

# game/hchar.rpy:10888
translate german PA_HC_EV33_aa63aee8:

    # c "Pero probablemente me tome bastante tiempo quedar lista."
    c "Aber es wird wahrscheinlich eine ganze Weile dauern, bis ich fertig bin."

# game/hchar.rpy:10890
translate german PA_HC_EV33_018011c4:

    # mc "Quizás sería una buena idea intentar ese cambio de estilo."
    mc "Vielleicht wäre es eine gute Idee, diesen Stilwechsel auszuprobieren."

# game/hchar.rpy:10892
translate german PA_HC_EV33_50365074_1:

    # c "Oui..."
    c "Oui..."

# game/hchar.rpy:10894
translate german PA_HC_EV33_f00596a9:

    # c "¿Con que cosa pervertida vas a salir ahora?"
    c "Was für eine perverse Idee hast du jetzt wieder?"

# game/hchar.rpy:10896
translate german PA_HC_EV33_d1f39c66:

    # mc "Espera, no se trata de nada pervertido."
    mc "Warte, es ist nichts Perverses dabei."

# game/hchar.rpy:10898
translate german PA_HC_EV33_972c4462:

    # c "Viniendo de ti podría esperar cualquier cosa."
    c "Von dir könnte ich alles erwarten."

# game/hchar.rpy:10900
translate german PA_HC_EV33_b7da370a:

    # mc "No voy a mentir diciendo que no me gustaría verte como eras antes."
    mc "Ich werde nicht lügen und sagen, dass ich dich nicht gerne so sehen würde, wie du früher warst."

# game/hchar.rpy:10901
translate german PA_HC_EV33_f88dc18c:

    # mc "Pero mi verdadera intención es otra."
    mc "Aber meine wahre Absicht ist eine andere."

# game/hchar.rpy:10902
translate german PA_HC_EV33_40cee6fa:

    # mc "Pensaba que si tu madre te viese como ella quiere."
    mc "Ich dachte, wenn deine Mutter dich sehen könnte, wie sie es sich wünscht."

# game/hchar.rpy:10903
translate german PA_HC_EV33_f8184aa2:

    # mc "Podría ayudar a que Juliette escuche tu punto de vista."
    mc "Könnte es Juliette helfen, sich deine Sichtweise anzuhören."

# game/hchar.rpy:10905
translate german PA_HC_EV33_f5fbfc29:

    # c "Pero eso sería como darle la razón."
    c "Aber das wäre, als würde man ihr Recht geben."

# game/hchar.rpy:10907
translate german PA_HC_EV33_ebfb2814:

    # mc "A veces para negociar necesitas ceder en lo menos importante."
    mc "Manchmal muss man bei Verhandlungen in weniger wichtigen Punkten nachgeben."

# game/hchar.rpy:10908
translate german PA_HC_EV33_825282d1:

    # mc "Para lograr obtener lo que realmente quieres."
    mc "Um das zu erreichen, was du wirklich willst."

# game/hchar.rpy:10909
translate german PA_HC_EV33_5c8a3783:

    # mc "Y lo importante es quitarte esa mochila llamada Juliette para seguir con tus sueños."
    mc "Und das Wichtigste ist, dass du diesen Rucksack namens Juliette ablegst, um deine Träume weiterzuverfolgen."

# game/hchar.rpy:10911
translate german PA_HC_EV33_ef9d88db:

    # c "Oui, quizás tengas razón."
    c "Oui, vielleicht hast du recht."

# game/hchar.rpy:10912
translate german PA_HC_EV33_96fb5035:

    # c "Podría hacer el intento."
    c "Ich könnte es versuchen."

# game/hchar.rpy:10913
translate german PA_HC_EV33_95a5ca41:

    # c "Pero me gustaría que esta vez estuvieses a mi lado antes de enfrentar a Juliette."
    c "Aber dieses Mal möchte ich, dass du an meiner Seite bist, bevor ich mich Juliette stelle."

# game/hchar.rpy:10915
translate german PA_HC_EV33_7ed0ccfd:

    # mc "No hay problema, aunque debo tener cuidado de no morir aplastado por una roca."
    mc "Kein Problem, obwohl ich aufpassen muss, dass ich nicht von einem Felsen erschlagen werde."

# game/hchar.rpy:10917
translate german PA_HC_EV33_34da931c:

    # c "Esto tomará tiempo, pero podrías esperarme en la playa Gold mientras tanto."
    c "Das wird eine Weile dauern, aber du könntest in der Zwischenzeit am Gold Beach auf mich warten."

# game/hchar.rpy:10919
translate german PA_HC_EV33_b8d90b97:

    # mc "Bien, nos vemos luego Charlotte."
    mc "Okay, bis später, Charlotte."

# game/hchar.rpy:10924
translate german PA_HC_EV33_97f722e4:

    # "<Te diriges a la playa Gold y te sientas sobre la arena para esperar a Charlotte.>"
    "<Du gehst zum Gold Beach und setzt dich in den Sand, um auf Charlotte zu warten.>"

# game/hchar.rpy:10925
translate german PA_HC_EV33_458606e4:

    # "<Aunque no imaginabas que sería tanto, te das cuenta qué han pasado unas cuantas horas.>"
    "<Obwohl du nicht gedacht hättest, dass es so lange dauern würde, merkst du, dass einige Stunden vergangen sind.>"

# game/hchar.rpy:10926
translate german PA_HC_EV33_bcad4b90:

    # "<Hasta que de pronto escuchas unos pasos sobre la arena detrás de ti.>"
    "<Bis du plötzlich Schritte im Sand hinter dir hörst.>"

# game/hchar.rpy:10931
translate german PA_HC_EV33_d1f4fbb5:

    # c "Lamento haberte hecho esperar tanto."
    c "Es tut mir leid, dass ich dich so lange warten liess."

# game/hchar.rpy:10932
translate german PA_HC_EV33_f4373502:

    # c "Ha sido mucho más difícil de lo que recordaba."
    c "Es war viel schwieriger, als ich es in Erinnerung hatte."

# game/hchar.rpy:10933
translate german PA_HC_EV33_772cc1fc:

    # c "Pero considero que ha sido un buen trabajo."
    c "Aber ich finde, es war eine gute Arbeit."

# game/hchar.rpy:10934
translate german PA_HC_EV33_0756b588:

    # c "[nombre]..."
    c "[nombre]..."

# game/hchar.rpy:10936
translate german PA_HC_EV33_2b35b6fe:

    # mc "..."
    mc "..."

# game/hchar.rpy:10938
translate german PA_HC_EV33_94c4d9fd:

    # c "¿Por qué te quedas callado?"
    c "Warum schweigst du?"

# game/hchar.rpy:10940
translate german PA_HC_EV33_0f4a6b41:

    # mc "Te ves hermosa."
    mc "Du siehst wunderschön aus."

# game/hchar.rpy:10942
translate german PA_HC_EV33_728dbd55:

    # c "Y-yo..."
    c "H-hey..."

# game/hchar.rpy:10943
translate german PA_HC_EV33_01106d82:

    # c "¿De verdad?"
    c "Wirklich?"

# game/hchar.rpy:10945
translate german PA_HC_EV33_97e37f4e:

    # mc "Creo que estoy sufriendo una erección y ni siquiera he necesitado verte desnuda."
    mc "Ich glaube, ich habe nen steifen, und ich musste dich nicht einmal nackt sehen."

# game/hchar.rpy:10947
translate german PA_HC_EV33_d6ddf845:

    # c "¿Por qué siempre tienes que estar pensando en cosas pervertidas?"
    c "Warum musst du immer an perverse Dinge denken?"

# game/hchar.rpy:10949
translate german PA_HC_EV33_a1081acb:

    # mc "¡Tú has sido la culpable!"
    mc "Du bist schuld daran!"

# game/hchar.rpy:10951
translate german PA_HC_EV33_f6bbe11b:

    # c "¡Hahaha!"
    c "Hahaha!"

# game/hchar.rpy:10953
translate german PA_HC_EV33_2a8e8be5:

    # c "Al menos tus chistes desatinados me ayudan a sentirme más tranquila."
    c "Zumindest helfen mir deine unangebrachten Witze dabei, mich ruhiger zu fühlen."

# game/hchar.rpy:10954
translate german PA_HC_EV33_c961da35:

    # c "No sabes lo nerviosa que sentía camino a la playa."
    c "Du weisst gar nicht, wie nervös ich auf dem Weg zum Strand war."

# game/hchar.rpy:10955
translate german PA_HC_EV33_9cd6b5cf:

    # c "Porque eso que acabas de decir era una broma."
    c "Denn was du gerade gesagt hast, war ein Scherz."

# game/hchar.rpy:10956
translate german PA_HC_EV33_d012109f:

    # c "¿Verdad?"
    c "Stimmt's?"

# game/hchar.rpy:10958
translate german PA_HC_EV33_1c4041f4:

    # mc "O quizás estaba pensando en voz alta."
    mc "Oder vielleicht habe ich nur laut gedacht."

# game/hchar.rpy:10960
translate german PA_HC_EV33_14d3cd6b:

    # c "Osea que de verdad te he provocado una..."
    c "Also habe ich dich wirklich provoziert und..."

# game/hchar.rpy:10962
translate german PA_HC_EV33_a12559a1:

    # mc "No me digas que te has antojado."
    mc "Sag mir nicht, dass du Lust darauf hast."

# game/hchar.rpy:10964
translate german PA_HC_EV33_6b8a2283:

    # c "¿Q-Qué?"
    c "W-Was?"

# game/hchar.rpy:10966
translate german PA_HC_EV33_3ab77406:

    # c "¡No digas estupideces [nombre]!"
    c "Red keinen Unsinn, [nombre]!"

# game/hchar.rpy:10967
translate german PA_HC_EV33_b5c5889a:

    # c "¡Y tampoco te atrevas a insinuar que soy una pervertida como tu!"
    c "Und wage es ja nicht, anzudeuten, dass ich genauso pervers bin wie du!"

# game/hchar.rpy:10969
translate german PA_HC_EV33_e03189c7:

    # mc "Aunque cambies de apariencia."
    mc "Auch wenn du dein Aussehen veränderst."

# game/hchar.rpy:10970
translate german PA_HC_EV33_78fe25aa:

    # mc "Nunca dejarás de ser una francesista calien..."
    mc "Du wirst nie aufhören, ein geile Fra..."

# game/hchar.rpy:10972
translate german PA_HC_EV33_a906677a:

    # c "¡No te atrevas a completar esa frase!"
    c "Wage es nicht, diesen Satz zu beenden!"

# game/hchar.rpy:10973
translate german PA_HC_EV33_0ad2e35e:

    # c "¡O te meteré un baguette completo por donde no te llega la luz!"
    c "Oder ich stecke dir ein ganzes Baguette dahin, wo du noch nie die Sonne gesehen hast!"

# game/hchar.rpy:10975
translate german PA_HC_EV33_7643e6f8:

    # mc "¡Está bien! ¡Me rindo!"
    mc "Okay! Ich gebe auf!"

# game/hchar.rpy:10977
translate german PA_HC_EV33_62423e3f:

    # c "¡Más te vale!"
    c "Das solltest du auch!"

# game/hchar.rpy:10979
translate german PA_HC_EV33_cc4ff71a:

    # mc "Supongo que esa agresividad es parte de la familia."
    mc "Ich nehme an, diese Aggressivität liegt in der Familie."

# game/hchar.rpy:10981
translate german PA_HC_EV33_6a6b5399:

    # c "Al menos yo tengo justificación porque siempre estás hablando tonterías."
    c "Zumindest habe ich eine Rechtfertigung, weil du immer Unsinn redest."

# game/hchar.rpy:10983
translate german PA_HC_EV33_41ca376e:

    # mc "Soy un poeta, una mente incomprendida."
    mc "Ich bin ein Poet, ein missverstandener Verstand."

# game/hchar.rpy:10985
translate german PA_HC_EV33_d6a93e92:

    # c "Lo que tú digas..."
    c "Was immer du sagst..."

# game/hchar.rpy:10987
translate german PA_HC_EV33_291143fb:

    # mc "Pero ahora hablando con toda sinceridad."
    mc "Aber jetzt ganz ehrlich gesagt."

# game/hchar.rpy:10988
translate german PA_HC_EV33_18211ba0:

    # mc "Te ves bastante bonita con esa apariencia."
    mc "Du siehst in diesem Look ziemlich hübsch aus."

# game/hchar.rpy:10990
translate german PA_HC_EV33_ee343dea:

    # c "M-merci, [nombre]."
    c "M-merci, [nombre]."

# game/hchar.rpy:10992
translate german PA_HC_EV33_eddbea29:

    # mc "Aunque parece que has olvidado tus lentes."
    mc "Allerdings scheinst du deine Brille vergessen zu haben."

# game/hchar.rpy:10994
translate german PA_HC_EV33_ed05b00e:

    # c "No se supone que utilice anteojos frente a Juliette."
    c "Ich soll vor Juliette keine Brille tragen."

# game/hchar.rpy:10996
translate german PA_HC_EV33_133876d6:

    # mc "Pero los necesitas para ver bien."
    mc "Aber du brauchst sie, um gut sehen zu können."

# game/hchar.rpy:10997
translate german PA_HC_EV33_6f182546:

    # mc "¿O solo son lentes estéticos?"
    mc "Oder sind es nur ästhetische Gläser?"

# game/hchar.rpy:10998
translate german PA_HC_EV33_8079ec6a:

    # mc "Ahora que lo pienso, es primera vez que me hago esa pregunta."
    mc "Wenn ich darüber nachdenke, ist es das erste Mal, dass ich mir diese Frage stelle."

# game/hchar.rpy:11000
translate german PA_HC_EV33_5a1f5b7d:

    # c "Lamentablemente no son solo estéticos."
    c "Leider sind sie nicht nur ästhetischer Natur."

# game/hchar.rpy:11001
translate german PA_HC_EV33_e29b001b:

    # c "Así que debo disimular para no entrecerrar los ojos intentando ver a lo lejos."
    c "Also muss ich mich verstellen, um nicht die Augen zusammenzukneifen, wenn ich versuche, in die Ferne zu sehen."

# game/hchar.rpy:11003
translate german PA_HC_EV33_35de2d3c:

    # mc "Así que tanto tú como Sophia necesitan lentes para ver bien."
    mc "Also brauchen sowohl du als auch Sophia eine Brille, um gut sehen zu können."

# game/hchar.rpy:11008
translate german PA_HC_EV33_8a2e9850:

    # so "Yo no los necesito."
    so "Ich brauche sie nicht."

# game/hchar.rpy:11010
translate german PA_HC_EV33_6af03351:

    # c "¿En qué momento has llegado Sophia?"
    c "Seit wann bist du hier Sophia?"

# game/hchar.rpy:11012
translate german PA_HC_EV33_50d5a6a8:

    # so "Solo hace unos segundos."
    so "Vor wenigen Sekunden angekommen."

# game/hchar.rpy:11016
translate german PA_HC_EV33_2fd36848:

    # so "Escuchaba su conversación de los anteojos."
    so "Ich hörte euer Gespräch über Brillen."

# game/hchar.rpy:11017
translate german PA_HC_EV33_8a13fc92:

    # so "Solo que en mi caso los utilizo por la estética."
    so "Nur dass ich sie in meinem Fall aus ästhetischen Gründen verwende."

# game/hchar.rpy:11019
translate german PA_HC_EV33_842fbef1:

    # mc "¿Y Juliette no se molesta contigo por eso, Sophia?"
    mc "Und Juliette ärgert sich darüber nicht, Sophia?"

# game/hchar.rpy:11021
translate german PA_HC_EV33_5bf8c055:

    # c "Bueno, es algo complicado de explicar y no creo que Sophia quiera tocar ese tema."
    c "Nun, das ist etwas kompliziert zu erklären, und ich glaube nicht, dass Sophia dieses Thema ansprechen möchte."

# game/hchar.rpy:11023
translate german PA_HC_EV33_d6bdbede:

    # so "Simplemente Madre no está tan interesada en mí, así que suele ser menos dura conmigo."
    so "Einfach gesagt, Mutter interessiert sich nicht so sehr für mich, deshalb ist sie mir gegenüber weniger streng."

# game/hchar.rpy:11024
translate german PA_HC_EV33_c80c27f8:

    # so "Y supongo que está bien..."
    so "Und ich denke, das ist in Ordnung..."

# game/hchar.rpy:11025
translate german PA_HC_EV33_acc4874a:

    # so "Soy la menor después de todo."
    so "Ich bin schliesslich die Jüngste."

# game/hchar.rpy:11026
translate german PA_HC_EV33_07455788:

    # so "Y Charlotte es mejor que yo en casi cualquier disciplina."
    so "Und Charlotte ist in fast jeder Disziplin besser als ich."

# game/hchar.rpy:11028
translate german PA_HC_EV33_ec122239:

    # c "No digas eso Sophia."
    c "Sag das nicht, Sophia."

# game/hchar.rpy:11030
translate german PA_HC_EV33_8c567689:

    # so "Pero es la verdad y estoy cansada de seguir fingiendo que no es así."
    so "Aber es ist die Wahrheit, und ich bin es leid, weiterhin so zu tun, als wäre es nicht so."

# game/hchar.rpy:11031
translate german PA_HC_EV33_2421a31c:

    # so "Comencé a utilizar mis anteojos para parecerme un poco más a ti."
    so "Ich habe angefangen, meine Brille zu tragen, um dir ein bisschen ähnlicher zu sehen."

# game/hchar.rpy:11033
translate german PA_HC_EV33_7209522d:

    # so "Incluso modifiqué mi peinado y mi forma de vestir..."
    so "Ich habe sogar meine Frisur und meinen Kleidungsstil geändert..."

# game/hchar.rpy:11034
translate german PA_HC_EV33_76f0756e:

    # so "Eso que dije sobre que Madre me había obligado a usarlos, era mentira."
    so "Was ich darüber gesagt habe, dass Mutter mich gezwungen hat, sie zu benutzen, war eine Lüge."

# game/hchar.rpy:11035
translate german PA_HC_EV33_b239c530:

    # so "Mientras Madre se enfurecía contigo por utilizar anteojos."
    so "Während Mutter sich darüber ärgerte, dass du eine Brille trägst."

# game/hchar.rpy:11036
translate german PA_HC_EV33_3ca66913:

    # so "Pasaba completamente de mí..."
    so "Hat sie mich völlig ignoriert..."

# game/hchar.rpy:11037
translate german PA_HC_EV33_3f33b835:

    # so "Ni siquiera se molestaría conmigo si yo me vistiese con harapos."
    so "Sie würde sich nicht einmal über mich ärgern, wenn ich Lumpen tragen würde."

# game/hchar.rpy:11039
translate german PA_HC_EV33_4592bea7:

    # c "Sophia..."
    c "Sophia..."

# game/hchar.rpy:11043
translate german PA_HC_EV33_2c602220:

    # so "Si ósea, es superduro decirlo de forma tan visceral y categórica."
    so "Ja, es ist wirklich schwer, das so emotional und kategorisch zu sagen."

# game/hchar.rpy:11044
translate german PA_HC_EV33_74ad1c6a:

    # so "Y me duele un montón saber que nunca seré lo suficientemente buena."
    so "Und es schmerzt mich sehr zu wissen, dass ich niemals gut genug sein werde."

# game/hchar.rpy:11046
translate german PA_HC_EV33_9ac428e4:

    # so "Soy la inútil de la familia después de todo."
    so "Ich bin doch die Niete in der Familie."

# game/hchar.rpy:11047
translate german PA_HC_EV33_f34b608b:

    # so "Si Madre vino hasta aquí es porque no confiaba en que yo podría llevarte de vuelta."
    so "Wenn Mutter hierher gekommen ist, dann deshalb, weil sie nicht darauf vertraut hat, dass ich dich zurückbringen könnte."

# game/hchar.rpy:11048
translate german PA_HC_EV33_f2a2c37d:

    # so "¡Y tenía razón!"
    so "Und sie hatte Recht!"

# game/hchar.rpy:11050
translate german PA_HC_EV33_62ecf5f1:

    # so "..."
    so "..."

# game/hchar.rpy:11052
translate german PA_HC_EV33_9492ccbd:

    # so "Yo te quiero mucho Charlotte."
    so "Ich liebe dich sehr, Charlotte."

# game/hchar.rpy:11053
translate german PA_HC_EV33_794f8288:

    # so "Como hermana y también como mejor amiga."
    so "Als Schwester und auch als beste Freundin."

# game/hchar.rpy:11054
translate german PA_HC_EV33_12abefbb:

    # so "Pero también..."
    so "Aber auch..."

# game/hchar.rpy:11056
translate german PA_HC_EV33_ae5c1be1:

    # so "Te odio."
    so "Ich hasse dich."

# game/hchar.rpy:11057
translate german PA_HC_EV33_8877a2ff:

    # so "Porque me haces sentir como la mugre que pisan tus tacones."
    so "Weil du mich fühlen lässt wie den Dreck, den deine Absätze zertreten."

# game/hchar.rpy:11059
translate german PA_HC_EV33_9f451898:

    # c "No sabía que te sentías así."
    c "Ich wusste nicht, dass du so empfindest."

# game/hchar.rpy:11061
translate german PA_HC_EV33_030d1669:

    # so "También me odio a mí misma por ser como soy."
    so "Ich hasse mich auch dafür, dass ich so bin, wie ich bin."

# game/hchar.rpy:11062
translate german PA_HC_EV33_be824ba5:

    # so "Pero no hay nada que pueda hacer..."
    so "Aber ich kann nichts tun..."

# game/hchar.rpy:11063
translate german PA_HC_EV33_dd763390:

    # so "Para que exista la mejor Charpentier también debe existir la peor."
    so "Damit es die beste Charpentier gibt, muss es auch die schlechteste geben."

# game/hchar.rpy:11065
translate german PA_HC_EV33_d1c51f31:

    # so "Y aun así..."
    so "Und dennoch..."

# game/hchar.rpy:11066
translate german PA_HC_EV33_5eef6236:

    # so "Al menos tengo mi rol más claro que tú, hermana."
    so "Zumindest ist mir meine Rolle klarer als dir, Schwester."

# game/hchar.rpy:11068
translate german PA_HC_EV33_17c73b7f:

    # c "¿Por qué me dices todo esto ahora?"
    c "Warum erzählst du mir das alles jetzt?"

# game/hchar.rpy:11070
translate german PA_HC_EV33_14ed35bd:

    # so "Creo que fue mi reacción al verte con esa apariencia."
    so "Ich glaube, es war meine Reaktion, als ich dich so sah."

# game/hchar.rpy:11071
translate german PA_HC_EV33_97e0871b:

    # so "Es como si la Charlotte del pasado estuviese aquí."
    so "Es ist, als wäre die Charlotte von früher hier."

# game/hchar.rpy:11073
translate german PA_HC_EV33_95311178:

    # c "Pensaba que el tiempo que pasamos juntas aquí, nos había ayudado a entendernos mejor."
    c "Ich dachte, die Zeit, die wir hier zusammen verbracht haben, hätte uns geholfen, einander besser zu verstehen."

# game/hchar.rpy:11075
translate german PA_HC_EV33_ce91c216:

    # so "Lo sé..."
    so "Ich weiss..."

# game/hchar.rpy:11076
translate german PA_HC_EV33_80da06e4:

    # so "Pero no puedo evitar sentir ese resentimiento cuando te veo."
    so "Aber ich kann diesen Groll nicht unterdrücken, wenn ich dich sehe."

# game/hchar.rpy:11077
translate german PA_HC_EV33_aa550917:

    # so "Creo que quiero más a esa Charlotte cocinera que a la de ahora."
    so "Ich glaube, ich mag die Charlotte, die Köchin, mehr als die Charlotte von heute."

# game/hchar.rpy:11078
translate german PA_HC_EV33_8693ca94:

    # so "Ósea, es superduro, de verdad."
    so "Ich meine, es ist wirklich superhart."

# game/hchar.rpy:11080
translate german PA_HC_EV33_31a3e4ec:

    # so "Y sé que soy yo la que está mal..."
    so "Und ich weiss, dass ich diejenige bin, die im Unrecht ist..."

# game/hchar.rpy:11081
translate german PA_HC_EV33_846f2937:

    # so "Pero no puedo evitar sentir este sentimiento de inferioridad cuando estoy frente a ti."
    so "Aber ich kann dieses Gefühl der Minderwertigkeit nicht vermeiden, wenn ich vor dir stehe."

# game/hchar.rpy:11082
translate german PA_HC_EV33_7bd61ea4:

    # so "O bueno..."
    so "Oder gut..."

# game/hchar.rpy:11083
translate german PA_HC_EV33_ce24f519:

    # so "Frente a la Charlotte que recordaba."
    so "Gegenüber der Charlotte, an die ich mich erinnerte."

# game/hchar.rpy:11085
translate german PA_HC_EV33_d19f855d:

    # c "..."
    c "..."

# game/hchar.rpy:11087
translate german PA_HC_EV33_7582e83c:

    # mc "¿Solo viniste aquí a decirle todo eso a Charlotte?"
    mc "Bist du nur hierher gekommen, um Charlotte das alles zu sagen?"

# game/hchar.rpy:11089
translate german PA_HC_EV33_81159b23:

    # so "Non..."
    so "Non..."

# game/hchar.rpy:11090
translate german PA_HC_EV33_73e366f4:

    # so "Venía para algo completamente distinto."
    so "Ich kam wegen etwas ganz anderem hierher."

# game/hchar.rpy:11092
translate german PA_HC_EV33_5ec52dd9:

    # c "¿Para qué?"
    c "Wozu?"

# game/hchar.rpy:11094
translate german PA_HC_EV33_f32847fe:

    # so "Quería decirte que a pesar de todo..."
    so "Ich wollte dir sagen, dass trotz allem..."

# game/hchar.rpy:11095
translate german PA_HC_EV33_f9fbd763:

    # so "Pienso darte todo mi apoyo."
    so "Werde ich dich voll und ganz unterstützen."

# game/hchar.rpy:11097
translate german PA_HC_EV33_06d56ae3:

    # c "¿Qué dices?"
    c "Warte... Was?"

# game/hchar.rpy:11099
translate german PA_HC_EV33_2449b328:

    # so "Cuando Madre vuelva, estaré a tu lado apoyándote."
    so "Wenn Mutter zurückkommt, werde ich dir zur Seite stehen und dich unterstützen."

# game/hchar.rpy:11101
translate german PA_HC_EV33_efb9dff5:

    # c "Pensaba que tenías miedo de Juliette."
    c "Ich dachte, du hättest Angst vor Juliette."

# game/hchar.rpy:11103
translate german PA_HC_EV33_e3df45d6:

    # so "O sea si, Madre es la persona que más terror puede llegar a causarme."
    so "Das heisst, ja, meine Mutter ist die Person, die mir am meisten Angst einflössen kann."

# game/hchar.rpy:11105
translate german PA_HC_EV33_c19e8679:

    # so "Pero aun así, siento que lo correcto es estar allí para ti."
    so "Aber trotzdem finde ich, dass es richtig ist, für dich da zu sein."

# game/hchar.rpy:11107
translate german PA_HC_EV33_a7665ba1:

    # c "No es necesario Sophia..."
    c "Das ist nicht nötig, Sophia..."

# game/hchar.rpy:11108
translate german PA_HC_EV33_2cfa882a:

    # c "Sobre todo después de todo lo que me acabas de decir."
    c "Vor allem nach allem, was du mir gerade erzählt hast."

# game/hchar.rpy:11110
translate german PA_HC_EV33_0a7a4570:

    # so "Lo que acabo de decir no tiene importancia."
    so "Was ich gerade gesagt habe, ist unwichtig."

# game/hchar.rpy:11111
translate german PA_HC_EV33_00c00e6c:

    # so "Solo quería sacarme del pecho toda esa podredumbre."
    so "Ich wollte nur all diesen faulen Ballast loswerden."

# game/hchar.rpy:11113
translate german PA_HC_EV33_026bcf9e:

    # so "Si no lo hacía ahora, entonces nunca..."
    so "Wenn ich es jetzt nicht tat, dann würde ich es nie tun..."

# game/hchar.rpy:11114
translate german PA_HC_EV33_c1b1123a:

    # so "Creo que podré recuperar tu confianza mostrando la parte más oscura de mí misma."
    so "Ich glaube, ich kann dein Vertrauen zurückgewinnen, indem ich dir meine dunkelste Seite zeige."

# game/hchar.rpy:11115
translate german PA_HC_EV33_f37ed510:

    # so "Y quiero que sepas que a pesar de esos sentimientos negativos hacia tu persona..."
    so "Und ich möchte, dass du weist, dass trotz dieser negativen Gefühle dir gegenüber..."

# game/hchar.rpy:11117
translate german PA_HC_EV33_4eadc4f6:

    # so "No voy a hacer algo que te haga daño jamás."
    so "Werde ich niemals etwas tun, was dir wehtut."

# game/hchar.rpy:11118
translate german PA_HC_EV33_cc240592:

    # so "Y siempre cuidaré de ti."
    so "Und ich werde immer für dich sorgen."

# game/hchar.rpy:11120
translate german PA_HC_EV33_6ff3f86a:

    # c "¿Dices esto porque dije que eras una traidora?"
    c "Sagst du das, weil ich gesagt habe, dass du eine Verräterin bist?"

# game/hchar.rpy:11122
translate german PA_HC_EV33_947cf262:

    # so "Oui..."
    so "Oui..."

# game/hchar.rpy:11124
translate german PA_HC_EV33_62ecf5f1_1:

    # so "..."
    so "..."

# game/hchar.rpy:11126
translate german PA_HC_EV33_5c2dfd75:

    # so "Yo no soy una traidora, Charlotte."
    so "Ich bin keine Verräterin, Charlotte."

# game/hchar.rpy:11127
translate german PA_HC_EV33_7e20cc7c:

    # so "Pero tampoco te voy a mentir sobre mis verdaderas intenciones."
    so "Aber ich werde dich auch nicht über meine wahren Absichten belügen."

# game/hchar.rpy:11129
translate german PA_HC_EV33_9b931ceb:

    # c "¿De qué estás hablando?"
    c "Wovon redest du?"

# game/hchar.rpy:11131
translate german PA_HC_EV33_5fb1fda9:

    # so "Tenía mis razones para llevarte a casa en un comienzo."
    so "Ich hatte meine Gründe, dich anfangs mit nach Hause zu nehmen."

# game/hchar.rpy:11132
translate german PA_HC_EV33_4d03f9c4:

    # so "Quería darle a entender a Madre que yo también puedo hacer las cosas bien."
    so "Ich wollte Mutter klar machen, dass auch ich Dinge richtig machen kann."

# game/hchar.rpy:11133
translate german PA_HC_EV33_0f96e14d:

    # so "Que también podía influenciar a mi hermana mayor."
    so "Dass ich auch meine ältere Schwester beeinflussen konnte."

# game/hchar.rpy:11134
translate german PA_HC_EV33_90f8c1b5:

    # so "Pero ella pasó de mí como siempre."
    so "Aber sie ignorierte mich wie immer."

# game/hchar.rpy:11135
translate german PA_HC_EV33_15655646:

    # so "Prefirió venir ella misma para hacerte regresar."
    so "Sie wollte lieber selbst kommen, um dich zurückzuholen."

# game/hchar.rpy:11136
translate german PA_HC_EV33_b5f843e8:

    # so "No tuvo duda de que yo no haría las cosas bien."
    so "Sie wusste, dass ich die Dinge nicht richtig machen würde."

# game/hchar.rpy:11138
translate german PA_HC_EV33_7614050e:

    # c "¿Entonces para qué quieres ahora ir en contra de su voluntad?"
    c "Warum willst du dann jetzt gegen ihren Willen handeln?"

# game/hchar.rpy:11140
translate german PA_HC_EV33_7529b707:

    # so "Porque pienso que tú eres la que me necesita."
    so "Weil ich denke, dass du diejenige bist, die mich braucht."

# game/hchar.rpy:11142
translate german PA_HC_EV33_1dec9cd4:

    # c "¿Incluso aunque eso signifique perder la herencia?"
    c "Auch wenn das bedeutet, das Erbe zu verlieren?"

# game/hchar.rpy:11144
translate german PA_HC_EV33_62ecf5f1_2:

    # so "..."
    so "..."

# game/hchar.rpy:11146
translate german PA_HC_EV33_7ad26fe0:

    # so "O-oui..."
    so "O-oui..."

# game/hchar.rpy:11148
translate german PA_HC_EV33_4592bea7_1:

    # c "Sophia..."
    c "Sophia..."

# game/hchar.rpy:11149
translate german PA_HC_EV33_82350cee:

    # c "Perdóname por haber dudado de ti, hermanita."
    c "Verzeih mir, dass ich an dir gezweifelt habe, kleine Schwester."

# game/hchar.rpy:11150
translate german PA_HC_EV33_f287279d:

    # c "Eres la mejor hermana que alguien como yo podría tener."
    c "Du bist die beste Schwester, die jemand wie ich haben könnte."

# game/hchar.rpy:11151
translate german PA_HC_EV33_57e28491:

    # c "No merezco todo esto después de como te he tratado."
    c "Ich verdiene das alles nicht, nachdem ich dich so behandelt habe."

# game/hchar.rpy:11153
translate german PA_HC_EV33_7d7d7cbe:

    # so "Juntas vamos a salir de esto."
    so "Gemeinsam werden wir das durchstehen."

# game/hchar.rpy:11154
translate german PA_HC_EV33_9e5e79ae:

    # so "Después ya encontrarás una forma de compensarme."
    so "Danach wirst du schon einen Weg finden, mich zu entschädigen."

# game/hchar.rpy:11156
translate german PA_HC_EV33_2a552c89:

    # c "¡Prometo que todo valdrá la pena!"
    c "Ich verspreche dir, dass sich alles lohnen wird!"

# game/hchar.rpy:11158
translate german PA_HC_EV33_8b70c82a:

    # mc "¿Entonces es momento de ir a la casa celeste por Juliette?"
    mc "Ist es also Zeit, zum Strandhaus zu Juliette zu gehen?"

# game/hchar.rpy:11160
translate german PA_HC_EV33_a7ab8a0a:

    # so "Madre no está en la casa celeste en este momento."
    so "Mutter ist gerade nicht im Strandhaus."

# game/hchar.rpy:11161
translate german PA_HC_EV33_aba646e2:

    # so "He estado vigilando desde primera hora y no he visto movimiento."
    so "Ich habe seit früh morgens Ausschau gehalten und keine Bewegung gesehen."

# game/hchar.rpy:11163
translate german PA_HC_EV33_f81d67f0:

    # so "Probablemente, haya salido supertemprano y regrese más tarde."
    so "Wahrscheinlich ist sie sehr früh losgegangen und kommt später zurück."

# game/hchar.rpy:11164
translate german PA_HC_EV33_981a53d4:

    # so "Tampoco la he visto acercarse al [restaurant]."
    so "Ich habe auch noch nicht gesehen, dass sie sich [restaurant] nähert."

# game/hchar.rpy:11166
translate german PA_HC_EV33_78f888be:

    # mc "Veo que estabas muy preparada."
    mc "Ich sehe, dass du sehr gut vorbereitet warst."

# game/hchar.rpy:11168
translate german PA_HC_EV33_a4760d5b:

    # so "Ósea, estoy haciendo mi mejor esfuerzo."
    so "Ich gebe also mein Bestes."

# game/hchar.rpy:11170
translate german PA_HC_EV33_79912f5b:

    # c "Lamento interrumpir pero..."
    c "Entschuldigt bitte die Unterbrechung, aber..."

# game/hchar.rpy:11171
translate german PA_HC_EV33_8c99c7ca:

    # c "Me gustaría volver a mi habitación para quitarme todo este maquillaje."
    c "Ich möchte zurück in mein Zimmer, um mein Make-up abzunehmen."

# game/hchar.rpy:11173
translate german PA_HC_EV33_f4b46f55:

    # mc "Bien, vamos los tres juntos y preparémonos para regresar más tarde aquí."
    mc "Gut, dann gehen wir drei zusammen und bereiten uns darauf vor, später hierher zurückzukehren."

# game/hchar.rpy:11174
translate german PA_HC_EV33_9658bd57:

    # mc "Seguramente tu madre vuelva a la casa celeste al anochecer."
    mc "Sicherlich kehrt deine Mutter bei Einbruch der Dunkelheit in das Strandhaus zurück."

# game/hchar.rpy:11179
translate german PA_HC_EV33_dc77b745:

    # "<Caminas junto a Charlotte y Sophia camino al [restaurant] y cuando abren la puerta principal...>"
    "<Du gehst zusammen mit Charlotte und Sophia zum [restaurant] und als ihr die Eingangstür öffnet...>"

# game/hchar.rpy:11180
translate german PA_HC_EV33_e9b58ce0:

    # "<Ocurre algo totalmente inesperado.>"
    "<Geschieht etwas völlig unerwartetes.>"

# game/hchar.rpy:11194
translate german PA_HC_EV33_ebd2a46e:

    # so "¡Te dije que Madre se volvería completamente loca!"
    so "Ich habe dir gesagt, dass Mutter völlig durchdrehen würde!"

# game/hchar.rpy:11195
translate german PA_HC_EV33_8d4f7c8b:

    # so "¡Se los dije! ¡Se los dije hasta el cansancio!"
    so "Ich habe es Ihnen gesagt! Ich habe es Ihnen bis zum Überdruss gesagt!"

# game/hchar.rpy:11197
translate german PA_HC_EV33_c5513ab0:

    # mc "No entiendo como se pudo ir todo a la mierda tan rápido."
    mc "Ich verstehe nicht, wie alles so schnell den Bach runtergehen konnte."

# game/hchar.rpy:11199
translate german PA_HC_EV33_20a181a0:

    # jul "¡Deja de correr Charlotte!"
    jul "Hör auf zu rennen, Charlotte!"

# game/hchar.rpy:11200
translate german PA_HC_EV33_e7501c78:

    # c "¡No pienso acercarme a ti!"
    c "Ich werde mich dir nicht nähern!"

# game/hchar.rpy:11201
translate german PA_HC_EV33_0a75d71a:

    # c "¡Estás loca Juliette!"
    c "Du bist verrückt, Juliette!"

# game/hchar.rpy:11206
translate german PA_HC_EV33_d871b29f:

    # jul "¡Deja de faltarme el respeto llamándome por mi nombre!"
    jul "Hör auf, mich zu beleidigen, indem du mich bei meinem Namen nennst!"

# game/hchar.rpy:11207
translate german PA_HC_EV33_32888aa8:

    # jul "¡Yo soy tu madre!"
    jul "Ich bin deine Mutter!"

# game/hchar.rpy:11212
translate german PA_HC_EV33_9139197a:

    # jul "¡¡¡MADREEEEEEEEEE!!!"
    jul "MUTTEEEEER!!!"

# game/hchar.rpy:11214
translate german PA_HC_EV33_1df2eedf:

    # so "No puedo creer que Madre nos esperaba aquí mientras estábamos en la playa."
    so "Ich kann nicht glauben, dass Mutter hier auf uns gewartet hat, während wir am Strand waren."

# game/hchar.rpy:11215
translate german PA_HC_EV33_a5846aa6:

    # so "Ni siquiera necesito ver para saber el desastre que están haciendo."
    so "Ich muss nicht einmal hinsehen, um zu wissen, was für ein Chaos sie anrichten wird."

# game/hchar.rpy:11217
translate german PA_HC_EV33_95c72f0c:

    # mc "A este paso van a romper todo."
    mc "Bei diesem Tempo wird sie alles kaputt machen."

# game/hchar.rpy:11219
translate german PA_HC_EV33_c534d9a0:

    # so "Ósea, perder cosas materiales es recuperable, pero aquí..."
    so "Das heisst, materielle Verluste sind wieder gutzumachen, aber hier..."

# game/hchar.rpy:11220
translate german PA_HC_EV33_66e1d937:

    # so "Siento que estamos en una situación de vida o muerte."
    so "Ich habe das Gefühl, dass wir uns in einer lebensbedrohlichen Situation befinden."

# game/hchar.rpy:11225
translate german PA_HC_EV33_6ca9ceb2:

    # c "¡¡¡Deberías estar internada en un manicomio, loca de mierda!!!"
