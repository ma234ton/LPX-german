    so "Oh non... Non non non..."

# game/hchar.rpy:9550
translate german PA_HC_EV32_bf37245d:

    # so "Porque me tiene que estar sucediendo esto a mi."
    so "Warum muss mir das gerade passieren?"

# game/hchar.rpy:9552
translate german PA_HC_EV32_1e8cfd9a:

