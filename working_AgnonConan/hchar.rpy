    jul "Okay, wie du willst."

# game/hchar.rpy:11305
translate german PA_HC_EV33_dc35008c:

    # jul "Solo si prometes que volveremos a casa a primera hora."
    jul "Nur wenn du versprichst, dass wir gleich morgen früh nach Hause fahren."

