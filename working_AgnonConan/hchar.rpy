    mc "Ich sage dir Bescheid, wenn es sicher ist, hinauszugehen."

# game/hchar.rpy:9625
translate german PA_HC_EV32_ae73c103:

    # so "¡Merci [nombre]!"
    so "Merci [nombre]!"

# game/hchar.rpy:9630
