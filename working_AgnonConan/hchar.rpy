    lissette "Das Leben ist zerbrechlicher, als du denkst."

# game/hchar.rpy:12918
translate german PA_HC_EV35_00fb4568_2:

    # ca "..."
    ca "..."

# game/hchar.rpy:12920
translate german PA_HC_EV35_32411b73:

    # lissette "Charlotte..."
    lissette "Charlotte..."

# game/hchar.rpy:12922
translate german PA_HC_EV35_9d40bcc0:

    # ca "¿Qué sucede mémé?"
    ca "Was ist los, mémé?"

