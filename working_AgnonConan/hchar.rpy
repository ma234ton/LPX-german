    # ca "Entonces..."
    ca "Also..."

# game/hchar.rpy:13054
translate german PA_HC_EV36_40ec7a9a:

    # ca "Solo espero no arrepentirme de esta decisión."
    ca "Ich hoffe nur, dass ich diese Entscheidung nicht bereuen werde."

# game/hchar.rpy:13056
