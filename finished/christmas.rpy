# game/christmas.rpy:28
translate german christmas_shogukuni_01_800badbe:

    # ka "¡Konnichiwa!"
    ka "Konnichiwa!"

# game/christmas.rpy:29
translate german christmas_shogukuni_01_abb5a6c4:

    # ka "¿Qué te trae de visita el día de hoy [nombre]?"
    ka "Wie kann ich dir heute behilflich sein [nombre]?"

# game/christmas.rpy:31
translate german christmas_shogukuni_01_d601f8e4:

    # ka "Espera... {w}No me digas que es porque sentiste el delicioso aroma del estofado Tami-Yami."
    ka "Warte... {w}Sag mir nicht, dass du dies vom Geruch erkennt hast, von der Tami-Yami Gerichtes."

# game/christmas.rpy:32
translate german christmas_shogukuni_01_93c40c90:

    # ka "Lamento decirte que has llegado demasiado tarde..."
    ka "Tut mir leid, ich habe vergessen dir zu sagen, dass du zu spät angekommen bist..."

# game/christmas.rpy:33
translate german christmas_shogukuni_01_de6e5dfa:

    # ka "Estaba tan bueno que me comí todo."
    ka "Es war gut alles aufzuessen."

# game/christmas.rpy:35
translate german christmas_shogukuni_01_05cc25f1:

    # ka "Incluyendo la porción de Shizuka, pero que quede entre nosotros."
    ka "Nur zwischen uns, Shizuka's Anteil habe ich auch gegessen."

# game/christmas.rpy:37
translate german christmas_shogukuni_01_3f231945:

    # ka "Y si no fuese porque Cassie se va de Argleton durante el invierno, podría haber comido más."
    ka "Und wenn Cassie nicht über den Winter von Argleton wegfahren würde, hätte ich vielleicht noch mehr gegessen."

# game/christmas.rpy:38
translate german christmas_shogukuni_01_ee207cd8:

    # ka "Ella odia el estofado Tami-Yami, así que me dejaría su porción."
    ka "Sie hasst Tami-Yami, deswegen hinterlässt Sie mir Ihre Portion."

# game/christmas.rpy:40
translate german christmas_shogukuni_01_5e7a1d8d:

    # mc "Ni siquiera sé que es el estofado Tami-Yami.."
    mc "Ich weiss nicht einmal was Tami-Yami ist..."

# game/christmas.rpy:42
translate german christmas_shogukuni_01_c55e7e28:

    # ka "Es un alivio que no lo conozcas."
    ka "Es beruhigt mich, dass du es nicht kennst."

# game/christmas.rpy:43
translate german christmas_shogukuni_01_e2603b06:

    # ka "De esa forma no tengo que sentirme culpable."
    ka "Dann muss ich mich nicht schlecht fühlen."

# game/christmas.rpy:45
translate german christmas_shogukuni_01_80b29d48:

    # shi "¿Sentirte culpable?"
    shi "Fühlst du dich schuldig?"

# game/christmas.rpy:46
translate german christmas_shogukuni_01_b302d395:

    # shi "¿Qué fue lo que hiciste ahora?"
    shi "Was hast du gerade gemacht?"

# game/christmas.rpy:48
translate german christmas_shogukuni_01_d139e6aa:

    # ka "¡Pero mira a quien tenemos aquí!"
    ka "Kuck mal wer hier ist!"

# game/christmas.rpy:49
translate german christmas_shogukuni_01_390ddae8:

    # ka "¡Al fin has vuelto de tu entrenamiento!"
    ka "Du bist endlich zurück von deinem Training!"

# game/christmas.rpy:51
translate german christmas_shogukuni_01_29c150b3:

    # shi "Tu también deberías estar entrenando, Kaede."
    shi "Du müsstest auch trainieren, Kaede."

# game/christmas.rpy:53
translate german christmas_shogukuni_01_da359b80:

    # ka "Habían unos problemas en el Shogukuni que tenía que solucionar."
    ka "Es gab ein paar Probleme in Shogukuni, dass ich erledigen musste."

# game/christmas.rpy:55
translate german christmas_shogukuni_01_2f56977e:

    # shi "Siempre las mismas excusas para evitar tus entrenamientos."
    shi "Immer die gleichen ausreden, damit du dein Training nicht absolvierst."

# game/christmas.rpy:56
translate german christmas_shogukuni_01_afd9a96f:

    # shi "Me pregunto cuando vas a madurar y dejaras de ser una mediocre."
    shi "Ich wundere mich, wann du mal erwachsen wirst und aufhörst mittelmässig zu sein."

# game/christmas.rpy:58
translate german christmas_shogukuni_01_965777e6:

    # shi "Y si no me equivoco, hoy es el día de estofado Tami-Yami."
    shi "Und wenn ich mich nicht Irre, ist heute Tami-Yami Tag."

# game/christmas.rpy:59
translate german christmas_shogukuni_01_62196125:

    # shi "Espero que al menos hayas servido la comida."
    shi "Ich hoffe, dass du mindestens das Essen serviert hast."

# game/christmas.rpy:61
translate german christmas_shogukuni_01_8b6dc4ed:

    # ka "Claro que lo hice, pero fue hace más de una hora."
    ka "Natürlich habe ich das, war aber vor einer Stunde her."

# game/christmas.rpy:63
translate german christmas_shogukuni_01_e9ac3406:

    # shi "¿Qué quieres decir con que ya has servido la comida?"
    shi "Wie du hast das Essen schon serviert?"

# game/christmas.rpy:64
translate german christmas_shogukuni_01_f747449e:

    # shi "Acabo de volver del comedor y no había nada."
    shi "Ich kam gerade zurück vom Esszimmer und da war nichts."

# game/christmas.rpy:66
translate german christmas_shogukuni_01_c7ec2c9a:

    # ka "Eso es porque después de comer tenía que dejar todo limpio."
    ka "Das kommt davon, weil ich nach dem Mittagessen alles sauber hinterlassen musste."

# game/christmas.rpy:67
translate german christmas_shogukuni_01_5f0b9d6b:

    # ka "Tú siempre dices que una persona disciplinada debe ocuparse de lo que ensucia."
    ka "Du sagst immer, das eine disziplinierte Person immer seinen eigenen Dreck aufräumen soll."

# game/christmas.rpy:69
translate german christmas_shogukuni_01_ec5593a0:

    # shi "Al menos debiste haberme esperado."
    shi "Du hättest mindestens auf mich warten können."

# game/christmas.rpy:70
translate german christmas_shogukuni_01_a0391f81:

    # shi "No sabes cuanta hambre siento después de todo ese entrenamiento."
    shi "Du weisst nicht wie hungrig ich bin, nach dem ganzen Training."

# game/christmas.rpy:71
translate german christmas_shogukuni_01_039319e2:

    # shi "Iré a buscar un poco estofado Tami-Yami para mí."
    shi "Ich hole mir etwas Tami-Yami für mich."

# game/christmas.rpy:73
translate german christmas_shogukuni_01_c114b2e6:

    # ka "Se ha acabado..."
    ka "Es ist vorbei..."

# game/christmas.rpy:78
translate german christmas_shogukuni_01_f755799c:

    # shi "..."
    shi "..."

# game/christmas.rpy:83
translate german christmas_shogukuni_01_de032ed5:

    # shi "¿Qué quieres decir con que se ha acabado?"
    shi "Was ist vorbei?"

# game/christmas.rpy:85
translate german christmas_shogukuni_01_3d1e9814:

    # ka "Ya no hay nada."
    ka "Es hat nichts mehr übrig."

# game/christmas.rpy:87
translate german christmas_shogukuni_01_22a47b0c:

    # shi "Kaede..."
    shi "Kaede..."

# game/christmas.rpy:89
translate german christmas_shogukuni_01_7a6fb152:

    # ka "¿Qué sucede Shizuka?"
    ka "Wie läufts Shizuka?"

# game/christmas.rpy:91
translate german christmas_shogukuni_01_2258d777:

    # shi "¿Sabías que el estofado Tami-Yami es mi comida favorita?"
    shi "Hast du gewusst, dass Tami-Yami mein Lieblingsessen ist?"

# game/christmas.rpy:93
translate german christmas_shogukuni_01_5ddb8fb8:

    # ka "¿De verdad?"
    ka "Wirklich?"

# game/christmas.rpy:94
translate german christmas_shogukuni_01_80b3a5c0:

    # ka "¡A mi también me encanta!"
    ka "Ich mag es auch!"

# game/christmas.rpy:96
translate german christmas_shogukuni_01_4f91f8f4:

    # shi "¿Sabías que solo sirven estofado Tami-Yami una vez al año por órdenes de la maestra?"
    shi "Hast du gewusst, dass Tami-Yami nur einmal im Jahr vom Meister bestellt wird?"

# game/christmas.rpy:97
translate german christmas_shogukuni_01_9e0d0839:

    # shi "Ya sabes... {w}Como premio por nuestro buen comportamiento..."
    shi "Weisst du... {w}Als Belohnung für dein gutes Verhalten..."

# game/christmas.rpy:99
translate german christmas_shogukuni_01_2665d13a:

    # ka "Sí, es injusto..."
    ka "Yoah, ein bisschen unfair..."

# game/christmas.rpy:100
translate german christmas_shogukuni_01_e472717d:

    # ka "Ahora debemos esperar todo un año para comer más."
    ka "Jetzt müssen wir ein ganzes Jahr warten bis wir mehr essen können."

# game/christmas.rpy:102
translate german christmas_shogukuni_01_79cd612e:

    # shi "Creo que ya conozco la respuesta, pero voy a preguntar de todas formas..."
    shi "Ich glaube, die Antwort darauf kenne ich schon, aber fragen möchte ich dich trotzdem..."

# game/christmas.rpy:103
translate german christmas_shogukuni_01_b83d5a18:

    # shi "¿Sabes por qué se ha acabado el estofado?"
    shi "Weisst du warum kein Essen mehr übrig ist?"

# game/christmas.rpy:105
translate german christmas_shogukuni_01_ab6b07f1:

    # ka "Bueno... {w}Sobre eso... {w}Tuve un día muy difícil y estaba hambrienta..."
    ka "Yoah... {w}Also, wegen dem... {wIch hätte alles essen können, weil ich einen harten Tag hatte..."

# game/christmas.rpy:106
translate german christmas_shogukuni_01_1967264a:

    # ka "Pude sentir el aroma desde el jardín y..."
    ka "Und als ich im Garten war, riechte es so gut..."

# game/christmas.rpy:108
translate german christmas_shogukuni_01_1576f351:

    # shi "Te voy a dar tres segundos para correr por tu vida."
    shi "Okay, hier ist der Plan. Ich gebe dir drei Sekunden wegzurennen für dein hungriges, dummes Leben."

# game/christmas.rpy:110
translate german christmas_shogukuni_01_30557e16:

    # ka "¿De verdad estás tan cabreada?"
    ka "Bist du wirklich so wütend?"

# game/christmas.rpy:112
translate german christmas_shogukuni_01_271983f4:

    # shi "Tres."
    shi "Drei."

# game/christmas.rpy:114
translate german christmas_shogukuni_01_0bab5481:

    # ka "Vamos Shizuka, no seas tan agresiva..."
    ka "Komm schon Shizuka, keine spässe mehr..."

# game/christmas.rpy:116
translate german christmas_shogukuni_01_ddb98f49:

    # shi "Dos..."
    shi "Zwei..."

# game/christmas.rpy:118
translate german christmas_shogukuni_01_ce72b014:

    # ka "¡Espera no cuentes tan rápido!"
    ka "Oh oh, du zählst mir zu schnell runter!"

# game/christmas.rpy:120
translate german christmas_shogukuni_01_c0ea0494:

    # shi "Uno..."
    shi "Eins..."

# game/christmas.rpy:122
translate german christmas_shogukuni_01_171a5634:

    # ka "¡¡¡Nigerundayou!!!"
    ka "Nigerundayou!!!"

# game/christmas.rpy:134
translate german christmas_shogukuni_01_6b827821:

    # ka "¡Ohwwwuchhh!"
    ka "Ohwwwuchhh!"

# game/christmas.rpy:135
translate german christmas_shogukuni_01_48059bc5:

    # ka "Me duele mucho el trasero."
    ka "Mein schöner Po..."

# game/christmas.rpy:136
translate german christmas_shogukuni_01_cd52d704:

    # ka "Esta vez te has pasado, Shizuka."
    ka "Dieses mal bist du zu weit gegangen, Shizuka."

# game/christmas.rpy:138
translate german christmas_shogukuni_01_5237d34c:

    # shi "¡Tú fuiste la que se pasó de la raya!"
    shi "Ich!? {w}Du bist diejenige, die alles aufgefressen hat!"

# game/christmas.rpy:139
translate german christmas_shogukuni_01_4306b4b5:

    # shi "¡Te lo mereces por comerte mi maldita comida!"
    shi "Das verdienst du zurecht, nachdem du mein Essen geklaut hast!"

# game/christmas.rpy:141
translate german christmas_shogukuni_01_91e15398:

    # ka "No puedo creer que me hayas golpeado solo por eso..."
    ka "Ich kann es nicht glauben, dass du mich nur für das gehauen hast..."

# game/christmas.rpy:143
translate german christmas_shogukuni_01_cc52b15d:

    # shi "¡Porque sabes bien que me gusta el estofado Tami-Yami y no dejaste nada!"
    shi "Du weisst ganz genau, dass ich Tami-Yami gerne habe und hast mir nichts übrig gelassen!"

# game/christmas.rpy:144
translate german christmas_shogukuni_01_3f1a56e9:

    # shi "¡Deberías agradecer de rodillas que aún sigues con vida!"
    shi "Du solltest dankbar auf deinen Knien sein, dass du noch am leben bist!"

# game/christmas.rpy:146
translate german christmas_shogukuni_01_0186b169:

    # ka "Si quieres puedo meterme los dedos en la garganta y devolver la comida."
    ka "Wenn du willst, kann ich meinen Finger in meinen Rachen reinstecken, damit das Essen wieder hochkommt."

# game/christmas.rpy:148
translate german christmas_shogukuni_01_f755799c_1:

    # shi "..."
    shi "..."

# game/christmas.rpy:150
translate german christmas_shogukuni_01_19293bbc:

    # ka "No te enojes, solo era una broma."
    ka "Wird jetzt nicht sauer, dass war nur ein Witz."

# game/christmas.rpy:152
translate german christmas_shogukuni_01_ecd2d2c4:

    # ka "Tampoco soy tan sucia... {w}Pero si lo quieres..."
    ka "So dreckig bin ich auch nicht... {w}Aber wenn du willst..."

# game/christmas.rpy:154
translate german christmas_shogukuni_01_a628f441:

    # shi "Eres una cerda..."
    shi "Du bist ein echtes Schwein..."

# game/christmas.rpy:156
translate german christmas_shogukuni_01_3bd6bc5a:

    # ka "¿Qué dijiste?"
    ka "Was hast du gesagt!?"

# game/christmas.rpy:158
translate german christmas_shogukuni_01_f642271c:

    # shi "¡Te voy a asesinar el trasero!"
    shi "Ich bring dich um!"

# game/christmas.rpy:160
translate german christmas_shogukuni_01_cbd8b0e8:

    # ka "¿Qué vas a asesinar mi que?"
    ka "Konnte dich nicht hören."

# game/christmas.rpy:162
translate german christmas_shogukuni_01_f330629d:

    # shi "¡¡¡TE VOY A ARRANCAR LA CABEZOTA!!!"
    shi "ICH REISSE DIR DEINEN KOPF AB!!!"

# game/christmas.rpy:164
translate german christmas_shogukuni_01_8a6891c9:

    # mc "Deberías calmarte un poco, Shizuka."
    mc "Beruhige dich, Shizuka."

# game/christmas.rpy:166
translate german christmas_shogukuni_01_e2fb12f9:

    # shi "¡No pienso calmarme hasta que vuelva a patearle el trasero a Kaede!"
    shi "Ich werde mich nicht beruhigen, bis ich Kaede's Arsch versohlt habe!"

# game/christmas.rpy:168
translate german christmas_shogukuni_01_9d2cc57e:

    # ka "¡No me mates!"
    ka "Bitte nicht!"

# game/christmas.rpy:169
translate german christmas_shogukuni_01_d5e01087:

    # ka "¡Me rindo!"
    ka "Ich gebe auf!"

# game/christmas.rpy:171
translate german christmas_shogukuni_01_24ebea06:

    # shi "Si no quieres morir..."
    shi "Wenn du nicht sterben möchtest..."

# game/christmas.rpy:172
translate german christmas_shogukuni_01_48d5a39d:

    # shi "Entonces debes prometerme que el próximo año me vas a dar tu ración de estofado Tami-Yami."
    shi "Dann musst du mir versprechen, dass du nächstes Jahr deine Portion Tami-Yami an mir gibst."

# game/christmas.rpy:174
translate german christmas_shogukuni_01_8fd1c558:

    # ka "¡Pero eso es injusto!"
    ka "Aber das ist unfair!"

# game/christmas.rpy:175
translate german christmas_shogukuni_01_861d27fc:

    # ka "¿No te basta con tu porción?"
    ka "Reicht dir deine Portion nicht?"

# game/christmas.rpy:177
translate german christmas_shogukuni_01_7246b151:

    # shi "¡Tú eres la que se comió las porciones de los demás!"
    shi "Du bist diejenige, die alle Portionen von allen aufgegessen hat!"

# game/christmas.rpy:178
translate german christmas_shogukuni_01_cb41b52a:

    # shi "¡Deja ya de ser tan hipócrita!"
    shi "Hör auf zu heucheln!"

# game/christmas.rpy:180
translate german christmas_shogukuni_01_0fbafa75:

    # ka "Pero..."
    ka "Aber..."

# game/christmas.rpy:181
translate german christmas_shogukuni_01_86152dc9:

    # ka "Está bien..."
    ka "Na schön..."

# game/christmas.rpy:183
translate german christmas_shogukuni_01_1c671956:

    # mc "Y se supone que la navidad es para compartir en familia."
    mc "Und Weihnachten ist dafür da, dass wir uns alles teilen als Familie."

# game/christmas.rpy:184
translate german christmas_shogukuni_01_deb867e0:

    # mc "Pero aquí estamos con las peleas de siempre."
    mc "Und jetzt sind wir hier, die gleichen Streitigkeiten."

# game/christmas.rpy:186
translate german christmas_shogukuni_01_719b877d:

    # ka "No digas eso [nombre]..."
    ka "Sag das nicht [nombre]..."

# game/christmas.rpy:187
translate german christmas_shogukuni_01_e8b577f9:

    # ka "Shizuka podría dejarte el culo colorado como lo hizo conmigo."
    ka "Shizuka färbt deinen Arsch rot, wie Sie es schon bei mir getan hat."

# game/christmas.rpy:188
translate german christmas_shogukuni_01_0eecd77b:

    # ka "Luego vamos a parecer un par de monos de trasero rojo como un mono."
    ka "Dann lass uns für ein paar rote Paviane Hintern suchen."

# game/christmas.rpy:190
translate german christmas_shogukuni_01_c36185f8:

    # ka "Aunque ahora que lo pienso, el dios Sun Wukong era un mono y era muy inteligente."
    ka "Gerade in den Sinn gekommen, der God Sun Wukong war ein Affe und dazu auch sehr intelligent."

# game/christmas.rpy:191
translate german christmas_shogukuni_01_817dedf1:

    # ka "Tampoco suena tan mal..."
    ka "Hört sich nicht so schlecht an..."

# game/christmas.rpy:192
translate german christmas_shogukuni_01_2bb94819:

    # ka "Aunque tendría que acostumbrarme a cambiar toda mi dieta y tragar muchas bananas."
    ka "Ich müsste aber mich daran gewöhnen, meine Diet auf dutzende von Bananen umzustellen."

# game/christmas.rpy:194
translate german christmas_shogukuni_01_bab5bd84:

    # shi "Si no te callas te meteré una banana por el trasero, Kaede."
    shi "Wenn du nicht dein Maul hältst, schiebe ich dir eine Banane in deinen Arsch, Kaede."

# game/christmas.rpy:196
translate german christmas_shogukuni_01_78cd059c:

    # ka "¿Qué tipo de banana?"
    ka "Welche Art von Banane?"

# game/christmas.rpy:198
translate german christmas_shogukuni_01_5c82e63f:

    # shi "Si sigues con el doble sentido te voy a obligar a hacer flexiones hasta que tus brazos exploten, Kaede."
    shi "Wenn du deine Zweideutigkeit nicht lässt, dann zwinge ich dich so viele Liegestütze zu machen, bis deine Arme explodieren, Kaede."

# game/christmas.rpy:200
translate german christmas_shogukuni_01_5c2d63bc:

    # ka "¡Sumimasen!"
    ka "Sumimasen!"

# game/christmas.rpy:202
translate german christmas_shogukuni_01_9ab863fa:

    # shi "No puedes imaginar la vergüenza ajenas que me haces sentir."
    shi "Du kannst dir garnicht vorstellen, wie beschämend das für mich ist."

# game/christmas.rpy:204
translate german christmas_shogukuni_01_9d91dd08:

    # ka "¡Ouch!"
    ka "Ouch!"

# game/christmas.rpy:205
translate german christmas_shogukuni_01_2ffd81e2:

    # ka "Aunque deberías escuchar a [nombre]."
    ka "Du solltest auf [nombre] hören."

# game/christmas.rpy:206
translate german christmas_shogukuni_01_f5b666b3:

    # ka "Somos familia y se acerca la navidad."
    ka "Wir sind eine Familie, Weihnachten ist bald da."

# game/christmas.rpy:208
translate german christmas_shogukuni_01_7097b0bc:

    # shi "Aquí no celebramos la navidad, Kaede."
    shi "Wir feiern hier kein Weihnachten, Kaede."

# game/christmas.rpy:210
translate german christmas_shogukuni_01_c03b24f7:

    # ka "Que amargada..."
    ka "So bitter..."

# game/christmas.rpy:212
translate german christmas_shogukuni_01_d729dc9f:

    # shi "¡Y tú eres una holgazán!"
    shi "Und du bist so Faul!"

# game/christmas.rpy:213
translate german christmas_shogukuni_01_41cebc88:

    # shi "¡Deberías estar entrenando!"
    shi "Du solltest trainieren!"

# game/christmas.rpy:215
translate german christmas_shogukuni_01_7af9a658:

    # mc "¿Qué quieres decir con que no celebran navidad?"
    mc "Wie wir feiern hier kein Weihnachten?"

# game/christmas.rpy:217
translate german christmas_shogukuni_01_c24f1eaa:

    # shi "Son las reglas..."
    shi "Regeln sind Regeln..."

# game/christmas.rpy:218
translate german christmas_shogukuni_01_8f014f7e:

    # shi "A veces olvido que eres el novato y no sabes nada de nada."
    shi "Manchmal vergesse ich, dass du eine Anfängerin bist und noch keine Ahnung hast."

# game/christmas.rpy:219
translate german christmas_shogukuni_01_6433b908:

    # shi "No están permitidas las celebraciones en el Shogukuni y menos que un Dragón participe de ellas."
    shi "Feiertage sind hier im Shogukuni nicht erlaubt, vor allem weniger für einen Drachen da mitzumachen."

# game/christmas.rpy:220
translate german christmas_shogukuni_01_ce1859ee:

    # shi "Al menos que se trate de una reunión de negocios con miembros de otra familia o quizás por órdenes del Viriato."
    shi "Ausser es ist ein geschäftliches Treffen mit den Mitglieder von einer anderen Familie, oder von den Befehlen der Viriato."

# game/christmas.rpy:221
translate german christmas_shogukuni_01_87179d90:

    # shi "Y depende de la maestra Felicia decir la última palabra."
    shi "Und Meisterin Felicia hat das letzte Wort in dem."

# game/christmas.rpy:223
translate german christmas_shogukuni_01_15d8229d:

    # mc "¿Qué hay de malo con celebrar?"
    mc "Was ist falsch dabei zu feiern?"

# game/christmas.rpy:225
translate german christmas_shogukuni_01_36df9da5:

    # shi "Son distracciones para nuestras rutinas y pueden afectar nuestro entrenamiento."
    shi "Sie sorgen für Ablenkung und zerstört damit die routine von unserem Training."

# game/christmas.rpy:227
translate german christmas_shogukuni_01_f4117d59:

    # mc "Eso suena aburrido."
    mc "Hört sich langweilig an."

# game/christmas.rpy:229
translate german christmas_shogukuni_01_35a9f9f8:

    # ka "¡Por fin alguien lo ha dicho!"
    ka "Endlich jemand, er hats gesagt!"

# game/christmas.rpy:231
translate german christmas_shogukuni_01_138e4690:

    # ka "¡Es aburrido que te cagas!"
    ka "Es ist langweilig wie Scheisse!"

# game/christmas.rpy:233
translate german christmas_shogukuni_01_2c8c0aea:

    # shi "¡Deberías cuidar tu vocabulario Kaede!"
    shi "Du solltest auf deine Wortwahl aufpassen Kaede!"

# game/christmas.rpy:235
translate german christmas_shogukuni_01_45bcc9f6:

    # ka "Sumimasen..."
    ka "Sumimasen..."

# game/christmas.rpy:237
translate german christmas_shogukuni_01_665d8ae7:

    # mc "¿Nunca han armado un árbol navideño?"
    mc "Hast du nie einen Weihnachtsbaum geschmückt?"

# game/christmas.rpy:239
translate german christmas_shogukuni_01_a1a957ce:

    # ka "¡Eso sería divertido!"
    ka "Das hört sich toll an!"

# game/christmas.rpy:240
translate german christmas_shogukuni_01_ec67100f:

    # ka "Podríamos intentarlo."
    ka "Wir können es versuchen."

# game/christmas.rpy:242
translate german christmas_shogukuni_01_3a0cb0d0:

    # shi "No quiero recibir una amonestación de parte de la maestra."
    shi "Ich möchte nicht wieder eine Zurechtweisung von unserem Meister bekommen."

# game/christmas.rpy:244
translate german christmas_shogukuni_01_1e10b463:

    # ka "Para que nos castiguen primero tendría que enterarse."
    ka "Um herauszufinden ob wir Ärger bekommen, müssen wir es ja zuerst probieren."

# game/christmas.rpy:246
translate german christmas_shogukuni_01_ead3187c:

    # shi "Tendríamos que guardar el secreto..."
    shi "Wir müssten dies geheimhalten..."

# game/christmas.rpy:247
translate german christmas_shogukuni_01_0ac4addc:

    # shi "¿Estás pensando en hacer nuestro propio árbol de navidad?"
    shi "Denkst du gerade, einen eigenen Weihnachtsbaum zu schmücken?"

# game/christmas.rpy:249
translate german christmas_shogukuni_01_b84166c6:

    # ka "Hacer un árbol de navidad..."
    ka "Einen Weihnachtsbaum schmücken..."

# game/christmas.rpy:250
translate german christmas_shogukuni_01_9322a42e:

    # ka "Eso suena a mucho trabajo y dedicación."
    ka "Das hört sich nach sehr viel Arbeit an."

# game/christmas.rpy:251
translate german christmas_shogukuni_01_3330f335:

    # ka "Pero sería divertido hacer algo especial para el día de navidad."
    ka "Komm schon, dass wird witzig, vor allem am Weihnachtstag."

# game/christmas.rpy:253
translate german christmas_shogukuni_01_1a054ad5:

    # ka "¿No piensas lo mismo [nombre]?"
    ka "Findest du auch, [nombre]?"

# game/christmas.rpy:255
translate german christmas_shogukuni_01_3bd2dc42:

    # mc "Puedes contar conmigo."
    mc "Du kannst auf mich zählen."

# game/christmas.rpy:257
translate german christmas_shogukuni_01_2390d71b:

    # ka "¡Yay!"
    ka "Yippie!"

# game/christmas.rpy:258
translate german christmas_shogukuni_01_332e1df4:

    # ka "¡Entonces hagamos algo especial!"
    ka "Dann lass und etwas speziells tun!"

# game/christmas.rpy:260
translate german christmas_shogukuni_01_7fa7a996:

    # shi "¿Qué tienes en mente?"
    shi "Was denkst du gerade?"

# game/christmas.rpy:262
translate german christmas_shogukuni_01_0f02bdfe:

    # ka "¿Eso significa que también vas a participar?"
    ka "Heisst das, dass du auch mitmachst?"

# game/christmas.rpy:264
translate german christmas_shogukuni_01_84c39ed2:

    # shi "Mientras no se trate de una de tus estupideces, si no estoy fuera."
    shi "Solange es nicht einer deiner bescheuerten Ideen ist, ansonsten bin ich da fein raus."

# game/christmas.rpy:266
translate german christmas_shogukuni_01_18bfbd82:

    # ka "Déjame pensar..."
    ka "Lass mich nachdenken..."

# game/christmas.rpy:267
translate german christmas_shogukuni_01_7d056a7e:

    # ka "Tiene que ser algo simple..."
    ka "Es muss etwas simples sein..."

# game/christmas.rpy:268
translate german christmas_shogukuni_01_95cda0ae:

    # ka "No quiero que la maestra se entere de que desobedecimos las reglas."
    ka "Ich möchte nicht, dass der Meister herausfindet, dass wir seine Regeln missachten."

# game/christmas.rpy:269
translate german christmas_shogukuni_01_b9dc4ef1:

    # ka "Pero no tengo idea de que podría ser..."
    ka "Aber ich habe keine Idee was es sein könnte..."

# game/christmas.rpy:271
translate german christmas_shogukuni_01_0e03055f:

    # mc "Yo puedo pensar en algo, pero deberíamos incluir a Yui en esto."
    mc "Ich hätte da etwas, aber wir sollten Yui auch mitmachen lassen."

# game/christmas.rpy:273
translate german christmas_shogukuni_01_4f5f7d77:

    # ka "¡Eso es muy inteligente [nombre]!"
    ka "Das ist eine super Idee [nombre]!"

# game/christmas.rpy:275
translate german christmas_shogukuni_01_aa2d7773:

    # shi "¿Qué cosa?"
    shi "Was?"

# game/christmas.rpy:277
translate german christmas_shogukuni_01_01f09ecf:

    # ka "¡Si el Dragón negro está involucrado entonces el castigo será menos severo para nosotras!"
    ka "Wenn der Schwarze Drache involviert ist, wird die Strafe mehr als nur hart sein für uns!"

# game/christmas.rpy:279
translate german christmas_shogukuni_01_a5766daf:

    # mc "No me refería a usar a Yui para disminuir el castigo."
    mc "Ich meinte nicht damit, das Yui unsere Strafe verringert."

# game/christmas.rpy:281
translate german christmas_shogukuni_01_637a9e77:

    # ka "Lo he dicho porque me gusta siempre ir un paso por delante."
    ka "Ich habe das nur gesagt, weil ich immer gerne einen Schritt voraus bin."

# game/christmas.rpy:283
translate german christmas_shogukuni_01_249823e3:

    # shi "Eres una idiota, Kaede."
    shi "Du bist so ein Idiot, Kaede."

# game/christmas.rpy:285
translate german christmas_shogukuni_01_73aa2115:

    # ka "¡Entonces es un trato!"
    ka "Dann ist das eine Abmachung!"

# game/christmas.rpy:286
translate german christmas_shogukuni_01_9460cc3c:

    # ka "Cuando tengas todo preparado nos reuniremos para ver los detalles."
    ka "Wenn du alles bereit hast, treffen wir uns um die Details zu besprechen."

# game/christmas.rpy:287
translate german christmas_shogukuni_01_ca09ec12:

    # ka "¡Se levanta la sesión!"
    ka "Diese Meeting ist somit beendet."

# game/christmas.rpy:289
translate german christmas_shogukuni_01_72b15180:

    # shi "Yo me largo..."
    shi "Ich gehe mal..."

# game/christmas.rpy:290
translate german christmas_shogukuni_01_7eed8b46:

    # shi "Iré a ver si esta glotona ha dejado un poco de estofado."
    shi "Ich geh mal nachschauen, ob dieser Vielfrass etwas zurückgelassen hat."

# game/christmas.rpy:296
translate german christmas_shogukuni_01_f8139c4c:

    # "<Después de despedirte de Shizuka y Kaede vuelves a tu habitación.>"
    "Nachdem du dich von Shizuka und Kaede verabschiedet hast, gehst du zurück zu deinem Zimmer."

# game/christmas.rpy:309
translate german christmas_shogukuni_02_fd287a15:

    # y "¡[nombre]!"
    y "[nombre]!"

# game/christmas.rpy:311
translate german christmas_shogukuni_02_6406bf11:

    # mc "Que bueno encontrarte aquí Yui."
    mc "Schön dich hier zu sehen, Yui."

# game/christmas.rpy:313
translate german christmas_shogukuni_02_557971a6:

    # y "¡Feliz!"
    y "Glücklich!"

# game/christmas.rpy:315
translate german christmas_shogukuni_02_c774d440:

    # mc "Yo también estoy feliz de verte."
    mc "Ich freue mich auch dich zu sehen."

# game/christmas.rpy:316
translate german christmas_shogukuni_02_0aac59d5:

    # mc "Acabo de recordar algo..."
    mc "Ich habe mich gerade an etwas erinnert..."

# game/christmas.rpy:317
translate german christmas_shogukuni_02_df9b2dc3:

    # mc "Kaede y Shizuka mencionaron que no es posible hacer celebraciones en el Shogukuni."
    mc "Kaede und Shizuka haben gemeint, dass es nicht möglich ist, Feste in Shogukuni zu halten."

# game/christmas.rpy:318
translate german christmas_shogukuni_02_52a83e97:

    # mc "¿Es verdad que nunca han celebrado una navidad?"
    mc "Ist es wahr, dass du noch nie Weihnachten gefeiert hast?"

# game/christmas.rpy:320
translate german christmas_shogukuni_02_99a90957:

    # y "Prohibido."
    y "Verboten."

# game/christmas.rpy:322
translate german christmas_shogukuni_02_0885a207:

    # mc "No puedo creer que esa tal Felicia sea tan severa..."
    mc "Ich kann es nicht glauben, das Felicia so streng ist..."

# game/christmas.rpy:323
translate german christmas_shogukuni_02_0b9fc7b3:

    # mc "Lo único que le falta es tener la piel verde para ser idéntico al Grinch."
    mc "Das einzige was ihr fehlt ist grüne Haut, dann würde sie wie der Grinch aussehen."

# game/christmas.rpy:325
translate german christmas_shogukuni_02_d57a7057:

    # y "¿Grinch?"
    y "Grinch?"

# game/christmas.rpy:327
translate german christmas_shogukuni_02_5f78674b:

    # mc "Es una especie de ermitaño que buscaba acabar con la navidad."
    mc "Es ist ein Schuft, der Weihnachten vernichten wollte."

# game/christmas.rpy:329
translate german christmas_shogukuni_02_094c36e0:

    # mc "¿De verdad no lo conoces?"
    mc "Du kennst Ihn wirklich nicht?"

# game/christmas.rpy:331
translate german christmas_shogukuni_02_785ee78f:

    # y "No."
    y "Nein."

# game/christmas.rpy:333
translate german christmas_shogukuni_02_36ebfdb0:

    # mc "Es alguien conocido por ser un amargado como Felicia."
    mc "Er ist jemand, der dafür bekannt ist, genauso launisch zu sein wie Felicia."

# game/christmas.rpy:334
translate german christmas_shogukuni_02_015c01bc:

    # mc "Supongo que aquí no tenías tiempo para ver películas o leer libros."
    mc "Ich schätze mal, dass du nicht Zeit hattest die Filme zu schauen, oder die Bücher zu lesen."

# game/christmas.rpy:335
translate german christmas_shogukuni_02_bededa6c:

    # mc "Estabas ocupada con el entrenamiento de Dragón Negro y respetando reglas sin sentido."
    mc "Du warst beschäftigt mit deinem Schwarzer Drache Training und die Regeln zu respektieren."

# game/christmas.rpy:337
translate german christmas_shogukuni_02_a7fc18fb:

    # y "Supongo..."
    y "Ich schätze..."

# game/christmas.rpy:339
translate german christmas_shogukuni_02_86b136a8:

    # mc "Cada año veíamos esa película en la academia y bueno..."
    mc "Jedes Jahr haben wir den Film in der Akademie angeschaut und naja..."

# game/christmas.rpy:340
translate german christmas_shogukuni_02_8fe52fec:

    # mc "No me quiero desviar del tema."
    mc "Ich möchte jetzt nicht abschweifen aber."

# game/christmas.rpy:341
translate german christmas_shogukuni_02_85abf2cf:

    # mc "La cosa es... {w}La cosa es que pienso que podríamos hacer algo aquí para celebrar la navidad."
    mc "Das Ding ist... {w}Ich denke wir könnten etwas hier tun, um Weihnachten zu feiern."

# game/christmas.rpy:343
translate german christmas_shogukuni_02_77934d6c:

    # y "Peligroso."
    y "Gefährlich."

# game/christmas.rpy:345
translate german christmas_shogukuni_02_6c0a3e22:

    # mc "No hay nada de que preocuparse."
    mc "Es gibt keine Grund zu Sorge."

# game/christmas.rpy:346
translate german christmas_shogukuni_02_f1c12332:

    # mc "Puede ser un pequeño detalle entre nosotros."
    mc "Es könnte ein kleines Geheimnis zwischen uns sein."

# game/christmas.rpy:347
translate german christmas_shogukuni_02_c604ed9b:

    # mc "Felicia no tiene que enterarse y Kaede también piensa que sería una buena idea."
    mc "Felicia muss davon nichts wissen. Kaede denkt auch, dass es eine gute Idee ist."

# game/christmas.rpy:349
translate german christmas_shogukuni_02_287425b0:

    # y "Está bien."
    y "Okay."

# game/christmas.rpy:351
translate german christmas_shogukuni_02_5dd73cea:

    # mc "Vaya, sí que fue fácil convencerte."
    mc "Wow, warst du einfach zu überzeugen."

# game/christmas.rpy:353
translate german christmas_shogukuni_02_67b7b463:

    # y "Confianza."
    y "Vertrauen."

# game/christmas.rpy:355
translate german christmas_shogukuni_02_90efba73:

    # mc "Creo que tengo una idea que encajaría a la perfección."
    mc "Ich denke, Ich habe eine perfekte Idee was passen würde."

# game/christmas.rpy:360
translate german christmas_shogukuni_02_9d91dd08:

    # ka "¡Ouch!"
    ka "Ouch!"

# game/christmas.rpy:365
translate german christmas_shogukuni_02_fa1e03d9:

    # mc "Pero que..."
    mc "Was zur..."

# game/christmas.rpy:366
translate german christmas_shogukuni_02_4afdd46c:

    # mc "¿Estás bien Kaede?"
    mc "Alles okay Kaede?"

# game/christmas.rpy:371
translate german christmas_shogukuni_02_07724059:

    # ka "¡Porque tiene que estar todo el suelo tan resbaladizo!"
    ka "Warum muss der Boden bloss so rutschig sein?"

# game/christmas.rpy:372
translate german christmas_shogukuni_02_45dcbfb1:

    # ka "Me duele la frente..."
    ka "Meine Stirn tut weh..."

# game/christmas.rpy:373
translate german christmas_shogukuni_02_bd3dc1f1:

    # ka "Y las tetas... {w}Y el trasero..."
    ka "Und meine Brüste... {w}Und mein Po..."

# game/christmas.rpy:375
translate german christmas_shogukuni_02_16ae1664:

    # mc "Pensaba que te habías golpeado la cara."
    mc "Ich dachte, du hast deine Gesicht verletzt."

# game/christmas.rpy:377
translate german christmas_shogukuni_02_64ae1fb1:

    # ka "La frente me duele por el golpe y lo otro..."
    ka "Meine Stirn tut weh, vom Schlag... Und das andere..."

# game/christmas.rpy:378
translate german christmas_shogukuni_02_45b3243c:

    # ka "¡Porque hace un frío que te cagas!"
    ka "Weil es verdammt nochmal Arschkalt ist!"

# game/christmas.rpy:379
translate german christmas_shogukuni_02_0291335b:

    # ka "Y se me está congelando el trasero y los pezones..."
    ka "Und meine Arsch und Nippel am frieren sind..."

# game/christmas.rpy:381
translate german christmas_shogukuni_02_eccfc7fd:

    # mc "¿No tienes algo abrigado para ponerte?"
    mc "Hast du nichts warmes zum anziehen?"

# game/christmas.rpy:382
translate german christmas_shogukuni_02_d95efe91:

    # mc "Yo también tendría las bolas congeladas si estuviese con esa ropa."
    mc "Ich hätte auch kalte Eier, wenn ich in deinen Kleider wäre."

# game/christmas.rpy:384
translate german christmas_shogukuni_02_5b8c28ee:

    # ka "Es por eso que vine a pedir tu ayuda [nombre]."
    ka "Deswegen kam ich zu dir, weil ich deine Hilfe brauche, [nombre]."

# game/christmas.rpy:385
translate german christmas_shogukuni_02_17493df5:

    # ka "Necesito que uses tus habilidades y técnicas secretas para patearle el trasero a Shizuka."
    ka "Ich brauche deine geheime Techniken, um Shizuka's Arsch zu versohlen."

# game/christmas.rpy:386
translate german christmas_shogukuni_02_00b7ea95:

    # ka "Aún está cabreada por lo del estofado y ha escondido mi ropa de invierno."
    ka "Sie ist immer noch wütend, wegen des Gerichts... Deswegen hat sie meine Winterklamotten versteckt."

# game/christmas.rpy:388
translate german christmas_shogukuni_02_23881037:

    # mc "Aunque fuese así de fuerte, no pienso pelear con Shizuka solo por eso."
    mc "Selbst wenn ich so stark wäre, plane ich nicht mit Shizuka zu kämpfen wegen so etwas."

# game/christmas.rpy:389
translate german christmas_shogukuni_02_2cb5b0ce:

    # mc "Seguramente podemos arreglar las cosas conversando."
    mc "Bestimmt können wir, in einer Konversation dies lösen."

# game/christmas.rpy:391
translate german christmas_shogukuni_02_872a2eca:

    # ka "¿Lo dices en serio [nombre]?"
    ka "Meinst du dies ernst [nombre]?"

# game/christmas.rpy:392
translate german christmas_shogukuni_02_2e8d41a2:

    # ka "Primero, para un hombre es imposible convencer a una mujer de hacer algo que no quiere."
    ka "Zuerst, ist es nicht möglich für einen Mann, eine Frau zu überzeugen etwas zu tun, wenn sie es nicht will."

# game/christmas.rpy:393
translate german christmas_shogukuni_02_dc6b4826:

    # ka "Y segundo..."
    ka "Zweitens..."

# game/christmas.rpy:395
translate german christmas_shogukuni_02_2fea160a:

    # ka "¡Sabes que no se puede hablar con Shizuka!"
    ka "Du weisst, du kannst nicht mit Shizuka reden!"

# game/christmas.rpy:396
translate german christmas_shogukuni_02_d6a8d996:

    # ka "Al menos que tus argumentos sean el asesinato y la tortura."
    ka "Ausser deine Argumente beinhalten Morde und Folter."

# game/christmas.rpy:398
translate german christmas_shogukuni_02_a0c89b91:

    # mc "Ella no es tan mala como aparenta."
    mc "Sie ist nicht so schlecht, wie sie aussieht."

# game/christmas.rpy:400
translate german christmas_shogukuni_02_17818de5:

    # ka "¿Entonces porque se llevaría mi ropa para el invierno?"
    ka "Warum hat Sie dann meine Winterklamotten genommen und versteckt?"

# game/christmas.rpy:402
translate german christmas_shogukuni_02_75641ec3:

    # mc "Porque te lo tenías merecido."
    mc "Weil du es verdient hast."

# game/christmas.rpy:403
translate german christmas_shogukuni_02_bc3580f6:

    # mc "Seria fácil arreglar esto pidiendo disculpas o pensar en alguna forma de compensar lo que hiciste."
    mc "Es wäre einfach für dich dies wieder rückgängig zu machen, indem du dich entschuldigst, oder etwas für deine Tat tust."

# game/christmas.rpy:405
translate german christmas_shogukuni_02_ac34fc65:

    # ka "Pedir disculpas..."
    ka "Entschuldigen..."

# game/christmas.rpy:406
translate german christmas_shogukuni_02_2f1f838b:

    # ka "Eso significa que..."
    ka "Das bedeutet etwa..."

# game/christmas.rpy:408
translate german christmas_shogukuni_02_77e2b931:

    # ka "¡Por favor Yui!"
    ka "Warte Yui!"

# game/christmas.rpy:409
translate german christmas_shogukuni_02_d5ab2eed:

    # ka "¡Eres mi última esperanza para no seguir sufriendo de frío!"
    ka "Du bist meine letzte Hoffnung, die Kälte zu überwinden!"

# game/christmas.rpy:411
translate german christmas_shogukuni_02_12498a1c:

    # y "¿Venganza?"
    y "Rache?"

# game/christmas.rpy:413
translate german christmas_shogukuni_02_2957e552:

    # ka "¡Tú lo has dicho!"
    ka "Du sagst es!"

# game/christmas.rpy:414
translate german christmas_shogukuni_02_15b50e31:

    # ka "¡Shizuka está llena de sentimientos de venganza y odio hacia mí!"
    ka "Shizuka ist mit Rache und Hass gefüllt gegenüber mir!"

# game/christmas.rpy:415
translate german christmas_shogukuni_02_8459764c:

    # ka "¡Ira y odio profundo con ojos rojos y dientes afilados!"
    ka "Wut und tiefer Hass mit roten Augen und scharfen Zähnen!"

# game/christmas.rpy:417
translate german christmas_shogukuni_02_5dd8b341:

    # y "¿Por qué?"
    y "Warum?"

# game/christmas.rpy:419
translate german christmas_shogukuni_02_b69f1d92:

    # ka "¿Como que por qué?"
    ka "Was meinst du mit, Warum?"

# game/christmas.rpy:420
translate german christmas_shogukuni_02_dd9455fc:

    # ka "¡Porque Shizuka es una psicópata!"
    ka "Weil Shizuka ein Psychopath ist!"

# game/christmas.rpy:422
translate german christmas_shogukuni_02_fbd197f9:

    # mc "Y por lo del estofado..."
    mc "Und wegen dem Gericht..."

# game/christmas.rpy:424
translate german christmas_shogukuni_02_0eda74ee:

    # y "¿Estofado?"
    y "Gericht?"

# game/christmas.rpy:426
translate german christmas_shogukuni_02_c5d5f4ac:

    # ka "Oh, bueno..."
    ka "Naja..."

# game/christmas.rpy:427
translate german christmas_shogukuni_02_4162a984:

    # ka "Tampoco es necesario dar tantos detalles..."
    ka "Die Details sind nicht so wichtig jetzt..."

# game/christmas.rpy:429
translate german christmas_shogukuni_02_aabd49cd:

    # y "¿Tami-Yami?"
    y "Tami-Yami?"

# game/christmas.rpy:431
translate german christmas_shogukuni_02_d5807c08:

    # ka "Eso no tiene importancia."
    ka "Das ist nicht wichtig."

# game/christmas.rpy:433
translate german christmas_shogukuni_02_fae5a458:

    # y "¿[nombre]?"
    y "[nombre]?"

# game/christmas.rpy:435
translate german christmas_shogukuni_02_489a8891:

    # mc "Parece ser que Kaede se comió la porción de estofado de Shizuka y no dejo nada."
    mc "Kaede hat anscheinend Shizuka's Portion gegessen und nichts hinterlassen."

# game/christmas.rpy:437
translate german christmas_shogukuni_02_71a86863:

    # y "..."
    y "..."

# game/christmas.rpy:439
translate german christmas_shogukuni_02_c5ad31f1:

    # ka "¿Por qué me miras así?"
    ka "Warum schaust du mich so an?"

# game/christmas.rpy:441
translate german christmas_shogukuni_02_cffd9dd6:

    # y "Estofado..."
    y "Gericht..."

# game/christmas.rpy:443
translate german christmas_shogukuni_02_73c85b57:

    # ka "No me digas que tu también querías comer."
    ka "Sag mir nicht, dass du das auch essen wolltest."

# game/christmas.rpy:445
translate german christmas_shogukuni_02_b5354ecb:

    # y "Kaede..."
    y "Kaede..."

# game/christmas.rpy:447
translate german christmas_shogukuni_02_955a82bb:

    # ka "Mira, te lo juro..."
    ka "Ich schwöre..."

# game/christmas.rpy:448
translate german christmas_shogukuni_02_d45fb96c:

    # ka "¡Hice todo lo posible por no comer más de la cuenta, pero no pude resistir!"
    ka "Ich habe mein bestes versucht, konnte aber nicht Wiederstehen!"

# game/christmas.rpy:449
translate german christmas_shogukuni_02_a79df210:

    # ka "¡Caí en la tentación y ahora me arrepiento de todo!"
    ka "Ich habe meiner Gier nachgelassen und jetzt bereue Ich alles!"

# game/christmas.rpy:450
translate german christmas_shogukuni_02_fe5586e7:

    # ka "¡Estaba muy hambrienta y estaba todo tan delicioso que no podía simplemente comerme mi ración!"
    ka "Ich war so hungrig und so köstlich, da konnte ich nicht nur meine Portion essen!"

# game/christmas.rpy:451
translate german christmas_shogukuni_02_ca48bea6:

    # ka "Por favor Yui... {w}Estoy segura de que tú puedes entenderme..."
    ka "Bitte Yui... {w}Ich bin mir sicher, du kannst mich verstehen..."

# game/christmas.rpy:453
translate german christmas_shogukuni_02_5b423b70:

    # ya "Tami-Yami..."
    ya "Tami-Yami..."

# game/christmas.rpy:455
translate german christmas_shogukuni_02_71123867:

    # ka "Lo sé, lo siento."
    ka "Ich weiss, tut mir Leid."

# game/christmas.rpy:456
translate german christmas_shogukuni_02_8c6248e9:

    # ka "Sé que a ti y a Shizuka les encanta ese estofado."
    ka "Ich weiss, dass du und Shizuka dieses Gericht liebt."

# game/christmas.rpy:458
translate german christmas_shogukuni_02_fdc86257:

    # mc "¿Te comiste lo de Shizuka y además lo de Yui?"
    mc "Hast du Shizuka's, sowie Yui's Portion gegessen?"

# game/christmas.rpy:460
translate german christmas_shogukuni_02_9632c001:

    # ka "Si lo dices de esa forma suena terrible..."
    ka "Wenn man es so betrachtet, hört es sich schlimm an..."

# game/christmas.rpy:462
translate german christmas_shogukuni_02_72491593:

    # mc "¡Pégale Yui!"
    mc "Schlage Sie, Yui!"

# game/christmas.rpy:464
translate german christmas_shogukuni_02_0966dd4f:

    # ka "¡Espera!"
    ka "Warte!"

# game/christmas.rpy:465
translate german christmas_shogukuni_02_5db3f972:

    # ka "¡Es a Shizuka a quien tienes que patearle el trasero!"
    ka "Es ist Shizuka's Arsch, welcher getreten werden muss!"

# game/christmas.rpy:466
translate german christmas_shogukuni_02_c67a1d7a:

    # ka "Necesito mi ropa de vuelta o tendré los mocos congelados antes de que oscurezca."
    ka "Ich brauche meine Kleider zurück, oder mein Schnöder friert ein bevor es zu dunkel wird."

# game/christmas.rpy:468
translate german christmas_shogukuni_02_1a05be8a:

    # shi "Deberías trotar durante una hora para entrar en calor."
    shi "Du solltest Joggen gehen für eine Stunde, um dich wieder aufzuwärmen."

# game/christmas.rpy:469
translate german christmas_shogukuni_02_82a2ee80:

    # shi "Pero también hay que considerar que eres una holgazán así que preferirás morir de frío antes de mover el trasero."
    shi "Aber du solltest auch beachten, dass du ziemlich Faul bist, also würdest du lieber erfrieren als deinen Arsch bewegen."

# game/christmas.rpy:471
translate german christmas_shogukuni_02_b16f951d:

    # ka "¡Mírala Yui!"
    ka "Schau Sie an Yui!"

# game/christmas.rpy:472
translate german christmas_shogukuni_02_ee1e2d87:

    # ka "¡Tiene mi ropa de invierno puesta!"
    ka "Sie hat meine Winterklamotten an!"

# game/christmas.rpy:474
translate german christmas_shogukuni_02_d0f9f40b:

    # ka "¡Te hemos pillado con las manos en la masa!"
    ka "Wir haben dich auf frischer Tat ertappt!"

# game/christmas.rpy:476
translate german christmas_shogukuni_02_a3c2e2de:

    # ka "¿Ven que no mentía?"
    ka "Siehst du? Ich habe nicht gelogen!"

# game/christmas.rpy:478
translate german christmas_shogukuni_02_bf2ed835:

    # ka "..."
    ka "..."

# game/christmas.rpy:484
translate german christmas_shogukuni_02_c15bc311:

    # ka "¿Adónde vas Yui?"
    ka "Wo gehst du hin Yui?"

# game/christmas.rpy:485
translate german christmas_shogukuni_02_fff02649:

    # ka "No me digas que piensas dejarme sola..."
    ka "Sag mir nicht, dass du mich jetzt alleine lässt..."

# game/christmas.rpy:487
translate german christmas_shogukuni_02_32006725:

    # ka "¡Vuelve aquí Yui!"
    ka "Komm zurück Yui!"

# game/christmas.rpy:488
translate german christmas_shogukuni_02_366b2fd8:

    # ka "¡Eres mi última esperanza!"
    ka "Du bist meine letzte Hoffnung!"

# game/christmas.rpy:490
translate german christmas_shogukuni_02_7e343bde:

    # mc "Yui se ha cabreado porque te comiste su estofado."
    mc "Yui ist sauer, weil du Ihre Portion gegessen hast."

# game/christmas.rpy:492
translate german christmas_shogukuni_02_a640f1a8:

    # shi "Entonces yo no he sido la única víctima de tus tonterías."
    shi "Dann bin ich nicht das einzige Opfer hier von deinen Scharaden."

# game/christmas.rpy:493
translate german christmas_shogukuni_02_991d4842:

    # shi "Eres una cerda Kaede."
    shi "Du bist ein Schwein, Kaede."

# game/christmas.rpy:494
translate german christmas_shogukuni_02_11876f52:

    # shi "Ni siquiera me explico como haces para tragar tanto y seguir siendo tan delgada."
    shi "Ich kann nicht mal erklären, wie viel Zeugs du in deinen Maul stopfst, und immer noch so dünn bleibst."

# game/christmas.rpy:495
translate german christmas_shogukuni_02_ef3ece29:

    # shi "Pero no importa, yo tampoco te guardo rencor por lo que hiciste."
    shi "Aber das macht nichts, ich hege auch keinen Groll für das, was du getan hast."

# game/christmas.rpy:497
translate german christmas_shogukuni_02_124d9bfe:

    # ka "¡Cómo dices eso si tienes mi ropita puesta!"
    ka "Wie kannst du das sagen, wenn du gerade meine Kleider trägst!?"

# game/christmas.rpy:499
translate german christmas_shogukuni_02_909c8bb2:

    # shi "¿De qué ropa estás hablando?"
    shi "Welche Kleider meinst du?"

# game/christmas.rpy:501
translate german christmas_shogukuni_02_39cce1ad:

    # ka "¡Deja de fingir que no sabes!"
    ka "Tut nicht so, als ob du keine Ahnung hast!"

# game/christmas.rpy:503
translate german christmas_shogukuni_02_b6743276:

    # shi "¿Te refieres a esta?"
    shi "Meinst du die hier?"

# game/christmas.rpy:504
translate german christmas_shogukuni_02_58285fc4:

    # shi "Me la he encontrado tirada en el suelo... {w}No sabía que era tu ropa, Kaede."
    shi "Ich habe sie auf dem Boden liegen sehen... {w}Wusste nicht mal, dass es deine Kleider sind, Kaede."

# game/christmas.rpy:505
translate german christmas_shogukuni_02_6077dd08:

    # shi "Pero como dice el dicho, el que se lo encuentra se lo queda."
    shi "Aber wie man das Sprichwort geht, der der es findet, behält es."

# game/christmas.rpy:507
translate german christmas_shogukuni_02_09ada04a:

    # ka "¡Si tiene el color de mi uniforme rojo allí!"
    ka "Wenn es die Farbe von meiner rotten Uniform hat!"

# game/christmas.rpy:508
translate german christmas_shogukuni_02_69e9b1f0:

    # ka "¿De quién más iba a ser?"
    ka "Wem würde dies sonst gehören?"

# game/christmas.rpy:510
translate german christmas_shogukuni_02_86e98bc5:

    # shi "Podrías estar mintiendo para apropiarte de algo ajeno como hiciste con el estofado."
    shi "Du könntest lügen, damit dir etwas am Schluss gehört was nicht dir ist, genau wie mit dem Gericht."

# game/christmas.rpy:511
translate german christmas_shogukuni_02_7a598d0b:

    # shi "Tengo entendido que tu ropa es la que guardas en tu armario..."
    shi "Ich verstehe, dass die Kleider, die dir gehören, sich in deinem Schrank befinden... {w}oder?"

# game/christmas.rpy:512
translate german christmas_shogukuni_02_89ca5150:

    # shi "En cambio, esta ropa la encontré tirada y arrugada, puedo suponer que su dueño no la necesitaba."
    shi "Dazu noch, habe ich die Kleider komplett zerknittert auf dem Boden gefunden, da kann ich davon ausgehen, dass die vorherige Besitzerin nicht mehr gebraucht hat."

# game/christmas.rpy:514
translate german christmas_shogukuni_02_10ab4ecf:

    # ka "¡Pero si estamos compartiendo la misma habitación!"
    ka "Aber wir leben unter dem selben Dach!"

# game/christmas.rpy:515
translate german christmas_shogukuni_02_5b5c8d53:

    # ka "¡Así que es nuestra habitación y no solo tuya!"
    ka "Demnach ist es unser Raum, und nicht nur deins!"

# game/christmas.rpy:517
translate german christmas_shogukuni_02_91f0cd10:

    # shi "Yo decidí acogerte allí y tú te has comportado de forma indisciplinada y has traicionando mi confianza."
    shi "Ich habe entschieden dich herzlich Willkommen zu heissen, danach hast du mit deinem Verhalten meinen Vertrauen zerstört."

# game/christmas.rpy:518
translate german christmas_shogukuni_02_c06fc728:

    # shi "Y si fueses un poco más ordenada no habrías perdido tu ropa."
    shi "Und wärst du etwas gründlicher, hättest du deine Kleider nicht verloren."

# game/christmas.rpy:520
translate german christmas_shogukuni_02_65823f8c:

    # ka "¡Había dejado todo a los pies del futón para vestirme en la mañana!"
    ka "Ich habe alles beim Fussbereich vom Futon gelassen, damit ich am Morgen mich anziehen kann!"

# game/christmas.rpy:521
translate german christmas_shogukuni_02_077057aa:

    # ka "Pero estaba tan cansada la noche anterior que me dormí enseguida..."
    ka "Aber ich war so müde von letzter nacht, dass ich sofort eingeschlafen bin..."

# game/christmas.rpy:523
translate german christmas_shogukuni_02_bee4de9c:

    # shi "Corrección... {w}Dormiste hasta tarde porque estaba con el estómago tan lleno que no podías despertar."
    shi "Korrigiere... {w}Du bist spät ins Bett, weil dein Bauch so voll war, dass du nicht aufwachen konntest."

# game/christmas.rpy:525
translate german christmas_shogukuni_02_40238dca:

    # ka "Bueno, algo así, pero eso no es importante."
    ka "Naja, so in etwa, aber dass ist nicht wichtig."

# game/christmas.rpy:527
translate german christmas_shogukuni_02_abb173b6:

    # shi "¿No recuerdas porque estabas con el estómago lleno?"
    shi "Erinnerst du dich nicht, warum du einen vollen Bauch hattest?"

# game/christmas.rpy:529
translate german christmas_shogukuni_02_50137896:

    # ka "Vamos Shizuka, ya he aprendido mi lección."
    ka "Na los Shizuka, ich habe meine Lektion gelernt."

# game/christmas.rpy:530
translate german christmas_shogukuni_02_30b20e2f:

    # ka "¡Devuélveme mi ropa o se me me van a congelar las tetas!"
    ka "Gib mir meine Kleider zurück, oder meine Titten frieren ein!"

# game/christmas.rpy:532
translate german christmas_shogukuni_02_27dbe5fc:

    # shi "Quizás si se te congelan las tetas dejaras de burlarte de las mías."
    shi "Vielleicht wenn deine Brüste mal eingefroren sind, hörst du mal auf, dich lustig zu machen über meine."

# game/christmas.rpy:533
translate german christmas_shogukuni_02_4df59933:

    # shi "Diciendo que soy plana como una tabla o que tengo menos relleno que una galleta de la suerte."
    shi "Sagst ständig, dass ich so flach wie ein Brett bin, habe weniger Füllung als ein Glückskeks."

# game/christmas.rpy:534
translate german christmas_shogukuni_02_684554ca:

    # shi "O peor aún... {w}Minitetitas Mcgruñona."
    shi "Oder schlimmer... {w}Minititten McBrett"

# game/christmas.rpy:536
translate german christmas_shogukuni_02_ecd6f1fb:

    # ka "¡Hahahaha!"
    ka "Hahahaha!"

# game/christmas.rpy:537
translate german christmas_shogukuni_02_8540f719:

    # ka "¡Deberías admitir que ese último es supergracioso!"
    ka "Du musst schon zugeben, dass letzte ist ziemlich lustig!"

# game/christmas.rpy:542
translate german christmas_shogukuni_02_f755799c:

    # shi "..."
    shi "..."

# game/christmas.rpy:547
translate german christmas_shogukuni_02_edd91898:

    # ka "Está bien, lo siento..."
    ka "Schön, schön, tut mir Leid..."

# game/christmas.rpy:548
translate german christmas_shogukuni_02_5f2c580d:

    # ka "Puedes decirme tetas congeladas desde ahora en adelante, pero por favor devuélveme mi ropa."
    ka "Du kannst mich ab jetzt 'Frier Titten' nennen, aber gib mir bitte meine Kleider zurück."

# game/christmas.rpy:550
translate german christmas_shogukuni_02_556ccbe4:

    # shi "¿Por que no vamos al Dojo e intentas quitármelas por la fuerza?"
    shi "Warum gehen wir nicht ausserhalb vom Dojo? Wo du dann mal versuchen kannst, sie mir abzunehmen"

# game/christmas.rpy:551
translate german christmas_shogukuni_02_570fe6fd:

    # shi "Si puedes luchar contra mí durante un minuto podría pensarlo."
    shi "Wenn du mich, für eine Minute bekämpfen kannst, überlege ich es mir."

# game/christmas.rpy:553
translate german christmas_shogukuni_02_aa864442:

    # ka "Que falta de espíritu navideño..."
    ka "Was für ein Mangel an Weihnachtsstimmung..."

# game/christmas.rpy:555
translate german christmas_shogukuni_02_66dac8b8:

    # ka "¡Por favor [nombre], dile algo!"
    ka "Bitte [nombre], sag doch etwas zu Ihr!"

# game/christmas.rpy:557
translate german christmas_shogukuni_02_4f7a2c5a:

    # mc "Hola Shizuka."
    mc "Hey Shizuka."

# game/christmas.rpy:559
translate german christmas_shogukuni_02_dbe309c4:

    # shi "Espero que no tengas tanto frío como Kaede, [nombre]."
    shi "Ich hoffe du bist nicht so kalt wie Kaede, [nombre]."

# game/christmas.rpy:560
translate german christmas_shogukuni_02_ba8006f8:

    # shi "Pero si es así, puedo pasarte este kimono para que puedas abrigarte."
    shi "Aber falls schon, kann ich dir das Kimono überreichen, damit du dich warm anziehen kannst."

# game/christmas.rpy:562
translate german christmas_shogukuni_02_f7584758:

    # mc "No te preocupes, estoy de maravilla."
    mc "Keine Sorge, mir gehts gut."

# game/christmas.rpy:563
translate german christmas_shogukuni_02_bf2ed835_1:

    # ka "..."
    ka "..."

# game/christmas.rpy:565
translate german christmas_shogukuni_02_1e06c527:

    # ka "¿De verdad tú tampoco vas a hacer nada [nombre]?"
    ka "Machst du nichts dagegen,[nombre]?"

# game/christmas.rpy:566
translate german christmas_shogukuni_02_8b8c2938:

    # ka "¡Todos ustedes son unas personas terribles!"
    ka "Ihr seit alle schlechte Menschen!"

# game/christmas.rpy:567
translate german christmas_shogukuni_02_f18205bc:

    # ka "Quieren que me muera de frío solo porque no quería morir de hambre."
    ka "Sie wollen, dass ich mich zu Tode frieren, nur weil ich nicht verhungern wollte."

# game/christmas.rpy:569
translate german christmas_shogukuni_02_d49c835f:

    # shi "Bien, es navidad así que intentaremos algo diferente."
    shi "Naja, es ist Weihnachten, also lass uns was anderes versuchen."

# game/christmas.rpy:570
translate german christmas_shogukuni_02_2c73c31d:

    # shi "Quiero que superes un desafío."
    shi "Ich möchte, dass du eine Herausforderung überwindest."

# game/christmas.rpy:572
translate german christmas_shogukuni_02_6542e73f:

    # ka "¡Lo que sea!"
    ka "Na schön!"

# game/christmas.rpy:574
translate german christmas_shogukuni_02_c2eb7d2c:

    # shi "Vamos al dojo y quítate la ropa."
    shi "Lass uns zum Dojo gehen, und ziehen unsere Kleider aus."

# game/christmas.rpy:576
translate german christmas_shogukuni_02_5f5f4ae3:

    # ka "¿Dices que debo encuerarme?"
    ka "Sagtest du gerade, dass ich nackt sein muss?"

# game/christmas.rpy:578
translate german christmas_shogukuni_02_d2ea5364:

    # ka "¡Pero si hace un frío que te cagas!"
    ka "Aber es ist Arschkalt!"

# game/christmas.rpy:579
translate german christmas_shogukuni_02_ee031b7c:

    # ka "¡Quiero mi ropa de invierno para no morir congelada y tú me dices que debo desvestirme!"
    ka "Ich möchte meine Winterkleider, damit ich nicht zu Tode friere... und du "

# game/christmas.rpy:581
translate german christmas_shogukuni_02_0a5f4fb8:

    # shi "¿Quieres recuperar tu ropa o no?"
    shi "Möchtest du deine Kleider zurück oder nicht?"

# game/christmas.rpy:582
translate german christmas_shogukuni_02_ade88675:

    # shi "También podemos arreglarlo con una pelea."
    shi "Wir können dies aber auch in einen Kampf entscheiden."

# game/christmas.rpy:584
translate german christmas_shogukuni_02_6b5d7ca9:

    # ka "¡Entonces acepto!"
    ka "Dann akzeptiere ich die Herausforderung!"

# game/christmas.rpy:585
translate german christmas_shogukuni_02_f023807a:

    # ka "¿Cuál es el desafío?"
    ka "Welches war die Herausforderung?"

# game/christmas.rpy:596
translate german christmas_shogukuni_02_3ce74050:

    # ka "UGGGggghhh..."
    ka "UGGGggghhh..."

# game/christmas.rpy:597
translate german christmas_shogukuni_02_9ac88b6b:

    # ka "¿Cuánto tiempo más debo soportar esta posición?"
    ka "Wie lange muss ich noch in dieser Position bleiben?"

# game/christmas.rpy:599
translate german christmas_shogukuni_02_5b640aa4:

    # shi "Te dije cuarenta minutos y llevas poco más de diez."
    shi "Ich sagte dir schon, 40 Minuten und du hast gerade mal zehn."

# game/christmas.rpy:601
translate german christmas_shogukuni_02_ce019705:

    # ka "¿Qué apenas llevo diez?"
    ka "Nur zehn?"

# game/christmas.rpy:602
translate german christmas_shogukuni_02_21b392f3:

    # ka "¡Pero si llevo aquí casi dos horas!"
    ka "Aber ich bin hier seit zwei Stunden!"

# game/christmas.rpy:604
translate german christmas_shogukuni_02_c35071a5:

    # ka "¡Por favor [nombre] dile algo!"
    ka "Komm schon [nombre], sag doch etwas!"

# game/christmas.rpy:605
translate german christmas_shogukuni_02_81350018:

    # ka "O te juro que voy a morir de cansancio."
    ka "Oder ich schwöre, dass ich vor Ermüdung sterbe."

# game/christmas.rpy:606
translate german christmas_shogukuni_02_b0630cb7:

    # ka "No puedo más..."
    ka "Ich kann nicht mehr..."

# game/christmas.rpy:608
translate german christmas_shogukuni_02_82621717:

    # shi "Si entrenaras lo suficiente no deberías tener problemas para hacer un ejercicio tan sencillo."
    shi "Wenn du genug trainieren würdest, hättest du kein Problem so eine einfache Übung abzuschliessen."

# game/christmas.rpy:610
translate german christmas_shogukuni_02_1543fab8:

    # ya "¿Castigo?"
    ya "Bestrafung?"

# game/christmas.rpy:612
translate german christmas_shogukuni_02_2f3a6c96:

    # ka "¡Ayúdame Yui!"
    ka "Hilf mir Yui!"

# game/christmas.rpy:613
translate german christmas_shogukuni_02_366b2fd8_1:

    # ka "¡Eres mi última esperanza!"
    ka "Du bist meine letzte Hoffnung!"

# game/christmas.rpy:615
translate german christmas_shogukuni_02_fd7c412a:

    # ka "¡Shizuka me quiere matar!"
    ka "Shizuka möchte mich umbringen!"

# game/christmas.rpy:617
translate german christmas_shogukuni_02_49ea74a3:

    # ya "Bien."
    ya "Gut."

# game/christmas.rpy:619
translate german christmas_shogukuni_02_c7d94ce1:

    # ka "¿Cómo que bien?"
    ka "Was meinst du mit gut?"

# game/christmas.rpy:620
translate german christmas_shogukuni_02_f172c323:

    # ka "No me digas que también estás cabreada por lo del estofado."
    ka "Sag mir nicht, dass du auch sauer darauf bist."

# game/christmas.rpy:622
translate german christmas_shogukuni_02_09730b5f:

    # ya "Sí."
    ya "Ja."

# game/christmas.rpy:624
translate german christmas_shogukuni_02_d6250c66:

    # mc "Ese estofado debe ser delicioso si incluso Yui está enojada contigo."
    mc "Dieses Gericht muss ja verdammt gut sein, wenn selbst Yui wütend auf dich ist."

# game/christmas.rpy:626
translate german christmas_shogukuni_02_78d70f1b:

    # ka "¡Es el mejor estofado!"
    ka "Es ist das beste Essen!"

# game/christmas.rpy:627
translate german christmas_shogukuni_02_f399af3e:

    # ka "Incluso podría confesar que todo este castigo ha válido la pena."
    ka "Ich gebe vielleicht zu, dass all diese Folter es wert ist."

# game/christmas.rpy:629
translate german christmas_shogukuni_02_a2484bb2:

    # shi "Treinta minutos extra por idiota."
    shi "Gib mir 30 Minuten, Idiot."

# game/christmas.rpy:631
translate german christmas_shogukuni_02_86c286f6:

    # ka "¡Eso es injusto!"
    ka "Das ist unfair!"

# game/christmas.rpy:632
translate german christmas_shogukuni_02_c7d29625:

    # ka "¡Estás cambiando las reglas a mitad del desafío!"
    ka "Du änderst die Regeln, während der Herausforderung!"

# game/christmas.rpy:634
translate german christmas_shogukuni_02_3a6dbaab:

    # shi "No voy a tomar en cuenta ninguna de tus quejas, Kaede."
    shi "Ich werde keine deiner Beschwerden acht geben, Kaede."

# game/christmas.rpy:635
translate german christmas_shogukuni_02_41faea65:

    # shi "Si hubiese sido otra persona la que se tragó todo el estofado, ahora mismo sería un cadáver muerto."
    shi "Wenn es jemand anderes wäre und alles weggegessen hätte, dann wäre diese Person schon längstens tot."

# game/christmas.rpy:637
translate german christmas_shogukuni_02_e7d538bb:

    # ka "¿Un cadáver muerto?"
    ka "Eine Leiche?"

# game/christmas.rpy:638
translate german christmas_shogukuni_02_dcd79b6f:

    # ka "¡Qué tonta eres!"
    ka "Du bist ein Idiot!"

# game/christmas.rpy:640
translate german christmas_shogukuni_02_b72311e3:

    # shi "¡Treinta minutos más por estúpida!"
    shi "30 Minuten dazu, JETZT!!!"

# game/christmas.rpy:642
translate german christmas_shogukuni_02_cc8f3dd7:

    # ka "¿Y qué clase de castigo es este?"
    ka "Und welche Art von Folter ist das jetzt?"

# game/christmas.rpy:643
translate german christmas_shogukuni_02_2f3db7a4:

    # ka "¿Por qué tengo que hacer esto estando con las tetas al aire?"
    ka "Warum muss ich dies mit meinen nicht bedeckten Titten machen?"

# game/christmas.rpy:644
translate german christmas_shogukuni_02_993f61a1:

    # ka "No me digas que es un fetiche raro."
    ka "Sag mir nicht, dass du eine Art von Fetisch hast."

# game/christmas.rpy:646
translate german christmas_shogukuni_02_ac6fc574:

    # shi "Si no eres capaz de hacer lo que te dije... {w}Entonces también me quedaré con el resto de tu ropa."
    shi "Wenn du nicht das tun kannst, was ich dir sage... {w}Dann behalte ich den Rest deiner Kleider."

# game/christmas.rpy:648
translate german christmas_shogukuni_02_5d1ecabc:

    # ka "¡Eso tampoco era parte del trato!"
    ka "Das war nicht Teil des Deals!"

# game/christmas.rpy:650
translate german christmas_shogukuni_02_21afee1d:

    # shi "No me dejas otra alternativa y quiero que te tomes este desafío con seriedad."
    shi "Du lässt mir keine andere Wahl, dazu solltest du die Herausforderung auch ernst nehmen."

# game/christmas.rpy:652
translate german christmas_shogukuni_02_53883f1e:

    # ka "¡Eres una tramposa!"
    ka "Du bist eine Betrügerin!"

# game/christmas.rpy:654
translate german christmas_shogukuni_02_9198c71a:

    # shi "Digamos que se me han pegado algunas costumbres de [nombre]."
    shi "Sagen wir, dass einpaar Angewohnheiten	von [nombre]'s sich an mir abgefangen haben."

# game/christmas.rpy:656
translate german christmas_shogukuni_02_e0aa0722:

    # ka "No puedo soportarlo más..."
    ka "I halte es nicht mehr aus..."

# game/christmas.rpy:657
translate german christmas_shogukuni_02_4d5c4d8e:

    # ka "Mis músculos están entumecidos..."
    ka "Meine Muskeln fühlen sich Taub an..."

# game/christmas.rpy:659
translate german christmas_shogukuni_02_b35a315d:

    # shi "Hoy me siento bondadosa, así que reduciré tu tiempo a cambio de otra cosa."
    shi "Heute fühle ich mich nett, deswegen reduziere ich deine Zeit, im Gegensatz für etwas anderes."

# game/christmas.rpy:661
translate german christmas_shogukuni_02_e4eaba80:

    # ka "No quiero preguntar..."
    ka "Ich möchte nicht einmal fragen..."

# game/christmas.rpy:663
translate german christmas_shogukuni_02_f44448de:

    # shi "No debes preguntar si sabes que puedes soportar esa posición por noventa minutos más."
    shi "Du sollst auch nicht fragen, wenn du schon weisst, dass du diese Position für 90 Minuten halten kannst, oder sogar mehr."

# game/christmas.rpy:665
translate german christmas_shogukuni_02_2e5ff1c2:

    # ka "Está bien."
    ka "Fein."

# game/christmas.rpy:666
translate german christmas_shogukuni_02_69b93939:

    # ka "¿Qué debo hacer para que reduzcas el tiempo?"
    ka "Was sollte ich tun, um meine Zeit zu reduzieren?"

# game/christmas.rpy:668
translate german christmas_shogukuni_02_7cbcaad7:

    # shi "Antes debes aceptar si quieres que te lo diga."
    shi "Du musst mir zuerst zustimmen, bevor ich es sage."

# game/christmas.rpy:670
translate german christmas_shogukuni_02_42504a92:

    # ka "¡Eres demasiado tramposa!"
    ka "Du mogelst zu viel!"

# game/christmas.rpy:672
translate german christmas_shogukuni_02_010caf4f:

    # shi "Quedan ochenta y nueve minutos."
    shi "89 Minuten übrig."

# game/christmas.rpy:674
translate german christmas_shogukuni_02_f89c50ec:

    # ka "¡Está bien!"
    ka "Fein!"

# game/christmas.rpy:675
translate german christmas_shogukuni_02_db4c9a56:

    # ka "¡Acepto!"
    ka "Akzeptiert!"

# game/christmas.rpy:678
translate german christmas_shogukuni_02_440f7214:

    # shi "Ve y dale unas nalgadas [nombre]."
    shi "Geh und versohl Ihr den Hintern, [nombre]."

# game/christmas.rpy:681
translate german christmas_shogukuni_02_8b732faa:

    # mc "¿De verdad quieres que la golpee?"
    mc "Willst du wirklich, dass ich sie schlage?"

# game/christmas.rpy:682
translate german christmas_shogukuni_02_11bb4483:

    # mc "¿No estarás exagerando un poco?"
    mc "Übertreibst du es nicht langsam?"

# game/christmas.rpy:684
translate german christmas_shogukuni_02_5ba58a69:

    # shi "Ella ya ha aceptado."
    shi "Sie hat es schon akzeptiert."

# game/christmas.rpy:686
translate german christmas_shogukuni_02_4214b131:

    # ka "¡Una nalgada por veinte minutos!"
    ka "Hintern versohlen für 20 Minuten!"

# game/christmas.rpy:688
translate german christmas_shogukuni_02_4db5d3f0:

    # shi "Dos minutos..."
    shi "Zwei Minuten..."

# game/christmas.rpy:690
translate german christmas_shogukuni_02_07e8760c:

    # ka "¡Deberían ser al menos quince!"
    ka "Es sollten mindestens 15 sein!"

# game/christmas.rpy:692
translate german christmas_shogukuni_02_9701ee6b:

    # shi "Cinco minutos."
    shi "Fünf Minuten."

# game/christmas.rpy:694
translate german christmas_shogukuni_02_792c6ca1:

    # ka "¡Diez minutos!"
    ka "Zehn Minuten!"

# game/christmas.rpy:696
translate german christmas_shogukuni_02_193421c2:

    # shi "Dejémoslo así..."
    shi "Lassen wir es dabei..."

# game/christmas.rpy:697
translate german christmas_shogukuni_02_1eab316e:

    # shi "Serán seis nalgadas y no debes bajar la posición en ningún momento."
    shi "Es wird insgesamt sechs Schläge auf den Arsch geben. Du darfst deine Position im keinen moment verlassen. "

# game/christmas.rpy:698
translate german christmas_shogukuni_02_e9c5e544:

    # shi "¿Entendido?"
    shi "Verstanden?"

# game/christmas.rpy:700
translate german christmas_shogukuni_02_90f99739:

    # ka "¡Claro que si!"
    ka "Ja, natürlich!"

# game/christmas.rpy:701
translate german christmas_shogukuni_02_035bab85:

    # ka "Querías engañarme pero mi mente ha sido más ágil que la tuya."
    ka "Du wolltest mich veräppeln, aber mein Kopf ist Agiler als deins."

# game/christmas.rpy:702
translate german christmas_shogukuni_02_af069e61:

    # ka "Unos cuantas cachetadas en mi trasero no es tan terrible."
    ka "Ein paar Klatscher am Arsch, ist nicht so schlimm."

# game/christmas.rpy:704
translate german christmas_shogukuni_02_f95d5fcc:

    # shi "Dale con toda tu fuerza [nombre]."
    shi "Versohl Ihr den Arsch mit all deiner Kraft, [nombre]."

# game/christmas.rpy:710
translate german christmas_shogukuni_02_40b6d754:

    # ka "¡UMh!"
    ka "UMh!"

# game/christmas.rpy:712
translate german christmas_shogukuni_02_e98f3274:

    # shi "¡Te dije que tenías que darle con fuerza!"
    shi "Ich habe dir gesagt, du solltest es so stark wie möglich machen!"

# game/christmas.rpy:713
translate german christmas_shogukuni_02_eb6de924:

    # ka "¡Me ha golpeado fuerte!"
    ka "Das war sehr hart!"

# game/christmas.rpy:714
translate german christmas_shogukuni_02_f9b4958a:

    # shi "Eres una mentirosa."
    shi "Du lügst."

# game/christmas.rpy:719
translate german christmas_shogukuni_02_02f3926f:

    # ka "¡Ahhh!"
    ka "Ahhh!"

# game/christmas.rpy:725
translate german christmas_shogukuni_02_90378c15:

    # ka "¡OHHhhh!"
    ka "OHHhhh!"

# game/christmas.rpy:731
translate german christmas_shogukuni_02_9d91dd08_1:

    # ka "¡Ouch!"
    ka "Ouch!"

# game/christmas.rpy:733
translate german christmas_shogukuni_02_0268881e:

    # ka "¡Ya van cuatro!"
    ka "Vier und...!"

# game/christmas.rpy:734
translate german christmas_shogukuni_02_774d543b:

    # shi "¡Si de verdad está doliendo deberías dejar de sonreír!"
    shi "Wenn es dir wirklich weh tut, solltest du aufhören zu lächeln!"

# game/christmas.rpy:735
translate german christmas_shogukuni_02_3259d726:

    # ka "¡Estoy sonriendo porque tengo el culo tan entumecido que siento cosquillas en las nalgas!"
    ka "Ich lächle, weil mein Arsch so taub ist, dass meine Backen kribbeln!"

# game/christmas.rpy:737
translate german christmas_shogukuni_02_e2555ee9:

    # shi "¡Si no golpeas fuerte a Kaede te voy a hacer lo mismo a ti [nombre]!"
    shi "Wenn du Kaede nicht härter schlägst [nombre], mache ich das selbe mit dir!"

# game/christmas.rpy:739
translate german christmas_shogukuni_02_1e70d300:

    # mc "Chica traviesa."
    mc "Unartiges Mädchen."

# game/christmas.rpy:741
translate german christmas_shogukuni_02_f755799c_1:

    # shi "..."
    shi "..."

# game/christmas.rpy:742
translate german christmas_shogukuni_02_fae61dbb:

    # ka "¡Te han dicho que eres una depravada Shizuka!"
    ka "Man hat dir gesagt, dass du verdorben bist, Shizuka!"

# game/christmas.rpy:744
translate german christmas_shogukuni_02_64039765:

    # shi "¡Termina de una vez!"
    shi "Beende es!"

# game/christmas.rpy:750
translate german christmas_shogukuni_02_ed7b0591:

    # ka "¡AHHHhhhh!"
    ka "AHHHhhhh!"

# game/christmas.rpy:756
translate german christmas_shogukuni_02_f23cfc4a:

    # ka "¡AAAAAAHHHHHHHHHH!"
    ka "AAAAAAHHHHHHHHHH!"

# game/christmas.rpy:766
translate german christmas_shogukuni_02_fcb9d4ad:

    # ka "¡Ouchhhh!"
    ka "Ouchhhh!"

# game/christmas.rpy:767
translate german christmas_shogukuni_02_872f84ff:

    # ka "¡Ese sí que me ha dolido un montón!"
    ka "Der hat jetzt sehr weh getan!"

# game/christmas.rpy:769
translate german christmas_shogukuni_02_ad171c47:

    # mc "Se me resbaló la mano."
    mc "Meine Hand is ausgerutscht."

# game/christmas.rpy:771
translate german christmas_shogukuni_02_e316c048:

    # ka "Duele y arde mucho..."
    ka "Es brennt und schmerzt..."

# game/christmas.rpy:772
translate german christmas_shogukuni_02_d6a9523c:

    # ka "Seguramente tengo el culo colorado ahora mismo..."
    ka "Mein Arsch ist vermutlich jetzt komplett rot."

# game/christmas.rpy:773
translate german christmas_shogukuni_02_d4dd131c:

    # ka "¡Me convertiré en un mono!"
    ka "Ich werde zu Affe!"

# game/christmas.rpy:774
translate german christmas_shogukuni_02_f1f29f76:

    # ka "¡Rechazo a la humanidad y volveré a mis orígenes!"
    ka "Ich lehne meine Menschheit ab und kehre als Affe zurück!"

# game/christmas.rpy:776
translate german christmas_shogukuni_02_b99f2496:

    # mc "En efecto."
    mc "In der Tat."

# game/christmas.rpy:778
translate german christmas_shogukuni_02_f755799c_2:

    # shi "..."
    shi "..."

# game/christmas.rpy:780
translate german christmas_shogukuni_02_d8804439:

    # shi "Ven a buscar tu ropa y deja de hacer el ridículo, Kaede."
    shi "Komm und nimm deine Kleider wieder... und hör auf dich wie ein Idiot zu benehmen, Kaede."

# game/christmas.rpy:790
translate german christmas_shogukuni_02_405fb6d3:

    # ka "Todos ustedes son unos envidiosos..."
    ka "Ihr seit doch alle neidisch..."

# game/christmas.rpy:791
translate german christmas_shogukuni_02_b4cabf62:

    # ka "Segundo día que termino con el trasero como un tomate por una tontería."
    ka "Zweiter Tag, wo er meinen Arsch rot hinterlässt, für so eine dumme Sache."

# game/christmas.rpy:793
translate german christmas_shogukuni_02_3321a221:

    # shi "No voy a negar que he disfrutado verte sufrir."
    shi "I gebe zu, dass ich spass hatte dir beim leiden zuzusehen."

# game/christmas.rpy:795
translate german christmas_shogukuni_02_fa40744e:

    # ka "Eres una mala persona..."
    ka "Du bist eine schlimme Person..."

# game/christmas.rpy:797
translate german christmas_shogukuni_02_f456f876:

    # shi "Créeme que podría ser mucho peor."
    shi "Glaub mir, es könnte schlimmer sein."

# game/christmas.rpy:799
translate german christmas_shogukuni_02_d33bc40c:

    # ka "Espero que ahora estemos a mano."
    ka "Hoffe wir sind jetzt quit."

# game/christmas.rpy:800
translate german christmas_shogukuni_02_8e24f5ff:

    # ka "¿Ha sido suficiente castigo Yui?"
    ka "War das eine ausreichende Strafe, Yui?"

# game/christmas.rpy:802
translate german christmas_shogukuni_02_4c308c28:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:804
translate german christmas_shogukuni_02_30397453:

    # ka "Que alivio..."
    ka "Was für eine Erleichterung..."

# game/christmas.rpy:805
translate german christmas_shogukuni_02_00f3f5ba:

    # ka "Me gustaría tener algún regalo de navidad en lugar de solo nalgadas..."
    ka "Ich möchte ein paar Weihnachtsgeschenke, anstatt nur Arsch versohlen..."

# game/christmas.rpy:807
translate german christmas_shogukuni_02_3e45f19c:

    # mc "¿No recibes regalos en navidad?"
    mc "Kriegst du keine Geschenke an Weihnachten?"

# game/christmas.rpy:809
translate german christmas_shogukuni_02_c6e9e8b7:

    # ka "Bueno, claro que me gustaría, pero hay reglas y todo eso..."
    ka "Najam ich würde schon gerne, aber Regeln und das ganze Zeugs..."

# game/christmas.rpy:810
translate german christmas_shogukuni_02_a45c1f4e:

    # ka "Nadie piensa en la navidad."
    ka "Niemand denkt an Weihnachten."

# game/christmas.rpy:812
translate german christmas_shogukuni_02_3984a9fd:

    # y "¡Navidad!"
    y "Weihnachten!"

# game/christmas.rpy:814
translate german christmas_shogukuni_02_34204563:

    # mc "¿A ti también te gusta la navidad Yui?"
    mc "Magst du auch Weihnachten, Yui?"

# game/christmas.rpy:816
translate german christmas_shogukuni_02_6a11be0a:

    # y "¡Si!"
    y "Ja!"

# game/christmas.rpy:818
translate german christmas_shogukuni_02_6d3ea5f5:

    # mc "Creo que tengo una idea que podría gustarles."
    mc "Ich glaube ich habe eine Idee, die du gerne hast."

# game/christmas.rpy:819
translate german christmas_shogukuni_02_690795ff:

    # mc "Podríamos hacer un pequeño evento del amigo secreto."
    mc "Wir könnten einen geheimen Weihnachtsmann Event machen."

# game/christmas.rpy:821
translate german christmas_shogukuni_02_4f26ce9d:

    # y "¿Amigo secreto?"
    y "Geheimer Weihnachtsmann?"

# game/christmas.rpy:823
translate german christmas_shogukuni_02_d9e4156a:

    # mc "Escribo nuestros nombres en cuatro papeles y cada uno de nosotros toma uno al azar."
    mc "Ich schreibe unsere Namen, auf vier verschiedene Stückchen Papier, danach nehmen wir alle eins zufällig raus."

# game/christmas.rpy:824
translate german christmas_shogukuni_02_23b04013:

    # mc "Luego debes regalar algo especial a la persona que salga en tu papel."
    mc "Dann musst du dieser Person etwas spezielles geben."

# game/christmas.rpy:825
translate german christmas_shogukuni_02_fbe27614:

    # mc "Sería una forma de celebrar la navidad sin romper las reglas."
    mc "Es wäre eine Art Weihnachten zu feiern, ohne die Regeln zu brechen."

# game/christmas.rpy:826
translate german christmas_shogukuni_02_3c48199f:

    # mc "O al menos haciendo algo poco grandilocuente para pasar desapercibidos."
    mc "Oder zumindest etwas weniger auffällig, um nicht aufzufallen."

# game/christmas.rpy:828
translate german christmas_shogukuni_02_f3ed767e:

    # y "¡Bien!"
    y "Gut!"

# game/christmas.rpy:830
translate german christmas_shogukuni_02_547e7e4c:

    # ka "Pero yo no tengo dinero para comprar regalos."
    ka "Aber ich habe kein Geld um Geschenke zu kaufen."

# game/christmas.rpy:832
translate german christmas_shogukuni_02_ed9564ba:

    # mc "No es necesario gastar dinero, puede ser algo con un significado emocional."
    mc "Es ist nicht wichtig Geld auszugeben, es kann etwas von emotionaler Bedeutung sein."

# game/christmas.rpy:834
translate german christmas_shogukuni_02_f3e02748:

    # ka "¿Incluso podría ser una piedra?"
    ka "Könnte es auch ein Stein sein?"

# game/christmas.rpy:836
translate german christmas_shogukuni_02_b3aed7db:

    # mc "Regalar algo como eso sería deprimente."
    mc "So etwas jemandem zu schenken, ist ziemlich depressiv."

# game/christmas.rpy:838
translate german christmas_shogukuni_02_ea7fa190:

    # shi "Si me regalas una piedra te la meteré por donde no llega el sol, Kaede."
    shi "Wenn du mir einen Stein gibst Kaede, schiebe ich Ihn dir dort, wo die Sonne nicht scheint!"

# game/christmas.rpy:840
translate german christmas_shogukuni_02_849fedd4:

    # ka "Nota mental, debo tener cuidado si mi regalo es para Shizuka o terminara metido en mi trasero."
    ka "Hinters Ohr geschrieben, ich muss mit meiner Geschenkwahl aufpassen, oder Shizuka schiebt es mir dann im Arsch rein."

# game/christmas.rpy:841
translate german christmas_shogukuni_02_00ebb61c:

    # ka "Quizás podría intentar regalando una zanahoria."
    ka "Vielleicht könntest du versuchen, eine Karotte zu schenken."

# game/christmas.rpy:843
translate german christmas_shogukuni_02_eaaf6811:

    # shi "No sé cómo lo haces para hacerme sentir tanta vergüenza..."
    shi "Ich weiss nicht, wie du es schaffst, dass ich mich so schäme..."

# game/christmas.rpy:845
translate german christmas_shogukuni_02_af42da6b:

    # mc "Bien, aquí tengo los trozos de papel."
    mc "Okay, hier sind die vier Zettel."

# game/christmas.rpy:846
translate german christmas_shogukuni_02_a4f99463:

    # mc "Tomen uno y no deben decirlo en voz alta."
    mc "Nehmt eins und sagt es nicht laut."

# game/christmas.rpy:848
translate german christmas_shogukuni_02_f87cdf37:

    # "<Cada uno saca un papel de tu mano y tú te quedas con el último.>"
    "Jeder von euch, nimmt ein stück Papier von eurer Hand und behaltet das letzte Stück."

# game/christmas.rpy:849
translate german christmas_shogukuni_02_2b35b6fe:

    # mc "..."
    mc "..."

# game/christmas.rpy:850
translate german christmas_shogukuni_02_d4e67709:

    # mc "(Maldita suerte.)"
    mc "(Scheiss Pech.)"

# game/christmas.rpy:852
translate german christmas_shogukuni_02_95610e8a:

    # mc "(No esperaba que saliera Shizuka.)"
    mc "(Ich habe nicht Shizuka erwartet.)"

# game/christmas.rpy:854
translate german christmas_shogukuni_02_8634ac84:

    # ka "Creo que he tenido suerte."
    ka "I glaube, das Glück hat mich nicht verlassen."

# game/christmas.rpy:856
translate german christmas_shogukuni_02_e07a9c82:

    # mc "No debes decirlo en voz alta."
    mc "Sag es nicht laut."

# game/christmas.rpy:858
translate german christmas_shogukuni_02_8b08c83a:

    # y "Regalo."
    y "Geschenk."

# game/christmas.rpy:860
translate german christmas_shogukuni_02_f755799c_3:

    # shi "..."
    shi "..."

# game/christmas.rpy:861
translate german christmas_shogukuni_02_1f56f218:

    # mc "¿Qué sucede Shizuka?"
    mc "Was ist los Shizuka?"

# game/christmas.rpy:863
translate german christmas_shogukuni_02_959b75f7:

    # shi "Me voy de aquí."
    shi "Ich verschwinde von hier."

# game/christmas.rpy:865
translate german christmas_shogukuni_02_2b35b6fe_1:

    # mc "..."
    mc "..."

# game/christmas.rpy:867
translate german christmas_shogukuni_02_3e0c14ae:

    # ka "Parece que a Shizuka no le ha gustado el nombre en su papel."
    ka "Sieht so aus, als würde sich Shizuka mit Ihrem Zettel nicht zufrieden geben."

# game/christmas.rpy:868
translate german christmas_shogukuni_02_406692cb:

    # ka "Seguramente he sido yo..."
    ka "Es war vermutlich ich..."

# game/christmas.rpy:869
translate german christmas_shogukuni_02_62414c9f:

    # ka "Me pregunto que me va a regalar."
    ka "Wundert mich, was sie mir gibt."

# game/christmas.rpy:871
translate german christmas_shogukuni_02_fbdd0871:

    # mc "Bien, en unos días vamos a entregar los regalos."
    mc "Naja, in ein paar Tagen, werden wir uns die Geschenke geben."

# game/christmas.rpy:872
translate german christmas_shogukuni_02_3f48d2a5:

    # mc "Y recuerda que debe ser algo especial..."
    mc "Und vergesst nicht, es muss etwas spezielles sein..."

# game/christmas.rpy:873
translate german christmas_shogukuni_02_6533fe9c:

    # mc "No una piedra o una zanahoria."
    mc "Kein Stein, keine Karotte."

# game/christmas.rpy:875
translate german christmas_shogukuni_02_3a3b4b83:

    # ka "Eso solo aplicaba si me tocaba Shizuka."
    ka "Das wäre dann der Fall, wenn mich Shizuka anfasst."

# game/christmas.rpy:877
translate german christmas_shogukuni_02_0668591b:

    # mc "Ha sido suficiente, no deberías hablar más de la cuenta o no será sorpresa."
    mc "Genug, du solltest nicht mehr darüber reden, oder die Überraschung verfliegt."

# game/christmas.rpy:879
translate german christmas_shogukuni_02_ba84689b:

    # ka "Está bien, lo siento."
    ka "Okay, tut mir Leid."

# game/christmas.rpy:880
translate german christmas_shogukuni_02_03595c92:

    # ka "Pero estoy feliz de que no ha sido Shizuka."
    ka "Ich freue mich, dass ich nicht Shizuka habe."

# game/christmas.rpy:882
translate german christmas_shogukuni_02_71a86863_1:

    # y "..."
    y "..."

# game/christmas.rpy:884
translate german christmas_shogukuni_02_ac54284a:

    # ka "¿Por qué me miras Yui?"
    ka "Warum schaust du mich so an, Yui?"

# game/christmas.rpy:886
translate german christmas_shogukuni_02_39703855:

    # y "Nada."
    y "Nichts."

# game/christmas.rpy:888
translate german christmas_shogukuni_02_8bb80ac5:

    # mc "(Seguramente se ha dado cuenta de que yo tengo a Shizuka por descarte.)"
    mc "(Bestimmt hast du gemerkt, dass ich Shizuka habe.)"

# game/christmas.rpy:890
translate german christmas_shogukuni_02_c170b5a6:

    # ka "¡Entonces es hora de buscar un regalo!"
    ka "Na dan, Zeit um für Geschenke zu schauen."

# game/christmas.rpy:891
translate german christmas_shogukuni_02_2af43da8:

    # ka "¡Hora de la despedida!"
    ka "Zeit zum Abschied!"

# game/christmas.rpy:896
translate german christmas_shogukuni_02_ee5d2719:

    # mc "(Me pregunto cuál sería un buen regalo para Shizuka.)"
    mc "(Ich frage mich, was ein gutes Geschenk für Shizuka wäre.)"

# game/christmas.rpy:911
translate german christmas_shogukuni_03_f823e85f:

    # y "¿Árbol?"
    y "Baum?"

# game/christmas.rpy:913
translate german christmas_shogukuni_03_b8ea6394:

    # ka "Debe haber alguna excepción a la regla..."
    ka "Es muss irgend eine Ausnahme geben zu dieser Regel..."

# game/christmas.rpy:914
translate german christmas_shogukuni_03_3d816bd4:

    # ka "Me gustaría hacer un árbol de navidad en los jardines del Shogukuni."
    ka "Ich möchte einen Weihnachtsbaum schmücken im Shogukuni Garten."

# game/christmas.rpy:916
translate german christmas_shogukuni_03_99a90957:

    # y "Prohibido."
    y "Verboten."

# game/christmas.rpy:918
translate german christmas_shogukuni_03_56032638:

    # ka "Ya lo sé..."
    ka "Ich weiss..."

# game/christmas.rpy:919
translate german christmas_shogukuni_03_c75207f0:

    # ka "Es tan aburrido todo eso de la disciplina y las reglas."
    ka "Es ist alles so langweilig, dass ganze Zeugs mit Disziplin und Regeln."

# game/christmas.rpy:920
translate german christmas_shogukuni_03_a948ec3a:

    # ka "Me pregunto que sucederá dentro de unos años..."
    ka "Wundere mich, wie es nach ein paar Jahren aussieht..."

# game/christmas.rpy:921
translate german christmas_shogukuni_03_45988689:

    # ka "No me gustaría convertirme en una mujer seria y dura como la maestra Felicia."
    ka "Ich will nicht, eine seriöse und harte Frau wie Meisterin Felicia werden."

# game/christmas.rpy:923
translate german christmas_shogukuni_03_cd30d4f4:

    # mc "¿Quieren decorar un árbol de navidad?"
    mc "Möchtest du einen Weihnachtsbaum dekorieren?"

# game/christmas.rpy:925
translate german christmas_shogukuni_03_1eb3c84b:

    # y "Si."
    y "Ja."

# game/christmas.rpy:927
translate german christmas_shogukuni_03_c3c18e70:

    # mc "Puedo darles una mano si necesitan ayuda."
    mc "Ich kann dir zu Hand kommen wenn du Hilfe brauchst."

# game/christmas.rpy:929
translate german christmas_shogukuni_03_08faadb5:

    # ka "Me gustaría poder aceptar tu amabilidad, pero no quiero ganarme un castigo."
    ka "Ich schätze deine Nettigkeit an, aber ich möchte nicht dafür bestraft werden."

# game/christmas.rpy:930
translate german christmas_shogukuni_03_ec395ac1:

    # ka "Shizuka dijo que sería una buena idea hacer un árbol de navidad."
    ka "Shizuka hat gesagt, es wäre eine gute Idee einen Weihnachtsbaum zu dekorieren."

# game/christmas.rpy:931
translate german christmas_shogukuni_03_d63771b2:

    # ka "Y entregar los regalos allí cuando llegue el momento."
    ka "Und danach die Geschenke zu übergeben, wenn die Zeit kommt."

# game/christmas.rpy:933
translate german christmas_shogukuni_03_8c88ac4e:

    # mc "¿Shizuka propuso algo como eso?"
    mc "Hat Shizuka das vorgeschlagen?"

# game/christmas.rpy:935
translate german christmas_shogukuni_03_2510144d:

    # ka "Parece que se le ha zafado un tornillo."
    ka "Glaube hat sie eine Schraube locker."

# game/christmas.rpy:936
translate german christmas_shogukuni_03_1389d5d1:

    # ka "Quizás tantos entrenamientos y golpes en la cabeza le dañaron el cerebro."
    ka "Vielleicht hat Ihr das ganze training, Gehirnschäden gegeben."

# game/christmas.rpy:938
translate german christmas_shogukuni_03_231055b3:

    # mc "Vaya, no esperaba eso de Shizuka."
    mc "Wow, dass habe ich jetzt nicht von Shizuka erwartet."

# game/christmas.rpy:939
translate german christmas_shogukuni_03_6872f3a6:

    # mc "¿Y a ti no te gusta esa idea?"
    mc "Und du hast die Idee nicht gerne?"

# game/christmas.rpy:941
translate german christmas_shogukuni_03_3ff5be7a:

    # ka "Por un lado suena a mucho trabajo y arriesgarse a un castigo tampoco es muy alentador."
    ka "Aus anderer Sicht, hört sich das ziemlich nach viel Arbeit an. Das Risiko für eine grosse Strafe, macht es leider auch nicht attraktiv."

# game/christmas.rpy:942
translate german christmas_shogukuni_03_b64580e8:

    # ka "Pero por otro significa que podremos agregar caramelos y chocolates a la decoración."
    ka "Das bedeutet aber auch, dass wir Schokolade und Süssigkeiten als Dekoration benutzen können."

# game/christmas.rpy:943
translate german christmas_shogukuni_03_c7fa0c29:

    # ka "Y así me podría tragar toda esa azúcar mientras nadie está vigilando."
    ka "Dann könnte ich, dass ganze Zucker essen während keiner zusieht."

# game/christmas.rpy:945
translate german christmas_shogukuni_03_9b863fd9:

    # mc "¿No sería más sencillo comprar chocolate directamente?"
    mc "Wäre es nicht einfacher, Schokolade direkt zu kaufen?"

# game/christmas.rpy:947
translate german christmas_shogukuni_03_a8d590dd:

    # ka "Este pueblo es tan pequeño que la maestra se enteraría de que he conseguido dulces sin su permiso."
    ka "Diese Stadt ist so klein, dass der Meister sofort herausfindet, dass ich Zucker hatte ohne Ihre Erlaubnis."

# game/christmas.rpy:948
translate german christmas_shogukuni_03_05d18a33:

    # ka "Al menos aquí puedo utilizar la decoración como una excusa."
    ka "Zumindest hier kann ich die Dekoration als Ausrede benutzen."

# game/christmas.rpy:950
translate german christmas_shogukuni_03_72b735eb:

    # ka "Y los castigos por no cuidar nuestra alimentación son severos."
    ka "Und dann noch die extra Strafe, dass wir nicht auf unsere Diet aufgepasst haben, ist extrem schlimm."

# game/christmas.rpy:951
translate german christmas_shogukuni_03_ff46f81f:

    # ka "Eso significa que el azúcar está prohibido."
    ka "Das bedeutet, dass Zucker komplett verboten ist."

# game/christmas.rpy:952
translate german christmas_shogukuni_03_3633bd09:

    # ka "Y sobre el árbol..."
    ka "Und wegen dem Baum..."

# game/christmas.rpy:953
translate german christmas_shogukuni_03_6cabf6af:

    # ka "Si Yui dice que no es posible, entonces no hay nada que hacer."
    ka "Wenn Yui sagt, dass es nicht möglich ist, dann können wir nichts dagegen unternehmen."

# game/christmas.rpy:955
translate german christmas_shogukuni_03_fa901953:

    # y "Perdón."
    y "Tut mir Leid."

# game/christmas.rpy:957
translate german christmas_shogukuni_03_fefda1bf:

    # ka "Tampoco deberías culparte."
    ka "Du solltest dich nicht beschuldigen."

# game/christmas.rpy:958
translate german christmas_shogukuni_03_f76f4a88:

    # ka "Tú no eres la que hizo las reglas."
    ka "Du bist nicht diejenige, welche die Regeln aufgestellt hat."

# game/christmas.rpy:960
translate german christmas_shogukuni_03_c6294b8c:

    # mc "¿Quién hizo las reglas?"
    mc "Wer hat die Regeln entschieden?"

# game/christmas.rpy:962
translate german christmas_shogukuni_03_ed6fdd07:

    # ka "¿De verdad no sabes quién fue?"
    ka "Du weisst nicht, wer das war?"

# game/christmas.rpy:963
translate german christmas_shogukuni_03_1586799e:

    # ka "¡Pero si es obvio!"
    ka "Es ist doch klar!"

# game/christmas.rpy:964
translate german christmas_shogukuni_03_f1ab3ff6:

    # ka "Se trata de esa persona que todos conocen..."
    ka "Es geht um die Person die jeder kennt..."

# game/christmas.rpy:966
translate german christmas_shogukuni_03_1dbc31d7:

    # mc "No tienes ni idea."
    mc "Du hast keine Ahnung, oder?"

# game/christmas.rpy:968
translate german christmas_shogukuni_03_0f62fe22:

    # ka "No."
    ka "Nein."

# game/christmas.rpy:970
translate german christmas_shogukuni_03_71a86863:

    # y "..."
    y "..."

# game/christmas.rpy:972
translate german christmas_shogukuni_03_dcc7d382:

    # y "Yo tampoco."
    y "Ich auch nicht."

# game/christmas.rpy:974
translate german christmas_shogukuni_03_79f690fd:

    # mc "¿De verdad ninguna de ustedes sabe quien invento estas reglas?"
    mc "Weiss wirklich niemand von euch, wer diese Regeln hier aufgestellt hat?"

# game/christmas.rpy:975
translate german christmas_shogukuni_03_bac9eaa2:

    # mc "Y aun así deciden obedecer todo al pie de la letra."
    mc "Und trotzdem beschliessen die, alles bis zum letzten Punkt zu befolgen."

# game/christmas.rpy:977
translate german christmas_shogukuni_03_8e871dbb:

    # ka "Se trata de seguir las reglas lo suficiente para no ganarme un castigo."
    ka "Es geht darum, die Regeln so weit zu befolgen, dass man keine Strafe bekommt."

# game/christmas.rpy:978
translate german christmas_shogukuni_03_b0a19da9:

    # ka "No importa si las ha inventado Juan o Pascual."
    ka "Es geht nicht darum ob John, oder Dominique diese aufgestellt hat."

# game/christmas.rpy:979
translate german christmas_shogukuni_03_2428608e:

    # ka "Son las antiguas leyes de la familia y es lo único que importa."
    ka "Es sind die alten Gesetze der Familie, und nur das zählt."

# game/christmas.rpy:981
translate german christmas_shogukuni_03_f7fa1055:

    # mc "Entonces podemos buscar un vacío legal."
    mc "Dann können wir nach einer Lücke suchen."

# game/christmas.rpy:983
translate german christmas_shogukuni_03_18578f84:

    # ka "¿Un vacío legal señor juez?"
    ka "Eine Gesetzeslücke, Euer Ehren?"

# game/christmas.rpy:985
translate german christmas_shogukuni_03_b0586328:

    # mc "Podemos transformar las reglas a nuestro beneficio."
    mc "Wir können die Regeln zu unserem Vorteil nutzen."

# game/christmas.rpy:987
translate german christmas_shogukuni_03_469dc39c:

    # ka "¡Protesto!"
    ka "Einspruch!"

# game/christmas.rpy:989
translate german christmas_shogukuni_03_5a7c104f:

    # mc "¿No te gusta la idea?"
    mc "Magst du die Idee nicht?"

# game/christmas.rpy:991
translate german christmas_shogukuni_03_ca017a7b:

    # ka "¡Si incluye dulces y chocolates entonces pueden contar conmigo!"
    ka "Wenn es Süssigkeiten und Schokolade dazu gibt, kannst du auf mich Zählen!"

# game/christmas.rpy:993
translate german christmas_shogukuni_03_d27d5185:

    # y "¿Idea?"
    y "Idee?"

# game/christmas.rpy:995
translate german christmas_shogukuni_03_a9a2d193:

    # mc "Las reglas dicen que no podemos decorar un árbol de navidad en los jardines."
    mc "Die Regeln sagen, dass wir, keinen Weihnachtsbaum im Garten dekorieren dürfen."

# game/christmas.rpy:996
translate german christmas_shogukuni_03_1d646398:

    # mc "Pero no hay una regla que diga que no podemos traer todo sobre nuestros hombros."
    mc "Aber es gibt keine Regel, die uns aufhält alles auf unseren Schultern zu tragen."

# game/christmas.rpy:998
translate german christmas_shogukuni_03_97e74377:

    # ka "Creo que entendí lo que quieres decir, pero sufrí una diarrea mental y me perdí por completo."
    ka "Ich glaube ich habe verstanden was du meintest, leider habe ich gerade einen Mentalen Zusammenbruch gekriegt und bin jetzt komplett vom Faden gekommen."

# game/christmas.rpy:999
translate german christmas_shogukuni_03_37d88d60:

    # ka "¿Piensas traer un árbol de navidad sobre tus hombros y sostenerlo allí para que no toque el suelo del Shogukuni?"
    ka "Planst du einen Weihnachtsbaum auf deinen Schultern zu tragen? Dann noch die ganze Zeit zu tragen, ohne dabei den Boden von Shogukuni zu berühren?"

# game/christmas.rpy:1000
translate german christmas_shogukuni_03_bcb616d4:

    # ka "Ni siquiera Shizuka con un cabreo demoniaco tendría tanta fuerza como para levantar un árbol."
    ka "Nicht einmal Shizuka mit ihrer teuflischen Wut hätte genug Kraft, um einen Baum hochzuheben."

# game/christmas.rpy:1002
translate german christmas_shogukuni_03_e2740d91:

    # mc "No sé que fue lo que te fumaste para sacar de contexto todo."
    mc "Ich weiss nicht, was du gerade geraucht hast, um alles falsch zu verstehen."

# game/christmas.rpy:1004
translate german christmas_shogukuni_03_68bb9416:

    # ka "Yo no he fumado nada, no sé dé que estás hablando."
    ka "Ich habe nichts zu mir genommen, ich verstehe einfach nicht was du laberst."

# game/christmas.rpy:1006
translate german christmas_shogukuni_03_d424fbce:

    # mc "Me refería a que no sé dé donde sacas tanta imaginación."
    mc "Ich meinte damit, dass ich nicht verstehe, woher du soviel Vorstellungen kriegst."

# game/christmas.rpy:1008
translate german christmas_shogukuni_03_000f16db:

    # ka "¿Entonces como piensas sostener todo ese peso sobre tus hombros?"
    ka "Also, wie planst du, dass ganze Gewicht auf deine Schultern zu tragen?"

# game/christmas.rpy:1010
translate german christmas_shogukuni_03_4deaaf12:

    # mc "Me refería a hacer ropa especial para navidad."
    mc "Ich habe dabei gedacht, spezielle Kleider für Weihnachten zu machen."

# game/christmas.rpy:1011
translate german christmas_shogukuni_03_a9cdbffc:

    # mc "Ropa navideña para las Dragonas."
    mc "Weihnachtskostüme für die Drachen."

# game/christmas.rpy:1013
translate german christmas_shogukuni_03_e3cdd938:

    # ka "¿Algo así como convertirnos a nosotras en árboles de navidad?"
    ka "Demnach uns zu Weihnachtsbäumen zu verwandeln?"

# game/christmas.rpy:1015
translate german christmas_shogukuni_03_f8c7c48a:

    # y "¡Trajes!"
    y "Kostüme!"

# game/christmas.rpy:1017
translate german christmas_shogukuni_03_5a8d6d4b:

    # ka "No logro visualizarlo."
    ka "Kann ich mir nicht vorstellen."

# game/christmas.rpy:1021
translate german christmas_shogukuni_03_0578716f:

    # y "¡Reno!"
    y "Rentier!"

# game/christmas.rpy:1023
translate german christmas_shogukuni_03_f98b48cf:

    # mc "Yui ya tiene un traje de reno."
    mc "Yui hat schon ein Rentier Kostüm."

# game/christmas.rpy:1025
translate german christmas_shogukuni_03_2cafa75a:

    # ka "¿De verdad Yui?"
    ka "Stimm das, Yui?"

# game/christmas.rpy:1027
translate german christmas_shogukuni_03_6a11be0a:

    # y "¡Si!"
    y "Ja!"

# game/christmas.rpy:1029
translate german christmas_shogukuni_03_46b22c6e:

    # ka "¿Por que no me habias dicho nada?"
    ka "Warum hast du mir nichts gesagt?"

# game/christmas.rpy:1031
translate german christmas_shogukuni_03_49b87914:

    # y "Secreto."
    y "Geheimnis."

# game/christmas.rpy:1034
translate german christmas_shogukuni_03_38a6a75c:

    # mc "Entonces debería comenzar buscando un traje para Yui."
    mc "Dann schaue ich mal, ein neues Kostüm für Yui zu finden."

# game/christmas.rpy:1035
translate german christmas_shogukuni_03_b77fe8e7:

    # mc "Seguramente en la tienda de Kamen Rainor encuentre algo."
    mc "Bestimmt hat es im Kamen Rainer Laden etwas für dich."

# game/christmas.rpy:1037
translate german christmas_shogukuni_03_14d6f88a:

    # y "¡Gracias!"
    y "Danke!"

# game/christmas.rpy:1039
translate german christmas_shogukuni_03_ce712f6c:

    # mc "Es un placer Yui."
    mc "Immer gerne, Yui."

# game/christmas.rpy:1042
translate german christmas_shogukuni_03_90c51dcd:

    # ka "Aunque primero deberías preguntar a la señora gruñona si esta de acuerdo."
    ka "Du solltest aber zuerst die alte Schachtel fragen, ob Sie damit einverstanden ist."

# game/christmas.rpy:1044
translate german christmas_shogukuni_03_865a6ed5:

    # mc "¿A Felicia?"
    mc "Felicia?"

# game/christmas.rpy:1046
translate german christmas_shogukuni_03_d81e4688:

    # ka "Me refería a Shizuka."
    ka "Ich meine Shizuka."

# game/christmas.rpy:1047
translate german christmas_shogukuni_03_60bcee27:

    # ka "La maestra es una mujer frígida y despiadada, pero no gruñona como Shizuka."
    ka "Die Herrin ist eine kaltherzige und skrupellose Frau, aber nicht mürrisch wie Shizuka."

# game/christmas.rpy:1048
translate german christmas_shogukuni_03_ff073ff9:

    # ka "Así que te deseo suerte con eso."
    ka "Also wünsche ich dir Glück."

# game/christmas.rpy:1049
translate german christmas_shogukuni_03_80e260b8:

    # ka "Porque no creo que sea fácil convencerla."
    ka "Weil ich glaube, dass es nicht einfach ist, sie zu Übereden."

# game/christmas.rpy:1051
translate german christmas_shogukuni_03_f19226df:

    # mc "No pierdo nada con hacer el intento."
    mc "Ich hab da gar nichts zu verlieren, indem ich es versuche."

# game/christmas.rpy:1052
translate german christmas_shogukuni_03_96d0ef85:

    # mc "Además fue ella la que tuvo la idea de decorar un árbol de navidad en primer lugar."
    mc "Sie war ja auch diejenige, mit der Idee einen Weihnachtsbaum zu Dekorieren. Nicht wahr?"

# game/christmas.rpy:1054
translate german christmas_shogukuni_03_462e631b:

    # ka "Nunca sabes qué esperar de Minitetitas Mcgruñona."
    ka "Du weisst nie, was von Minititten McBrett kommt."

# game/christmas.rpy:1056
translate german christmas_shogukuni_03_147a2809:

    # mc "Entonces veré que resulta de todo esto."
    mc "Dann mal schauen was passiert."

# game/christmas.rpy:1057
translate german christmas_shogukuni_03_ba38c925:

    # mc "Nos vemos en otro momento."
    mc "Dann sehen wir uns später!"

# game/christmas.rpy:1059
translate german christmas_shogukuni_03_afa8b9de:

    # y "¡Suerte!"
    y "Glück!"

# game/christmas.rpy:1064
translate german christmas_shogukuni_03_751a8b68:

    # mc "(Aunque antes de convencer a Shizuka me gustaria tener los trajes listos.)"
    mc "(Bevor ich aber Shizuka überrede, sollte ich die Kostüme zuerst bereit haben."

# game/christmas.rpy:1078
translate german christmas_shogukuni_04_9dd089ad:

    # Y "¡[nombre]!"
    Y "[nombre]!"

# game/christmas.rpy:1080
translate german christmas_shogukuni_04_9bc28877:

    # mc "Hola Yui."
    mc "Hi Yui."

# game/christmas.rpy:1082
translate german christmas_shogukuni_04_3094fb08:

    # y "Trajes."
    y "Kostüme."

# game/christmas.rpy:1084
translate german christmas_shogukuni_04_14f2ef44:

    # mc "Por ahora solo tenemos un traje de navidad para ti."
    mc "Zurzeit haben wir nur ein Weihnachtskostüm für dich."

# game/christmas.rpy:1086
translate german christmas_shogukuni_04_abb216e0:

    # y "¿Kaede?"
    y "Kaede?"

# game/christmas.rpy:1087
translate german christmas_shogukuni_04_6464c89b:

    # y "¿Shizuka?"
    y "Shizuka?"

# game/christmas.rpy:1089
translate german christmas_shogukuni_04_7810a239:

    # mc "Tendremos que pensar en alguna alternativa para ellas."
    mc "Wir müssen für die beiden auch eine Alternative finden."

# game/christmas.rpy:1091
translate german christmas_shogukuni_04_35a44d19:

    # ka "¿Nani?"
    ka "Nani?"

# game/christmas.rpy:1092
translate german christmas_shogukuni_04_5599090a:

    # y "Hola, Kaede."
    y "Hallo, Kaede."

# game/christmas.rpy:1093
translate german christmas_shogukuni_04_bdfb8e03:

    # mc "¿Podrías bajarte de mi espalda?"
    mc "Könntest du bitte von meinem Rücken runtergehen?"

# game/christmas.rpy:1094
translate german christmas_shogukuni_04_8cf9a6fc:

    # ka "¡Hai!"
    ka "Hai!"

# game/christmas.rpy:1096
translate german christmas_shogukuni_04_78926a7c:

    # ka "¡Te ves adorable con esa ropa!"
    ka "Du siehst süss aus in diesen Kleider!"

# game/christmas.rpy:1097
translate german christmas_shogukuni_04_fc026e59:

    # ka "No pensaba que iba en serio lo de hacer trajes de navidad."
    ka "Ich wusste nicht, dass er es ernst meint, Weihnachtskleider selber zu machen."

# game/christmas.rpy:1098
translate german christmas_shogukuni_04_ef72a475:

    # ka "¡Pero esto es aún mejor de lo que imaginaba!"
    ka "Aber das ist besser, als ich mir vorgestellt habe!"

# game/christmas.rpy:1099
translate german christmas_shogukuni_04_b82e64e3:

    # ka "Incluso creo que empiezo a sentir un poco de envidia..."
    ka "Ich glaube sogar, dass ich langsam ein bisschen neidisch werde..."

# game/christmas.rpy:1101
translate german christmas_shogukuni_04_8764affd:

    # ka "¿Me trajiste un traje para mí [nombre]?"
    ka "Hast du ein Kleid für mich besorgt [nombre]?"

# game/christmas.rpy:1103
translate german christmas_shogukuni_04_1a2c0989:

    # mc "Sobre eso..."
    mc "Wegen dem..."

# game/christmas.rpy:1105
translate german christmas_shogukuni_04_fa0ca4fd:

    # ka "¿Es tan bonito como el de Yui?"
    ka "Ist es so schön wie das von Yui?"

# game/christmas.rpy:1107
translate german christmas_shogukuni_04_184a5c3d:

    # mc "Aún no tengo nada."
    mc "Bis jetzt nichts."

# game/christmas.rpy:1109
translate german christmas_shogukuni_04_2d8a71d6:

    # ka "¿Queeeeeeeee?"
    ka "Waaaaaaaas?"

# game/christmas.rpy:1110
translate german christmas_shogukuni_04_0559d389:

    # ka "¡Te has olvidado del traje navideño para tu maestra!"
    ka "Du hast das Weihnachtskostüm von deiner Meisterin vergessen!"

# game/christmas.rpy:1111
translate german christmas_shogukuni_04_c9a55726:

    # ka "Mi corazón se ha roto en mil pedazos..."
    ka "Mein Herz ist in tausend Stücke gebrochen..."

# game/christmas.rpy:1112
translate german christmas_shogukuni_04_947b047d:

    # ka "Y esos mil pedazos se han pulverizado hasta convertirse en polvo."
    ka "Und diese tausend Stücke wurden zu Pulver zermalmt."

# game/christmas.rpy:1114
translate german christmas_shogukuni_04_a9d30f05:

    # ka "Me siento tan deprimida que empezaré a comer y llorar..."
    ka "Ich fühle mich so depressiv, dass ich wieder essen und weinen möchte..."

# game/christmas.rpy:1115
translate german christmas_shogukuni_04_fb6bbab6:

    # ka "Hasta calmar mi tristeza y ansiedad..."
    ka "Um meine Traurigkeit und Angstzustände zu beruhigen..."

# game/christmas.rpy:1116
translate german christmas_shogukuni_04_d63bfdfc:

    # ka "¡Y Shizuka se va a enojar conmigo por comer más de la cuenta!"
    ka "Und Shizuka wird wütend auf mich sein, wegen meines Übergewichtes!"

# game/christmas.rpy:1117
translate german christmas_shogukuni_04_0c542a05:

    # ka "Y yo dire..."
    ka "Und dann sage ich..."

# game/christmas.rpy:1118
translate german christmas_shogukuni_04_7332461e:

    # ka "¡Todo esto es culpa de [nombre] por olvidar mi traje de navidad!"
    ka "Das ist alles [nombre]'s Schuld, weil er mein Weihnachtskostüm vergessen hat!"

# game/christmas.rpy:1120
translate german christmas_shogukuni_04_1d720dc0:

    # ka "Y luego..."
    ka "Und dann..."

# game/christmas.rpy:1121
translate german christmas_shogukuni_04_c63c8111:

    # ka "Shizuka volverá a torturarme como sucedió en el Dojo."
    ka "Wird mich Shizuka wieder foltern, genau wie es im Dojo passiert ist."

# game/christmas.rpy:1123
translate german christmas_shogukuni_04_7c90ad05:

    # mc "Creo que estás exagerando."
    mc "Ich glaube du übertreibst da etwas."

# game/christmas.rpy:1125
translate german christmas_shogukuni_04_49ddb87f:

    # ka "¿De verdad piensas que estoy exagerando [nombre]?"
    ka "Denkst du wirklich, dass ich gerade übertreibe, [nombre]?"

# game/christmas.rpy:1126
translate german christmas_shogukuni_04_7acab85d:

    # ka "Yo solo estoy reaccionando al destino cruel al que he sido arrastrada."
    ka "Ich reagiere nur auf das grausame Schicksal, in das ich hineingezogen wurde."

# game/christmas.rpy:1127
translate german christmas_shogukuni_04_0dc57f5b:

    # ka "Ahora lo único que puede borrar mi decepción... {w}Es un enorme filete... {w}A las brazas... {w}Con salsa de champiñones... {w}A la crema..."
    ka "Jetzt wär das einzige, was meinen schlechten Tag verbessern könnte... {w}Ein Riesen Steak... {w}in Barbecue Sauce... {w}Mit Pilzen... {w}A la crème..."

# game/christmas.rpy:1129
translate german christmas_shogukuni_04_71a86863:

    # y "..."
    y "..."

# game/christmas.rpy:1130
translate german christmas_shogukuni_04_50eb1f18:

    # mc "También podríamos buscar un traje para ti."
    mc "Wir könnten auch für ein Kostüm für dich schauen."

# game/christmas.rpy:1132
translate german christmas_shogukuni_04_3f6e5864:

    # ka "¿Un traje de filetes para cubrir mi cuerpo?"
    ka "Ein Steak Kostüm um meinen Körper zu bedecken?"

# game/christmas.rpy:1134
translate german christmas_shogukuni_04_c3e963ae:

    # mc "¿Acaso quieres parecerte a Lady Gaga?"
    mc "Möchtest du aussehen wie Lady Gaga?"

# game/christmas.rpy:1135
translate german christmas_shogukuni_04_cc0d22ed:

    # mc "Me refería a tu traje de navidad."
    mc "Ich meinte eher ein Weihnachtskostüm."

# game/christmas.rpy:1137
translate german christmas_shogukuni_04_85818989:

    # ka "Por supuesto, lo había olvidado."
    ka "Oh, natürlich, habs vergessen."

# game/christmas.rpy:1138
translate german christmas_shogukuni_04_95d47a8b:

    # ka "Estoy tan antojada de carne que olvidé lo de navidad."
    ka "Ich bin so hungrig auf Fleisch, dass ich komplett über Weihnachten vergessen habe."

# game/christmas.rpy:1140
translate german christmas_shogukuni_04_240b4f82:

    # y "Distraída."
    y "Abgelenkt."

# game/christmas.rpy:1142
translate german christmas_shogukuni_04_08410507:

    # ka "Mi cabeza funciona mal cuando tengo el estómago vacío."
    ka "Mein Kopf funktioniert schlecht, wenn mein Bauch leer ist."

# game/christmas.rpy:1144
translate german christmas_shogukuni_04_4469fc08:

    # ka "¿Por qué no bajamos al Yorokobi a comer algo?"
    ka "Warum gehen wir nicht zum Yorokobi und holen uns da was zu essen?"

# game/christmas.rpy:1146
translate german christmas_shogukuni_04_1a32bf94:

    # mc "¿Qué dices, Yui?"
    mc "Was meinst du, Yui?"

# game/christmas.rpy:1148
translate german christmas_shogukuni_04_c69e4cb2:

    # y "¡Hambre!"
    y "Hungrig!"

# game/christmas.rpy:1150
translate german christmas_shogukuni_04_a51a3e0a:

    # mc "Vaya..."
    mc "Na dann..."

# game/christmas.rpy:1151
translate german christmas_shogukuni_04_f13578c5:

    # mc "Aunque ir a comer no suena tan mal."
    mc "Essen zu gehen hört sich gar nicht so schlecht an."

# game/christmas.rpy:1153
translate german christmas_shogukuni_04_6a017829:

    # ka "¡Entonces vamos por un sabroso filete!"
    ka "Dann holen wir uns ein leckeres Steak!"

# game/christmas.rpy:1162
translate german christmas_shogukuni_04_a47502de:

    # ka "AWWwwwwhhhh."
    ka "AWWwwwwhhhh."

# game/christmas.rpy:1164
translate german christmas_shogukuni_04_452b5c5c:

    # ka "Siento que el estómago me va a explotar."
    ka "Ich habe das Gefühl, dass mein Bauch bald explodiert."

# game/christmas.rpy:1166
translate german christmas_shogukuni_04_8e8d40dd:

    # y "Glotona."
    y "Vielfrass."

# game/christmas.rpy:1168
translate german christmas_shogukuni_04_6392106b:

    # ka "Tampoco es necesario decir eso en voz alta, Yui."
    ka "Du musst es ja nicht laut aussprechen, Yui."

# game/christmas.rpy:1169
translate german christmas_shogukuni_04_e5c2069e:

    # ka "Tu también sentirías la barriga así de pesada después de comer tanto."
    ka "Du würdest dich auch so schwer fühlen"

# game/christmas.rpy:1171
translate german christmas_shogukuni_04_19db1da8:

    # ka "¿Tú tampoco quieres nada más [nombre]?"
    ka "Möchtest du nichts anderes, [nombre]?"

# game/christmas.rpy:1173
translate german christmas_shogukuni_04_245a8320:

    # mc "Ya me siento satisfecho."
    mc "Ich bin satt."

# game/christmas.rpy:1175
translate german christmas_shogukuni_04_7191ceb9:

    # ka "¿Ni siquiera tienes espacio para el postre?"
    ka "Nicht mal Platz für Dessert?"

# game/christmas.rpy:1176
translate german christmas_shogukuni_04_d0d6d34f:

    # ka "Siento un antojo de unos buenos panqueques con helado de vainilla bañados en chocolate."
    ka "Ich habe gerade grosses verlangen nach, Pfannkuchen mit Vanille Eiscreme, gebadet in Schokolade. "

# game/christmas.rpy:1177
translate german christmas_shogukuni_04_b0aa4f78:

    # ka "Después de comer tantas cosas saladas mi sangre necesita azúcar."
    ka "Nachdem ich soviel salziges Zeugs gegessen habe, braucht mein Blut jetzt etwas Zucker."

# game/christmas.rpy:1179
translate german christmas_shogukuni_04_b4635b60:

    # mc "No sé cómo lo haces para comer más que nosotros juntos."
    mc "Ich verstehe nicht, wie do so viel essen konntest, wie wir zusammen."

# game/christmas.rpy:1181
translate german christmas_shogukuni_04_f386de05:

    # ka "Es gracias a una técnica milenaria..."
    ka "Da danke ich der Tausendjahre alten Technik..."

# game/christmas.rpy:1182
translate german christmas_shogukuni_04_65e6ed9c:

    # ka "Transmitida de generación en generación entre los miembros del clan Shiori."
    ka "Von Generation zu Generation unter den Mitgliedern des Shiori-Clans weitergegeben."

# game/christmas.rpy:1183
translate german christmas_shogukuni_04_64e9e5a9:

    # ka "Pero que solo algunos elegidos pueden realizar de forma correcta."
    ka "Aber das können nur wenige richtig machen."

# game/christmas.rpy:1185
translate german christmas_shogukuni_04_b9d4a301:

    # mc "¿De verdad existe una técnica para comer en exceso?"
    mc "Gibt es ernsthaft eine Technik, fürs zu viel fressen?"

# game/christmas.rpy:1187
translate german christmas_shogukuni_04_343c1a22:

    # ka "Es solo digestión rápida, pero intentaba construir un poco de dramatismo."
    ka "Es ist nur schnelle Verdauung, wollte eigentlich nur etwas Drama aufbauen..."

# game/christmas.rpy:1188
translate german christmas_shogukuni_04_5204e484:

    # ka "Aunque si existiese una técnica para eso..."
    ka "Aber wenns eine Technik dafür gebe..."

# game/christmas.rpy:1189
translate german christmas_shogukuni_04_8ee7e187:

    # ka "Creo que mi apetito sería infinito."
    ka "Wäre mein Appetit unendlich gross."

# game/christmas.rpy:1191
translate german christmas_shogukuni_04_fe898339:

    # y "Trajes..."
    y "Kostüme..."

# game/christmas.rpy:1193
translate german christmas_shogukuni_04_1fa0a126:

    # ka "¿Qué trajes?"
    ka "Welche Kostüme?"

# game/christmas.rpy:1194
translate german christmas_shogukuni_04_ee9b234c:

    # ka "¡Si claro los trajes!"
    ka "Ja, natürlich, die Kostüme!"

# game/christmas.rpy:1195
translate german christmas_shogukuni_04_e558d8f6:

    # ka "¡Casi lo había olvidado!"
    ka "Ich habe es ja fast vergessen!"

# game/christmas.rpy:1196
translate german christmas_shogukuni_04_7ed8cae0:

    # ka "Vinimos aquí para hablar de los trajes de navidad."
    ka "Wir sind hierher gekommen, um über die Weihnachtskleider zu reden."

# game/christmas.rpy:1198
translate german christmas_shogukuni_04_4c308c28:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:1200
translate german christmas_shogukuni_04_d7b32594:

    # ka "Pensaba que con un gorro y unos guantes sería suficiente, pero ahora me gustaría algo más elaborado."
    ka "Ich dachte, eine Mütze und Handschuhe würden ausreichen, aber jetzt hätte ich gerne etwas Ausgefalleneres."

# game/christmas.rpy:1201
translate german christmas_shogukuni_04_763f8be7:

    # ka "Después de ver el traje de Yui, me gustaría algo así de elaborado."
    ka "Nachdem ich Yui's Kostüm gesehen habe, möchte ich auch so etwas Aufwendiges."

# game/christmas.rpy:1203
translate german christmas_shogukuni_04_e9afd775:

    # mc "También puede ser un diseño sencillo, pero con un significado especial."
    mc "Es kann auch etwas simples sein, dafür mit einer speziellen Bedeutung."

# game/christmas.rpy:1204
translate german christmas_shogukuni_04_b1eb976a:

    # mc "Ustedes son las tres Dragonas del Shogukuni y visten uniformes de acuerdo a sus rangos."
    mc "Du bist eine von den Drei Drachen von Shogukuni, Trägt eine Uniform, passend zu deinem Rang."

# game/christmas.rpy:1206
translate german christmas_shogukuni_04_2683818f:

    # ka "Espera, creo que estoy entendiendo."
    ka "Warte, ich glaube ich weiss worauf du hinaus willst."

# game/christmas.rpy:1207
translate german christmas_shogukuni_04_b3f553bf:

    # ka "Lo que tú estás diciendo es que deberíamos..."
    ka "Was du sagst ist, dass wir..."

# game/christmas.rpy:1208
translate german christmas_shogukuni_04_1c49d2a5:

    # ka "¡Usar un kimono de Santa Claus!"
    ka "Einen Weihnachtsmann Kimono anziehen! "

# game/christmas.rpy:1209
translate german christmas_shogukuni_04_7cafa84e:

    # ka "Aunque debería comer mucho más si quiero tener la barriga tan enorme como ese señor."
    ka "Ich sollte aber viel mehr essen, damit ich auch so einen grossen Bauch, wie die Gentlemen kriege."

# game/christmas.rpy:1210
translate german christmas_shogukuni_04_354b5dce:

    # ka "Y podría quedarme todos los regalos para mí y entregar solo los que no me gusten."
    ka "Und ich könnte die Geschenke alle für mich behalten, danach nur diese geben weggeben, die ich nicht gerne habe."

# game/christmas.rpy:1212
translate german christmas_shogukuni_04_b5354ecb:

    # y "Kaede..."
    y "Kaede..."

# game/christmas.rpy:1214
translate german christmas_shogukuni_04_f72a203f:

    # ka "¿Dije algo malo?"
    ka "Habe ich was falsch gesagt?"

# game/christmas.rpy:1216
translate german christmas_shogukuni_04_2ab70eab:

    # mc "Pensaba en una combinación entre el color de sus uniformes de Dragón y el traje de navidad de Yui."
    mc "Ich habe mir dabei gedacht, eine Kombination zwischen den Farben deiner Drachen Uniform und Yui's Weihnachtsoutfit."

# game/christmas.rpy:1218
translate german christmas_shogukuni_04_5608cd6c:

    # ka "En ese caso el traje de Yui debería ser de color negro y no marrón."
    ka "In dem Sinne, wäre dann Yui's Outfit schwarz, nicht braun."

# game/christmas.rpy:1220
translate german christmas_shogukuni_04_13c7bf75:

    # mc "Se supone que es un traje de Rudolf, aunque en lugar de la nariz roja, es el rojo del ojo derecho."
    mc "Es sollte ein Rudolph Kostüm sein, aber anstatt der roten Nase, ist es das rechte rote Auge von Yui."

# game/christmas.rpy:1222
translate german christmas_shogukuni_04_9028e723:

    # ka "¡Eso es genial!"
    ka "Das ist cool!"

# game/christmas.rpy:1223
translate german christmas_shogukuni_04_f12fdc75:

    # ka "Y ahora me siento como una tonta por no haber entendido el significado."
    ka "Jetzt fühle ich mich wie ein Idiot, weil ich es nicht verstanden habe."

# game/christmas.rpy:1224
translate german christmas_shogukuni_04_cf4a833a:

    # ka "¡Entonces necesitamos unos trajes de renos!"
    ka "Das heisst, wir brauchen Rentier Kostüme!"

# game/christmas.rpy:1225
translate german christmas_shogukuni_04_f12ddc7d:

    # ka "Aunque llevar esos cuernos en la cabeza suena a mucho trabajo..."
    ka "Aber das tragen der Hörner auf dem Kopf, hört sich nach sehr viel Arbeit an..."

# game/christmas.rpy:1227
translate german christmas_shogukuni_04_1af30b1c:

    # mc "¿Tienes una idea mejor?"
    mc "Hast du eine bessere Idee?"

# game/christmas.rpy:1229
translate german christmas_shogukuni_04_18bfbd82:

    # ka "Déjame pensar..."
    ka "Lass mich nachdenken..."

# game/christmas.rpy:1231
translate german christmas_shogukuni_04_6439e021:

    # y "¿Duendes?"
    y "Elfen?"

# game/christmas.rpy:1233
translate german christmas_shogukuni_04_24af4e59:

    # ka "No lo sé, Yui."
    ka "Ich weiss nicht, Yui."

# game/christmas.rpy:1234
translate german christmas_shogukuni_04_c904e876:

    # ka "El trabajo de los duendes es hacer juguetes y el de nosotras es patear traseros."
    ka "Die Arbeit der Elfen ist es Spielzeuge bereit zu machen, wir treten aber Hintern von anderen."

# game/christmas.rpy:1236
translate german christmas_shogukuni_04_fabb85b3:

    # y "¿Santa?"
    y "Weihnachtsmann?"

# game/christmas.rpy:1238
translate german christmas_shogukuni_04_331361b5:

    # ka "Podría ser un traje en miniatura del anciano de los regalos."
    ka "Es könnte eine Kleineres Kostüm, vom alten Mann der Geschenke sein."

# game/christmas.rpy:1239
translate german christmas_shogukuni_04_174d05b9:

    # ka "Uno de color rojo y otro azul, como propuso [nombre]."
    ka "Eines in rot, eines in blau, genau wie [nombre] es vorgeschlagen hat."

# game/christmas.rpy:1241
translate german christmas_shogukuni_04_9ccc73bd:

    # y "Perfecto."
    y "Perfekt."

# game/christmas.rpy:1243
translate german christmas_shogukuni_04_a90ee610:

    # ka "¿De verdad piensas que podría funcionar?"
    ka "Denkst du wirklich, dass könnte funktionieren?"

# game/christmas.rpy:1244
translate german christmas_shogukuni_04_509643be:

    # ka "También estaba pensando en un traje de nieve para mí y en uno de Yeti para la pesada de Shizuka."
    ka "Ich dachte mir einen Schneeanzug für mich, sowie einen Yeti Anzug für Shizuka's schweren..."

# game/christmas.rpy:1246
translate german christmas_shogukuni_04_98e0ee25:

    # y "Mala."
    y "Gemein."

# game/christmas.rpy:1248
translate german christmas_shogukuni_04_26246eea:

    # ka "¿Dices que yo soy la mala?"
    ka "War ich jetzt gemein?"

# game/christmas.rpy:1249
translate german christmas_shogukuni_04_1979c11c:

    # ka "¿Acaso de Shizuka no merece usar un feo traje de Yeti?"
    ka "Verdient Shizuka es nicht, einen hässlichen Yeti Kostüm anzuziehen?"

# game/christmas.rpy:1251
translate german christmas_shogukuni_04_5e1cbc1b:

    # y "Navidad feliz."
    y "Weihnachten frohe."

# game/christmas.rpy:1253
translate german christmas_shogukuni_04_e3f3c4b0:

    # ka "Ya sé que debe ser una navidad feliz..."
    ka "Ich weiss das es ein frohes Weihnachten sein muss..."

# game/christmas.rpy:1255
translate german christmas_shogukuni_04_60f8eada:

    # ka "Pero un traje de Yeti para Shizuka sería gracioso que te cagas."
    ka "Aber ein Yeti Kostüm für Shizuka, wäre doch zum Totschiessen."

# game/christmas.rpy:1257
translate german christmas_shogukuni_04_77934d6c:

    # y "Peligroso."
    y "Gefährlich."

# game/christmas.rpy:1259
translate german christmas_shogukuni_04_439e5fd1:

    # ka "Ahora que lo dices..."
    ka "Gerade wo du es erwähnst..."

# game/christmas.rpy:1260
translate german christmas_shogukuni_04_131f8a89:

    # ka "Posiblemente Shizuka haría pedazos el traje antes de usarlo."
    ka "Shizuka würde vermutlich das Kostüm zerreissen, bevor sie es anzieht."

# game/christmas.rpy:1262
translate german christmas_shogukuni_04_7c25a109:

    # mc "¿Entonces que es lo que prefieres?"
    mc "Also was schlägst du lieber vor?"

# game/christmas.rpy:1264
translate german christmas_shogukuni_04_76c36baf:

    # ka "No importa que es lo que prefiero."
    ka "Es ist egal was ich möchte."

# game/christmas.rpy:1265
translate german christmas_shogukuni_04_741a7155:

    # ka "¡Esta es una tarea perfecta para Kaede la ninja de la moda!"
    ka "Das ist eher der perfekte Auftrag für, Kaede der Fashion Ninja!"

# game/christmas.rpy:1266
translate german christmas_shogukuni_04_d778f763:

    # ka "¡Voy a hacerme de todo lo necesario para conseguir los mejores trajes de navidad!"
    ka "Ich werde mal alles holen wo ich brauche, um die besten Weihnachtskostüme zu machen!"

# game/christmas.rpy:1268
translate german christmas_shogukuni_04_b540e32f:

    # ka "¡Ejemmmm!"
    ka "Ejemmmm!"

# game/christmas.rpy:1274
translate german christmas_shogukuni_04_81bc0b55:

    # akiko "¿Necesita de mi ayuda señorita Kaede?"
    akiko "Brauchst du meine Hilfe, Fräulein Kaede?"

# game/christmas.rpy:1276
translate german christmas_shogukuni_04_18834fc3:

    # ka "Tengo una misión muy importante que entregarte Akiko."
    ka "Ich habe eine sehr wichtige Mission für dich, Akiko."

# game/christmas.rpy:1277
translate german christmas_shogukuni_04_009494bb:

    # ka "Quiero que sepas que es una situación de vida o muerte."
    ka "Ich möchte das du weisst, dass es um Leben oder Tot geht."

# game/christmas.rpy:1278
translate german christmas_shogukuni_04_d68f3974:

    # ka "Será algo muy importante para la familia y todo eso que siempre dice la maestra Felicia."
    ka "Es wird etwas sehr wichtiges, für die Familie und das ganze Zeugs, wie Meisterin Felicia das immer sagt."

# game/christmas.rpy:1280
translate german christmas_shogukuni_04_659e54b3:

    # akiko "¿Necesitaremos armamento?"
    akiko "Brauchen wir Waffen?"

# game/christmas.rpy:1282
translate german christmas_shogukuni_04_708d604d:

    # ka "Nada de armamento…"
    ka "Keine Waffen..."

# game/christmas.rpy:1283
translate german christmas_shogukuni_04_16f7aef5:

    # ka "¿Dónde dejas el espíritu navideño Akiko?"
    ka "Wo hast du deine Weihnachtsstimmung gelassen, Akiko?"

# game/christmas.rpy:1285
translate german christmas_shogukuni_04_82760949:

    # akiko "¿Entonces cuál es la misión?"
    akiko "Ja, was ist dann die Mission?"

# game/christmas.rpy:1287
translate german christmas_shogukuni_04_bd2fd9ff:

    # ka "Primero debes jurar con tu vida que la misión ultra secreta de la que te voy a hablar no saldrá de esta mesa."
    ka "Zuerst, musst du auf dein Leben schwören, dass diese Information, über die sehr geheime mission, nicht diesen Tisch verlässt."

# game/christmas.rpy:1288
translate german christmas_shogukuni_04_7c5a86b3:

    # ka "Ni siquiera la maestra Felicia puede enterarse."
    ka "Nicht einmal die Herrin Felicia, darf es erfahren."

# game/christmas.rpy:1290
translate german christmas_shogukuni_04_887ef495:

    # akiko "..."
    akiko "..."

# game/christmas.rpy:1292
translate german christmas_shogukuni_04_b8e4049d:

    # akiko "Puede confiar en mi señorita Kaede."
    akiko "Du kannst mir vertrauen, Fräulein Kaede."

# game/christmas.rpy:1294
translate german christmas_shogukuni_04_cabc0045:

    # ka "¡Entonces ahora te encomendaré tu misión!"
    ka "Dann kann ich dich ja einführen, in unserer Mission!"

# game/christmas.rpy:1296
translate german christmas_shogukuni_04_64c44eb0:

    # ka "¡Necesito unos trajes de navidad para mí y para Shizuka!"
    ka "Ich brauche ein paar Weihnachtskostüme, für mich und Shizuka!"

# game/christmas.rpy:1298
translate german christmas_shogukuni_04_5f913fc9:

    # akiko "¿T-trajes de navidad?"
    akiko "Weihnachtskostüme...?"

# game/christmas.rpy:1300
translate german christmas_shogukuni_04_b2472920:

    # ka "Uno en rojo y otro azul, más tarde te daré los detalles para los diseños y los materiales que vamos a necesitar."
    ka "Eines in rot, das andere in blau. Später gebe ich dir alle Details, die du für das Design und Material brauchst."

# game/christmas.rpy:1302
translate german christmas_shogukuni_04_887ef495_1:

    # akiko "..."
    akiko "..."

# game/christmas.rpy:1304
translate german christmas_shogukuni_04_77775734:

    # ka "¿Por qué te has quedado callada?"
    ka "Warum bist du so ruhig?"

# game/christmas.rpy:1306
translate german christmas_shogukuni_04_ae721e32:

    # akiko "¿Está segura de que esto se trata de una misión importante?"
    akiko "Bist du dir sicher, dass die Mission so ernsthaft ist?"

# game/christmas.rpy:1308
translate german christmas_shogukuni_04_8d8b46e6:

    # ka "Es una misión de vida o muerte."
    ka "Es ist eine Mission um Leben und Tot."

# game/christmas.rpy:1310
translate german christmas_shogukuni_04_1b9da2ba:

    # akiko "¿No sería más sencillo comprar unos trajes en la tienda del pueblo?"
    akiko "Wäre es nicht einfacher, die im nächsten Laden zu kaufen?"

# game/christmas.rpy:1312
translate german christmas_shogukuni_04_6f2f7a2b:

    # ka "Claro que sería más fácil, pero..."
    ka "Natürlich wäre dies einfacher, aber..."

# game/christmas.rpy:1313
translate german christmas_shogukuni_04_acb23683:

    # ka "Esos no tendrían el estilo que estoy buscando, Akiko."
    ka "Diese hätten nicht den Style, den ich gerne hätte, Akiko."

# game/christmas.rpy:1314
translate german christmas_shogukuni_04_ad88beaf:

    # ka "¿Alguna otra pregunta?"
    ka "Fragen?"

# game/christmas.rpy:1316
translate german christmas_shogukuni_04_c7c76480:

    # akiko "No señorita Kaede, tampoco buscaba cuestionar sus órdenes."
    akiko "Nein Fräulein Kaede, ich suche auch keine frage um Ihre befehle entgegen zu gehen."

# game/christmas.rpy:1318
translate german christmas_shogukuni_04_80eed87e:

    # ka "Entonces te esperaré durante la tarde en el Shogukuni para darte todos los detalles."
    ka "Denn warte ich, währenddessen beim Shogukuni am Nachmittag, um dir weitere Details zu geben."

# game/christmas.rpy:1319
translate german christmas_shogukuni_04_672161bb:

    # ka "Ahora puedes irte, Akiko."
    ka "Du kannst jetzt gehen, Akiko."

# game/christmas.rpy:1321
translate german christmas_shogukuni_04_1b2523f4:

    # akiko "Entendido, señorita Kaede."
    akiko "Verstanden, Fräulein Kaede."

# game/christmas.rpy:1323
translate german christmas_shogukuni_04_3e3211cc:

    # ka "Me hace sentir muy poderosa cuando siguen mis órdenes."
    ka "Es gibt mir ein Gefühl von Macht, wenn sie meinen Anweisungen folgen."

# game/christmas.rpy:1325
translate german christmas_shogukuni_04_71a86863_1:

    # y "..."
    y "..."

# game/christmas.rpy:1327
translate german christmas_shogukuni_04_a5dda1e4:

    # ka "¿Y ahora que hice?"
    ka "Was habe ich jetzt wieder angestellt?"

# game/christmas.rpy:1329
translate german christmas_shogukuni_04_d1133295:

    # y "Mentira."
    y "Lüge."

# game/christmas.rpy:1331
translate german christmas_shogukuni_04_bf9a4155:

    # ka "Ya sé que es una mentira lo de la misión superimportante."
    ka "Ich weiss, dass es eine Lüge ist, bezüglich der Super geheimen Mission."

# game/christmas.rpy:1332
translate german christmas_shogukuni_04_e1b4c008:

    # ka "Pero para qué voy a hacer todo el trabajo si puedo hacer que alguien más lo haga por mí."
    ka "Aber warum soll ich mir die ganze mühe machen, wenn es doch jemand einfach für mich machen kann."

# game/christmas.rpy:1333
translate german christmas_shogukuni_04_464fcd86:

    # ka "De esta forma tengo que pensar en un diseño y luego Akiko se encargara del resto."
    ka "So muss ich mich nur ums Design mühe geben, Akiko kann sich dann um den Rest kümmern."

# game/christmas.rpy:1335
translate german christmas_shogukuni_04_13767feb:

    # ka "¿No es mágico?"
    ka "Ist das nicht magisch?"

# game/christmas.rpy:1337
translate german christmas_shogukuni_04_53e2fbb0:

    # mc "Mente de tiburón."
    mc "Haifischverstand."

# game/christmas.rpy:1339
translate german christmas_shogukuni_04_afd7e5ae:

    # ka "¡Cuidado que muerdo!"
    ka "Pass auf, oder ich beisse zu!"

# game/christmas.rpy:1341
translate german christmas_shogukuni_04_16dcd7fd:

    # y "[nombre]..."
    y "[nombre]..."

# game/christmas.rpy:1343
translate german christmas_shogukuni_04_ecc46fc6:

    # mc "Dejemos que Kaede se encargue del asunto."
    mc "Lass Kaede dies auf eigene Faust erledigen."

# game/christmas.rpy:1344
translate german christmas_shogukuni_04_35548f86:

    # mc "Parece que sabe lo que tiene que hacer."
    mc "Du weisst schon selbst, was du zu tun hast."

# game/christmas.rpy:1346
translate german christmas_shogukuni_04_c1fd11ef:

    # ka "¡Entonces ya tenemos un plan!"
    ka "Dann haben wir einen Plan!"

# game/christmas.rpy:1347
translate german christmas_shogukuni_04_0731ce51:

    # ka "Cuando los trajes hayan sido terminados, ven al Shogukuni para que hagamos la segunda fase de la misión."
    ka "Wenn die Kostüme bereit sind, komm zum Shogukuni für die zweite Phase der Mission."

# game/christmas.rpy:1349
translate german christmas_shogukuni_04_2db7c2fe:

    # mc "¿Segunda fase?"
    mc "Zweite Phase?"

# game/christmas.rpy:1351
translate german christmas_shogukuni_04_ec7d1807:

    # ka "Tendrás que encargarte de convencer a Shizuka de usar el traje que voy a diseñar."
    ka "Du bist verantwortlich, Shizuka dazu zu überzeugen das Kleid anzuziehen, was ich dann gestalte."

# game/christmas.rpy:1352
translate german christmas_shogukuni_04_e8adad4f:

    # ka "Si soy yo la que se lo dice..."
    ka "Wenn ich es ihr sage..."

# game/christmas.rpy:1353
translate german christmas_shogukuni_04_d34b80db:

    # ka "Lo va a enrollar y me lo meterá en el trasero."
    ka "Wird sie es zusammenrollen und in meinem Arsch schieben."

# game/christmas.rpy:1355
translate german christmas_shogukuni_04_1b187bd7:

    # mc "Está bien, no hay problema."
    mc "Sollte machbar sein, kein Problem."

# game/christmas.rpy:1357
translate german christmas_shogukuni_04_e66ad432:

    # ka "¡Genial!"
    ka "Super!"

# game/christmas.rpy:1358
translate german christmas_shogukuni_04_e3edd9e5:

    # ka "Y antes de despedirnos quería hacerle una pregunta a ustedes dos."
    ka "And bevor wir uns verabschieden, habe ich noch zwei fragen an dich."

# game/christmas.rpy:1359
translate german christmas_shogukuni_04_a1db0064:

    # ka "¿Ya tienen listos los regalos para el amigo secreto?"
    ka "Hast du schon ein Geschenk für deinen Beschenkten bereit?"

# game/christmas.rpy:1361
translate german christmas_shogukuni_04_6a11be0a:

    # y "¡Si!"
    y "Ja!"

# game/christmas.rpy:1363
translate german christmas_shogukuni_04_f8db5935:

    # mc "Bueno, yo..."
    mc "Naja, ich..."

# game/christmas.rpy:1365
translate german christmas_shogukuni_04_2b650632:

    # ka "¿Tampoco has tenido tiempo para encontrar el regalo perfecto [nombre]?"
    ka "Konntest demnach auch noch nicht, dass perfekte Geschenk finden, nicht wahr [nombre]?"

# game/christmas.rpy:1366
translate german christmas_shogukuni_04_97046b3f:

    # ka "¡Entonces no soy la única!"
    ka "Dann bin ich nicht die einzige!"

# game/christmas.rpy:1367
translate german christmas_shogukuni_04_729d9bc8:

    # ka "No sabía que encontrar el regalo perfecto para el amigo secreto podría ser tan difícil."
    ka "Ich wusste nicht, dass es so schwierig ist, ein Geschenk für jemand anderen zu finden."

# game/christmas.rpy:1368
translate german christmas_shogukuni_04_f70f9c1e:

    # ka "¡Y quiero que mi regalo sea el mejor de todos!"
    ka "Und ich möchte, dass es das beste Geschenk von allen ist!"

# game/christmas.rpy:1370
translate german christmas_shogukuni_04_51d17a5b:

    # mc "La intención no era hacer una competencia por quien hace el mejor regalo."
    mc "Der Sinn ist nicht dabei, einen Wettstreit anzufangen, um zu sagen, wer das beste Geschenk hat."

# game/christmas.rpy:1372
translate german christmas_shogukuni_04_36e728af:

    # ka "¡Ya lo sé!"
    ka "Ich weiss!"

# game/christmas.rpy:1373
translate german christmas_shogukuni_04_0b451690:

    # ka "El espíritu navideño y todo eso, y que lo más importante es compartir."
    ka "Weihnachtsstimmung und all das, und dass das Wichtigste ist, zu teilen."

# game/christmas.rpy:1375
translate german christmas_shogukuni_04_faeb7af3:

    # ka "Pero mi regalo va a ser el mejor de todos."
    ka "Aber mein Geschenk, wird das allerbeste von allen sein!"

# game/christmas.rpy:1377
translate german christmas_shogukuni_04_50a7d610:

    # ka "¡Ya que me ha salido el mejor amigo secreto!"
    ka "Nachdem ich jetzt, den besten Beschenkten bekommen habe!"

# game/christmas.rpy:1378
translate german christmas_shogukuni_04_5203d262:

    # ka "Y ahora me despido porque tengo muchas cosas de las que ocuparme."
    ka "Und jetzt verabschiede ich mich, weil ich noch viel zu erledigen habe."

# game/christmas.rpy:1380
translate german christmas_shogukuni_04_646f53f9:

    # ka "¡Sayonara!"
    ka "Sayonara!"

# game/christmas.rpy:1385
translate german christmas_shogukuni_04_fce2f244:

    # "<Después de despedirte de Kaede y Yui vuelves a tu habitación.>"
    "<Nachdem du dich von Kaede und Yui verabschiedet hast, kehrst du in dein Zimmer zurück.>"

# game/christmas.rpy:1399
translate german christmas_shogukuni_05_71dc61ae:

    # ka "¡Misión cumplida!"
    ka "Mission abgeschlossen!"

# game/christmas.rpy:1401
translate german christmas_shogukuni_05_98eecb70:

    # mc "¿Eso significa que ya tienes los trajes de navidad?"
    mc "Heisst das, du hast die Weihnachtsoutfits schon?"

# game/christmas.rpy:1403
translate german christmas_shogukuni_05_138a3435:

    # ka "Por supuesto que si, pequeño saltamontes."
    ka "Natürlich haben wir das, kleiner Grashüpfer."

# game/christmas.rpy:1404
translate german christmas_shogukuni_05_fc4b68a2:

    # ka "No hay ninguna misión que la Gran Kaede no pueda realizar."
    ka "Es gibt keine Mission, welche die Grosse Kaede nicht erledigen kann."

# game/christmas.rpy:1406
translate german christmas_shogukuni_05_ca0750c5:

    # mc "¿Estás segura de haber sido tú y no Akiko?"
    mc "Bist du sicher, dass du es warst, und nicht Akiko?"

# game/christmas.rpy:1408
translate german christmas_shogukuni_05_d6e92386:

    # ka "Akiko ha hecho el trabajo sucio, pero fui yo la que tenía el liderazgo necesario para completar el objetivo."
    ka "Akiko hat nur die dreckige Arbeit erledigt, aber ich war die jenige mit dem Lead, um diesen Auftrag zu erledigen."

# game/christmas.rpy:1409
translate german christmas_shogukuni_05_9f296d08:

    # ka "Y no tienes que olvidar que fui yo la que hizo los diseños."
    ka "Und du darfst nicht vergessen, dass ich ja das Design gemacht habe."

# game/christmas.rpy:1410
translate german christmas_shogukuni_05_d4130a92:

    # ka "Si quieres ver como me queda tendrás que venir conmigo."
    ka "Wenn du sehen willst, wie es aussieht, musst du mit mir mitkommen."

# game/christmas.rpy:1411
translate german christmas_shogukuni_05_f2bca351:

    # ka "Necesitaré que me des una mano para afinar los detalles."
    ka "Du musst mir schnell dabei helfen, die Details ein bisschen anzupassen."

# game/christmas.rpy:1412
translate german christmas_shogukuni_05_15cdf652:

    # ka "¡Marchando!"
    ka "Komme!"

# game/christmas.rpy:1422
translate german christmas_shogukuni_05_2b35b6fe:

    # mc "..."
    mc "..."

# game/christmas.rpy:1423
translate german christmas_shogukuni_05_42b36c73:

    # mc "¿Cuánto tiempo tengo que seguir esperando?"
    mc "Wie lange soll ich noch warten?"

# game/christmas.rpy:1425
translate german christmas_shogukuni_05_b9d7a3c7:

    # ka "Unos segundos más, lo juro."
    ka "Ein paar Sekunden noch, ich schwöre."

# game/christmas.rpy:1426
translate german christmas_shogukuni_05_0927f353:

    # ka "Estaba añadiendo algunos adornos hace unas horas y ahora ha desaparecido."
    ka "Ich habe vor ein paar Stunden einige Verzierungen hinzugefügt, und jetzt sind sie verschwunden."

# game/christmas.rpy:1427
translate german christmas_shogukuni_05_9933c6a5:

    # ka "Solo veo el traje de Shizuka, pero no logro recordar en que lugar ha quedado el mío."
    ka "Ich sehe nur Shizukas Anzug, aber ich kann mich nicht erinnern, wo meiner geblieben ist."

# game/christmas.rpy:1428
translate german christmas_shogukuni_05_15eadded:

    # ka "Sería una lástima arruinar la sorpresa por un descuido..."
    ka "Es wäre schade, die Überraschung durch ein Versehen zu ruinieren..."

# game/christmas.rpy:1429
translate german christmas_shogukuni_05_75eba35d:

    # ka "Y además con este frío siento que se me congelaran las tetas."
    ka "Ausserdem habe ich bei dieser Kälte das Gefühl, dass meine Titten frieren."

# game/christmas.rpy:1431
translate german christmas_shogukuni_05_8fad2ab6:

    # mc "¿Por qué no te pones ropa y vamos a buscar?"
    mc "Warum ziehst du dir keine Kleider an, danach können wir es zusammen suchen."

# game/christmas.rpy:1433
translate german christmas_shogukuni_05_12df2096:

    # ka "He estado en muchos lugares durante en las últimas horas..."
    ka "Ich war in sehr vielen Orten, die letzten paar Stunden..."

# game/christmas.rpy:1434
translate german christmas_shogukuni_05_8dc81f45:

    # ka "El traje podría estar en cualquiera de ellos."
    ka "Das Kostüm könnte überall sein."

# game/christmas.rpy:1436
translate german christmas_shogukuni_05_50ac5d1b:

    # mc "Podemos visitar uno por uno para ir descartando."
    mc "Wir können sie, ja alle nach dem anderen durch gehen."

# game/christmas.rpy:1437
translate german christmas_shogukuni_05_f81b22e5:

    # mc "En algún momento tendrá que aparecer."
    mc "Irgendwann taucht es dann schon auf."

# game/christmas.rpy:1439
translate german christmas_shogukuni_05_bf2ed835:

    # ka "..."
    ka "..."

# game/christmas.rpy:1440
translate german christmas_shogukuni_05_5a7c104f:

    # mc "¿No te gusta la idea?"
    mc "Magst du die Idee nicht?"

# game/christmas.rpy:1441
translate german christmas_shogukuni_05_d238e89c:

    # ka "MMM..."
    ka "MMM..."

# game/christmas.rpy:1442
translate german christmas_shogukuni_05_176bb2c4:

    # mc "¿Kaede?"
    mc "Kaede?"

# game/christmas.rpy:1444
translate german christmas_shogukuni_05_ae95758c:

    # ka "No me distraigas, estoy intentando recordar."
    ka "Lenk mich nicht ab, ich bin immer noch am denken."

# game/christmas.rpy:1445
translate german christmas_shogukuni_05_f154ae9f:

    # ka "Creo que ahora no será el momento de ver a la hermosa Kaede vistiendo su traje de navidad."
    ka "Ich schätze, jetzt ist nicht der richtige Zeitpunkt, um die schöne Kaede in ihrem Weihnachtsoutfit zu sehen."

# game/christmas.rpy:1446
translate german christmas_shogukuni_05_89f0424d:

    # ka "Y además es algo que puede esperar."
    ka "Und es ist ja etwas, was noch warten kann."

# game/christmas.rpy:1448
translate german christmas_shogukuni_05_3460f2ac:

    # ka "Ahora tengo una misión que encomendarte pequeño saltamontes."
    ka "Jetzt habe ich eine Mission, die ich dir anvertraue, kleiner Grashüpfer."

# game/christmas.rpy:1449
translate german christmas_shogukuni_05_ee102915:

    # ka "¡Será una misión épica y muy importante para la familia!"
    ka "Es wird eine epische und sehr wichtige Mission für die Familie sein!"

# game/christmas.rpy:1450
translate german christmas_shogukuni_05_324efd99:

    # ka "Ahora que lo pienso, es tu primer trabajo bajo el mando de los Bagliore."
    ka "Wenn ich so darüber nachdenke, ist das dein erster Job unter Bagliore."

# game/christmas.rpy:1451
translate german christmas_shogukuni_05_70eef5d0:

    # ka "Y tendrás que cumplir las órdenes del Dragón Rojo."
    ka "Und du wirst die Befehle des Roten Drachen ausführen müssen."

# game/christmas.rpy:1452
translate german christmas_shogukuni_05_ddc6863b:

    # ka "¡Ahora pon mucha atención [nombre]!"
    ka "Jetzt pass gut auf, [nombre]!"

# game/christmas.rpy:1453
translate german christmas_shogukuni_05_14a77006:

    # ka "Porque tus órdenes pondrán a prueba tus técnicas y fortaleza mental."
    ka "Denn der Aufträg wird deine Fähigkeiten und die mentale Stärke auf die Probe stellen."

# game/christmas.rpy:1454
translate german christmas_shogukuni_05_6313a8a5:

    # ka "Tendrás que poner en práctica cada una de las lección que has aprendido bajo mis enseñanzas."
    ka "Sie müssen jede der Lektionen, die Sie unter meiner Anleitung gelernt haben, in die Praxis umsetzen."

# game/christmas.rpy:1455
translate german christmas_shogukuni_05_bd390dab:

    # ka "¿Alguna pregunta?"
    ka "Fragen?"

# game/christmas.rpy:1457
translate german christmas_shogukuni_05_449f4862:

    # mc "Pero si no he aprendido nada..."
    mc "Aber ich hab doch gar nichts gelernt..."

# game/christmas.rpy:1459
translate german christmas_shogukuni_05_d61b54e4:

    # ka "¿No recuerda mi más importante enseñanza [nombre] san?"
    ka "Erinnerst du dich an meine wichtigste Lehre, [nombre] san?"

# game/christmas.rpy:1460
translate german christmas_shogukuni_05_bfaad7fc:

    # ka "Encerar mano derecha, quitar cera mano izquierda."
    ka "Wachs auf die rechte Hand, Wachs von der linken Hand."

# game/christmas.rpy:1461
translate german christmas_shogukuni_05_d3bd7939:

    # ka "Debes entender que esta misión pondrá tu vida en peligro."
    ka "Du musst dir bewusst sein, dass diese Mission dein Leben in Gefahr bringt... {w}Und denk daran zu atmen."

# game/christmas.rpy:1463
translate german christmas_shogukuni_05_81f1e4b2:

    # mc "Solo dame las órdenes de una vez."
    mc "Gib mir doch einfach die Aufgabe, sofort."

# game/christmas.rpy:1465
translate german christmas_shogukuni_05_aca4a389:

    # ka "Quiero que lleves el traje que he hecho a Shizuka."
    ka "Ich möchte, dass du das Outfit trägst, wo ich für Shizuka gemacht habe."

# game/christmas.rpy:1466
translate german christmas_shogukuni_05_1926f26a:

    # ka "Debes convencer al Dragón Azul para que acceda a vestir estas ornamentas."
    ka "Du musst den Blauen Drachen davon überzeugen, diese Ornamente zu tragen."

# game/christmas.rpy:1467
translate german christmas_shogukuni_05_7c78b020:

    # ka "¡Suena genial que te cagas dar órdenes!"
    ka "Es hört sich geil an, Befehle zu geben!"

# game/christmas.rpy:1468
translate german christmas_shogukuni_05_9e1c8ab7:

    # ka "Creo que me volveré adicta a esto."
    ka "Ich glaube, ich werde süchtig danach."

# game/christmas.rpy:1470
translate german christmas_shogukuni_05_138ade8b:

    # mc "Si es un traje para ridiculizar a Shizuka, entonces no lo voy a hacer."
    mc "Wenn es ein Kostüm ist, um Shizuka lächerlich zu machen, dann werde ich das nicht tun."

# game/christmas.rpy:1472
translate german christmas_shogukuni_05_87ed7fbe:

    # ka "¿¡Nani!?"
    ka "Nani!?"

# game/christmas.rpy:1474
translate german christmas_shogukuni_05_e694bbeb:

    # ka "¿Qué clase de persona crees que soy yo como para hacer algo así?"
    ka "Was für ein Mensch muss ich denn sein, um so etwas zu tun?"

# game/christmas.rpy:1475
translate german christmas_shogukuni_05_6895aba0:

    # ka "Ufff... {w}Que frío que hace..."
    ka "Ufff... {w}Wie kalt es gerade ist..."

# game/christmas.rpy:1477
translate german christmas_shogukuni_05_f18a4b46:

    # mc "No me gustaría arruinar la navidad de Shizuka por una broma."
    mc "Ich möchte nicht Shizuka's Weihnachten, für einen Witz ruinieren."

# game/christmas.rpy:1479
translate german christmas_shogukuni_05_74bc378a:

    # ka "Vamos [nombre], te prometo que ha quedado lindísimo."
    ka "Komm schon [nombre], Ich schwöre dir, dass es schön ist."

# game/christmas.rpy:1480
translate german christmas_shogukuni_05_25362fa5:

    # ka "Me he esforzado liderando a Akiko para buscar los mejores componentes."
    ka "Ich habe hart daran gearbeitet, Akiko herum zu kommandieren, um die besten Teile zu finden."

# game/christmas.rpy:1482
translate german christmas_shogukuni_05_c129eb82:

    # mc "Si todo está bien, yo me encargo."
    mc "Wenn alles gut ist, nehme ich es in Kauf."

# game/christmas.rpy:1483
translate german christmas_shogukuni_05_44331a1d:

    # mc "Ahora ve a ponerte algo encima antes de que agarres un resfriado."
    mc "Jetzt zieh dir was an, bevor du dich erkältest."

# game/christmas.rpy:1493
translate german christmas_shogukuni_05_95acd728:

    # ka "¡Haaaaaai!"
    ka "Haaaaaai!"

# game/christmas.rpy:1495
translate german christmas_shogukuni_05_882ef879:

    # ka "Que frío..."
    ka "So kalt..."

# game/christmas.rpy:1497
translate german christmas_shogukuni_05_fc4422e0:

    # shi "Solo vas a hacer que nos ganemos un castigo por tu culpa."
    shi "Wegen dir, werden wir noch bestraft."

# game/christmas.rpy:1498
translate german christmas_shogukuni_05_44136182:

    # shi "No sé cómo lo haces para que Kaede y Yui te sigan el juego."
    shi "Ich weiss nicht, wie du es hingekriegt hast, Yui und Kaede dabei mitzumachen."

# game/christmas.rpy:1500
translate german christmas_shogukuni_05_ad279781:

    # mc "¿De verdad piensas que es tan malo?"
    mc "Denkst du wirklich, dass es so schlecht ist?"

# game/christmas.rpy:1501
translate german christmas_shogukuni_05_cf0e3242:

    # mc "Pensaba que te gustaba la idea de hacer algo especial para navidad."
    mc "Ich dachte, du hättest es gerne, was spezielles für Weihnachten zu tun."

# game/christmas.rpy:1502
translate german christmas_shogukuni_05_fa11c54c:

    # mc "Por lo menos en ese momento no dijiste nada en contra de la iniciativa."
    mc "Zumindest an diesem Zeitpunkt, hast du nichts gegen unsere Bewegung gesagt."

# game/christmas.rpy:1504
translate german christmas_shogukuni_05_b4bed991:

    # shi "Y tú asumes lo que pienso por algo que no he dicho."
    shi "Und du behauptest, ich hätte etwas gesagt, was ich im Endeffekt nicht getan habe?"

# game/christmas.rpy:1506
translate german christmas_shogukuni_05_0b14e3bf:

    # mc "A veces lo que no se dice, dice más de lo que se dice."
    mc "Manchmal sagt das, was nicht gesagt wird, mehr als das, was gesagt wird."

# game/christmas.rpy:1508
translate german christmas_shogukuni_05_9a17e68d:

    # shi "No por hacer juegos de palabras vas a hacerme cambiar de opinión."
    shi "Du wirst meine Meinung nicht ändern, mit deinen Wortspielen."

# game/christmas.rpy:1509
translate german christmas_shogukuni_05_0b44726a:

    # shi "Todo esto nos va a traer un montón de problemas."
    shi "All das, wird uns in ziemliche Schwierigkeiten bringen."

# game/christmas.rpy:1510
translate german christmas_shogukuni_05_55d25b3c:

    # shi "Y tampoco le veo el sentido..."
    shi "Und ich sehe deinen Punkt auch nicht..."

# game/christmas.rpy:1511
translate german christmas_shogukuni_05_4249e62b:

    # shi "¿Celebrar el día de navidad en el Shogukuni?"
    shi "Weihnachten feiern im Shogukuni?"

# game/christmas.rpy:1512
translate german christmas_shogukuni_05_ddf509f7:

    # shi "¿Hacer un regalo a tu amigo secreto?"
    shi "Ein Geschenk für der Beschenkten organisieren?"

# game/christmas.rpy:1513
translate german christmas_shogukuni_05_15c7b961:

    # shi "¡Los que estamos aquí no somos amigos ni familia realmente!"
    shi "Diejenigen von uns hier sind nicht wirklich Freunde oder Familie!"

# game/christmas.rpy:1514
translate german christmas_shogukuni_05_3e340ae5:

    # shi "¡Para mí ustedes solo son rivales a los que debo derrotar!"
    shi "Für mich seid ihr nur Rivalen, die ich besiegen muss!"

# game/christmas.rpy:1515
translate german christmas_shogukuni_05_dbde6b80:

    # shi "Un paso más en la escalera que debo subir para lograr mis objetivos."
    shi "Ein weiterer Schritt auf der Leiter, die ich erklimmen muss, um meine Ziele zu erreichen."

# game/christmas.rpy:1517
translate german christmas_shogukuni_05_36dfc167:

    # mc "¿De verdad lo ves de esa forma?"
    mc "Siehst du es wirklich so?"

# game/christmas.rpy:1519
translate german christmas_shogukuni_05_93be24d5:

    # shi "Esa es la verdad."
    shi "Das ist die Wahrheit."

# game/christmas.rpy:1520
translate german christmas_shogukuni_05_e29d9d76:

    # shi "Solo fuimos reclutadas para cumplir una tarea y realizar nuestro trabajo de la forma más perfecta posible."
    shi "Wir wurden nur eingestellt, um eine Aufgabe zu erfüllen und unsere Arbeit so perfekt wie möglich zu erledigen."

# game/christmas.rpy:1521
translate german christmas_shogukuni_05_75497c6f:

    # shi "En caso contrario, buscarán a alguien mejor que ocupe nuestro lugar."
    shi "Andernfalls werden sie jemanden suchen, der besser ist und unseren Platz einnimmt."

# game/christmas.rpy:1522
translate german christmas_shogukuni_05_4ef0e604:

    # shi "¿De verdad puedes llamar amigos o familia a algo como eso?"
    shi "Kann man so etwas wirklich als Freunde oder Familie bezeichnen?"

# game/christmas.rpy:1523
translate german christmas_shogukuni_05_56204f07:

    # shi "Y espero que tú no te sientas superior por lo que sucedió en tu peregrinaje."
    shi "Und ich hoffe, du fühlst dich nicht überlegen wegen dem, was auf deiner Pilgerreise passiert ist."

# game/christmas.rpy:1524
translate german christmas_shogukuni_05_2725c14e:

    # shi "Mi derrota fue debido a un descuido, no gracias a tu capacidad de pelea."
    shi "Meine Niederlage war auf ein Versehen zurückzuführen, nicht auf deine Kampffähigkeiten."

# game/christmas.rpy:1526
translate german christmas_shogukuni_05_918b0169:

    # mc "Aunque te hubiese derrotado, no pienso que soy superior a ti."
    mc "Selbst wenn ich dich besiegt hätte, glaube ich nicht, dass ich dir überlegen bin."

# game/christmas.rpy:1528
translate german christmas_shogukuni_05_d55e5008:

    # shi "Eso lo dices porque eres un debilucho."
    shi "Das sagst du nur, weil du ein Weichei bist."

# game/christmas.rpy:1530
translate german christmas_shogukuni_05_a800038c:

    # mc "Tampoco tienes que ser tan densa..."
    mc "Du musst auch nicht so begriffsstutzig sein..."

# game/christmas.rpy:1531
translate german christmas_shogukuni_05_a900b352:

    # mc "Yui y Kaede parecían entusiasmadas con la idea de hacer algo especial para navidad."
    mc "Yui und Kaede schienen begeistert davon zu sein, etwas Besonderes zu Weihnachten zu unternehmen."

# game/christmas.rpy:1532
translate german christmas_shogukuni_05_e5eaab94:

    # mc "Aunque ya sé que es ir contra las reglas hacer una celebración... {w}Y sigo pensando que es una tontería."
    mc "Obwohl ich weiß, dass es gegen die Regeln ist, eines zu halten... {wIch finde es immer noch albern."

# game/christmas.rpy:1534
translate german christmas_shogukuni_05_62ba568f:

    # shi "Un Dragón no puede bajar la guardia con actividades distractoras."
    shi "Ein Drache darf sich nicht durch ablenkende Aktivitäten in seiner Wachsamkeit beeinträchtigen lassen."

# game/christmas.rpy:1535
translate german christmas_shogukuni_05_e51f4050:

    # shi "Podrían atacarnos en cualquier momento aprovechando nuestro descuido."
    shi "Sie könnten uns jeden Moment angreifen und unsere Unachtsamkeit ausnutzen."

# game/christmas.rpy:1537
translate german christmas_shogukuni_05_a5a15d8e:

    # mc "¿No estarás exagerando?"
    mc "Übertreibst du nicht?"

# game/christmas.rpy:1539
translate german christmas_shogukuni_05_a677cf17:

    # shi "¿Qué dices?"
    shi "Was sagst du?"

# game/christmas.rpy:1541
translate german christmas_shogukuni_05_4e500d0f:

    # mc "Argleton es el lugar más tranquilo de todos los que he conocido en mi vida."
    mc "Argleton ist der friedlichste Ort, den ich je gesehen habe."

# game/christmas.rpy:1542
translate german christmas_shogukuni_05_ea0af7c4:

    # mc "No podría imaginar que alguien intentara atacar el pueblo o acercarse al Shogukuni."
    mc "Ich könnte mir nicht vorstellen, dass jemand versuchen würde, das Dorf anzugreifen oder sich dem Shogukuni zu nähern."

# game/christmas.rpy:1543
translate german christmas_shogukuni_05_46ec557f:

    # mc "¿Qué ganarían con eso?"
    mc "Was würden die davon haben?"

# game/christmas.rpy:1545
translate german christmas_shogukuni_05_3f26d037:

    # shi "Si otra organización descubre nuestro lugar de reunión en este pueblo."
    shi "Wenn eine andere Organisation unseren Treffpunkt an diesem Ort entdeckt."

# game/christmas.rpy:1546
translate german christmas_shogukuni_05_42ecba70:

    # shi "Entonces si podría ser una posibilidad."
    shi "Also ja, es gibt eine Chance, dass es passieren kann."

# game/christmas.rpy:1547
translate german christmas_shogukuni_05_4e373e8b:

    # shi "Aunque si debo ser sincera..."
    shi "Ich muss aber ehrlich sein..."

# game/christmas.rpy:1548
translate german christmas_shogukuni_05_8d6d21f4:

    # shi "Son reglas que han estado desde la fundación y siempre hemos respetado por tradición."
    shi "Diese Regeln gelten seit der Gründung und wir haben sie traditionell stets eingehalten."

# game/christmas.rpy:1549
translate german christmas_shogukuni_05_a6699200:

    # shi "Pero según lo que dice la maestra, los Bagliore era una familia mucho más agresiva en el pasado."
    shi "Aber laut dem Meister waren die Bagliore's in der Vergangenheit eine viel aggressivere Familie."

# game/christmas.rpy:1550
translate german christmas_shogukuni_05_c5439f98:

    # shi "Ahora la maestra Felicia solo se dedica a administrar algunos negocios financieros y los casinos que tiene la familia alrededor del país."
    shi "Jetzt widmet sich die Meisterin Felicia ausschliesslich der Verwaltung einiger Finanzgeschäfte und dem Casino, die die Familie im ganzen Land besitzt."

# game/christmas.rpy:1552
translate german christmas_shogukuni_05_81762dc8:

    # mc "¿Cuántos enemigos tienen los Bagliore?"
    mc "Wie viele Gegner hat Bagliore denn?"

# game/christmas.rpy:1554
translate german christmas_shogukuni_05_03bbbe0d:

    # shi "No lo sé..."
    shi "Ich weiss es nicht..."

# game/christmas.rpy:1555
translate german christmas_shogukuni_05_5e4a827a:

    # shi "Pero que yo sepa, no hemos tenido enemigos desde que llegue aquí."
    shi "Aber soweit ich weiss, gab es keine Gegner, seit ich hier bin."

# game/christmas.rpy:1557
translate german christmas_shogukuni_05_8c973c66:

    # mc "Entonces no es necesario respetar una regla como esa."
    mc "Dann ist es nicht wichtig, so eine Regel zu respektieren."

# game/christmas.rpy:1559
translate german christmas_shogukuni_05_c364920d:

    # shi "Pero..."
    shi "Aber..."

# game/christmas.rpy:1560
translate german christmas_shogukuni_05_004d9132:

    # shi "Siempre ha sido de esa forma..."
    shi "Es ist immer so gewesen..."

# game/christmas.rpy:1561
translate german christmas_shogukuni_05_808afb76:

    # shi "La maestra Felicia dice que debemos mantener las tradiciones de la familia Bagliore."
    shi "Meisterin Felicia sagt immer, dass wir die Familien Traditionen beibehalten müssen."

# game/christmas.rpy:1562
translate german christmas_shogukuni_05_64502971:

    # shi "Que debemos ser fieles a nuestros orígenes y mantenernos firmes en nuestras creencias."
    shi "Dass wir unseren Ursprüngen treu bleiben und fest zu unseren Überzeugungen stehen müssen."

# game/christmas.rpy:1564
translate german christmas_shogukuni_05_1b09a227:

    # mc "Pero tu misma dijiste que no son reglas necesarias."
    mc "Aber du hast selber gesagt, dass dies keine notwendigen Regeln sind."

# game/christmas.rpy:1565
translate german christmas_shogukuni_05_2bc76b1e:

    # mc "Las reglas deben ser reinventadas o cambiadas según el contexto y los tiempos."
    mc "Regeln müssen entsprechend dem Kontext und den Zeiten neu erfunden oder geändert werden."

# game/christmas.rpy:1567
translate german christmas_shogukuni_05_5870a08d:

    # shi "Quizás..."
    shi "Vielleicht..."

# game/christmas.rpy:1569
translate german christmas_shogukuni_05_12c73258:

    # mc "Vamos Shizuka, no seas tan negativa."
    mc "Komm schon Shizuka, sei nicht so negativ."

# game/christmas.rpy:1570
translate german christmas_shogukuni_05_fceaf599:

    # mc "Al menos podrías probarte el traje para ver como te queda."
    mc "Du könntest zumindest dein Kostüm anprobieren, schauen wie es dir passt."

# game/christmas.rpy:1572
translate german christmas_shogukuni_05_c364920d_1:

    # shi "Pero..."
    shi "Aber..."

# game/christmas.rpy:1574
translate german christmas_shogukuni_05_c8138e95:

    # mc "¿Por qué no le das una oportunidad?"
    mc "Ein Versuch?"

# game/christmas.rpy:1575
translate german christmas_shogukuni_05_f755799c:

    # shi "..."
    shi "..."

# game/christmas.rpy:1576
translate german christmas_shogukuni_05_1f56f218:

    # mc "¿Qué sucede Shizuka?"
    mc "Was ist los Shizuka?"

# game/christmas.rpy:1577
translate german christmas_shogukuni_05_a7396f4a:

    # mc "¿Hay alguna otra razón para actuar de esa forma?"
    mc "Gibt es irgendeinen Grund, so zu reagieren?"

# game/christmas.rpy:1579
translate german christmas_shogukuni_05_f052578e:

    # shi "Que te importa..."
    shi "Juckt..."

# game/christmas.rpy:1581
translate german christmas_shogukuni_05_acaa806d:

    # mc "Me gustaría saber como te sientes."
    mc "Ich möchte gerne wissen, wie du dich fühlst."

# game/christmas.rpy:1582
translate german christmas_shogukuni_05_b1f93586:

    # mc "Cuando hablamos sobre hacer una pequeña celebración, Kaede dijo que fue tu idea hacer un árbol de navidad."
    mc "Als wir über eine kleine Feier sprachen, sagte Kaede, es sei deine Idee gewesen, einen Weihnachtsbaum zu schmücken."

# game/christmas.rpy:1583
translate german christmas_shogukuni_05_475a2b9f:

    # mc "Pensaba que te agradaría todo esto y es menos riesgoso..."
    mc "Ich dachte, das würde dir gefallen, und es ist weniger riskant..."

# game/christmas.rpy:1585
translate german christmas_shogukuni_05_ee7510bf:

    # shi "¿Para qué quieres saber mis razones?"
    shi "Warum willst du meine Gründe wissen?"

# game/christmas.rpy:1586
translate german christmas_shogukuni_05_92f91073:

    # shi "¿Para burlarte de mi igual que como hace Kaede?"
    shi "Um dich über mich lustig zu machen, genau wie Kaede?"

# game/christmas.rpy:1588
translate german christmas_shogukuni_05_6b3868da:

    # mc "Si eres honesta conmigo, no volveré a hablar del tema."
    mc "Wenn du ehrlich zu mir bist, werde ich nicht mehr darüber sprechen."

# game/christmas.rpy:1589
translate german christmas_shogukuni_05_cbe91a73:

    # mc "Y será un secreto entre nosotros, te prometo que no voy a decir nada a Kaede."
    mc "Und es wird ein Geheimnis zwischen uns. Ich verspreche dir, Kaede wird nichts davon erfahren."

# game/christmas.rpy:1591
translate german christmas_shogukuni_05_f755799c_1:

    # shi "..."
    shi "..."

# game/christmas.rpy:1592
translate german christmas_shogukuni_05_e563493b:

    # mc "Tampoco pienso que odies la navidad."
    mc "Ich glaube nicht, dass du Weihnachten hasst."

# game/christmas.rpy:1594
translate german christmas_shogukuni_05_75bca997:

    # shi "Ya sé que no existe una verdadera amenaza para nosotras aquí en Argleton."
    shi "Ich weiss, dass es keine Gefahr für uns hier gibt in Argleton."

# game/christmas.rpy:1595
translate german christmas_shogukuni_05_7c4e7872:

    # shi "Pero debemos seguir las tradiciones que nos dice la maestra Felicia."
    shi "Aber wir müssen die Traditionen folgen, die uns Meisterin Felicia gesagt hat."

# game/christmas.rpy:1596
translate german christmas_shogukuni_05_457e361e:

    # shi "Porque si no es así..."
    shi "Weil wenn nicht..."

# game/christmas.rpy:1597
translate german christmas_shogukuni_05_050c5292:

    # shi "Si las cosas cambian como tú dices..."
    shi "Wenn sich Sachen ändern wie du es sagst..."

# game/christmas.rpy:1598
translate german christmas_shogukuni_05_b5f15be8:

    # shi "Todos sabrán que los Dragones no tenemos ninguna función real dentro de la familia."
    shi "Jeder wird wissen, dass wir Drachen innerhalb der Familie keine wirkliche Funktion haben."

# game/christmas.rpy:1599
translate german christmas_shogukuni_05_1f0057f1:

    # shi "¿Para qué necesitarías maestros de artes marciales para proteger este lugar si no existe ninguna amenaza?"
    shi "Warum braucht man Kampfkunstmeister, um diesen Ort zu beschützen, wenn keine Gefahr besteht?"

# game/christmas.rpy:1600
translate german christmas_shogukuni_05_ec747469:

    # shi "Ni siquiera podemos proteger al Viriato porque siempre ha estado desaparecido..."
    shi "Wir können nicht einmal den Viriato beschützen, weil er schon immer verschwunden war..."

# game/christmas.rpy:1602
translate german christmas_shogukuni_05_bd187da2:

    # shi "¿Cuánto tiempo ha pasado desde la última vez que alguien lo vio?"
    shi "Wie lange ist es her, dass ihn jemand zuletzt gesehen hat?"

# game/christmas.rpy:1603
translate german christmas_shogukuni_05_b6b39f5c:

    # shi "¿Diez o Quince años?"
    shi "10-15 Jahre?"

# game/christmas.rpy:1604
translate german christmas_shogukuni_05_9afeb01a:

    # shi "Nadie lo sabe... {w}Incluso yo he perdido la cuenta..."
    shi "Niemand weiss es... {w}Selbst ich habe den Überblick verloren..."

# game/christmas.rpy:1605
translate german christmas_shogukuni_05_232c7557:

    # shi "Pero gracias a esas tradiciones y reglas sin sentido."
    shi "Aber dank dieser sinnlosen Traditionen und Regeln."

# game/christmas.rpy:1606
translate german christmas_shogukuni_05_c8af9386:

    # shi "Yo tengo mi lugar en este mundo."
    shi "Ich habe meinen Platz in dieser Welt."

# game/christmas.rpy:1607
translate german christmas_shogukuni_05_d56cec97:

    # shi "Cumplo una función que le da sentido a mi vida."
    shi "Ich erfülle eine Funktion, die meinem Leben Sinn gibt."

# game/christmas.rpy:1608
translate german christmas_shogukuni_05_562532ed:

    # shi "Romper esas reglas significa la destrucción de todo eso."
    shi "Diese Regeln zu brechen bedeutet die Zerstörung all dessen."

# game/christmas.rpy:1610
translate german christmas_shogukuni_05_b3f59bd5:

    # mc "La función de los Dragones puede cambiar, al igual que esas reglas."
    mc "Die Funktion von Drachen kann sich ändern, ebenso wie diese Regeln."

# game/christmas.rpy:1612
translate german christmas_shogukuni_05_ccc34cc7:

    # shi "No es tan fácil..."
    shi "Es ist nicht so einfach..."

# game/christmas.rpy:1614
translate german christmas_shogukuni_05_6a32dc44:

    # mc "Nadie dijo que seria fácil."
    mc "Niemand hat gesagt, dass es einfach wird."

# game/christmas.rpy:1615
translate german christmas_shogukuni_05_b7f60c56:

    # mc "Y tampoco es malo romper las reglas una sola vez."
    mc "Und es schadet auch nicht, einmal gegen die Regeln zu verstossen."

# game/christmas.rpy:1617
translate german christmas_shogukuni_05_5870a08d_1:

    # shi "Quizás..."
    shi "Vielleicht..."

# game/christmas.rpy:1618
translate german christmas_shogukuni_05_c364920d_2:

    # shi "Pero..."
    shi "Aber..."

# game/christmas.rpy:1620
translate german christmas_shogukuni_05_a3fcd8df:

    # mc "¿Pero qué?"
    mc "Aber was?"

# game/christmas.rpy:1622
translate german christmas_shogukuni_05_f755799c_2:

    # shi "..."
    shi "..."

# game/christmas.rpy:1624
translate german christmas_shogukuni_05_e2ec65aa:

    # shi "Nada..."
    shi "Nichts..."

# game/christmas.rpy:1626
translate german christmas_shogukuni_05_02c8d6f8:

    # mc "¿Segura que no hay nada más?"
    mc "Bist du sicher, dass da nichts mehr ist?"

# game/christmas.rpy:1628
translate german christmas_shogukuni_05_6238af03:

    # shi "Yo..."
    shi "Ich..."

# game/christmas.rpy:1629
translate german christmas_shogukuni_05_bba752e2:

    # shi "Es todo..."
    shi "Das ist alles..."

# game/christmas.rpy:1631
translate german christmas_shogukuni_05_9a2a7c2b:

    # mc "Bien."
    mc "Gut."

# game/christmas.rpy:1632
translate german christmas_shogukuni_05_f755799c_3:

    # shi "..."
    shi "..."

# game/christmas.rpy:1633
translate german christmas_shogukuni_05_37b2500f:

    # mc "¿Eso significa que intentaras usar el traje de navidad que hizo Kaede?"
    mc "Heisst das, dass du versuchen wirst, das Weihnachtskostüm zu tragen, das Kaede gemacht hat?"

# game/christmas.rpy:1635
translate german christmas_shogukuni_05_62082be9:

    # shi "Está bien..."
    shi ""Es ist schön...

# game/christmas.rpy:1636
translate german christmas_shogukuni_05_3b9f7a71:

    # shi "Dame eso y vuelvo en unos momentos para que dejes de molestarme."
    shi "Gib mir das, und ich bin gleich wieder da, damit du mich nicht mehr nervst."

# game/christmas.rpy:1641
translate german christmas_shogukuni_05_ec2e5562:

    # "<Shizuka vuelve con el traje puesto y te pide ayuda para peinar su cabello.>"
    "<Shizuka kommt in ihrem Kostüm zurück und bittet dich, ihr beim Kämmen ihrer Haare zu helfen.>"

# game/christmas.rpy:1642
translate german christmas_shogukuni_05_dd1c2ebb:

    # shi "¿Has hecho lo que te dije?"
    shi "Hast du gemacht, was ich dir gesagt habe?"

# game/christmas.rpy:1647
translate german christmas_shogukuni_05_d580edab:

    # mc "Creo que ha quedado bien."
    mc "Ich denke es sieht gut aus."

# game/christmas.rpy:1649
translate german christmas_shogukuni_05_82fc3bf3:

    # mc "He seguido tus instrucciones al pie de la letra."
    mc "Ich habe deine Anweisungen genau befolgt."

# game/christmas.rpy:1651
translate german christmas_shogukuni_05_c3c44a24:

    # shi "Entonces ya estoy lista."
    shi "Dann bin ich bereit."

# game/christmas.rpy:1653
translate german christmas_shogukuni_05_b16f9616:

    # mc "Debo ser sincero..."
    mc "Wenn ich ehrlich sein soll..."

# game/christmas.rpy:1654
translate german christmas_shogukuni_05_fe5e3f01:

    # mc "Te queda mucho mejor de lo que esperaba."
    mc "Es sieht besser aus, als ich erwartet habe."

# game/christmas.rpy:1656
translate german christmas_shogukuni_05_f755799c_4:

    # shi "..."
    shi "..."

# game/christmas.rpy:1657
translate german christmas_shogukuni_05_793d4648:

    # mc "Kaede si se ha esforzado en buscar algo que te quedase perfecto."
    mc "Kaede hat sich wirklich viel Mühe gegeben, etwas zu finden, das dir perfekt stehen würde."

# game/christmas.rpy:1659
translate german christmas_shogukuni_05_f3c171a7:

    # shi "A mi también me ha sorprendido."
    shi "I was surprised too."

# game/christmas.rpy:1660
translate german christmas_shogukuni_05_53d5a0c5:

    # shi "Con Kaede nunca se sabe si va a hacer una tontería o está hablando en serio..."
    shi "Bei Kaede weiss man nie, ob sie etwas Dummes machen wird oder ob sie es ernst meint..."

# game/christmas.rpy:1661
translate german christmas_shogukuni_05_e0d4c108:

    # shi "Pensaba que haría un traje de Yeti o algo peor, solo para provocarme."
    shi "Ich dachte, sie würde ein Yeti Kostüm basteln oder noch schlimmer, nur um mich zu provozieren."

# game/christmas.rpy:1662
translate german christmas_shogukuni_05_df03ace0:

    # shi "¿Por qué te estás riendo?"
    shi "Warum lachst du?"

# game/christmas.rpy:1664
translate german christmas_shogukuni_05_9f8b3400:

    # mc "El traje de Yeti me ha causado gracia."
    mc "Yeti Kostüm ist lustig für mich.-"

# game/christmas.rpy:1666
translate german christmas_shogukuni_05_683f59c4:

    # shi "Aún me cuesta creer que no hay algún aparato de broma escondido por algún lado."
    shi "Ich kann immer noch kaum glauben, dass da nicht irgendwo ein Gag-Gadget versteckt ist."

# game/christmas.rpy:1668
translate german christmas_shogukuni_05_b8e430da:

    # ka "¿¡¡¡¡¡NAAAAAAAAAAAAAANIIIIIIIIIIIIIIIIII!!!!!?"
    ka "NAAAAAAAAAAAAAANIIIIIIIIIIIIIIIIII!!!!!?"

# game/christmas.rpy:1669
translate german christmas_shogukuni_05_adff2d48:

    # ka "¿¡Pero que es lo que estoy viendo!?"
    ka "Aber was sehe ich da?!"

# game/christmas.rpy:1671
translate german christmas_shogukuni_05_e806ea0e:

    # ka "¿Qué fue lo que hiciste con Shizuka, [nombre]?"
    ka "Was hast du mit Shizuka gemacht, [nombre]?"

# game/christmas.rpy:1673
translate german christmas_shogukuni_05_0234351b:

    # shi "Si sigues sobreactuando te voy a patear el trasero, Kaede."
    shi "Wenn du weiter so überreagierst, werde ich dir in den Arsch treten, Kaede."

# game/christmas.rpy:1675
translate german christmas_shogukuni_05_ba84689b:

    # ka "Está bien, lo siento."
    ka "Es ist okay, tut mir Leid."

# game/christmas.rpy:1676
translate german christmas_shogukuni_05_b622ec3d:

    # ka "Pero no podía ocultar mi sorpresa cuando te vi."
    ka "Aber ich konnte meine Überraschung nicht verbergen, als ich dich sah."

# game/christmas.rpy:1677
translate german christmas_shogukuni_05_7b60907f:

    # ka "¡Si ha quedado incluso mejor de lo que había imaginado!"
    ka "Es ist sogar noch besser geworden, als ich es mir vorgestellt hatte!"

# game/christmas.rpy:1678
translate german christmas_shogukuni_05_e9cb359a:

    # ka "¿Y tú que opinas de mi Shizuka?"
    ka "Was hältst du von mir, Shizuka?"

# game/christmas.rpy:1679
translate german christmas_shogukuni_05_57079c36:

    # ka "¿No me veo linda que te cagas?"
    ka "Sehe ich nicht verdammt gut aus?"

# game/christmas.rpy:1681
translate german christmas_shogukuni_05_72f775be:

    # shi "Horrible como siempre."
    shi "Schrecklich wie immer."

# game/christmas.rpy:1683
translate german christmas_shogukuni_05_4e9b5ba3:

    # ka "Eres una pesada sin arreglo, Shizuka."
    ka "Du bist eine Nervensäge, Shizuka."

# game/christmas.rpy:1684
translate german christmas_shogukuni_05_cb5fc0dc:

    # ka "Me dices eso después de tanto esfuerzo confeccionando un traje perfecto para ti..."
    ka "Du sagst mir das, nachdem ich mich so sehr bemüht habe, einen perfekten Anzug für dich zu schneidern..."

# game/christmas.rpy:1685
translate german christmas_shogukuni_05_6b43c5e6:

    # ka "Cosiendo y cosiendo las telas durante noches en vela solo para verte feliz..."
    ka "Nähen und Nähen der Stoffe während schlafloser Nächte, nur um dich glücklich zu sehen..."

# game/christmas.rpy:1686
translate german christmas_shogukuni_05_a0bdcfe1:

    # ka "Y ahora me dices que yo me veo horrible."
    ka "Und jetzt sagst du mir, dass ich schrecklich aussehe."

# game/christmas.rpy:1688
translate german christmas_shogukuni_05_d94c2c3e:

    # shi "Seguramente enviaste a Akiko a que hiciera todo el trabajo sucio."
    shi "Du hast wahrscheinlich Akiko geschickt, um die ganze Drecksarbeit zu erledigen."

# game/christmas.rpy:1690
translate german christmas_shogukuni_05_0fa378fd:

    # ka "¿Cómo puedes pensar tan mal de mí?"
    ka "Wie kannst du so schlecht von mir denken?"

# game/christmas.rpy:1691
translate german christmas_shogukuni_05_89dc94b2:

    # ka "¡Te juro que me he esforzado un montón!"
    ka "Ich schwöre, ich habe mich sehr bemüht!"

# game/christmas.rpy:1692
translate german christmas_shogukuni_05_8a1f8b22:

    # ka "Quería que tuvieses una ropa linda para navidad y te sacaras esa ropa fea que sueles usar."
    ka "Ich wollte, dass du zu Weihnachten ein schönes Outfit trägst und diese hässlichen Klamotten ausziehst, die du normalerweise trägst."

# game/christmas.rpy:1694
translate german christmas_shogukuni_05_ecf79821:

    # shi "¿Qué hay de malo con mi ropa?"
    shi "Was stimmt mit meinen Kleidern nicht?"

# game/christmas.rpy:1696
translate german christmas_shogukuni_05_3d0234f0:

    # ka "Esa vestimenta sí que es horrible y aburrida."
    ka "Dieses Outfit ist wirklich schrecklich und langweilig."

# game/christmas.rpy:1698
translate german christmas_shogukuni_05_c67058f7:

    # shi "¡Pero si es la misma que usas tú, pero en otro color!"
    shi "Aber es ist dasselbe, dass du verwendest, nur in einer anderen Farbe!"

# game/christmas.rpy:1700
translate german christmas_shogukuni_05_b716e272:

    # ka "¿Y tú que opinas de mi apariencia [nombre]?"
    ka "Und wie findest du meinen Look, [nombre]?"

# game/christmas.rpy:1701
translate german christmas_shogukuni_05_15e69261:

    # ka "¿Verdad que me queda bonita y linda y fantástica?"
    ka "Sieht es nicht schön und hübsch und fantastisch an mir aus?"

# game/christmas.rpy:1703
translate german christmas_shogukuni_05_df928799:

    # mc "Te queda perfecto, son trajes muy bonitos."
    mc "Es steht dir perfekt, das sind sehr schöne Outfits."

# game/christmas.rpy:1705
translate german christmas_shogukuni_05_8ad579a5:

    # ka "Ojalas Shizuka fuese tan honesta como tú."
    ka "Ich wünschte, Shizuka wäre so ehrlich wie du."

# game/christmas.rpy:1707
translate german christmas_shogukuni_05_f755799c_5:

    # shi "..."
    shi "..."

# game/christmas.rpy:1708
translate german christmas_shogukuni_05_a505e936:

    # mc "Al menos podrías agradecer a Kaede su amabilidad."
    mc "Du könntest Kaede wenigstens für ihre Freundlichkeit danken."

# game/christmas.rpy:1710
translate german christmas_shogukuni_05_58bca6cc:

    # shi "Gracias Kaede."
    shi "Danke Kaede."

# game/christmas.rpy:1712
translate german christmas_shogukuni_05_c78d1788:

    # ka "Ha sido un placer."
    ka "Meine Freude deinerseits."

# game/christmas.rpy:1713
translate german christmas_shogukuni_05_54a3811b:

    # ka "Era mi forma de pedirte disculpa por haberme comido tu estofado."
    ka "Das war meine Art, mich dafür zu entschuldigen, dass ich deinen Eintopf gegessen habe."

# game/christmas.rpy:1714
translate german christmas_shogukuni_05_eb77a988:

    # ka "No dejaba de sentirme culpable por eso."
    ka "Ich hatte deswegen ständig Schuldgefühle."

# game/christmas.rpy:1716
translate german christmas_shogukuni_05_8f518fcc:

    # shi "¿Tu sintiendo remordimiento por tus tonterías?"
    shi "Hast du Reue wegen deiner Dummheit?"

# game/christmas.rpy:1717
translate german christmas_shogukuni_05_0bb34118:

    # shi "Eso sí que debe ser un milagro navideño."
    shi "Das muss wohl ein Weihnachtswunder sein."

# game/christmas.rpy:1719
translate german christmas_shogukuni_05_92edfd1a:

    # ka "¡Entonces ya tenemos todo listo para el día del amigo secreto!"
    ka "Dann sind wir alle bereit für den geheimen Weihnachtstag!"

# game/christmas.rpy:1720
translate german christmas_shogukuni_05_85fd4588:

    # ka "Solo faltarían los regalos."
    ka "Das Einzige, was fehlt, sind die Geschenke."

# game/christmas.rpy:1722
translate german christmas_shogukuni_05_ffe8e8ba:

    # shi "El regalo..."
    shi "Die Geschenke..."

# game/christmas.rpy:1724
translate german christmas_shogukuni_05_465acfbf:

    # ka "Ahora los dejaré solos para que piensen en eso."
    ka "Jetzt lasse ich Sie allein, damit Sie darüber nachdenken können."

# game/christmas.rpy:1725
translate german christmas_shogukuni_05_646f53f9:

    # ka "¡Sayonara!"
    ka "Sayonara!"

# game/christmas.rpy:1735
translate german christmas_shogukuni_05_471c1294:

    # shi "¿Ya has buscado un regalo para tu amigo secreto?"
    shi "Hast du schon nach einem Geschenk für den Beschenkten gesucht?"

# game/christmas.rpy:1737
translate german christmas_shogukuni_05_bb47238d:

    # mc "Aún no he encontrado ese algo especial."
    mc "Ich habe dieses gewisse Etwas noch nicht gefunden."

# game/christmas.rpy:1739
translate german christmas_shogukuni_05_dbaed869:

    # shi "Ya veo..."
    shi "Verstehe..."

# game/christmas.rpy:1741
translate german christmas_shogukuni_05_84982130:

    # shi "¿No es posible cambiar mi amigo secreto por el tuyo?"
    shi "Ist es nicht möglich, meinen Beschenkten gegen deins auszutauschen?"

# game/christmas.rpy:1743
translate german christmas_shogukuni_05_74dd6927:

    # mc "Perdería el encanto."
    mc "Es würde seinen Charme verlieren."

# game/christmas.rpy:1745
translate german christmas_shogukuni_05_4c0e24cd:

    # shi "Mi amigo secreto es alguien difícil..."
    shi "Mein Beschenkter ist jemand, der schwierig ist..."

# game/christmas.rpy:1746
translate german christmas_shogukuni_05_b628219a:

    # shi "¿Podrías ayudarme con alguna idea?"
    shi "Kannst du mir mit Ideen helfen?"

# game/christmas.rpy:1748
translate german christmas_shogukuni_05_ebef3293:

    # mc "¿No soy yo tu amigo secreto por casualidad?"
    mc "Bin ich zufällig dein Wichtel?"

# game/christmas.rpy:1750
translate german christmas_shogukuni_05_c48fca82:

    # shi "Es Yui."
    shi "Es ist Yui."

# game/christmas.rpy:1751
translate german christmas_shogukuni_05_773e65fa:

    # shi "Me siento extraña al pensar en un regalo para ella."
    shi "Es ist seltsam, über ein Geschenk für sie nachzudenken."

# game/christmas.rpy:1752
translate german christmas_shogukuni_05_945f539b:

    # shi "Después de todo lo que sucedió entre nosotras..."
    shi "Nach allem, was zwischen uns passiert ist..."

# game/christmas.rpy:1753
translate german christmas_shogukuni_05_43496f6b:

    # shi "Quizás debería devolverle eso..."
    shi "Vielleicht sollte ich ihr das zurückgeben..."

# game/christmas.rpy:1755
translate german christmas_shogukuni_05_5fb144e9:

    # mc "Será una buena oportunidad para arreglar las cosas."
    mc "Es wird eine gute Gelegenheit sein, die Dinge wieder gerade zu bringen."

# game/christmas.rpy:1757
translate german christmas_shogukuni_05_2ac08787:

    # shi "¿De verdad lo crees?"
    shi "Glaubst du das wirklich?"

# game/christmas.rpy:1758
translate german christmas_shogukuni_05_23c63f0c:

    # shi "Todo esto de la navidad me produce sentimientos encontrados."
    shi "Diese ganze Weihnachtssache weckt gemischte Gefühle in mir."

# game/christmas.rpy:1759
translate german christmas_shogukuni_05_3b6ea533:

    # shi "Hay muchas cosas que no puedo quitar de mi cabeza..."
    shi "Es gibt viele Dinge, die mir nicht aus dem Kopf gehen..."

# game/christmas.rpy:1761
translate german christmas_shogukuni_05_d727884d:

    # mc "¿Eso significa que vas a participar?"
    mc "Heisst das, dass du mitmachen wirst?"

# game/christmas.rpy:1763
translate german christmas_shogukuni_05_29b705fd:

    # shi "Aún tengo que pensarlo."
    shi "Ich muss noch darüber nachdenken."

# game/christmas.rpy:1764
translate german christmas_shogukuni_05_aabce403:

    # shi "Para mí... {w}Es complicado..."
    shi "Für mich... {w}Es ist irgendwie kompliziert..."

# game/christmas.rpy:1765
translate german christmas_shogukuni_05_572d1ec2:

    # shi "Por ahora prefiero volver a mi entrenamiento."
    shi "Im Moment ziehe ich es vor, mich wieder meinem Training zu widmen."

# game/christmas.rpy:1767
translate german christmas_shogukuni_05_05c5c239:

    # mc "¿No hay algo de lo que necesites hablar?"
    mc "Gibt es nicht etwas, worüber du reden musst?"

# game/christmas.rpy:1769
translate german christmas_shogukuni_05_6238af03_1:

    # shi "Yo..."
    shi "Ich..."

# game/christmas.rpy:1771
translate german christmas_shogukuni_05_f183345d:

    # mc "Tu problema no es con las reglas del Shogukuni realmente."
    mc "Dein Problem liegt nicht wirklich bei den Regeln der Shogukuni."

# game/christmas.rpy:1772
translate german christmas_shogukuni_05_7e7fada5:

    # mc "Sabes que puedes confiar en mí."
    mc "Du weisst, du kannst mir vertrauen."

# game/christmas.rpy:1774
translate german christmas_shogukuni_05_143b6e78:

    # shi "Métete en tus asuntos..."
    shi "Kümmere dich um deine eigenen Angelegenheiten..."

# game/christmas.rpy:1775
translate german christmas_shogukuni_05_a5272faf:

    # shi "Voy a participar de su pequeña celebración así que ya no me hagas más preguntas."
    shi "Ich werde an deiner kleinen Feier teilnehmen, also stell mir keine weiteren Fragen."

# game/christmas.rpy:1776
translate german christmas_shogukuni_05_8bb59d59:

    # shi "Ahora volveré a mi entrenamiento y tu deberías hacer lo mismo si no quieres morir."
    shi "Jetzt werde ich zu meinem Training zurückkehren, und du solltest dasselbe tun, wenn du nicht sterben willst."

# game/christmas.rpy:1781
translate german christmas_shogukuni_05_7330d216:

    # mc "(Y así es como volvió la Shizuka de siempre...)"
    mc "(Und so kam die gewohnte Shizuka zurück...)"

# game/christmas.rpy:1795
translate german christmas_shogukuni_06_79453dac:

    # mc "Buenos días."
    mc "Guten Morgen."

# game/christmas.rpy:1797
translate german christmas_shogukuni_06_fd287a15:

    # y "¡[nombre]!"
    y "[nombre]!"

# game/christmas.rpy:1799
translate german christmas_shogukuni_06_7d31ba90:

    # mc "Necesito tu ayuda, Yui."
    mc "Ich brauche deine Hilfe, Yui."

# game/christmas.rpy:1801
translate german christmas_shogukuni_06_d3f95325:

    # y "¿Ayuda?"
    y "Hilfe?"

# game/christmas.rpy:1803
translate german christmas_shogukuni_06_3b895c2c:

    # mc "No sé cómo hacer esta pregunta sin revelar demasiada información..."
    mc "Ich weiss nicht, wie ich diese Frage stellen soll, ohne zu viele Informationen preiszugeben."

# game/christmas.rpy:1804
translate german christmas_shogukuni_06_43cc8f6b:

    # mc "Me gustaría saber un poco más sobre ustedes para el regalo del amigo secreto."
    mc "Ich würde gerne etwas mehr über dich erfahren, damit ich ein passendes Geschenk machen kann."

# game/christmas.rpy:1805
translate german christmas_shogukuni_06_c833b490:

    # mc "No puedo decir quien es específicamente para no arruinar la sorpresa."
    mc "Ich kann nicht sagen, wer es genau ist, um die Überraschung nicht zu verderben."

# game/christmas.rpy:1807
translate german christmas_shogukuni_06_6464c89b:

    # y "¿Shizuka?"
    y "Shizuka?"

# game/christmas.rpy:1809
translate german christmas_shogukuni_06_627f65a1:

    # mc "Al menos que ya lo sepas..."
    mc "Es sei denn, du weisst es bereits..."

# game/christmas.rpy:1810
translate german christmas_shogukuni_06_69851040:

    # mc "¿Cómo sabes que es Shizuka?"
    mc "Wie weisst du, dass es Shizuka ist?"

# game/christmas.rpy:1812
translate german christmas_shogukuni_06_045dcf38:

    # y "Magia."
    y "Magie."

# game/christmas.rpy:1814
translate german christmas_shogukuni_06_08e83b44:

    # mc "¿Sabes quienes son los amigos secretos de todos?"
    mc "Weisst du, wer die Beschenkten aller sind?"

# game/christmas.rpy:1816
translate german christmas_shogukuni_06_785ee78f:

    # y "No."
    y "Nein."

# game/christmas.rpy:1818
translate german christmas_shogukuni_06_47283b46:

    # mc "¿Solo sabes quien me ha tocado a mí?"
    mc "Weisst du nur, wer mich berührt hat?"

# game/christmas.rpy:1820
translate german christmas_shogukuni_06_1eb3c84b:

    # y "Si."
    y "Ja."

# game/christmas.rpy:1822
translate german christmas_shogukuni_06_91b2bde9:

    # mc "Yo estoy seguro de no haber mencionado nada."
    mc "Ich bin mir sicher, dass ich nichts erwähnt habe."

# game/christmas.rpy:1823
translate german christmas_shogukuni_06_e9770903:

    # mc "Y el día que escogimos los papeles al azar tu tampoco habías visto."
    mc "Und an dem Tag, an dem wir die Rollen zufällig ausgewählt haben, hast du auch nichts gesehen."

# game/christmas.rpy:1825
translate german christmas_shogukuni_06_4e03319f:

    # y "Sentimientos."
    y "Gefühle."

# game/christmas.rpy:1827
translate german christmas_shogukuni_06_9c1f1d04:

    # mc "¿Viste mis sentimientos en ese momento?"
    mc "Hast du meine Gefühle in diesem Moment gesehen?"

# game/christmas.rpy:1829
translate german christmas_shogukuni_06_22cc527f:

    # y "Disgustado."
    y "Enttäuscht."

# game/christmas.rpy:1831
translate german christmas_shogukuni_06_3d31300b:

    # mc "¿Dices que viste mis sentimientos al momento de sacar el papel?"
    mc "Du sagst, du hast meine Gefühle im Moment gesehen, als ich das Papier herauszog?"

# game/christmas.rpy:1832
translate german christmas_shogukuni_06_baf0ad66:

    # mc "Y al ver mi disgusto llegaste a la conclusión de que era Shizuka..."
    mc "Und als du mein Gesicht sahst, kamst du zu dem Schluss, dass es Shizuka war?"

# game/christmas.rpy:1834
translate german christmas_shogukuni_06_6a11be0a:

    # y "¡Si!"
    y "Ja!"

# game/christmas.rpy:1836
translate german christmas_shogukuni_06_2b44a779:

    # mc "Me has leído como si fuese un libro."
    mc "Du kennst mich vorne bis hinten."

# game/christmas.rpy:1837
translate german christmas_shogukuni_06_92920df1:

    # mc "Aunque ahora que lo sabes, quizás puedas ayudarme a buscar un regalo."
    mc "Da du es jetzt weisst, kannst du mir vielleicht helfen, ein Geschenk zu finden."

# game/christmas.rpy:1838
translate german christmas_shogukuni_06_4b3dfa02:

    # mc "Me siento un poco perdido ahora mismo..."
    mc "Ich fühle mich gerade etwas verloren..."

# game/christmas.rpy:1839
translate german christmas_shogukuni_06_ee1786df:

    # mc "Y no conozco a Shizuka lo suficiente para encontrar algo que sea de su gusto."
    mc "Und ich kenne Shizuka nicht gut genug, um etwas zu finden, das ihrem Geschmack entspricht."

# game/christmas.rpy:1840
translate german christmas_shogukuni_06_8b237ca5:

    # mc "Lo único que conozco sobre ella es que practica artes marciales."
    mc "Das Einzige, was ich über sie weiss, ist, dass sie Kampfsport betreibt."

# game/christmas.rpy:1842
translate german christmas_shogukuni_06_fbf876d3:

    # y "Difícil."
    y "Schwierig."

# game/christmas.rpy:1844
translate german christmas_shogukuni_06_1386296a:

    # mc "¿Qué opinas sobre unos guantes de boxeo?"
    mc "Wie wäre es mit Boxhandschuhen?"

# game/christmas.rpy:1845
translate german christmas_shogukuni_06_9e03afec:

    # mc "Sería más sencillo aguantar sus golpes si los utiliza en los entrenamientos."
    mc "Es wäre einfacher, ihren Schlägen standzuhalten, wenn sie sie im Training einsetzt."

# game/christmas.rpy:1847
translate german christmas_shogukuni_06_785ee78f_1:

    # y "No."
    y "Nein."

# game/christmas.rpy:1849
translate german christmas_shogukuni_06_3202da13:

    # mc "Me parecía una buena idea..."
    mc "Es schien eine gute Idee zu sein..."

# game/christmas.rpy:1851
translate german christmas_shogukuni_06_c51ce11b:

    # y "Furiosa."
    y "Wütend."

# game/christmas.rpy:1853
translate german christmas_shogukuni_06_a5d4738d:

    # mc "Tienes razón... {w}Shizuka podría pensar que es un sinónimo de debilidad..."
    mc "Du hast Recht... {w}Shizuka könnte man dann als ein Wort für Schwäche betrachten..."

# game/christmas.rpy:1854
translate german christmas_shogukuni_06_385982b5:

    # mc "Tampoco tengo una mejor idea y se me agota el tiempo."
    mc "Ich habe auch keine bessere Idee und mir läuft die Zeit davon..."

# game/christmas.rpy:1856
translate german christmas_shogukuni_06_066cf106:

    # y "Regalo..."
    y "Geschenk..."

# game/christmas.rpy:1858
translate german christmas_shogukuni_06_3c55ac97:

    # mc "¿Se te ha ocurrido una idea?"
    mc "Ist dir was eingefallen?"

# game/christmas.rpy:1860
translate german christmas_shogukuni_06_cc207125:

    # y "Vestido."
    y "Kleid."

# game/christmas.rpy:1862
translate german christmas_shogukuni_06_2401fa9a:

    # mc "¿Un vestido?"
    mc "Ein Kleid?"

# game/christmas.rpy:1863
translate german christmas_shogukuni_06_104bcb3b:

    # mc "Podría ser algo rudo para una chica como ella."
    mc "Für ein Mädchen wie sie könnte es etwas hart sein."

# game/christmas.rpy:1865
translate german christmas_shogukuni_06_785ee78f_2:

    # y "No."
    y "Nein."

# game/christmas.rpy:1867
translate german christmas_shogukuni_06_06692dd6:

    # mc "¿Dices que busque algo más delicado?"
    mc "Are you saying to look for something more delicate?"

# game/christmas.rpy:1869
translate german christmas_shogukuni_06_4c308c28:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:1871
translate german christmas_shogukuni_06_c7221590:

    # mc "Entiendo, pero..."
    mc "Ich verstehe, aber..."

# game/christmas.rpy:1872
translate german christmas_shogukuni_06_e589a609:

    # mc "¿Estás segura de que es buena idea regalar algo así a alguien como Shizuka?"
    mc "Bist du sicher, dass es eine gute Idee ist, jemandem wie Shizuka so etwas zu schenken?"

# game/christmas.rpy:1874
translate german christmas_shogukuni_06_fe9941ea:

    # y "Posibilidad."
    y "Möglichkeit."

# game/christmas.rpy:1876
translate german christmas_shogukuni_06_7fa1443b:

    # mc "Ni siquiera tú estás segura..."
    mc "Selbst wenn du dir nicht sicher bist..."

# game/christmas.rpy:1878
translate german christmas_shogukuni_06_327bb99a:

    # y "¡Apuesta!"
    y "Wetten!"

# game/christmas.rpy:1880
translate german christmas_shogukuni_06_1ad5af62:

    # mc "¿Dices que debería correr el riesgo con un regalo así?"
    mc "Meinst du, ich sollte so etwas wagen?"

# game/christmas.rpy:1881
translate german christmas_shogukuni_06_488cdb92:

    # mc "Algo me dice que es una mala idea..."
    mc "Irgendetwas sagt mir, dass das eine schlechte Idee ist..."

# game/christmas.rpy:1883
translate german christmas_shogukuni_06_5be07bcc:

    # y "¡Posibilidad!"
    y "Möglichkeit!"

# game/christmas.rpy:1885
translate german christmas_shogukuni_06_f809e2c3:

    # mc "Si tú dices que existe una posibilidad..."
    mc "Wenn du meinst, dass es eine Möglichkeit gibt..."

# game/christmas.rpy:1886
translate german christmas_shogukuni_06_d6785c10:

    # mc "Si lo pienso detenidamente creo que sería un regalo interesante."
    mc "Wenn ich darüber nachdenke, wäre das ein interessantes Geschenk."

# game/christmas.rpy:1887
translate german christmas_shogukuni_06_50cae572:

    # mc "Aunque solo sea para ver la reacción de Shizuka."
    mc "Auch wenn es nur darum geht, ihre Reaktion zu sehen."

# game/christmas.rpy:1889
translate german christmas_shogukuni_06_de57db27:

    # y "Divertido."
    y "Witzig."

# game/christmas.rpy:1891
translate german christmas_shogukuni_06_4a75065a:

    # mc "Espero que ella no se sienta incomoda al recibir algo como eso."
    mc "Ich hoffe, sie fühlt sich nicht unwohl, wenn sie so etwas bekommt."

# game/christmas.rpy:1892
translate german christmas_shogukuni_06_b1f98d6e:

    # mc "¿Me ayudarías a buscar un vestido para ella?"
    mc "Hilfst du mir, ein Kleid für sie zu finden?"

# game/christmas.rpy:1894
translate german christmas_shogukuni_06_50f00414:

    # y "¡Comprar!"
    y "Kaufen!"

# game/christmas.rpy:1896
translate german christmas_shogukuni_06_fbea1732:

    # mc "Entonces vamos a la tienda de Kamen Rainor."
    mc "Dann gehen wir zu Kamen Rainor's Laden."

# game/christmas.rpy:1898
translate german christmas_shogukuni_06_b9a8dd77:

    # "<Buscas junto a Yui durante unas horas un vestido perfecto para Shizuka y al terminar caminan juntos al Shogukuni.>"
    "<Du suchst zusammen mit Yui ein paar Stunden lang nach einem perfekten Kleid für Shizuka, und als ihr fertig seid, geht ihr gemeinsam zum Shogukuni.>"

# game/christmas.rpy:1900
translate german christmas_shogukuni_06_a6074484:

    # mc "No entiendo por qué no me dejaste pagar a mí por el regalo."
    mc "Ich verstehe nicht, warum du mich das Geschenk nicht bezahlen lassen hast."

# game/christmas.rpy:1902
translate german christmas_shogukuni_06_2bc2935b:

    # y "Familia."
    y "Familie."

# game/christmas.rpy:1904
translate german christmas_shogukuni_06_fa6fde19:

    # mc "Sé que puedes utilizar el dinero de los Bagliore, pero eso también te puedes meter en problemas."
    mc "Ich weiss, dass du das Geld von Bagliore gebrauchen kannst, aber das kann dich auch in Schwierigkeiten bringen."

# game/christmas.rpy:1905
translate german christmas_shogukuni_06_0feb4fbf:

    # mc "Estás usando tus ventajas de Dragón Negro y eso podría dejar algunas huellas."
    mc "Du nutzt deine Black Dragon-Vorteile, und das könnte Spuren hinterlassen."

# game/christmas.rpy:1907
translate german christmas_shogukuni_06_d7d42da2:

    # y "Tranquilo."
    y "Ruhe."

# game/christmas.rpy:1909
translate german christmas_shogukuni_06_4574de6d:

    # mc "Si tú dices que no hay problema..."
    mc "Wenn du sagst, es gibt kein Problem..."

# game/christmas.rpy:1910
translate german christmas_shogukuni_06_56fd73a5:

    # mc "Aunque ahora me siento en deuda contigo."
    mc "Obwohl ich mich dir jetzt verpflichtet fühle."

# game/christmas.rpy:1912
translate german christmas_shogukuni_06_7899b7b8:

    # y "Peregrino."
    y "Pilgrim."

# game/christmas.rpy:1914
translate german christmas_shogukuni_06_2ac0ea70:

    # mc "¿Qué hay con eso de ser el Peregrino?"
    mc "Was ist dabei, der Pilger zu sein?"

# game/christmas.rpy:1916
translate german christmas_shogukuni_06_2bc2935b_1:

    # y "Familia."
    y "Familie."

# game/christmas.rpy:1918
translate german christmas_shogukuni_06_4d280a43:

    # mc "El Peregrino también es parte de la familia..."
    mc "Der Pilgrim gehört auch zur Familie..."

# game/christmas.rpy:1919
translate german christmas_shogukuni_06_b7efb0cf:

    # mc "¿Quieres decir que también tengo derecho a gastar el dinero de los Bagliore?"
    mc "Du meinst, ich habe auch das Recht, das Geld von Bagliore auszugeben?"

# game/christmas.rpy:1921
translate german christmas_shogukuni_06_1eb3c84b_1:

    # y "Si."
    y "Ja."

# game/christmas.rpy:1923
translate german christmas_shogukuni_06_15b9c722:

    # mc "Ya veo... {w}No es tan malo hacer el Peregrinaje después de todo."
    mc "Ich verstehe... {w}Es ist doch gar nicht so schlimm, die Pilgerreise zu machen."

# game/christmas.rpy:1924
translate german christmas_shogukuni_06_b05184e2:

    # mc "Gracias por ayudarme a escoger un vestido para Shizuka."
    mc "Danke, dass du mir geholfen hast, ein Kleid für Shizuka auszuwählen."

# game/christmas.rpy:1925
translate german christmas_shogukuni_06_1ab374ee:

    # mc "¿Tú ya tienes el regalo para tu amigo secreto?"
    mc "Haben Sie schon ein Geschenk für den Beschenkten?"

# game/christmas.rpy:1927
translate german christmas_shogukuni_06_4c308c28_1:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:1929
translate german christmas_shogukuni_06_0d4111be:

    # mc "Si todos están listos entonces podríamos hacer la celebración el día de mañana."
    mc "Wenn alle bereit sind, könnten wir morgen die Feier veranstalten."

# game/christmas.rpy:1931
translate german christmas_shogukuni_06_5ef27faf:

    # y "Noche."
    y "Nacht."

# game/christmas.rpy:1933
translate german christmas_shogukuni_06_cc6ff420:

    # mc "Tienes razón, la noche es el mejor momento, ya que habrá más privacidad en el Shogukuni."
    mc "Du hast Recht, der Abend ist die beste Zeit, da es im Shogukuni dann ruhiger ist."

# game/christmas.rpy:1934
translate german christmas_shogukuni_06_0a32e368:

    # mc "Entonces mañana durante la noche haremos nuestra pequeña celebración."
    mc "Dann werden wir morgen Abend unsere kleine Feier veranstalten."

# game/christmas.rpy:1935
translate german christmas_shogukuni_06_52a9fe0b:

    # mc "¿Podrías avisar a Kaede y Shizuka sobre esto?"
    mc "Könntest du bitte Kaede und Shizuka darüber informieren?"

# game/christmas.rpy:1937
translate german christmas_shogukuni_06_6a11be0a_1:

    # y "¡Si!"
    y "Ja!"

# game/christmas.rpy:1939
translate german christmas_shogukuni_06_ad238fdc:

    # mc "¡Entonces es un trato!"
    mc "Dann ist es abgemacht!"

# game/christmas.rpy:1940
translate german christmas_shogukuni_06_10fe78f1:

    # mc "Ahora iré a dormir..."
    mc "Jetzt mache ich einfach Schluss für heute..."

# game/christmas.rpy:1941
translate german christmas_shogukuni_06_c0ae405c:

    # mc "Ha sido un largo día buscando el regalo para Shizuka y estoy agotado."
    mc "Es war ein langer Tag auf der Suche nach einem Geschenk für Shizuka und ich bin erschöpft."

# game/christmas.rpy:1942
translate german christmas_shogukuni_06_6ce47012:

    # mc "Buenas noches, Yui."
    mc "Gute Nacht, Yui."

# game/christmas.rpy:1944
translate german christmas_shogukuni_06_e478c113:

    # y "¡Buenas noches!"
    y "Gute Nacht!"

# game/christmas.rpy:1950
translate german christmas_shogukuni_06_9044321a:

    # "<Después de despedirte de Yui vuelves a tu habitación a dormir.>"
    "<Nachdem du dich von Yui verabschiedet hast, gehst du zurück in dein Zimmer, um zu schlafen.>"

# game/christmas.rpy:1964
translate german christmas_shogukuni_07_1ad7b9bc:

    # y "¡Celebración!"
    y "Feier!"

# game/christmas.rpy:1966
translate german christmas_shogukuni_07_a1ff2893:

    # mc "Antes de partir recuerda llevar el regalo."
    mc "Bevor du gehst, denk daran, das Geschenk mitzunehmen."

# game/christmas.rpy:1968
translate german christmas_shogukuni_07_50cbfaee:

    # y "Preparada."
    y "Bereit."

# game/christmas.rpy:1970
translate german christmas_shogukuni_07_e966f435:

    # mc "Entonces es hora de ir al Shogukuni."
    mc "Dann ist es Zeit, nach Shogukuni aufzubrechen."

# game/christmas.rpy:1979
translate german christmas_shogukuni_07_47da3649:

    # ka "¡BRRRrrrrh!"
    ka "BRRRrrrrh!"

# game/christmas.rpy:1981
translate german christmas_shogukuni_07_c4c963ad:

    # ka "¿Por qué tiene que hacer tanto frío durante la noche?"
    ka "Warum muss es nachts so kalt sein?"

# game/christmas.rpy:1982
translate german christmas_shogukuni_07_6b8d5884:

    # ka "Se me va a congelar la nariz si seguimos aquí esperando."
    ka "Wenn wir hier weiter warten, friere ich mir noch den Rotz ab."

# game/christmas.rpy:1984
translate german christmas_shogukuni_07_f3be0437:

    # shi "Espero que tu regalo sea una mantita para abrigarte."
    shi "Ich hoffe, dein Geschenk ist eine Decke, die dich warm hält."

# game/christmas.rpy:1986
translate german christmas_shogukuni_07_b9bc9eae:

    # ka "Estoy segura de que no será ese mi regalo."
    ka "Ich bin mir sicher, dass das nicht mein Geschenk sein wird."

# game/christmas.rpy:1988
translate german christmas_shogukuni_07_16a12b16:

    # shi "Ya veremos..."
    shi "Sehen wir..."

# game/christmas.rpy:1990
translate german christmas_shogukuni_07_967b6e3f:

    # ka "No, estoy completamente segura de que no será eso."
    ka "Nein, ich bin mir absolut sicher, dass es das nicht sein wird."

# game/christmas.rpy:1991
translate german christmas_shogukuni_07_fe1469c8:

    # ka "Digamos que soy una adivina y puedo ver el futuro."
    ka "Nehmen wir an, ich bin Wahrsager und kann in die Zukunft sehen."

# game/christmas.rpy:1992
translate german christmas_shogukuni_07_24e73933:

    # ka "Mi regalo secreto será una deliciosa cena."
    ka "Mein geheimes Geschenk wird ein köstliches Abendessen sein."

# game/christmas.rpy:1994
translate german christmas_shogukuni_07_2a26273f:

    # shi "¿Cómo puedes estar tan segura?"
    shi "Wie kannst du dir so sicher sein?"

# game/christmas.rpy:1996
translate german christmas_shogukuni_07_683af1cc:

    # ka "Porque pienso que todos ustedes me conocen muy bien y saben que ese es el regalo perfecto."
    ka "Weil ich denke, dass ihr mich alle sehr gut kennt und wisst, dass dies das perfekte Geschenk ist."

# game/christmas.rpy:1997
translate german christmas_shogukuni_07_a343aaf0:

    # ka "La mantita podría ser un regalo de segunda categoría."
    ka "Die Decke könnte ein sekundäres Geschenk sein."

# game/christmas.rpy:1999
translate german christmas_shogukuni_07_3b8504c7:

    # shi "Si no fueses tan sedentaria, no te afectaría tanto el frío."
    shi "Wenn du nicht so bewegungsarm wärst, würde dich die Kälte nicht so sehr beeinträchtigen."

# game/christmas.rpy:2001
translate german christmas_shogukuni_07_ae1a9d32:

    # ka "¿Qué tiene que ver las cerezas con una chuleta?"
    ka "Was haben Kirschen mit einem Hackbeil zu tun?"

# game/christmas.rpy:2003
translate german christmas_shogukuni_07_ff2926d3:

    # shi "Que ambos pueden ser comidos por una tonta como tú, Kaede."
    shi "Dass beides von einem Dummkopf wie dir gegessen werden kann, Kaede."

# game/christmas.rpy:2005
translate german christmas_shogukuni_07_05286f69:

    # ka "Podría tener eso en común... {w}Seguramente estarán deliciosas."
    ka "Das könnte ich auch haben... {w}Die sind bestimmt lecker."

# game/christmas.rpy:2007
translate german christmas_shogukuni_07_66953392:

    # ka "¡Pero lo que quería decir era otra cosa!"
    ka "Aber ich wollte eigentlich etwas anderes sagen!"

# game/christmas.rpy:2008
translate german christmas_shogukuni_07_6d494069:

    # ka "¿Qué tiene que ver el entrenamiento con tener las tetas congeladas?"
    ka "Was hat Training mit gefrorenen Brüsten zu tun?"

# game/christmas.rpy:2009
translate german christmas_shogukuni_07_25abb29f:

    # ka "Si puedo comer más, significa que tendré más grasa para cubrirme del frío."
    ka "Wenn ich mehr essen kann, bedeutet das, dass ich mehr Fett habe, um mich vor der Kälte zu schützen."

# game/christmas.rpy:2010
translate german christmas_shogukuni_07_8c312fad:

    # ka "Y además si entreno a estas horas, estaré muy cansada y no podre dormir bien."
    ka "Ausserdem werde ich, wenn ich zu dieser Uhrzeit trainiere, sehr müde sein und nicht gut schlafen können."

# game/christmas.rpy:2011
translate german christmas_shogukuni_07_09da3df7:

    # ka "Así que no, Shizuka... {w}No tiene nada que ver el sedentarismo con tener el trasero como pista de patinaje."
    ka "Also, nein, Shizuka... {w}Ein sitzender Lebensstil hat nichts damit zu tun, dass dein Hintern eine Rollschuhbahn ist."

# game/christmas.rpy:2013
translate german christmas_shogukuni_07_a4444434:

    # y "Buenas noches."
    y "Gute Nacht."

# game/christmas.rpy:2015
translate german christmas_shogukuni_07_fed4db2c:

    # ka "¡Por fin están aquí ustedes dos!"
    ka "Endlich seid ihr beide da!"

# game/christmas.rpy:2016
translate german christmas_shogukuni_07_d4b7f6ec:

    # ka "¡He tenido que soportar sola a Shizuka durante todo este tiempo!"
    ka "Ich musste mich die ganze Zeit allein mit Shizuka abfinden!"

# game/christmas.rpy:2018
translate german christmas_shogukuni_07_9686ee69:

    # shi "¿Por qué dices que tuviste que soportarme?"
    shi "Warum sagst du, dass du mich ertragen musstest?"

# game/christmas.rpy:2020
translate german christmas_shogukuni_07_31fceb70:

    # ka "Porque es la pura y santa verdad."
    ka "Weil es die reine und heilige Wahrheit ist."

# game/christmas.rpy:2022
translate german christmas_shogukuni_07_1b545ef3:

    # shi "¡Pero si ni quiera te he molestado!"
    shi "Aber ich habe dich doch gar nicht belästigt!"

# game/christmas.rpy:2024
translate german christmas_shogukuni_07_9c137036:

    # ka "Oh no, se ha enojado de nuevo."
    ka "Oh nein, sie ist schon wieder sauer."

# game/christmas.rpy:2026
translate german christmas_shogukuni_07_836efece:

    # shi "¿No te cansas de ser una estúpida Kaede?"
    shi "Hast du es nicht satt, dumm zu sein, Kaede?"

# game/christmas.rpy:2028
translate german christmas_shogukuni_07_ddf7d377:

    # y "Tranquilas."
    y "Ruhe."

# game/christmas.rpy:2030
translate german christmas_shogukuni_07_07755fe9:

    # ka "¡Ella empezó!"
    ka "Sie hat angefangen!"

# game/christmas.rpy:2032
translate german christmas_shogukuni_07_5fc54898:

    # shi "Eres insoportable..."
    shi "Du bist unerträglich..."

# game/christmas.rpy:2034
translate german christmas_shogukuni_07_676c7abd:

    # ka "Tú más..."
    ka "Du viel mehr..."

# game/christmas.rpy:2036
translate german christmas_shogukuni_07_c7e440b9:

    # shi "¡Tú aún más!"
    shi "Du immer einmal mehr!"

# game/christmas.rpy:2038
translate german christmas_shogukuni_07_1a645e30:

    # ka "No copies lo que digo."
    ka "Wiederhole nicht, was ich sage."

# game/christmas.rpy:2040
translate german christmas_shogukuni_07_78d65f84:

    # shi "¡Tú estás copiando lo que digo!"
    shi "Du wiederholst wieder, was ich gesagt habe, dass du wieder gesagt hast!"

# game/christmas.rpy:2042
translate german christmas_shogukuni_07_152eca55:

    # ka "¿Pueden ver como se comporta?"
    ka "Siehst du, wie sie sich verhält?"

# game/christmas.rpy:2043
translate german christmas_shogukuni_07_e6d5751c:

    # ka "Y tuve que soportar todo esto sola mientras ustedes llegaban."
    ka "Und ich musste das alles alleine ertragen, während du angekommen bist."

# game/christmas.rpy:2045
translate german christmas_shogukuni_07_f755799c:

    # shi "..."
    shi "..."

# game/christmas.rpy:2047
translate german christmas_shogukuni_07_226dcb67:

    # y "¿Regalos?"
    y "Geschenke?"

# game/christmas.rpy:2049
translate german christmas_shogukuni_07_2bdb38cb:

    # ka "Yo y Shizuka tenemos todo listo para entregar nuestros regalos."
    ka "Shizuka und ich haben alles bereit, um unsere Geschenke zu überreichen."

# game/christmas.rpy:2050
translate german christmas_shogukuni_07_700f8538:

    # ka "También nos aseguramos de que no haya algún entrometido cerca."
    ka "Wir sorgen auch dafür, dass keine Wichtigtuer in der Nähe sind."

# game/christmas.rpy:2051
translate german christmas_shogukuni_07_3887b0d8:

    # ka "No me gustaría ganarme un castigo de la maestra."
    ka "Ich möchte keine Strafe vom Meister bekommen."

# game/christmas.rpy:2053
translate german christmas_shogukuni_07_8b0bdf56:

    # y "Bien."
    y "Gut."

# game/christmas.rpy:2055
translate german christmas_shogukuni_07_f7a43812:

    # ka "Aunque podríamos ir a un lugar más abrigado para abrir los regalos."
    ka "Wir könnten zwar irgendwohin gehen, wo es wärmer ist, um die Geschenke auszupacken."

# game/christmas.rpy:2057
translate german christmas_shogukuni_07_6dd90073:

    # ka "BRRrrr..."
    ka "BRRrrr..."

# game/christmas.rpy:2058
translate german christmas_shogukuni_07_a6c27960:

    # ka "¡Hace demasiado frío aquí afuera!"
    ka "Es ist so kalt hier!"

# game/christmas.rpy:2059
translate german christmas_shogukuni_07_b23d8d7e:

    # ka "¡Me muero!"
    ka "Ich sterbe!"

# game/christmas.rpy:2071
translate german christmas_shogukuni_07_b1bf0009:

    # ka "¿Y como hay que hacer?"
    ka "nd wie sollte das gemacht werden?"

# game/christmas.rpy:2072
translate german christmas_shogukuni_07_c54f3d03:

    # ka "¿Quién tiene que entregar primero el regalo y quien último?"
    ka "Wer muss das Geschenk zuerst überbringen und wer muss es zuletzt überbringen?"

# game/christmas.rpy:2074
translate german christmas_shogukuni_07_3df38f15:

    # shi "¿Por qué no empiezas tú?"
    shi "Warum fängst du nicht an?"

# game/christmas.rpy:2076
translate german christmas_shogukuni_07_01cfcb53:

    # ka "Preferiría ir al final."
    ka "Ich würde lieber am Ende gehen."

# game/christmas.rpy:2077
translate german christmas_shogukuni_07_715524d9:

    # ka "¡De esa forma podré sorprenderlos a todos con mi gran sorpresa!"
    ka "So kann ich euch alle mit meiner grossen Überraschung überraschen!"

# game/christmas.rpy:2078
translate german christmas_shogukuni_07_b0f22a46:

    # ka "Así que deberías ir tu primero Shizuka."
    ka "Also solltest du zuerst gehen, Shizuka."

# game/christmas.rpy:2080
translate german christmas_shogukuni_07_8d61c447:

    # shi "Me niego."
    shi "Ich möchte nicht."

# game/christmas.rpy:2081
translate german christmas_shogukuni_07_ce4e6aee:

    # shi "No es justo que tu regalo se sienta más especial que los otros."
    shi "Es ist nicht fair, dein Geschenk als etwas Besonderes darzustellen."

# game/christmas.rpy:2083
translate german christmas_shogukuni_07_16fdf8f6:

    # ka "Solo quieres molestar."
    ka "Du willst mich nur ärgern."

# game/christmas.rpy:2085
translate german christmas_shogukuni_07_1482920f:

    # shi "¡Tú has estado molestándome desde antes!"
    shi "Du nervst mich schon seit Ewigkeiten!"

# game/christmas.rpy:2087
translate german christmas_shogukuni_07_3fadbaa2:

    # ka "¿Por qué no lo tiramos a la suerte?"
    ka "Warum werfen wir es nicht einfach weg?"

# game/christmas.rpy:2088
translate german christmas_shogukuni_07_5145377a:

    # ka "Lanzamos una moneda al aire y si sale cara, vas tú y si sale cruz, vas tú."
    ka "Hör mal, wir werfen eine Münze, und wenn Kopf kommt, gehst du, und wenn Zahl kommt, gehst du."

# game/christmas.rpy:2090
translate german christmas_shogukuni_07_57ebf474:

    # shi "¿No te das cuenta de que tú ganas en ambos casos?"
    shi "Ist dir nicht klar, dass du in beiden Fällen gewinnst?"

# game/christmas.rpy:2092
translate german christmas_shogukuni_07_fce2d9a4:

    # ka "Claro que me doy cuenta."
    ka "Na, klar."

# game/christmas.rpy:2093
translate german christmas_shogukuni_07_a186804e:

    # ka "¿Si no que gracia tendría?"
    ka "Was wäre sonst der Sinn davon?"

# game/christmas.rpy:2095
translate german christmas_shogukuni_07_ead2fed4:

    # shi "Eres una estúpida."
    shi "Idiot."

# game/christmas.rpy:2097
translate german christmas_shogukuni_07_d7abff45:

    # mc "Si quieren yo empiezo entregando mi regalo y luego sigue el que lo reciba."
    mc "Wenn ihr wollt, fange ich mit meinem Geschenk an, und dann ist der Beschenkte an der Reihe."

# game/christmas.rpy:2099
translate german christmas_shogukuni_07_e3580e77:

    # ka "¡Eso es lo que quería escuchar!"
    ka "Das wollte ich hören!"

# game/christmas.rpy:2100
translate german christmas_shogukuni_07_e04a49f4:

    # ka "¿Ya vez como es más fácil cuando hay alguien con iniciativa?"
    ka "Siehst du, wie viel einfacher es ist, wenn jemand Initiative zeigt?"

# game/christmas.rpy:2102
translate german christmas_shogukuni_07_2c3b0cd2:

    # shi "Cierra la bocota."
    shi "Halt doch einfach deine blöde Klappe."

# game/christmas.rpy:2104
translate german christmas_shogukuni_07_3291e9ec:

    # mc "La idea es decir unas palabras antes de entregar el regalo."
    mc "Die Idee ist, vor der Übergabe des Geschenks ein paar Worte zu sagen."

# game/christmas.rpy:2106
translate german christmas_shogukuni_07_413dba5f:

    # shi "Nunca dijiste nada sobre eso."
    shi "Davon hast du nie etwas gesagt."

# game/christmas.rpy:2108
translate german christmas_shogukuni_07_38e81cc2:

    # mc "Pienso que sería más interesante."
    mc "Ich denke, das wäre interessanter."

# game/christmas.rpy:2110
translate german christmas_shogukuni_07_7775c3fb:

    # ka "¡Wojojo!"
    ka "Hou Hou Hou!"

# game/christmas.rpy:2111
translate german christmas_shogukuni_07_e10690b9:

    # ka "¡Necesitaré tiempo para pensar en un discurso!"
    ka "Ich brauche Zeit, um mir eine Rede zu überlegen!"

# game/christmas.rpy:2113
translate german christmas_shogukuni_07_849d1d06:

    # mc "No es necesario que sea algo grandilocuente."
    mc "Es muss nichts Grosses sein."

# game/christmas.rpy:2114
translate german christmas_shogukuni_07_6ce89a98:

    # mc "Con unas cuentas palabras honestas es suficiente."
    mc "Ein paar ehrliche Worte werden genügen."

# game/christmas.rpy:2115
translate german christmas_shogukuni_07_87d8004a:

    # mc "¿Qué les parece si comenzamos?"
    mc "Wie wär's, wenn wir anfangen?"

# game/christmas.rpy:2116
translate german christmas_shogukuni_07_fdadd2e0:

    # mc "Sé que soy el más nuevo y no fui parte de los Bagliore hasta hace poco."
    mc "Ich weiß, dass ich der Neuling bin und bis vor kurzem noch nicht zu den Bagliore gehörte."

# game/christmas.rpy:2117
translate german christmas_shogukuni_07_51543b3e:

    # mc "Pero he compartido un tiempo agradable con ustedes."
    mc "Aber ich habe eine angenehme Zeit mit dir verbracht."

# game/christmas.rpy:2118
translate german christmas_shogukuni_07_1f370b65:

    # mc "Aunque a veces ha sido más doloroso de lo que esperaba."
    mc "Obwohl es manchmal schmerzhafter war, als ich erwartet hatte."

# game/christmas.rpy:2119
translate german christmas_shogukuni_07_e6cbd050:

    # mc "Sobre todo por la persona a la que debo entregar un regalo el día de hoy."
    mc "Besonders für die Person, der ich heute ein Geschenk überreichen muss."

# game/christmas.rpy:2120
translate german christmas_shogukuni_07_ada3e2b4:

    # mc "Mi amigo secreto es Shizuka."
    mc "Meine Beschenkte ist, Shizuka."

# game/christmas.rpy:2122
translate german christmas_shogukuni_07_a677cf17:

    # shi "¿Qué dices?"
    shi "Was?"

# game/christmas.rpy:2123
translate german christmas_shogukuni_07_c0901648:

    # shi "Pensaba que seria Kaede y me regalaría alguna tontería..."
    shi "Ich dachte, es wäre Kaede und sie würde mir irgendwelchen Unsinn erzählen..."

# game/christmas.rpy:2124
translate german christmas_shogukuni_07_e61c4db7:

    # shi "Esto es... {w}Inesperado..."
    shi "Das ist... {w}unerwartet..."

# game/christmas.rpy:2126
translate german christmas_shogukuni_07_2b0354c1:

    # ka "Ahora tienes que pararte frente a [nombre] para recibir tu regalo, Shizuka."
    ka "Jetzt musst du dich vor [nombre] stellen, um dein Geschenk zu erhalten, Shizuka."

# game/christmas.rpy:2128
translate german christmas_shogukuni_07_23720bea:

    # shi "¿Qué?"
    shi "Was?"

# game/christmas.rpy:2129
translate german christmas_shogukuni_07_f6e702fd:

    # shi "¡Si!"
    shi "Ja!"

# game/christmas.rpy:2131
translate german christmas_shogukuni_07_cd44652c:

    # ka "Debes decir un discurso antes de entregar el regalo, [nombre]."
    ka "Du musst eine Rede halten, bevor du das Geschenk überreichst, [nombre]."

# game/christmas.rpy:2133
translate german christmas_shogukuni_07_29406909:

    # shi "¡Que no es un discurso!"
    shi "Das ist keine Rede!"

# game/christmas.rpy:2135
translate german christmas_shogukuni_07_e64f9863:

    # mc "Solo unas palabras."
    mc "Nur ein paar Worte."

# game/christmas.rpy:2136
translate german christmas_shogukuni_07_f755799c_1:

    # shi "..."
    shi "..."

# game/christmas.rpy:2137
translate german christmas_shogukuni_07_6532d77b:

    # mc "Shizuka... {w}Cuando te conocí por primera vez creo que no nos soportábamos el uno al otro."
    mc "Shizuka... {w}Als ich dich zum ersten Mal traf, konnten wir uns, glaube ich, nicht ausstehen."

# game/christmas.rpy:2138
translate german christmas_shogukuni_07_1a4d3dcf:

    # mc "Lo único que sabía sobre ti era que le habías hecho daño a Yui, así que era imposible sentir simpatía por ti."
    mc "Das Einzige, was ich über dich wusste, war, dass du Yui verletzt hattest, daher war es mir unmöglich, Mitgefühl für dich zu empfinden."

# game/christmas.rpy:2140
translate german christmas_shogukuni_07_f755799c_2:

    # shi "..."
    shi "..."

# game/christmas.rpy:2141
translate german christmas_shogukuni_07_a074e9c1:

    # mc "Pero luego durante mi peregrinaje me di cuenta de que en realidad no eres una mala persona."
    mc "Aber dann habe ich während meiner Pilgerreise erkannt, dass du eigentlich kein schlechter Mensch bist."

# game/christmas.rpy:2142
translate german christmas_shogukuni_07_459c53fa:

    # mc "Quizás solo necesitabas que alguien te escuchara."
    mc "Vielleicht brauchst du einfach nur jemanden, der dir zuhört."

# game/christmas.rpy:2144
translate german christmas_shogukuni_07_f755799c_3:

    # shi "..."
    shi "..."

# game/christmas.rpy:2146
translate german christmas_shogukuni_07_612f29d1:

    # mc "Pienso que esto te podría gustar, aunque puedo entender si no te gusta."
    mc "Ich denke, das könnte dir gefallen, aber ich kann auch verstehen, wenn es dir nicht gefällt."

# game/christmas.rpy:2147
translate german christmas_shogukuni_07_474e4ce9:

    # mc "Creo que será una sorpresa inesperada."
    mc "Ich denke, es wird eine unerwartete Überraschung sein."

# game/christmas.rpy:2149
translate german christmas_shogukuni_07_f755799c_4:

    # shi "..."
    shi "..."

# game/christmas.rpy:2160
translate german christmas_shogukuni_07_fe045a69:

    # shi "Gracias."
    shi "Danke."

# game/christmas.rpy:2162
translate german christmas_shogukuni_07_2b35b6fe:

    # mc "..."
    mc "..."

# game/christmas.rpy:2163
translate german christmas_shogukuni_07_d8ae036b:

    # mc "¿No piensas abrir el regalo?"
    mc "Möchtest du es nicht öffnen?"

# game/christmas.rpy:2165
translate german christmas_shogukuni_07_b09afe6b:

    # shi "Prefiero estar sola cuando vea lo que hay dentro."
    shi "Ich bin lieber allein, wenn ich mir das Innere ansehe."

# game/christmas.rpy:2166
translate german christmas_shogukuni_07_194a60de:

    # shi "Espero que no sea un regalo de broma."
    shi "Ich hoffe, es ist kein Scherzgeschenk."

# game/christmas.rpy:2168
translate german christmas_shogukuni_07_cb1e0aa8:

    # mc "Claro que no."
    mc "Natürlich nicht."

# game/christmas.rpy:2169
translate german christmas_shogukuni_07_79b6924c:

    # mc "De verdad pienso que te podría gustar."
    mc "Ich glaube wirklich, dass es Ihnen gefallen könnte."

# game/christmas.rpy:2171
translate german christmas_shogukuni_07_9ca26f6d:

    # shi "Bien."
    shi "Gut."

# game/christmas.rpy:2173
translate german christmas_shogukuni_07_c40a6f29:

    # mc "Bueno... {w}Si prefieres ver el regalo en otro momento, entonces es tu turno."
    mc "Nun... {w}Wenn du das Geschenk lieber zu einem anderen Zeitpunkt sehen möchtest, dann bist du dran."

# game/christmas.rpy:2175
translate german christmas_shogukuni_07_f5adb56f:

    # shi "Muchas gracias."
    shi "Danke dir vielmals."

# game/christmas.rpy:2179
translate german christmas_shogukuni_07_f755799c_5:

    # shi "..."
    shi "..."

# game/christmas.rpy:2181
translate german christmas_shogukuni_07_f5f914cd:

    # shi "No se me ocurre como comenzar..."
    shi "Ich weiss nicht, wie ich anfangen soll..."

# game/christmas.rpy:2182
translate german christmas_shogukuni_07_c364920d:

    # shi "Pero..."
    shi "Aber..."

# game/christmas.rpy:2183
translate german christmas_shogukuni_07_ddaddff1:

    # shi "Supongo que tengo que hacerlo de todas formas..."
    shi "Ich schätze, ich muss es sowieso tun..."

# game/christmas.rpy:2185
translate german christmas_shogukuni_07_75b9b4e5:

    # ka "¡Menos vuelta y más acción!"
    ka "Weniger reden und mehr handeln!"

# game/christmas.rpy:2187
translate german christmas_shogukuni_07_d8465ce6:

    # shi "¡Cállate Kaede!"
    shi "Maul halten, Kaede!"

# game/christmas.rpy:2189
translate german christmas_shogukuni_07_1ef80db3:

    # shi "Siento haberme enojado..."
    shi "Es tut mir leid, dass ich wütend geworden bin..."

# game/christmas.rpy:2190
translate german christmas_shogukuni_07_12aaa736:

    # shi "Sé que no he sido una persona amable durante todos estos años."
    shi "Ich weiss, dass ich all die Jahre kein netter Mensch gewesen bin."

# game/christmas.rpy:2191
translate german christmas_shogukuni_07_c0c8d105:

    # shi "Que a pesar de todo el tiempo que convivimos juntos, nunca pude apreciarte lo suficiente."
    shi "Dass ich dich trotz all der Zeit, die wir zusammen gelebt haben, nie genug schätzen konnte."

# game/christmas.rpy:2192
translate german christmas_shogukuni_07_faec91d7:

    # shi "Y a pesar de mi hostilidad... {w}Tú siempre fuiste amable conmigo..."
    shi "Und trotz meiner Feindseligkeit... {w}Du warst immer freundlich zu mir..."

# game/christmas.rpy:2194
translate german christmas_shogukuni_07_b1d624b0:

    # ka "Seguramente está hablando de mí."
    ka "Sie spricht sicherlich von mir."

# game/christmas.rpy:2196
translate german christmas_shogukuni_07_e6e52d34:

    # shi "¡Que te calles Kaede!"
    shi "Halt die Klappe, Kaede!"

# game/christmas.rpy:2198
translate german christmas_shogukuni_07_9f90b048:

    # ka "Que gruñona..."
    ka "So mürrisch.."

# game/christmas.rpy:2200
translate german christmas_shogukuni_07_39c5b613:

    # shi "¿En qué estaba?"
    shi "Wo war ich?"

# game/christmas.rpy:2201
translate german christmas_shogukuni_07_37ad3429:

    # shi "¡Acabas de matar el momento!"
    shi "Du hast gerade den Moment ruiniert!"

# game/christmas.rpy:2202
translate german christmas_shogukuni_07_c70b4310:

    # shi "Como sea, mi amigo secreto es Yui."
    shi "Wie auch immer, meine Beschenkte ist Yui."

# game/christmas.rpy:2204
translate german christmas_shogukuni_07_71a86863:

    # y "..."
    y "..."

# game/christmas.rpy:2208
translate german christmas_shogukuni_07_2afee434:

    # shi "Aquí tienes tu regalo."
    shi "Hier ist Ihr Geschenk."

# game/christmas.rpy:2210
translate german christmas_shogukuni_07_71a86863_1:

    # y "..."
    y "..."

# game/christmas.rpy:2211
translate german christmas_shogukuni_07_62ef91ea:

    # ka "¿Ni siquiera lo envolviste en papel de regalo?"
    ka "Du hast es nicht einmal in Geschenkpapier eingepackt?"

# game/christmas.rpy:2212
translate german christmas_shogukuni_07_39a20679:

    # ka "Eres una desatinada."
    ka "Wie schlimm du sein kannst..."

# game/christmas.rpy:2214
translate german christmas_shogukuni_07_3e9b0264:

    # shi "Kaede, la próxima vez te haré callar de una patada en la cara."
    shi "Kaede, das nächste Mal trete ich dir ins Gesicht und bringe dich zum Schweigen."

# game/christmas.rpy:2216
translate german christmas_shogukuni_07_6c134c7e:

    # shi "Yui..."
    shi "Yui..."

# game/christmas.rpy:2217
translate german christmas_shogukuni_07_30d1a32d:

    # shi "Sé que te quité esto hace bastantes años atras..."
    shi "Ich weiss, dass ich dir das vor langer Zeit weggenommen habe..."

# game/christmas.rpy:2218
translate german christmas_shogukuni_07_2e6c14b6:

    # shi "Lo guardé durante todo este tiempo, porque me sentía culpable por habértelo quitado."
    shi "Ich habe es die ganze Zeit aufbewahrt, weil ich mich schuldig fühlte, es dir weggenommen zu haben."

# game/christmas.rpy:2219
translate german christmas_shogukuni_07_00ffbe1b:

    # shi "Pero nunca tuve la oportunidad de devolvértelo."
    shi "Aber ich hatte nie die Gelegenheit, es dir zurückzugeben."

# game/christmas.rpy:2220
translate german christmas_shogukuni_07_68ac5fd0:

    # shi "Espero que perdones mi comportamiento."
    shi "Ich hoffe, du kannst mir mein Verhalten verzeihen."

# game/christmas.rpy:2222
translate german christmas_shogukuni_07_b5182534:

    # y "Perdonada."
    y "Vergeben."

# game/christmas.rpy:2224
translate german christmas_shogukuni_07_666d18cb:

    # shi "¿Cómo puedes perdonarme tan fácilmente por todo lo que te hice?"
    shi "Wie kannst du mir so leicht alles verzeihen, was ich dir angetan habe?"

# game/christmas.rpy:2226
translate german christmas_shogukuni_07_50fce59d:

    # y "Hermana."
    y "Schwester."

# game/christmas.rpy:2228
translate german christmas_shogukuni_07_4aac3dfe:

    # shi "¿De verdad me consideras como parte de tu familia?"
    shi "Betrachtest du mich wirklich als Teil deiner Familie?"

# game/christmas.rpy:2230
translate german christmas_shogukuni_07_4c308c28:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:2232
translate german christmas_shogukuni_07_2a2e6566:

    # shi "Gracias Yui."
    shi "Danke Yui."

# game/christmas.rpy:2233
translate german christmas_shogukuni_07_a72e13fb:

    # shi "Bueno, supongo que ahora es tu turno para entregar tu regalo Yui."
    shi "Nun, ich glaube, es ist Zeit für dein Geschenk, Yui."

# game/christmas.rpy:2237
translate german christmas_shogukuni_07_71a86863_2:

    # y "..."
    y "..."

# game/christmas.rpy:2239
translate german christmas_shogukuni_07_9b2bd284:

    # y "[nombre]."
    y "[nombre]."

# game/christmas.rpy:2241
translate german christmas_shogukuni_07_f181b98f:

    # mc "¿Qué sucede Yui?"
    mc "Was?"

# game/christmas.rpy:2243
translate german christmas_shogukuni_07_8b08c83a:

    # y "Regalo."
    y "Geschenk."

# game/christmas.rpy:2245
translate german christmas_shogukuni_07_f8bbfdf0:

    # mc "¿El regalo es para mí?"
    mc "Ist das Geschenk für mich?"

# game/christmas.rpy:2247
translate german christmas_shogukuni_07_1eb3c84b:

    # y "Si."
    y "Ja."

# game/christmas.rpy:2249
translate german christmas_shogukuni_07_efa8092a:

    # mc "¿No vas a hacer un discurso?"
    mc "Und die Rede?"

# game/christmas.rpy:2251
translate german christmas_shogukuni_07_71a86863_3:

    # y "..."
    y "..."

# game/christmas.rpy:2253
translate german christmas_shogukuni_07_ef3ae6b2:

    # y "Te quiero."
    y "Ich liebe dich."

# game/christmas.rpy:2255
translate german christmas_shogukuni_07_d48ab4ea:

    # mc "Eso fue breve."
    mc "Stimmt, du redest nicht viel."

# game/christmas.rpy:2256
translate german christmas_shogukuni_07_887b12e0:

    # mc "Aunque se suponía que la siguiente en recibir un regalo debería ser Kaede."
    mc "Obwohl eigentlich Kaede als Nächste dran gewesen wäre..."

# game/christmas.rpy:2258
translate german christmas_shogukuni_07_bf2ed835:

    # ka "..."
    ka "..."

# game/christmas.rpy:2260
translate german christmas_shogukuni_07_34773516:

    # ka "¿Qué?"
    ka "Was?"

# game/christmas.rpy:2261
translate german christmas_shogukuni_07_ae986be7:

    # ka "¿Por qué me estás mirando con esa cara?"
    ka "Warum schaust du mich so an?"

# game/christmas.rpy:2263
translate german christmas_shogukuni_07_d27268ef:

    # mc "¿A quién le pertenece tu regalo Kaede?"
    mc "Wem gehört dein Geschenk, Kaede?"

# game/christmas.rpy:2265
translate german christmas_shogukuni_07_fbafccfc:

    # ka "Tendrás que esperar para saberlo."
    ka "Das wirst du erst später erfahren."

# game/christmas.rpy:2266
translate german christmas_shogukuni_07_1c8eb96c:

    # ka "No seas tramposo."
    ka "Sei kein Betrüger."

# game/christmas.rpy:2268
translate german christmas_shogukuni_07_066cf106:

    # y "Regalo..."
    y "Na schön..."

# game/christmas.rpy:2270
translate german christmas_shogukuni_07_71a86863_4:

    # y "..."
    y "..."

# game/christmas.rpy:2271
translate german christmas_shogukuni_07_8aef6eec:

    # mc "Gracias Yui, pero..."
    mc "Danke Yui, aber..."

# game/christmas.rpy:2272
translate german christmas_shogukuni_07_7acf415e:

    # mc "¿Esto es una mascara de lobo?"
    mc "Was soll das denn sein?"

# game/christmas.rpy:2281
translate german christmas_shogukuni_07_1065be3a:

    # y "Alma."
    y "Seele."

# game/christmas.rpy:2282
translate german christmas_shogukuni_07_cda80f73:

    # mc "¿Un alma?"
    mc "Eine Seele?"

# game/christmas.rpy:2284
translate german christmas_shogukuni_07_d3099257:

    # ka "Esa máscara que te ha dado Yui recibe el nombre de Alma."
    ka "Die Seele, die dir Yui gegeben hat..."

# game/christmas.rpy:2285
translate german christmas_shogukuni_07_f7bbee94:

    # ka "Es lo que demuestras que eres un Dragón y lo recibes justo en el momento de recibir el título."
    ka "Es ist das, was du zeigst, dass du ein Drache bist, und du erhältst es genau in dem Moment, in dem du den Titel erhältst."

# game/christmas.rpy:2286
translate german christmas_shogukuni_07_e7bdfe0a:

    # ka "Eso te da el poder dar órdenes al Dragón Negro."
    ka "Das gibt dir die Macht, dem Schwarzen Drachen Befehle zu erteilen."

# game/christmas.rpy:2287
translate german christmas_shogukuni_07_f1426738:

    # ka "Significa que Yui estará dispuesta a hacer lo posible para realizar cualquier deseo que quieras."
    ka "Das bedeutet, dass Yui bereit ist, ihr Bestes zu geben, um jeden Wunsch von dir zu erfüllen."

# game/christmas.rpy:2288
translate german christmas_shogukuni_07_f1d6a772:

    # ka "Sin importar ninguna consecuencia."
    ka "Unabhängig von den Konsequenzen."

# game/christmas.rpy:2289
translate german christmas_shogukuni_07_a6f368fe:

    # ka "Incluso podrías decirle a Yui que salte al vacío si quieres y ella no podría negarse."
    ka "Du könntest Yui sogar sagen, sie solle in die Leere springen, wenn du wolltest, und sie könnte sich nicht weigern."

# game/christmas.rpy:2290
translate german christmas_shogukuni_07_b862f366:

    # ka "Se supone que el Alma es algo que ganas una vez que has derrotado al Dragón Negro para convertirte en su sucesor."
    ka "Die Seele soll etwas sein, das man erhält, wenn man den Schwarzen Drachen besiegt hat, um sein Nachfolger zu werden."

# game/christmas.rpy:2291
translate german christmas_shogukuni_07_f18e79fe:

    # ka "Es un concepto similar a lo que sucede en la lucha libre enmascarada, solo que esto también representa tú título de Dragón y tu vida."
    ka "Es ist ein ähnliches Konzept wie beim maskierten Wrestling, nur dass es hier auch um deinen Drachen-Titel und dein Leben geht."

# game/christmas.rpy:2293
translate german christmas_shogukuni_07_b1cb8dd4:

    # mc "Ya veo..."
    mc "Verstehe..."

# game/christmas.rpy:2294
translate german christmas_shogukuni_07_fcce3f92:

    # mc "¿Estás segura de darme algo como esto Yui?"
    mc "Bist du dir sicher, dass du mir so etwas geben willst, Yui?"

# game/christmas.rpy:2295
translate german christmas_shogukuni_07_ce1e147d:

    # mc "Creo que parece algo demasiado importante."
    mc "Ich finde, das scheint ein bisschen zu wichtig zu sein."

# game/christmas.rpy:2297
translate german christmas_shogukuni_07_4c308c28_1:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:2299
translate german christmas_shogukuni_07_0986a535:

    # shi "Yui en pocas palabras está entregándote su vida a cambio de nada."
    shi "Yui gibt dir, kurz gesagt, ihr Leben für nichts."

# game/christmas.rpy:2300
translate german christmas_shogukuni_07_ccf9d66d:

    # shi "Si tú aceptas el Alma, la vida del Dragón Negro te pertenecerá por completo."
    shi "Wenn du die Seele akzeptierst, wird das Leben des Schwarzen Drachen vollständig dir gehören."

# game/christmas.rpy:2301
translate german christmas_shogukuni_07_8d6b65f8:

    # shi "Es una deuda de vida y solo se anula después de la muerte."
    shi "Es handelt sich um eine Lebensschuld, die erst nach dem Tod getilgt ist."

# game/christmas.rpy:2302
translate german christmas_shogukuni_07_6a2836eb:

    # shi "Eso demuestra que Yui confía en ti más que cualquier otra persona."
    shi "Das zeigt, dass Yui dir mehr vertraut als jedem anderen."

# game/christmas.rpy:2304
translate german christmas_shogukuni_07_027b8442:

    # mc "No me gustaría aceptar algo que condicione la vida de Yui de esa forma."
    mc "Ich möchte nichts akzeptieren, was Yuis Leben in dieser Weise beeinflussen würde."

# game/christmas.rpy:2306
translate german christmas_shogukuni_07_b32ec1bc:

    # ka "Entonces puedes guardarlo y utilizarlo solo cuando lo necesites."
    ka "Dann kannst du es sparen und nur dann verwenden, wenn du es brauchst."

# game/christmas.rpy:2307
translate german christmas_shogukuni_07_8ede93da:

    # ka "El Dragón Negro tendrá que hacerte caso aunque sea en contra de su voluntad."
    ka "Der Schwarze Drache wird dir gehorchen müssen, auch wenn es gegen seinen Willen ist."

# game/christmas.rpy:2309
translate german christmas_shogukuni_07_583c2860:

    # mc "Lo guardaré como lo más preciado que tengo, Yui."
    mc "Ich werde es als mein kostbarstes Gut bewahren, Yui."

# game/christmas.rpy:2310
translate german christmas_shogukuni_07_c525ee03:

    # mc "Será el símbolo que prueba que nos conocimos."
    mc "Es wird das Symbol sein, das beweist, dass wir uns getroffen haben."

# game/christmas.rpy:2311
translate german christmas_shogukuni_07_33dd0aba:

    # mc "Y si algún día lo quieres de vuelta, solo tienes que pedírmelo."
    mc "Und wenn du es jemals zurückhaben möchtest, frag mich einfach."

# game/christmas.rpy:2313
translate german christmas_shogukuni_07_557971a6:

    # y "¡Feliz!"
    y "Glücklich!"

# game/christmas.rpy:2315
translate german christmas_shogukuni_07_00b9f3ec:

    # mc "Gracias Yui."
    mc "Danke Yui."

# game/christmas.rpy:2317
translate german christmas_shogukuni_07_620557be:

    # y "¿Listo?"
    y "Bereit?"

# game/christmas.rpy:2319
translate german christmas_shogukuni_07_f5b11793:

    # shi "Espera... {w}Pero si aún falta que Kaede entregue su regalo."
    shi "Warte... {w}Aber Kaede muss ihr Geschenk überreichen."

# game/christmas.rpy:2321
translate german christmas_shogukuni_07_ebc5980f:

    # ka "¡Casi lo olvidaba!"
    ka "Äh, ja, klar!"

# game/christmas.rpy:2322
translate german christmas_shogukuni_07_d34c7e2a:

    # ka "¡Aquí voy!"
    ka "Auf geht's!"

# game/christmas.rpy:2326
translate german christmas_shogukuni_07_ba24c2a7:

    # ka "Mi regalo es para la persona más especial y linda que he conocido."
    ka "Mein Geschenk ist für den besondersten und schönsten Menschen, den ich je getroffen habe."

# game/christmas.rpy:2327
translate german christmas_shogukuni_07_f3e39e6d:

    # ka "Cada vez que la veo me hace sentir especial y admiro mucho todo lo que ha logrado."
    ka "Jedes Mal, wenn ich sie sehe, gibt sie mir das Gefühl, etwas Besonderes zu sein, und ich bewundere alles, was sie erreicht hat."

# game/christmas.rpy:2329
translate german christmas_shogukuni_07_b8b1fe00:

    # ka "Es una persona simpática, inteligente, visionaria y muy fuerte también."
    ka "Sie ist eine nette Person, intelligent, visionär und auch sehr stark."

# game/christmas.rpy:2330
translate german christmas_shogukuni_07_724ba5e8:

    # ka "Desde que comenzó el Peregrinaje descubrí nuevas facetas de su vida y me ayudo a ser una mejor persona."
    ka "Seit Beginn der Pilgerreise habe ich neue Facetten ihres Lebens entdeckt, und das hat mir geholfen, ein besserer Mensch zu werden."

# game/christmas.rpy:2331
translate german christmas_shogukuni_07_18918cbf:

    # ka "Así que deseo relagar a esa persona una enorme cena para comer sin límites en el Yorokobi."
    ka "Deshalb möchte ich im Yorokobi ein groses Abendessen ohne Einschränkungen anbieten."

# game/christmas.rpy:2337
translate german christmas_shogukuni_07_c29fc013:

    # ka "¡Mi amigo secreto soy yo, Kaede!"
    ka "Mein Beschenkter bin ich selbst, Kaede!"

# game/christmas.rpy:2342
translate german christmas_shogukuni_07_e1bd1e9a:

    # ka "¿De verdad me vas a regalar toda esa cantidad de comida?"
    ka "Willst du mir wirklich so viel zu essen geben?"

# game/christmas.rpy:2344
translate german christmas_shogukuni_07_90f99739:

    # ka "¡Claro que si!"
    ka "Natürlich!"

# game/christmas.rpy:2345
translate german christmas_shogukuni_07_aa85ebf8:

    # ka "¡Te lo mereces totalmente después de todo ese trabajo duro!"
    ka "Nach all der harten Arbeit hast du es dir wirklich verdient!"

# game/christmas.rpy:2347
translate german christmas_shogukuni_07_f701a520:

    # ka "No puedo creerlo, me dejas completamente asombrada y sin palabras."
    ka "Ich kann es nicht glauben, du machst mich völlig sprachlos und verblüfft."

# game/christmas.rpy:2349
translate german christmas_shogukuni_07_b6c99369:

    # ka "Eso es porque te conozco como una tostada con mantequilla."
    ka "Das liegt daran, dass ich weiss, dass du Buttertoast magst."

# game/christmas.rpy:2351
translate german christmas_shogukuni_07_4742fb51:

    # ka "¡Eres asombrosa Kaede y muchas gracias!"
    ka "Du bist grossartig, Kaede, und vielen Dank!"

# game/christmas.rpy:2353
translate german christmas_shogukuni_07_22a47b0c:

    # shi "Kaede..."
    shi "Kaede..."

# game/christmas.rpy:2355
translate german christmas_shogukuni_07_7a6fb152:

    # ka "¿Qué sucede Shizuka?"
    ka "Was geht, Shizuka?"

# game/christmas.rpy:2357
translate german christmas_shogukuni_07_4ee5027f:

    # shi "¿Saliste tu misma en el papel y no dijiste nada?"
    shi "Warum hast du nichts davon gesagt, dass du selbst der Beschenkte bist?"

# game/christmas.rpy:2359
translate german christmas_shogukuni_07_593a60af:

    # ka "¿Qué quería que dijera?"
    ka "Was hätte ich sagen sollen?"

# game/christmas.rpy:2360
translate german christmas_shogukuni_07_deef1fd8:

    # ka "Se suponía que era un secreto y me conozco muy bien como para darme un regalo perfecto."
    ka "Es sollte ein Geheimnis bleiben, und ich kenne mich selbst zu gut, um mir selbst ein perfektes Geschenk zu machen."

# game/christmas.rpy:2362
translate german christmas_shogukuni_07_b6e0a0f0:

    # shi "¿No te das cuenta de que acabas de arruinar el momento?"
    shi "Bist du dumm oder merkst du nicht, dass du gerade den Moment ruiniert hast?"

# game/christmas.rpy:2364
translate german christmas_shogukuni_07_c27d8784:

    # ka "Si quieres puede venir a comer conmigo."
    ka "Wenn du möchtest, kannst du mit mir zu Mittag essen kommen."

# game/christmas.rpy:2366
translate german christmas_shogukuni_07_22a47b0c_1:

    # shi "Kaede..."
    shi "Kaede..."

# game/christmas.rpy:2368
translate german christmas_shogukuni_07_d3083e2c:

    # ka "¿Estás interesada en venir?"
    ka "Bist du bereit dafür?"

# game/christmas.rpy:2370
translate german christmas_shogukuni_07_a8018bcc:

    # shi "Tienes cinco segundos para correr... {w}Y no pienso contenerme para darte una paliza."
    shi "Du hast fünf Sekunden Zeit, um wegzulaufen... {w}Und ich werde mich nicht zurückhalten, dich zu verprügeln."

# game/christmas.rpy:2372
translate german christmas_shogukuni_07_3b3e9eca:

    # ka "Pero no seas así de amargada."
    ka "Sei nicht so grimmig..."

# game/christmas.rpy:2374
translate german christmas_shogukuni_07_d736037c:

    # shi "Cinco."
    shi "Fünf"

# game/christmas.rpy:2376
translate german christmas_shogukuni_07_bbbd74a5:

    # ka "¡Ya entendí es hora de correr!"
    ka "Okay, verstanden, verstanden!"

# game/christmas.rpy:2377
translate german christmas_shogukuni_07_171a5634:

    # ka "¡¡¡Nigerundayou!!!"
    ka "Nigerundayou!!!"

# game/christmas.rpy:2382
translate german christmas_shogukuni_07_c8d6edce:

    # shi "Esa Kaede no tiene arreglo."
    shi "Kaede ist nicht mehr zu retten."

# game/christmas.rpy:2383
translate german christmas_shogukuni_07_8ba39eea:

    # shi "Siempre arruina todo..."
    shi "Sie ruiniert jeden moment..."

# game/christmas.rpy:2385
translate german christmas_shogukuni_07_36e18afa:

    # y "Tranquila."
    y "Ruhe."

# game/christmas.rpy:2387
translate german christmas_shogukuni_07_a61854c4:

    # shi "No sé cómo tienes tanta paciencia, Yui."
    shi "Ich weiss nicht, wie du so viel Geduld haben kannst, Yui."

# game/christmas.rpy:2389
translate german christmas_shogukuni_07_62082be9:

    # shi "Está bien..."
    shi "Alles gut..."

# game/christmas.rpy:2390
translate german christmas_shogukuni_07_78bb8960:

    # shi "Hoy haré una excepción porque ha sido nuestra navidad."
    shi "Heute werde ich eine Ausnahme machen, weil es unser Weihnachtsfest war."

# game/christmas.rpy:2392
translate german christmas_shogukuni_07_e2280862:

    # y "Feliz."
    y "Glücklich."

# game/christmas.rpy:2394
translate german christmas_shogukuni_07_aa4312e2:

    # mc "¿Estás segura del regalo que me has dado Yui?"
    mc "Bist du dir sicher, was das Geschenk angeht, das du mir gegeben hast, Yui?"

# game/christmas.rpy:2396
translate german christmas_shogukuni_07_4c308c28_2:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:2398
translate german christmas_shogukuni_07_f1c3fc76:

    # mc "No sabía que el Dragón Negro tenía un objeto para representar su vida..."
    mc "Ich wusste nicht, dass der Schwarze Drache etwas so Wichtiges hatte."

# game/christmas.rpy:2400
translate german christmas_shogukuni_07_526f26c3:

    # shi "El Dragón Negro ha entregado el Alma muy pocas veces en la historia de la familia."
    shi "Der Schwarze Drache hat die Seele in der Geschichte der Familie nur sehr selten überbracht."

# game/christmas.rpy:2401
translate german christmas_shogukuni_07_d06ffd4c:

    # shi "Cuando un Dragón Negro es derrotado... {w}La mayoría prefiere morir a manos de su sucesor o practicarse el harakiri."
    shi "Wenn ein Schwarzer Drache besiegt wird... {w}Die meisten ziehen es vor, durch die Hand ihres Nachfolgers zu sterben oder Harakiri zu begehen."

# game/christmas.rpy:2402
translate german christmas_shogukuni_07_8f9d9fe6:

    # shi "Esta vez Yui rompió nuestro código y te ha entregado el Alma sin ser derrotada."
    shi "Dieses Mal hat Yui unseren Kodex gebrochen und dir die Seele übergeben, ohne besiegt worden zu sein."

# game/christmas.rpy:2403
translate german christmas_shogukuni_07_65e21d40:

    # shi "Debe confiar mucho en ti."
    shi "Sie vertraut dir mit ihrem Leben."

# game/christmas.rpy:2405
translate german christmas_shogukuni_07_3b11ab04:

    # mc "Creo que nunca me habían regalado algo tan importante antes."
    mc "Ich glaube, ich habe noch nie zuvor ein so wichtiges Geschenk bekommen."

# game/christmas.rpy:2407
translate german christmas_shogukuni_07_67b7b463:

    # y "Confianza."
    y "Vertrauen."

# game/christmas.rpy:2409
translate german christmas_shogukuni_07_eab46b4d:

    # mc "Confianza."
    mc "Vertrauen."

# game/christmas.rpy:2411
translate german christmas_shogukuni_07_72e47faa:

    # shi "Bien, ya es tarde y debemos ir a dormir Yui."
    shi "Nun, es ist spät und wir sollten schlafen gehen, Yui."

# game/christmas.rpy:2412
translate german christmas_shogukuni_07_ed49f592:

    # shi "Si nos quedamos dormidas mañana nos ganaremos un castigo."
    shi "Wenn wir einschlafen, werden wir bestraft."

# game/christmas.rpy:2414
translate german christmas_shogukuni_07_4c308c28_3:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:2416
translate german christmas_shogukuni_07_c949954d:

    # mc "Shizuka..."
    mc "Shizuka..."

# game/christmas.rpy:2417
translate german christmas_shogukuni_07_55c7d428:

    # mc "¿Puedo venir mañana durante la noche a verte?"
    mc "Kann ich morgen Abend zu dir kommen?"

# game/christmas.rpy:2419
translate german christmas_shogukuni_07_f246489c:

    # shi "¿Qué clase de pregunta es esa?"
    shi "Was ist das denn für eine Frage?"

# game/christmas.rpy:2421
translate german christmas_shogukuni_07_8c546c15:

    # mc "Es para saber si te ha gustado mi regalo."
    mc "Ich möchte wissen, ob dir mein Geschenk gefallen hat."

# game/christmas.rpy:2423
translate german christmas_shogukuni_07_04b7bde2:

    # shi "Como quieras, pero voy a ser honesta con mi opinión."
    shi "Wie du willst, aber ich werde meine Meinung ehrlich sagen."

# game/christmas.rpy:2425
translate german christmas_shogukuni_07_dc3e02cb:

    # mc "Entonces será aún mejor."
    mc "Dann wird es noch besser."

# game/christmas.rpy:2426
translate german christmas_shogukuni_07_68b5fd78:

    # mc "Ahora me despido para que puedan ir a descansar."
    mc "Jetzt verabschiede ich mich, damit du dich ausruhen kannst."

# game/christmas.rpy:2428
translate german christmas_shogukuni_07_a4444434_1:

    # y "Buenas noches."
    y "Gute Nacht."

# game/christmas.rpy:2430
translate german christmas_shogukuni_07_6ce47012:

    # mc "Buenas noches, Yui."
    mc "Gute Nacht, Yui."

# game/christmas.rpy:2436
translate german christmas_shogukuni_07_f8466c53:

    # "<Después de despedirte de Yui y Shizuka vuelves a tu habitación a dormir.>"
    "<Nachdem du dich von Yui und Shizuka verabschiedet hast, gehst du zurück in dein Zimmer, um zu schlafen.>"

# game/christmas.rpy:2449
translate german christmas_shogukuni_08_cc2c2862:

    # mc "(Prometí ir a ver a Shizuka durante la noche y eso es lo que voy a hacer.)"
    mc "(Ich habe versprochen, Shizuka heute Abend zu besuchen, und das werde ich auch tun.)"

# game/christmas.rpy:2450
translate german christmas_shogukuni_08_d4df8c99:

    # mc "(Solo espero que ella lo haya recordado.)"
    mc "(Ich hoffe nur, dass sie sich daran erinnert hat.)"

# game/christmas.rpy:2462
translate german christmas_shogukuni_08_4f95484b:

    # mc "Hola..."
    mc "Hallo..."

# game/christmas.rpy:2464
translate german christmas_shogukuni_08_ebf3c9c7:

    # shi "Hola [nombre]."
    shi "Hallo [nombre]."

# game/christmas.rpy:2465
translate german christmas_shogukuni_08_348605f3:

    # shi "Te estaba esperando."
    shi "Ich habe auf dich gewartet."

# game/christmas.rpy:2466
translate german christmas_shogukuni_08_5ff81542:

    # shi "No me habia dado cuenta que ya habias llegado."
    shi "Ich hatte nicht bemerkt, dass du schon da warst."

# game/christmas.rpy:2468
translate german christmas_shogukuni_08_414b8cad:

    # shi "Quizás llevo unas horas aquí mirando la nieve y pensado... {w}No lo sé con exactitud, perdí la noción del tiempo."
    shi "Vielleicht bin ich schon seit ein paar Stunden hier und schaue mir den Schnee an und denke nach... {w}Ich weiß es nicht genau, ich habe das Zeitgefühl verloren."

# game/christmas.rpy:2469
translate german christmas_shogukuni_08_87020c7c:

    # shi "Parece una noche extraña."
    shi "Es scheint eine seltsame Nacht zu sein."

# game/christmas.rpy:2471
translate german christmas_shogukuni_08_b1a7dbbf:

    # shi "Como sea, quería agradecerte por el vestido."
    shi "Wie auch immer, ich wollte mich für das Kleid bedanken."

# game/christmas.rpy:2473
translate german christmas_shogukuni_08_b609aa69:

    # mc "Te vez bonita con eso puesto."
    mc "Das steht dir gut."

# game/christmas.rpy:2475
translate german christmas_shogukuni_08_f755799c:

    # shi "..."
    shi "..."

# game/christmas.rpy:2476
translate german christmas_shogukuni_08_f7df9ee1:

    # mc "Debo admitir que fue difícil pensar en un regalo perfecto para ti."
    mc "Ich muss zugeben, dass es schwierig war, ein passendes Geschenk für dich zu finden."

# game/christmas.rpy:2478
translate german christmas_shogukuni_08_9bf9e3df:

    # shi "Has tenido buen gusto para escoger."
    shi "Du hast eine gute Wahl getroffen."

# game/christmas.rpy:2480
translate german christmas_shogukuni_08_8db5a5ef:

    # mc "Yui me ha ayudado."
    mc "Yui hat mir geholfen."

# game/christmas.rpy:2482
translate german christmas_shogukuni_08_5ea85739:

    # shi "Espera..."
    shi "Warte..."

# game/christmas.rpy:2483
translate german christmas_shogukuni_08_71d312e2:

    # shi "¿Ella dijo que me regalaras un vestido?"
    shi "Hat sie gesagt, du sollst mir ein Kleid geben?"

# game/christmas.rpy:2485
translate german christmas_shogukuni_08_9df83ffa:

    # mc "Tuve que hacer trampa y pedir un consejo, ya que no tenía ni idea de tus gustos."
    mc "Ich musste schummeln und um einen Rat fragen, da ich keine Ahnung von deinen Vorlieben hatte."

# game/christmas.rpy:2486
translate german christmas_shogukuni_08_d080a08a:

    # mc "Cuando Yui dijo que podría ser un vestido me sorprendí."
    mc "Als Yui sagte, es könnte ein Kleid sein, war ich überrascht."

# game/christmas.rpy:2487
translate german christmas_shogukuni_08_ed0f5272:

    # mc "No pensaba que alguien como tu tendría ganas de usar algo así."
    mc "Ich hätte nicht gedacht, dass jemand wie du so etwas tragen möchte."

# game/christmas.rpy:2488
translate german christmas_shogukuni_08_9a5052e8:

    # mc "Estuvo durante horas ayudándome a encontrar el mejor vestido."
    mc "Sie verbrachte Stunden damit, mir bei der Suche nach dem besten Kleid zu helfen."

# game/christmas.rpy:2489
translate german christmas_shogukuni_08_bf589eb7:

    # mc "Tenía miedo de que pensaras que mi regalo se trataba de una broma."
    mc "Ich hatte Angst, du würdest mein Geschenk für einen Scherz halten."

# game/christmas.rpy:2491
translate german christmas_shogukuni_08_9401cf5f:

    # shi "¿Por qué pensabas eso?"
    shi "Warum denkst du das?"

# game/christmas.rpy:2493
translate german christmas_shogukuni_08_34d8155d:

    # mc "Porque no te conozco lo suficiente."
    mc "Weil ich dich nicht gut genug kenne."

# game/christmas.rpy:2494
translate german christmas_shogukuni_08_96ce5495:

    # mc "Ni siquiera estaba seguro de que querías participar en la pequeña celebración de navidad."
    mc "Ich war mir nicht einmal sicher, ob du an der kleinen Weihnachtsfeier teilnehmen wolltest."

# game/christmas.rpy:2495
translate german christmas_shogukuni_08_c547a3b6:

    # mc "Cuando quería convencerte de usar el traje navideño de Kaede..."
    mc "Als ich dich davon überzeugen wollte, Kaedes Weihnachtskostüm zu tragen..."

# game/christmas.rpy:2496
translate german christmas_shogukuni_08_fe6deebb:

    # mc "Te notaba distante y dubitativa."
    mc "Ich habe gemerkt, dass du distanziert und unsicher warst."

# game/christmas.rpy:2497
translate german christmas_shogukuni_08_a97e361c:

    # mc "Y fue cuando dijiste que solo veías a Yui y Kaede como rivales y nada más."
    mc "Und da hast du gesagt, dass du Yui und Kaede nur als Rivalinnen siehst und sonst nichts."

# game/christmas.rpy:2498
translate german christmas_shogukuni_08_a097c6b0:

    # mc "Que querías mantener las tradiciones porque los Dragones no tienen una utilidad real en estos tiempos."
    mc "Dass du die Traditionen bewahren wolltest, weil die Drachen heutzutage keinen wirklichen Nutzen mehr haben."

# game/christmas.rpy:2499
translate german christmas_shogukuni_08_e825ddbd:

    # mc "Y puede que tengas razón, pero pienso que solo estás viendo un lado de la moneda."
    mc "Vielleicht hast du recht, aber ich glaube, du siehst nur eine Seite der Medaille."

# game/christmas.rpy:2501
translate german christmas_shogukuni_08_4af6d4eb:

    # shi "Lo recuerdo..."
    shi "Ich erinnere mich daran..."

# game/christmas.rpy:2503
translate german christmas_shogukuni_08_77ea3a1d:

    # mc "Espero que no te molestase demasiado."
    mc "Ich hoffe, es hat dich nicht allzu sehr gestört."

# game/christmas.rpy:2505
translate german christmas_shogukuni_08_1fc9338b:

    # shi "Me ha gustado compartir con ustedes."
    shi "Es hat mir gefallen, mit dir zu teilen."

# game/christmas.rpy:2506
translate german christmas_shogukuni_08_f2687fe5:

    # shi "Aunque Kaede ha arruinado el momento con sus estupideces."
    shi "Obwohl Kaede mit ihrem Unsinn den Moment ruiniert hat."

# game/christmas.rpy:2508
translate german christmas_shogukuni_08_1f15794a:

    # mc "Pudo haber sido peor..."
    mc "Es hätte schlimmer sein können."

# game/christmas.rpy:2509
translate german christmas_shogukuni_08_a1dc3078:

    # mc "¿Qué crees que te habría regalado Kaede si hubieses sido tu su amigo secreto?"
    mc "Was glaubst du, hätte sie dir gegeben, wenn du sein Beschenkter gewesen wärst?"

# game/christmas.rpy:2511
translate german christmas_shogukuni_08_8f9cf45d:

    # shi "Seguramente una tonteria y se habría ganado una paliza por eso."
    shi "Sicherlich eine Dummheit, für die er eine Tracht Prügel verdient hätte."

# game/christmas.rpy:2512
translate german christmas_shogukuni_08_59e1298a:

    # shi "Pero tienes razón, pudo haber sido peor."
    shi "Aber du hast Recht, es hätte schlimmer kommen können."

# game/christmas.rpy:2514
translate german christmas_shogukuni_08_cd3259de:

    # shi "Esto..."
    shi "Das..."

# game/christmas.rpy:2515
translate german christmas_shogukuni_08_7cd249cd:

    # shi "No quiero que pienses que odio la navidad, [nombre]."
    shi "Ich möchte nicht, dass du denkst, ich würde Weihnachten hassen, [nombre]."

# game/christmas.rpy:2516
translate german christmas_shogukuni_08_28bad7a9:

    # shi "Solo es una época del año que no me trae buenos recuerdos..."
    shi "Es ist einfach eine Jahreszeit, die keine guten Erinnerungen weckt..."

# game/christmas.rpy:2517
translate german christmas_shogukuni_08_016aefde:

    # shi "Cuando dijiste por primera vez que querías hacer algo especial."
    shi "Als du zum ersten Mal gesagt hast, dass du etwas Besonderes machen willst."

# game/christmas.rpy:2518
translate german christmas_shogukuni_08_c014272f:

    # shi "Me agradaba la idea..."
    shi "Das fand ich gut..."

# game/christmas.rpy:2519
translate german christmas_shogukuni_08_fc4e7140:

    # shi "Pero al pensar sobre eso durante la noche, me trajo malos recuerdos."
    shi "Aber das Nachdenken während der Nacht brachte schlechte Erinnerungen zurück."

# game/christmas.rpy:2521
translate german christmas_shogukuni_08_0154d673:

    # mc "Buscabas una excusa para no tener que participar."
    mc "Du hast nach einer Ausrede gesucht, um nicht teilnehmen zu müssen."

# game/christmas.rpy:2523
translate german christmas_shogukuni_08_30bd2ff9:

    # shi "Algo así..."
    shi "Ja, so etwas in der Art..."

# game/christmas.rpy:2524
translate german christmas_shogukuni_08_f391f3aa:

    # shi "Sé que fue una inmadurez de mi parte."
    shi "Ich weiss, dass das unreif von mir war."

# game/christmas.rpy:2525
translate german christmas_shogukuni_08_25fa8993:

    # shi "Y cuando vi el nombre en el papel que decía Yui..."
    shi "Und als ich den Namen auf dem Zettel sah, auf dem „Yui“ stand..."

# game/christmas.rpy:2526
translate german christmas_shogukuni_08_d2723d1b:

    # shi "Se me hizo un nudo en el estómago."
    shi "Ich hatte einen Knoten im Magen."

# game/christmas.rpy:2527
translate german christmas_shogukuni_08_e933c8b0:

    # shi "Pero también pensaba que sería una buena oportunidad para devolverle lo que le había quitado."
    shi "Aber ich dachte auch, dass dies eine gute Gelegenheit wäre, ihm zurückzugeben, was ich ihm genommen hatte."

# game/christmas.rpy:2529
translate german christmas_shogukuni_08_82bc287b:

    # mc "¿Qué era eso?"
    mc "Was war das denn?"

# game/christmas.rpy:2531
translate german christmas_shogukuni_08_85657a4d:

    # shi "Era un juguete que traía Yui de su vida pasada, antes de ser adoptada por Donato."
    shi "Es war ein Spielzeug, das Yui aus ihrem früheren Leben mitgebracht hatte, bevor sie von Donato adoptiert wurde."

# game/christmas.rpy:2532
translate german christmas_shogukuni_08_df613fe0:

    # shi "Pero cuando nuestra rivalidad se intensificó..."
    shi "Aber als unsere Rivalität immer größer wurde..."

# game/christmas.rpy:2534
translate german christmas_shogukuni_08_cb9179ec:

    # shi "Decidí quitárselo de las manos después de una pelea de entrenamiento."
    shi "Nach einem Trainingskampf beschloss ich, es ihr wegzunehmen."

# game/christmas.rpy:2535
translate german christmas_shogukuni_08_75c57b10:

    # shi "Yui había perdido porque no se había defendido."
    shi "Yui hatte verloren, weil sie sich nicht verteidigt hatte."

# game/christmas.rpy:2536
translate german christmas_shogukuni_08_d32e15b4:

    # shi "Ese día me sentí muy culpable por lo que había hecho..."
    shi "An diesem Tag fühlte ich mich sehr schuldig für das, was ich getan hatte..."

# game/christmas.rpy:2537
translate german christmas_shogukuni_08_2f0285f8:

    # shi "Pero no quería devolvérselo porque me haría ver débil y vulnerable."
    shi "Aber ich wollte es ihm nicht zurückgeben, weil ich dadurch schwach und verletzlich gewirkt hätte."

# game/christmas.rpy:2539
translate german christmas_shogukuni_08_25e25592:

    # mc "Creo que tomaste la decisión correcta al devolverlo en una ocasión como esta."
    mc "Ich denke, du hast die richtige Entscheidung getroffen."

# game/christmas.rpy:2540
translate german christmas_shogukuni_08_834259e3:

    # mc "¿Eso era lo que te trae malos recuerdos?"
    mc "Ist es das, was schlechte Erinnerungen zurückbringt?"

# game/christmas.rpy:2542
translate german christmas_shogukuni_08_a9ac6d34:

    # shi "No..."
    shi "Nein..."

# game/christmas.rpy:2543
translate german christmas_shogukuni_08_6238af03:

    # shi "Yo..."
    shi "Ich..."

# game/christmas.rpy:2544
translate german christmas_shogukuni_08_cee9d96c:

    # shi "Me duele decir en esto..."
    shi "Es schmerzt mich, dies zu sagen..."

# game/christmas.rpy:2546
translate german christmas_shogukuni_08_b5095011:

    # shi "Pero fue en navidad cuando deje mi hogar por primera vez..."
    shi "Aber es war zu Weihnachten, als ich zum ersten Mal von zu Hause weggegangen bin..."

# game/christmas.rpy:2547
translate german christmas_shogukuni_08_4466a046:

    # shi "Se supone que la navidad es una fecha de reunión familiar y para compartir con tus seres queridos..."
    shi "Weihnachten ist eigentlich ein Fest, an dem man mit der Familie zusammenkommt und Zeit mit seinen Lieben verbringt..."

# game/christmas.rpy:2548
translate german christmas_shogukuni_08_08d3f417:

    # shi "Una época llena de regalos y felicidad..."
    shi "Eine Zeit voller Geschenke und Freude..."

# game/christmas.rpy:2549
translate german christmas_shogukuni_08_71b0fb5e:

    # shi "Pero para mí fue totalmente lo contrario."
    shi "Aber für mich war es genau das Gegenteil."

# game/christmas.rpy:2551
translate german christmas_shogukuni_08_6ff908ab:

    # shi "Fue en navidad cuando mis padres me entregaron a los Bagliore."
    shi "Es war zu Weihnachten, als meine Eltern mich den Bagliores übergaben."

# game/christmas.rpy:2552
translate german christmas_shogukuni_08_d4e3a263:

    # shi "Fue en navidad cuando conocí a la maestra Felicia por primera vez."
    shi "Als ich Meisterin Felicia zum ersten Mal traf."

# game/christmas.rpy:2553
translate german christmas_shogukuni_08_6f4a5ceb:

    # shi "Fue en navidad cuando me di cuenta de que no era suficiente para mi propia familia..."
    shi "Als mir klar wurde, dass ich für meine eigene Familie nicht genug war..."

# game/christmas.rpy:2554
translate german christmas_shogukuni_08_9f049e31:

    # shi "Que por ser una mujer no serviría para los propósitos de mis padres..."
    shi "Dass ich als Frau nicht den Vorstellungen meiner Eltern entsprechen würde..."

# game/christmas.rpy:2556
translate german christmas_shogukuni_08_8231ec70:

    # mc "¿Qué propósitos eran esos?"
    mc "Was waren diese Zwecke?"

# game/christmas.rpy:2558
translate german christmas_shogukuni_08_a7282d75:

    # shi "Vengo de una familia de maestros de artes marciales."
    shi "Ich komme aus einer Familie von Kampfkunstlehrern."

# game/christmas.rpy:2559
translate german christmas_shogukuni_08_f0f3f3f7:

    # shi "Algunos incluso han ganando premios y reconocimiento alrededor del mundo."
    shi "Einige davon wurden sogar weltweit mit Preisen und Auszeichnungen geehrt."

# game/christmas.rpy:2560
translate german christmas_shogukuni_08_c9b4a33d:

    # shi "Pero yo era una chica débil... {w}De estatura baja y cuerpo delgado..."
    shi "Aber ich war ein schwaches Mädchen... {w}Klein und sehr dünn..."

# game/christmas.rpy:2561
translate german christmas_shogukuni_08_0f179aa0:

    # shi "Por alguna razón la herencia genética había salido de la peor forma posible conmigo..."
    shi "Aus irgendeinem Grund hatte sich das Erbgut bei mir auf die schlimmste Weise entwickelt..."

# game/christmas.rpy:2562
translate german christmas_shogukuni_08_fbead946:

    # shi "Comparada con mis hermanos... {w}No tenía lo necesario para luchar y traer honor a mis padres..."
    shi "Im Vergleich zu meinen Brüdern ... {w}hatte ich nicht das Zeug dazu, zu kämpfen und meinen Eltern Ehre zu machen .."

# game/christmas.rpy:2563
translate german christmas_shogukuni_08_b7053dc6:

    # shi "La única que me tendió una mano fue la maestra Felicia."
    shi "Die Einzige, die mir die Hand reichte, war Meisterin Felicia."

# game/christmas.rpy:2564
translate german christmas_shogukuni_08_75c10271:

    # shi "Mientras mi familia se deshizo de mí como si fuese un perro callejero..."
    shi "Während meine Familie mich wie einen streunenden Hund weggeworfen hat..."

# game/christmas.rpy:2565
translate german christmas_shogukuni_08_17bbe7fd:

    # shi "Y cuando llegue al Shogukuni por primera vez..."
    shi "Und als ich zum ersten Mal nach Shogukuni gekommen bin..."

# game/christmas.rpy:2567
translate german christmas_shogukuni_08_8cf326a5:

    # shi "¡Quería demostrar a todos esos malditos que se equivocaban!"
    shi "Wollte ich es allen diesen Mistkerlen beweisen, dass sie sich irrten!"

# game/christmas.rpy:2569
translate german christmas_shogukuni_08_1b82753d:

    # shi "¡Iba a entrenar tan duro que podría superar todas esas barreras físicas que me limitaban!"
    shi "Ich wollte so hart trainieren, dass ich all die körperlichen Barrieren überwinden konnte, die mich einschränkten!"

# game/christmas.rpy:2571
translate german christmas_shogukuni_08_79daa525:

    # shi "Pero al llegar al Shogukuni conocí a Yui y a Kaede."
    shi "Als ich jedoch im Shogukuni ankam, traf ich Yui und Kaede."

# game/christmas.rpy:2572
translate german christmas_shogukuni_08_5ef0264e:

    # shi "Yui siempre parecía ser la favorita de Donato..."
    shi "Yui schien immer Donato's Liebling zu sein..."

# game/christmas.rpy:2573
translate german christmas_shogukuni_08_c0ad98ff:

    # shi "Mientras que Kaede siempre había pertenecido a este lugar."
    shi "Während Kaede schon immer hierher gehörte."

# game/christmas.rpy:2574
translate german christmas_shogukuni_08_543f334f:

    # shi "Incluso su padre, el maestro Takeo, había heredado el linaje del Dragón Rojo a su hija."
    shi "Sogar ihr Vater, Meister Takeo, hatte die Seele des Roten Drachen an seine Tochter weitervererbt."

# game/christmas.rpy:2575
translate german christmas_shogukuni_08_46fc3785:

    # shi "Y sería entrenada directamente por la maestra Felicia que había sido su discípula."
    shi "Und sie würde direkt von der Meisterin Felicia ausgebildet werden, die ihre Schülerin gewesen war."

# game/christmas.rpy:2577
translate german christmas_shogukuni_08_2e055f8b:

    # shi "Entre medio de todo eso estaba yo..."
    shi "Mitten in all dem war ich..."

# game/christmas.rpy:2578
translate german christmas_shogukuni_08_0abd64c0:

    # shi "Shizuka... {w}Ya ni siquiera poseía el apellido Takemura..."
    shi "Shizuka... {w}Ich trug nicht einmal mehr den Nachnamen Takemura..."

# game/christmas.rpy:2580
translate german christmas_shogukuni_08_477605b6:

    # shi "Así que tendría que ganarme mi lugar aquí."
    shi "Also müsste ich mir meinen Platz hier verdienen."

# game/christmas.rpy:2581
translate german christmas_shogukuni_08_63370da1:

    # shi "Y la única forma para lograrlo era volviéndome más fuerte que cualquiera."
    shi "Und der einzige Weg, dies zu erreichen, war, stärker als alle anderen zu werden."

# game/christmas.rpy:2582
translate german christmas_shogukuni_08_02910aaa:

    # shi "No iba a dejar que nadie me arrebatase el título de Dragón Azul."
    shi "Ich wollte mir den Titel „Blauer Drache“ von niemandem wegnehmen lassen."

# game/christmas.rpy:2584
translate german christmas_shogukuni_08_7f9ae462:

    # shi "Fue durante ese tiempo que el maestro Takeo decidió darme su apellido y me convertí en una Shiori."
    shi "In dieser Zeit beschloss Meister Takeo, mir seinen Nachnamen zu geben, und ich wurde eine Shiori."

# game/christmas.rpy:2585
translate german christmas_shogukuni_08_e5017a91:

    # shi "Pero nunca pude dejar de pensar que este lugar era un campo de batalla."
    shi "Aber ich konnte nie aufhören zu bedauern, dass dieser Ort ein Schlachtfeld war."

# game/christmas.rpy:2586
translate german christmas_shogukuni_08_7b341922:

    # shi "Y que tanto Kaede como Yui eran mis rivales a derrotar."
    shi "Und dass sowohl Kaede als auch Yui meine Rivalinnen waren, die ich besiegen musste."

# game/christmas.rpy:2587
translate german christmas_shogukuni_08_f3a63a25:

    # shi "Desde entonces han pasado muchas cosas..."
    shi "Seitdem ist viel passiert..."

# game/christmas.rpy:2589
translate german christmas_shogukuni_08_fa596023:

    # shi "Pero nunca había pensado en la navidad como un momento para compartir."
    shi "Aber ich hatte Weihnachten nie als eine Zeit des Teilens betrachtet."

# game/christmas.rpy:2590
translate german christmas_shogukuni_08_234c0337:

    # shi "Cada año me sentaba a comer estofado Tami-Yami para recordar a mi familia."
    shi "Jedes Jahr setzte ich mich hin, um Tami-Yami-Eintopf zu essen und meiner Familie zu gedenken."

# game/christmas.rpy:2592
translate german christmas_shogukuni_08_dd430f3b:

    # shi "Pero me producía mucho dolor pensar en que no me querían..."
    shi "Aber es tat mir sehr weh, zu denken, dass sie mich nicht wollten..."

# game/christmas.rpy:2593
translate german christmas_shogukuni_08_52388e9d:

    # shi "Porque yo no sería suficiente para ellos..."
    shi "Weil es für sie ausreichen würde..."

# game/christmas.rpy:2594
translate german christmas_shogukuni_08_b2015234:

    # shi "Porque olvidaron que soy una persona... {w}Que era una hija y una hermana..."
    shi "Weil sie vergessen haben, dass ich ein Mensch bin... {w}Dass ich eine Tochter und eine Schwester war..."

# game/christmas.rpy:2595
translate german christmas_shogukuni_08_d5dd9af4:

    # shi "Esperaban muchas cosas de mí y yo no era suficiente..."
    shi "Sie haben viel von mir erwartet, und ich war nicht gut genug..."

# game/christmas.rpy:2597
translate german christmas_shogukuni_08_f8ed50f7:

    # shi "¡Y lo intentaba!"
    shi "Und ich habe es versucht!"

# game/christmas.rpy:2598
translate german christmas_shogukuni_08_f56bd0e9:

    # shi "¡De verdad lo intentaba con todas mis fuerzas!"
    shi "Ich habe mich wirklich sehr bemüht!"

# game/christmas.rpy:2599
translate german christmas_shogukuni_08_60f5d92e:

    # shi "¡He odiado mi vida todo este maldito tiempo!"
    shi "Ich habe mein Leben die ganze verdammte Zeit gehasst!"

# game/christmas.rpy:2600
translate german christmas_shogukuni_08_4bdf41af:

    # shi "¡Me sentía como el ser más miserable!"
    shi "Ich fühlte mich wie das elendeste Wesen!"

# game/christmas.rpy:2602
translate german christmas_shogukuni_08_309f4884:

    # mc "Ya basta, Shizuka..."
    mc "Genug, Shizuka..."

# game/christmas.rpy:2613
translate german christmas_shogukuni_08_f755799c_1:

    # shi "..."
    shi "..."

# game/christmas.rpy:2615
translate german christmas_shogukuni_08_7ee1f675:

    # shi "Me gustaria sentir de alguna forma que pertenezco a este lugar..."
    shi "Ich würde gerne irgendwie das Gefühl haben, dass ich hierher gehöre..."

# game/christmas.rpy:2616
translate german christmas_shogukuni_08_bfc1129a:

    # shi "Que mi función como Dragón Azul es realmente de utilidad..."
    shi "Dass meine Rolle als Blauer Drache wirklich nützlich ist..."

# game/christmas.rpy:2617
translate german christmas_shogukuni_08_612abff6:

    # shi "Y voy a luchar hasta que mi cuerpo ya no pueda más."
    shi "Und ich werde kämpfen, bis mein Körper nicht mehr kann."

# game/christmas.rpy:2618
translate german christmas_shogukuni_08_3ca3fdb8:

    # shi "Hasta que cada uno de los huesos rompa y ya no quede sangre en mis venas."
    shi "Bis jeder Knochen bricht und kein Blut mehr in meinen Adern fliesst."

# game/christmas.rpy:2620
translate german christmas_shogukuni_08_23fb6566:

    # mc "¿De verdad es necesario todo eso?"
    mc "Ist das alles wirklich notwendig?"

# game/christmas.rpy:2621
translate german christmas_shogukuni_08_b3944222:

    # mc "Te estás esforzando demasiado."
    mc "Du bemühst dich zu sehr."

# game/christmas.rpy:2623
translate german christmas_shogukuni_08_a677cf17:

    # shi "¿Qué dices?"
    shi "Was sagst du da?"

# game/christmas.rpy:2625
translate german christmas_shogukuni_08_534d1e25:

    # mc "No tienes que demostrar nada a nadie."
    mc "Du musst niemandem etwas beweisen."

# game/christmas.rpy:2627
translate german christmas_shogukuni_08_38b037a9:

    # shi "Pero estoy sola..."
    shi "Aber ich bin allein..."

# game/christmas.rpy:2629
translate german christmas_shogukuni_08_b34b281c:

    # mc "Me tienes a mí y a Yui."
    mc "Du hast mich und Yui."

# game/christmas.rpy:2631
translate german christmas_shogukuni_08_f755799c_2:

    # shi "..."
    shi "..."

# game/christmas.rpy:2632
translate german christmas_shogukuni_08_3293a50c:

    # mc "Sé cómo se siente ir de un lugar a otro buscando un lugar en el mundo."
    mc "Ich weiss, wie es sich anfühlt, von Ort zu Ort zu ziehen, auf der Suche nach einem Platz in der Welt."

# game/christmas.rpy:2633
translate german christmas_shogukuni_08_9f8b3915:

    # mc "¿Pero es necesario encontrar ese lugar?"
    mc "Aber ist es notwendig, einen solchen Ort zu finden?"

# game/christmas.rpy:2634
translate german christmas_shogukuni_08_89962ea2:

    # mc "Creo que lo mejor es disfrutar del viaje."
    mc "Ich denke, das Beste ist, die Reise zu geniessen."

# game/christmas.rpy:2636
translate german christmas_shogukuni_08_bcae246d:

    # shi "Ojalá fuese tan fácil..."
    shi "Wenn es nur so einfach wäre..."

# game/christmas.rpy:2638
translate german christmas_shogukuni_08_551d61e4:

    # mc "¿Esta ha sido tu primera navidad con regalos?"
    mc "War das dein erstes Weihnachtsfest mit Geschenken?"

# game/christmas.rpy:2640
translate german christmas_shogukuni_08_099d27d9:

    # shi "Sí..."
    shi "Yeah..."

# game/christmas.rpy:2641
translate german christmas_shogukuni_08_08d6181f:

    # shi "Ha sido agradable."
    shi "Es war schön."

# game/christmas.rpy:2642
translate german christmas_shogukuni_08_4b57ac7a:

    # shi "Gracias por compartir la navidad conmigo, [nombre]."
    shi "Danke, dass du Weihnachten mit mir verbracht hast, [nombre]."

# game/christmas.rpy:2644
translate german christmas_shogukuni_08_d1cf0cbe:

    # mc "La próxima vez será mejor."
    mc "Das nächste Mal wird es besser."

# game/christmas.rpy:2646
translate german christmas_shogukuni_08_3df62407:

    # shi "¿Próxima vez?"
    shi "Nächste Mal?"

# game/christmas.rpy:2648
translate german christmas_shogukuni_08_95e30b4b:

    # mc "¿No sería interesante celebrar navidad a lo grande?"
    mc "Wäre es nicht interessant, Weihnachten groß zu feiern?"

# game/christmas.rpy:2649
translate german christmas_shogukuni_08_fc69fe44:

    # mc "Con un árbol de navidad gigante y muchas luces."
    mc "Mit einem riesigen Weihnachtsbaum und vielen Lichtern."

# game/christmas.rpy:2653
translate german christmas_shogukuni_08_db975641:

    # y "¡Listo!"
    y "Fertig!"

# game/christmas.rpy:2655
translate german christmas_shogukuni_08_015cabcc:

    # shi "¿Estabas aquí Yui?"
    shi "Wo warst du Yui?"

# game/christmas.rpy:2657
translate german christmas_shogukuni_08_f8069993:

    # y "Shizuka."
    y "Shizuka."

# game/christmas.rpy:2659
translate german christmas_shogukuni_08_54b20900:

    # shi "¿Has hecho un muñeco de nieve de mí?"
    shi "Hast du einen Schneemann, dass so aussieht wie ich gemacht?"

# game/christmas.rpy:2661
translate german christmas_shogukuni_08_725e1b98:

    # y "Hogar."
    y "Zuhause."

# game/christmas.rpy:2663
translate german christmas_shogukuni_08_a7a921fc:

    # shi "Ha quedado muy bien."
    shi "Es ist sehr gut geworden."

# game/christmas.rpy:2665
translate german christmas_shogukuni_08_df8babef:

    # mc "¿Cuánto tiempo llevabas escuchando Yui?"
    mc "Wie lange hörst du schon zu, Yui?"

# game/christmas.rpy:2667
translate german christmas_shogukuni_08_0890433d:

    # y "Suficiente."
    y "Genug."

# game/christmas.rpy:2669
translate german christmas_shogukuni_08_f755799c_3:

    # shi "..."
    shi "..."

# game/christmas.rpy:2671
translate german christmas_shogukuni_08_50fce59d:

    # y "Hermana."
    y "Schwester."

# game/christmas.rpy:2673
translate german christmas_shogukuni_08_b70fea67:

    # shi "Debes pensar que soy debil y patética..."
    shi "Du musst denken, dass ich schwach und erbärmlich bin..."

# game/christmas.rpy:2674
translate german christmas_shogukuni_08_cfdfa913:

    # shi "¿Aunque escuchaste todo eso sigues considerándome tu hermana?"
    shi "Obwohl du das alles gehört hast, betrachtest du mich immer noch als deine Schwester?"

# game/christmas.rpy:2676
translate german christmas_shogukuni_08_4c308c28:

    # y "Sí."
    y "Ja."

# game/christmas.rpy:2678
translate german christmas_shogukuni_08_f755799c_4:

    # shi "..."
    shi "..."

# game/christmas.rpy:2680
translate german christmas_shogukuni_08_9b5c61dd:

    # y "¿Triste?"
    y "Traurig?"

# game/christmas.rpy:2682
translate german christmas_shogukuni_08_b3f1f853:

    # shi "Solo me ha entrado algo en el ojo."
    shi "Nichts, es ist nur..."

# game/christmas.rpy:2684
translate german christmas_shogukuni_08_40a32bc3:

    # shi "No puedo creer que sigas considerándome tu hermana después de todo lo que dije..."
    shi "Ich kann nicht glauben, dass du mich nach allem, was ich gesagt habe, immer noch als deine Schwester betrachtest..."

# game/christmas.rpy:2686
translate german christmas_shogukuni_08_00bfce51:

    # mc "Los que te aceptan a pesar de tus defectos son tus verdaderos amigos, Shizuka."
    mc "Diejenigen, die dich trotz deiner Schwächen akzeptieren, sind deine wahren Freunde, Shizuka."

# game/christmas.rpy:2687
translate german christmas_shogukuni_08_3c418bd1:

    # mc "Y cuando puedes confiar en esos amigos, puedes decir que tienes una familia."
    mc "Und wenn man sich auf diese Freunde verlassen kann, kann man sagen, dass man eine Familie hat."

# game/christmas.rpy:2688
translate german christmas_shogukuni_08_ee831fa4:

    # mc "Este es el lugar al que perteneces y no dejes que nadie diga lo contrario."
    mc "Hier gehörst du hin, und lass dir von niemandem etwas anderes einreden."

# game/christmas.rpy:2690
translate german christmas_shogukuni_08_8d0a912b:

    # shi "[nombre]..."
    shi "[nombre]..."

# game/christmas.rpy:2692
translate german christmas_shogukuni_08_581e6bbf:

    # mc "Te lo has ganado."
    mc "Du hast dir das alles selbst verdient."

# game/christmas.rpy:2693
translate german christmas_shogukuni_08_f755799c_5:

    # shi "..."
    shi "..."

# game/christmas.rpy:2695
translate german christmas_shogukuni_08_099d27d9_1:

    # shi "Sí..."
    shi "Ja..."

# game/christmas.rpy:2697
translate german christmas_shogukuni_08_0e931096:

    # y "Frío..."
    y "Kalt..."

# game/christmas.rpy:2699
translate german christmas_shogukuni_08_be45ba18:

    # shi "Podríamos beber un poco de té para calentar."
    shi "Wir könnten etwas Tee trinken, um uns aufzuwärmen."

# game/christmas.rpy:2701
translate german christmas_shogukuni_08_b413b8c1:

    # y "¡Habitación!"
    y "Zimmer!"

# game/christmas.rpy:2706
translate german christmas_shogukuni_08_03225d8a:

    # "<Vas a la habitación de Yui a beber té mientras conversan hasta altas horas de la noche.>"
    "<Du gehst in Yui's Zimmer, um Tee zu trinken, während ihr euch bis spät in die Nacht unterhaltet.>"

# game/christmas.rpy:2708
translate german christmas_shogukuni_08_84e2219c:

    # mc "Y esa es la historia completa."
    mc "Und das ist die ganze Geschichte."

# game/christmas.rpy:2709
translate german christmas_shogukuni_08_ef83371d:

    # mc "Como pueden ver, a veces se gana y a veces se pierde."
    mc "Wie du siehst, gewinnt man manchmal und manchmal verliert man."

# game/christmas.rpy:2711
translate german christmas_shogukuni_08_2b35b6fe:

    # mc "..."
    mc "..."

# game/christmas.rpy:2712
translate german christmas_shogukuni_08_a5bb31b4:

    # mc "¿Hola?"
    mc "Hallo?"

# game/christmas.rpy:2713
translate german christmas_shogukuni_08_38d9d04b:

    # mc "(No puedo creer que las dos se durmieron sin escuchar el final.)"
    mc "(Ich kann nicht glauben, dass sie beide eingeschlafen sind, ohne das Ende zu hören.)"

# game/christmas.rpy:2714
translate german christmas_shogukuni_08_a34248b6:

    # mc "(Ha sido una navidad extraña, pero me ha dejado un buen sentimiento.)"
    mc "(Es war ein seltsames Weihnachtsfest, aber es hat mir ein gutes Gefühl hinterlassen.)"

# game/christmas.rpy:2716
translate german christmas_shogukuni_08_9258fe65:

    # mc "(Supongo que es hora de volver a mi cueva.)"
    mc "(Ich glaube, es ist Zeit, zurück in meine Höhle zu gehen.)"

# game/christmas.rpy:2717
translate german christmas_shogukuni_08_73fbfc1e:

    # mc "Buenas noches Yui... {w}Buenas noches Shizuka."
    mc "Gute Nacht, Yui... {w}Gute Nacht, Shizuka."

# game/christmas.rpy:2722
translate german christmas_shogukuni_08_133ece57:

    # shi "Espera [nombre]..."
    shi "Warte [nombre]..."

# game/christmas.rpy:2724
translate german christmas_shogukuni_08_ff569141:

    # mc "Pensaba que te habías dormido al escuchar una de mis anécdotas de casinos."
    mc "Ich dachte, du wärst eingeschlafen, als ich dir eine meiner Casino-Anekdoten erzählte."

# game/christmas.rpy:2726
translate german christmas_shogukuni_08_eeb8f0ae:

    # shi "Lo siento, no pude dormir anoche."
    shi "Es tut mir leid, ich konnte letzte Nacht nicht schlafen."

# game/christmas.rpy:2728
translate german christmas_shogukuni_08_3b2ff3d5:

    # mc "Quizás era una anécdota aburrida de escuchar."
    mc "Vielleicht war es eine langweilige Anekdote."

# game/christmas.rpy:2730
translate german christmas_shogukuni_08_f6ae540d:

    # shi "Eso también."
    shi "Das auch."

# game/christmas.rpy:2732
translate german christmas_shogukuni_08_70f619e5:

    # mc "Gracias por la sinceridad."
    mc "Danke für deine Ehrlichkeit."

# game/christmas.rpy:2734
translate german christmas_shogukuni_08_9a466ee2:

    # shi "Hablando de sinceridad... {w}Sobre lo que dije cuando estábamos afuera..."
    shi "Apropos Ehrlichkeit... {w}Was ich draußen gesagt habe..."

# game/christmas.rpy:2735
translate german christmas_shogukuni_08_95e12b99:

    # shi "Dije cosas que no debí haber dicho."
    shi "Ich habe Dinge gesagt, die ich nicht hätte sagen sollen."

# game/christmas.rpy:2737
translate german christmas_shogukuni_08_8b6289c4:

    # mc "No te sientas culpable, Shizuka."
    mc "Mach dir keine Vorwürfe, Shizuka."

# game/christmas.rpy:2738
translate german christmas_shogukuni_08_406cbadc:

    # mc "Todos necesitamos un momento para desahogarnos."
    mc "Wir alle brauchen manchmal einen Moment, um Dampf abzulassen."

# game/christmas.rpy:2739
translate german christmas_shogukuni_08_21aa8343:

    # mc "A veces nuestras ambiciones y la necesidad de competir por lo que queremos, no ciega por completo."
    mc "Manchmal lassen uns unsere Ambitionen und das Bedürfnis, um das zu kämpfen, was wir wollen, nicht völlig blind werden."

# game/christmas.rpy:2740
translate german christmas_shogukuni_08_6b323149:

    # mc "Y antes de darnos cuenta, podemos llegar a convertirnos en una peor versión de nosotros mismos."
    mc "Und bevor wir uns versehen, können wir zu einer schlechteren Version unserer selbst werden."

# game/christmas.rpy:2741
translate german christmas_shogukuni_08_eb11e862:

    # mc "Solo por lograr el anhelado éxito y la aceptación de los demás."
    mc "Nur um den ersehnten Erfolg und die Anerkennung anderer zu erreichen."

# game/christmas.rpy:2743
translate german christmas_shogukuni_08_a3b76979:

    # shi "¿Te has sentido así alguna vez?"
    shi "Hast du dich jemals so gefühlt?"

# game/christmas.rpy:2745
translate german christmas_shogukuni_08_cbbde1aa:

    # mc "Muchas veces en el pasado... {w}Pero tú me diste una valiosa lección durante el peregrinaje."
    mc "Oft in der Vergangenheit... {w}Aber du hast mir während der Pilgerreise eine wertvolle Lektion erteilt."

# game/christmas.rpy:2746
translate german christmas_shogukuni_08_3c45a10d:

    # mc "Cuando te empujé durante la pelea de entrenamiento y tu orgullo se hizo pedazos, pude entenderlo."
    mc "Als ich dich während des Trainingskampfs gestossen habe und dein Stolz in Stücke zerbrach, konnte ich das verstehen."

# game/christmas.rpy:2747
translate german christmas_shogukuni_08_f60dcfc2:

    # mc "Pudiste haberte levantado y seguir con el entrenamiento..."
    mc "Du hättest aufstehen und mit dem Training weitermachen können..."

# game/christmas.rpy:2748
translate german christmas_shogukuni_08_62dd9a07:

    # mc "Pero tus propias pretenciones te habían derrotado... {w}No había sido yo."
    mc "Aber deine eigenen Ansprüche hatten dich besiegt... {w}Ich war es nicht gewesen."

# game/christmas.rpy:2750
translate german christmas_shogukuni_08_d81f3e4c:

    # shi "Y por eso durante el duelo oficial no querías hacer trampa."
    shi "Und deshalb wolltest du während des offiziellen Duells nicht schummeln."

# game/christmas.rpy:2752
translate german christmas_shogukuni_08_637e8939:

    # mc "Podría apostar que seguramente Yui evitaba enfrentarte por esa razón."
    mc "Ich könnte wetten, dass Yui dich aus diesem Grund wahrscheinlich gemieden hat."

# game/christmas.rpy:2754
translate german christmas_shogukuni_08_95f79017:

    # shi "Solo quiero ser fuerte para que la maestra se sienta orgullosa de mí."
    shi "Ich möchte einfach nur stark sein, damit der Meister stolz auf mich ist."

# game/christmas.rpy:2755
translate german christmas_shogukuni_08_7226f8b8:

    # shi "Quiero ser fuerte para demostrar a la familia que me rechazo..."
    shi "Ich möchte stark sein, um der Familie, die mich abgelehnt hat, zu zeigen..."

# game/christmas.rpy:2756
translate german christmas_shogukuni_08_b0fa80cf:

    # shi "Que yo podía cumplir sus expectativas."
    shi "Dass ich ihre Erwartungen erfüllen konnte."

# game/christmas.rpy:2757
translate german christmas_shogukuni_08_1b994a60:

    # shi "Pero no importa cuantas veces lo intente."
    shi "Aber egal, wie oft ich es versuche."

# game/christmas.rpy:2758
translate german christmas_shogukuni_08_569d585f:

    # shi "No importa que tan duro entrene..."
    shi "Egal, wie hart du trainierst..."

# game/christmas.rpy:2759
translate german christmas_shogukuni_08_2c51e9a9:

    # shi "Siempre aparece una dificultad que me impide alcanzar esa meta."
    shi "Es gibt immer eine Schwierigkeit, die mich daran hindert, dieses Ziel zu erreichen."

# game/christmas.rpy:2761
translate german christmas_shogukuni_08_e6c8e71a:

    # shi "Y yo..."
    shi "Und ich..."

# game/christmas.rpy:2762
translate german christmas_shogukuni_08_3c053c53:

    # shi "Solo veía a Yui como una barrera que debía destrozar para estar donde quería estar."
    shi "Ich sah Yui nur als ein Hindernis, das ich überwinden musste, um dahin zu gelangen, wo ich hinwollte."

# game/christmas.rpy:2764
translate german christmas_shogukuni_08_bccba2ce:

    # shi "Pero ella siempre me ha tratado con amabilidad..."
    shi "Aber sie hat mich immer freundlich behandelt..."

# game/christmas.rpy:2765
translate german christmas_shogukuni_08_5696da05:

    # shi "Sin importar que tan mala haya sido en el pasado."
    shi "Egal, wie schlecht ich in der Vergangenheit war."

# game/christmas.rpy:2767
translate german christmas_shogukuni_08_8af41b6b:

    # mc "¿No te has dado cuenta aún verdad?"
    mc "Du hast es noch nicht bemerkt, oder?"

# game/christmas.rpy:2769
translate german christmas_shogukuni_08_aa2d7773:

    # shi "¿Qué cosa?"
    shi "Was?"

# game/christmas.rpy:2771
translate german christmas_shogukuni_08_7815671d:

    # mc "Perder la pelea de entrenamiento por una tontería te ha vuelto más fuerte."
    mc "Der Trainingskampf wegen einer Kleinigkeit verloren zu haben, hat dich stärker gemacht."

# game/christmas.rpy:2773
translate german christmas_shogukuni_08_441cb000:

    # shi "¿Qué quieres decir?"
    shi "Was meinst du damit?"

# game/christmas.rpy:2775
translate german christmas_shogukuni_08_e7595b12:

    # mc "Tu nivel de exigencia era tan exagerado, que no te permitías fallar ni cometer errores."
    mc "Deine Ansprüche waren so übertrieben, dass du dir keine Fehler oder Misserfolge erlaubtest."

# game/christmas.rpy:2776
translate german christmas_shogukuni_08_5fe9e229:

    # mc "Si sucedía lo inesperado y perdías, entonces te veias a ti misma como un fracaso absoluto."
    mc "Wenn das Unerwartete eintrat und man verlor, dann war man ein absoluter Versager."

# game/christmas.rpy:2777
translate german christmas_shogukuni_08_49578422:

    # mc "Pero cuando logras aceptar esos errores, es cuando puedes levantarte con más fuerza."
    mc "Aber wenn man es schafft, diese Fehler zu akzeptieren, kann man gestärkt daraus hervorgehen."

# game/christmas.rpy:2778
translate german christmas_shogukuni_08_a034a89d:

    # mc "Porque la vida esta llena de sucesos inesperados que escapan de nuestro control."
    mc "Weil das Leben voller unerwarteter Ereignisse ist, die ausserhalb unserer Kontrolle liegen."

# game/christmas.rpy:2779
translate german christmas_shogukuni_08_363c1467:

    # mc "Está bien aceptar eso y seguir adelante..."
    mc "Es ist in Ordnung, das zu akzeptieren und weiterzumachen..."

# game/christmas.rpy:2780
translate german christmas_shogukuni_08_85715c7d:

    # mc "Es la lógica que debes usar si quieres ganar en las cartas."
    mc "Das ist die Logik, die du anwenden musst, wenn du beim Kartenspiel gewinnen willst."

# game/christmas.rpy:2781
translate german christmas_shogukuni_08_88e4740e:

    # mc "A veces puedes tener una buena mano y otras no, lo que importa es mantener la calma y hacer la mejor jugada."
    mc "Manchmal hat man ein gutes Blatt, manchmal nicht. Wichtig ist, ruhig zu bleiben und das Beste daraus zu machen."

# game/christmas.rpy:2782
translate german christmas_shogukuni_08_d2474156:

    # mc "Y saber identificar las derrotas antes de que estas sucedan, te permite minimizar las perdidas."
    mc "Und wenn man weiss, wie man Niederlagen erkennt, bevor sie eintreten, kann man Verluste minimieren."

# game/christmas.rpy:2783
translate german christmas_shogukuni_08_78e35fdf:

    # mc "De esa forma puede seguir estando dentro del juego para robar una nueva mano de cartas."
    mc "Auf diese Weise kannst du im Spiel bleiben, um eine neue Hand Karten zu ziehen."

# game/christmas.rpy:2784
translate german christmas_shogukuni_08_8bd9fb75:

    # mc "En cambio, tú estabas apostándolo todo en cada mano que jugabas y eso te haría perder en cualquier momento."
    mc "Stattdessen hast du bei jeder Hand alles gesetzt, und das würde dazu führen, dass du jederzeit verlieren könntest."

# game/christmas.rpy:2785
translate german christmas_shogukuni_08_b409660c:

    # mc "Incluso por una tontería como distraerte discutiendo con Kaede."
    mc "Selbst für so etwas Dummes wie sich selbst abzulenken, indem man sich mit Kaede streitet."

# game/christmas.rpy:2787
translate german christmas_shogukuni_08_20537818:

    # shi "No lo había visto de esa forma..."
    shi "So hatte ich das noch nicht gesehen..."

# game/christmas.rpy:2789
translate german christmas_shogukuni_08_1db1167e:

    # mc "En el póker a veces debes hacer creer a tu oponente que tiene el juego ganado, para luego dar vuelta la jugada."
    mc "Beim Poker muss man manchmal den Gegner glauben lassen, dass er das Spiel gewonnen hat, um dann das Blatt zu wenden."

# game/christmas.rpy:2790
translate german christmas_shogukuni_08_c317daf6:

    # mc "Lo que quiero decir es que... {w}Antes que saber como ganar, debes saber como perder."
    mc "Was ich meine ist... {w}Bevor man weiß, wie man gewinnt, muss man wissen, wie man verliert."

# game/christmas.rpy:2792
translate german christmas_shogukuni_08_665e4940:

    # shi "Entonces he aprendido a perder."
    shi "Also habe ich gelernt zu verlieren."

# game/christmas.rpy:2794
translate german christmas_shogukuni_08_203a29b1:

    # mc "Si no conoces la derrota, tampoco sabes si estás consiguiendo la victoria por habilidad o por suerte."
    mc "If you don't know defeat, you also don't know if you are achieving victory by skill or by luck."

# game/christmas.rpy:2796
translate german christmas_shogukuni_08_3c818038:

    # shi "Solo quería taparles la boca a todos..."
    shi "Ich wollte sie einfach alle zum Schweigen bringen..."

# game/christmas.rpy:2797
translate german christmas_shogukuni_08_a505a190:

    # shi "Y si es verdad lo que dices, significa que aún me queda mucho por aprender."
    shi "Und wenn das, was Du sagst, wahr ist, bedeutet das, dass ich noch viel zu lernen habe."

# game/christmas.rpy:2798
translate german christmas_shogukuni_08_329f88e7:

    # shi "Al final... {w}Quizás nunca pueda lograrlo... {w}Seguiré siendo un fracaso..."
    shi ""

# game/christmas.rpy:2799
translate german christmas_shogukuni_08_2c942f97:

    # shi "Debería rendirme y aceptar mi derrota."
    shi "Ich sollte aufgeben und meine Niederlage akzeptieren."

# game/christmas.rpy:2801
translate german christmas_shogukuni_08_e8565001:

    # mc "No tiene sentido seguir apostando si sabes que vas a perder de todas formas."
    mc "Es macht keinen Sinn, weiter zu wetten, wenn man weiss, dass man sowieso verlieren wird."

# game/christmas.rpy:2803
translate german christmas_shogukuni_08_95986846:

    # shi "¿Y qué piensas tú?"
    shi "And what do you think?"

# game/christmas.rpy:2804
translate german christmas_shogukuni_08_a84cf286:

    # shi "¿Crees que alguien como yo podría llegar a convertirse en el Dragón Negro?"
    shi "Glaubst du, dass jemand wie ich jemals zum Schwarzen Drachen werden könnte?"

# game/christmas.rpy:2827
translate german christmas_shogukuni_08_311e5c54:

    # mc "Sigue así, estoy seguro de que lo vas a lograr antes de lo que te imaginas."
    mc "Weiter so, ich bin sicher, du wirst es schneller schaffen, als du denkst."

# game/christmas.rpy:2829
translate german christmas_shogukuni_08_d033c036:

    # shi "Gracias [nombre]."
    shi "Danke [nombre]."

# game/christmas.rpy:2831
translate german christmas_shogukuni_08_f755799c_6:

    # shi "..."
    shi "..."

# game/christmas.rpy:2833
translate german christmas_shogukuni_08_6a3616a6:

    # shi "Espero que no le digas a nada de esta conversación a Kaede o te mato."
    shi "Ich hoffe, du erzählst Kaede nichts von diesem Gespräch, sonst bringe ich dich um."

# game/christmas.rpy:2835
translate german christmas_shogukuni_08_a70c2e34:

    # mc "No te preocupes, aprecio mi vida lo suficiente."
    mc "Keine Sorge, ich weiss mein Leben zu schätzen."

# game/christmas.rpy:2837
translate german christmas_shogukuni_08_6d748f1c:

    # shi "Lo de matarte era una broma."
    shi "Der Teil, in dem es darum ging, dich zu töten, war ein Scherz."

# game/christmas.rpy:2838
translate german christmas_shogukuni_08_78495cfa:

    # shi "Ha sido una noche agradable y siento que me he quitado un peso de encima."
    shi "Es war ein schöner Abend und ich fühle mich, als wäre mir eine Last von den Schultern genommen worden."

# game/christmas.rpy:2839
translate german christmas_shogukuni_08_24f2ec2c:

    # shi "Pero ya es hora de ir a dormir."
    shi "Aber es ist Zeit, schlafen zu gehen."

# game/christmas.rpy:2840
translate german christmas_shogukuni_08_1d400e61:

    # shi "Debo levantarme temprano a entrenar si quiero vencer a Yui algún día."
    shi "Ich muss früh aufstehen, um zu trainieren, wenn ich Yui eines Tages schlagen will."

# game/christmas.rpy:2842
translate german christmas_shogukuni_08_1746a976:

    # mc "Es hora de que yo también vuelva al [restaurant]."
    mc "Es ist Zeit für mich, auch ins [restaurant] zurückzukehren."

# game/christmas.rpy:2843
translate german christmas_shogukuni_08_9130595b:

    # mc "Buenas noches, Shizuka."
    mc "Gute Nacht, Shizuka."

# game/christmas.rpy:2845
translate german christmas_shogukuni_08_9b9186ae:

    # shi "Sayonara, [nombre]."
    shi "Sayonara, [nombre]."

# game/christmas.rpy:2850
translate german christmas_shogukuni_08_9cf73a73:

    # "<Después de despedirte de Shizuka, cubres con una manta a Yui y vuelves a tu habitación en él [restaurant].>"
    "<Nachdem du dich von Shizuka verabschiedet hast, deckst du Yui mit einer Decke zu und kehrst in dein Zimmer im [restaurant] zurück.>""

# game/christmas.rpy:2868
translate german christmas_shogukuni_09_d6f81f6b:

    # unknown "¡Ouch!" with vpunch
    unknown "Ouch!" with vpunch

# game/christmas.rpy:2869
translate german christmas_shogukuni_09_d65363b3:

    # unknown "Maldita ventana sin suficiente espacio... {w}Que dolor de trasero..."
    unknown "Verdammtes Fenster ohne genügend Platz... {w}Was für eine Scheiße..."

# game/christmas.rpy:2870
translate german christmas_shogukuni_09_b7db878e:

    # unknown "¡Vamos!" with vpunch
    unknown "Komm schon!" with vpunch

# game/christmas.rpy:2871
translate german christmas_shogukuni_09_e7e0e0b9:

    # unknown "Vamos, no seas holgazán y deja de dormir."
    unknown "Komm schon, sei nicht faul und wach endlich auf."

# game/christmas.rpy:2873
translate german christmas_shogukuni_09_0bf5fe3f:

    # mc "Q-que..."
    mc "W-was?"

# game/christmas.rpy:2878
translate german christmas_shogukuni_09_176bb2c4:

    # mc "¿Kaede?"
    mc "Kaede?"

# game/christmas.rpy:2879
translate german christmas_shogukuni_09_fa2eb0bb:

    # mc "¿Qué estás haciendo aquí?"
    mc "Was tust du hier?"

# game/christmas.rpy:2881
translate german christmas_shogukuni_09_cbaff567:

    # ka "No puedo creer que tengas el sueño tan pesado."
    ka "Ich kann nicht glauben, dass du so einen tiefen Schlaf hast."

# game/christmas.rpy:2882
translate german christmas_shogukuni_09_6e3db8ff:

    # ka "Deberías estar siempre alerta como un buen discípulo de Dragón."
    ka "Als guter Drachen-Schüler solltest du immer wachsam sein."

# game/christmas.rpy:2884
translate german christmas_shogukuni_09_9c3cb875:

    # mc "Si solo vienes a molestar, entonces seguiré durmiendo."
    mc "Wenn du nur gekommen bist, um mich zu ärgern, dann gehe ich wieder schlafen."

# game/christmas.rpy:2886
translate german christmas_shogukuni_09_1aa8f389:

    # mc "No hagas tanto ruido al salir..."
    mc "Mach nicht so viel Lärm, wenn du gehst..."

# game/christmas.rpy:2887
translate german christmas_shogukuni_09_7bcf0ba5:

    # ka "¡No te duermas!" with vpunch
    ka "Schlafen nicht ein!" with vpunch

# game/christmas.rpy:2888
translate german christmas_shogukuni_09_95c0427b:

    # ka "¡Arriba pequeño saltamontes!" with vpunch
    ka "Auf geht's, kleine Heuschrecke!" with vpunch

# game/christmas.rpy:2889
translate german christmas_shogukuni_09_22e7e090:

    # mc "Que molesta..."
    mc "Das nervt..."

# game/christmas.rpy:2890
translate german christmas_shogukuni_09_1d2d770d:

    # mc "Ya voy..."
    mc "Ich komme schon..."

# game/christmas.rpy:2892
translate german christmas_shogukuni_09_28a5080c:

    # mc "¿Qué hora es?"
    mc "Wie spät ist es?"

# game/christmas.rpy:2894
translate german christmas_shogukuni_09_9db01583:

    # ka "Ya sé que es temprano y afuera hace un frío horrible..."
    ka "Ich weiss, es ist früh am Morgen und draußen ist es eiskalt..."

# game/christmas.rpy:2895
translate german christmas_shogukuni_09_3ce326dc:

    # ka "Y que estás mejor estando calentito en tu cama."
    ka "Und dass du besser in deinem warmen Bett bleibst."

# game/christmas.rpy:2896
translate german christmas_shogukuni_09_08a2c3f1:

    # ka "Pero la maestra Kaede debe encomendarte una misión urgente."
    ka "Aber Meister Kaede muss dir eine dringende Mission anvertrauen."

# game/christmas.rpy:2898
translate german christmas_shogukuni_09_b0a1f23a:

    # mc "¿No puedes decirme la misión y dejarme dormir?"
    mc "Kannst du mir nicht einfach die Mission sagen und mich schlafen lassen?"

# game/christmas.rpy:2899
translate german christmas_shogukuni_09_5670a218:

    # mc "Ayer estuve con Shizuka hasta altas horas de la noche..."
    mc "Yesterday I was with Shizuka until late at night..."

# game/christmas.rpy:2901
translate german christmas_shogukuni_09_0f2ae2ba:

    # ka "¿Qué estuvieron haciendo?"
    ka "Was hast du gemacht?"

# game/christmas.rpy:2902
translate german christmas_shogukuni_09_75a4cf8a:

    # ka "No me digas que estuvieron haciendo marranadas."
    ka "Warte, sag mir nicht, dass du ein Verbrechen begangen hast."

# game/christmas.rpy:2904
translate german christmas_shogukuni_09_edd3eaa0:

    # mc "Solo fui a comprobar si le había gustado mi regalo."
    mc "Ich wollte nur nachgesehen, ob ihr mein Geschenk gefallen hat."

# game/christmas.rpy:2906
translate german christmas_shogukuni_09_ef191308:

    # ka "Eso suena super aburrido..."
    ka "Das klingt ja super langweilig..."

# game/christmas.rpy:2907
translate german christmas_shogukuni_09_c566d722:

    # ka "Y yo que pensaba que Shizuka sentía cosas por ti y tomaría ventaja de la situación."
    ka "Und ich dachte, Shizuka hätte Gefühle für dich und würde die Situation ausnutzen."

# game/christmas.rpy:2909
translate german christmas_shogukuni_09_2c389d49:

    # mc "¿Qué dices de Shizuka?"
    mc "Moment, was hast du gerade gesagt?"

# game/christmas.rpy:2911
translate german christmas_shogukuni_09_f307e02a:

    # ka "No sé dé que hablas... {w}Yo no he dicho nada..."
    ka "Ich weiß nicht, wovon du sprichst... {w}Ich habe nichts gesagt..."

# game/christmas.rpy:2912
translate german christmas_shogukuni_09_66be00f7:

    # ka "De todas formas eso no importa, ahora pon atención porque voy a darte los detalles de tu misión."
    ka "Wie auch immer, das spielt keine Rolle. Jetzt pass gut auf, denn ich werde dir die Details deiner Mission mitteilen."

# game/christmas.rpy:2913
translate german christmas_shogukuni_09_49042646:

    # ka "La cosa es que hoy, yo he despertado antes que Shizuka."
    ka "Die Sache ist die: Heute bin ich vor Shizuka aufgewacht."

# game/christmas.rpy:2915
translate german christmas_shogukuni_09_dd725ef8:

    # mc "¿Qué hay con eso?"
    mc "Und was ist damit?"

# game/christmas.rpy:2917
translate german christmas_shogukuni_09_6564b3ec:

    # ka "Que Shizuka siempre despierta unas horas antes que yo."
    ka "Dass Shizuka immer ein paar Stunden vor mir aufwacht."

# game/christmas.rpy:2918
translate german christmas_shogukuni_09_b40fd619:

    # ka "Hoy estaba actuando extraño..."
    ka "Heute hat sie sich seltsam verhalten..."

# game/christmas.rpy:2920
translate german christmas_shogukuni_09_b54895c3:

    # mc "¿Por qué no preguntaste que sucedía con ella?"
    mc "Warum hast du nicht gefragt, was mit ihr los war?"

# game/christmas.rpy:2922
translate german christmas_shogukuni_09_312dd33c:

    # ka "¿Para qué me gane unas nalgadas?"
    ka "Damit ich ein paar Schläge auf den Arsch bekomme...?"

# game/christmas.rpy:2923
translate german christmas_shogukuni_09_5c8de1c4:

    # ka "¡No gracias!"
    ka "Danke, aber nein danke."

# game/christmas.rpy:2925
translate german christmas_shogukuni_09_87c654b3:

    # ka "Y ahora mismo debo ir al Yorokobi para recibir mi regalo del amigo secreto."
    ka "Und jetzt muss ich zum Yorokobi gehen, um mein Geschenk abholen."

# game/christmas.rpy:2927
translate german christmas_shogukuni_09_0abb1748:

    # mc "El regalo que tu misma te diste..."
    mc "Das Geschenk, das du dir selbst gemacht hast..."

# game/christmas.rpy:2929
translate german christmas_shogukuni_09_fee3a7b8:

    # ka "Corrección... {w}El regalo de Kaede para Kaede."
    ka "Korrektur... {w}Kaedes Geschenk an Kaede."

# game/christmas.rpy:2931
translate german christmas_shogukuni_09_1090c9b4:

    # mc "Entonces quieres que yo vaya al Shogukuni para ver que sucede con Shizuka."
    mc "Dann möchtest du, dass ich zum Shogukuni gehe, um zu sehen, was mit Shizuka los ist."

# game/christmas.rpy:2933
translate german christmas_shogukuni_09_523543c0:

    # ka "¿Acaso no tengo el discípulo más inteligente del mundo?"
    ka "Habe ich nicht den klügsten Schüler der Welt?"

# game/christmas.rpy:2934
translate german christmas_shogukuni_09_ca3028c1:

    # ka "¿Entonces aceptas la misión?"
    ka "Du nimmst also den Auftrag an?"

# game/christmas.rpy:2936
translate german christmas_shogukuni_09_ba021737:

    # mc "Si lo que dices es verdad, entonces no tengo elección."
    mc "Wenn das, was du sagst, wahr ist, dann habe ich keine Wahl."

# game/christmas.rpy:2938
translate german christmas_shogukuni_09_e66ad432:

    # ka "¡Genial!"
    ka "Super!"

# game/christmas.rpy:2939
translate german christmas_shogukuni_09_1afb3d8d:

    # ka "Recuérdame darte una estrellita como premio."
    ka "Erinnere mich daran, dir einen Stern als Belohnung zu geben."

# game/christmas.rpy:2940
translate german christmas_shogukuni_09_e661c323:

    # ka "Nota personal... {w}Debemos dar estrellitas autoadhesivas a los peregrinos por buen comportamiento."
    ka "Persönliche Notiz... {w}Wir sollten Pilgern für gutes Benehmen Aufkleber geben."

# game/christmas.rpy:2941
translate german christmas_shogukuni_09_0ea89513:

    # ka "¡Ahora si podré ir al Yorokobi y comer todo lo que pueda!"
    ka "Jetzt kann ich ins Yorokobi gehen und so viel essen, wie ich will!"

# game/christmas.rpy:2943
translate german christmas_shogukuni_09_1c5e9971:

    # mc "Yo iré al Shogukuni ahora mismo..."
    mc "Ich werde sofort nach Shogukuni gehen..."

# game/christmas.rpy:2969
translate german christmas_shogukuni_09_2b35b6fe:

    # mc "..."
    mc "..."

# game/christmas.rpy:2970
translate german christmas_shogukuni_09_c10e0b94:

    # mc "(No hay nadie aquí ni en el Dojo.)"
    mc "(Hier und im Dojo ist niemand.)"

# game/christmas.rpy:2971
translate german christmas_shogukuni_09_45fa7f59:

    # mc "(Seguramente está cansada por lo de anoche y necesitaba dormir.)"
    mc "(Sie ist wahrscheinlich müde von letzter Nacht und musste schlafen.)"

# game/christmas.rpy:2972
translate german christmas_shogukuni_09_abf9a7e6:

    # mc "(Supongo que Shizuka debe seguir en su habitación.)"
    mc "(Ich vermute, Shizuka ist noch in ihrem Zimmer.)"

# game/christmas.rpy:2973
translate german christmas_shogukuni_09_bf9fe0ed:

    # mc "(Solo espero que no se trate de una broma de Kaede.)"
    mc "(Ich hoffe nur, dass es nicht Kaedes Scherz ist.)"

# game/christmas.rpy:2979
translate german christmas_shogukuni_09_2b35b6fe_1:

    # mc "..."
    mc "..."

# game/christmas.rpy:2980
translate german christmas_shogukuni_09_7c143a7d:

    # mc "Oh mierda..."
    mc "Oh Scheisse..."

# game/christmas.rpy:2981
translate german christmas_shogukuni_09_d6c20bd9:

    # mc "(Espero que no sea una de tus bromas Kaede.)"
    mc "(Ich hoffe, das ist nicht wieder einer deiner Witze, Kaede.)"

# game/christmas.rpy:2982
translate german christmas_shogukuni_09_bb2a8845:

    # mc "(¡Tenías que haberme dicho que Shizuka estaba desnuda!)"
    mc "(Du hättest mir sagen sollen, dass Shizuka nackt war!)"

# game/christmas.rpy:2983
translate german christmas_shogukuni_09_22bbf484:

    # mc "(Si despierta estoy muerto.)"
    mc "(Wenn sie aufwacht, bin ich ein toter Mann.)"

# game/christmas.rpy:2985
translate german christmas_shogukuni_09_2b35b6fe_2:

    # mc "..."
    mc "..."

# game/christmas.rpy:2986
translate german christmas_shogukuni_09_8951ebb5:

    # mc "(Pero como lo suponía... {w}Ella sigue durmiendo.)"
    mc "(Aber wie ich vermutet habe... {w}Sie schläft noch.)"

# game/christmas.rpy:2987
translate german christmas_shogukuni_09_632db5b2:

    # mc "(Que situación más incómoda...)"
    mc "(Was für eine unangenehme Situation...)"

# game/christmas.rpy:2989
translate german christmas_shogukuni_09_f755799c:

    # shi "..."
    shi "..."

# game/christmas.rpy:2990
translate german christmas_shogukuni_09_fafe8cf2:

    # mc "(¿Qué es lo que supone que debería hacer?)"
    mc "(Was soll ich tun?)"

# game/christmas.rpy:2991
translate german christmas_shogukuni_09_88448ff3:

    # mc "(No quiero que despierte de un susto.)"
    mc "(Ich möchte nicht, dass sie vor Schreck aufwacht.)"

# game/christmas.rpy:2992
translate german christmas_shogukuni_09_046034b8:

    # mc "(Quizás despertarla de forma más delicada y preguntar qué sucede.)"
    mc "(Vielleicht, wenn ich sie sanfter wecke und frage, was los ist.)"

# game/christmas.rpy:2993
translate german christmas_shogukuni_09_3d780d10:

    # mc "(Suponiendo que no se trate de una broma de Kaede...)"
    mc "(Vorausgesetzt, das ist kein Scherz von Kaede...)"

# game/christmas.rpy:2994
translate german christmas_shogukuni_09_eb46230d:

    # mc "(O podría tentar a la suerte.)"
    mc "(Oder ich könnte mein Glück versuchen.)"

# game/christmas.rpy:2995
translate german christmas_shogukuni_09_f2df0ec0:

    # mc "(Tomar un poco de ventaja de la situación no suena tan mal.)"
    mc "(Die Situation ein wenig auszunutzen, klingt gar nicht so schlecht.)"

# game/christmas.rpy:2996
translate german christmas_shogukuni_09_e961b7bc:

    # mc "(Si despierta cabreada, moriré de todas formas.)"
    mc "(Wenn sie wütend aufwacht, sterbe ich sowieso.)"

# game/christmas.rpy:2997
translate german christmas_shogukuni_09_8f0a78dc:

    # mc "(Me gustaría escuchar a mi subconsciente...)"
    mc "(Ich möchte auf mein Unterbewusstsein hören...)"

# game/christmas.rpy:2998
translate german christmas_shogukuni_09_416861ce:

    # mc "(¿Qué es lo que debería hacer?)"
    mc "(Was soll ich tun?)"

# game/christmas.rpy:3001
translate german christmas_shogukuni_09_913ae9ab:

    # mc "(Como dice el dicho, la ocasión hace al ladrón.)"
    mc "(Wie das Sprichwort sagt: Die Gelegenheit macht den Dieb.)"

# game/christmas.rpy:3002
translate german christmas_shogukuni_09_f31d5cf7:

    # mc "(Así que aquí vamos...)"
    mc "(Also, los geht's...)"

# game/christmas.rpy:3003
translate german christmas_shogukuni_09_3ce91b8b:

    # mc "(Tengo que acercarme un poco más.)"
    mc "(Nur noch ein bisschen näher.)"

# game/christmas.rpy:3004
translate german christmas_shogukuni_09_75b12315:

    # mc "(Un solo crujido del suelo y estoy muerto.)"
    mc "(Ein einziger Knirsch auf dem Boden und ich bin tot.)"

# game/christmas.rpy:3006
translate german christmas_shogukuni_09_f755799c_1:

    # shi "..."
    shi "..."

# game/christmas.rpy:3007
translate german christmas_shogukuni_09_18fa410c:

    # mc "(Creo que no se ha dado cuenta...)"
    mc "(Sie hat es nicht bemerkt... {w}Oder?)"

